#. Extension description
#.
msgctxt "extension_description"
msgid "Easily override the browser's User-Agent string"
msgstr ""

#. Text of checkbox option to show/hide the two to four letter abbreviations displayed ontop of the browser action icon
#.
msgctxt "option_badge_text"
msgid "Show User-Agent as badge text on icon?"
msgstr ""

#. Text of checkbox option to force the extension popup to a static/larger size than it would normally be according to the browser defaults (note that this also has a description attached)
#.
msgctxt "option_popup_override_size"
msgid "Force default popup/menu size"
msgstr ""

#. Description text of checkbox option to force the extension popup to a static/larger size than it would normally be according to the browser defaults
#.
msgctxt "option_popup_override_size_description"
msgid "Enable if popup is rendered too small on your device"
msgstr ""

#. Text of numeric option (0-100) for setting by how many per-cent the random-interval value should (on avarage) be varied
#.
msgctxt "option_random_jitter"
msgid "Random mode variance"
msgstr ""

#. Header text of the entries editor in the extension options
#.
msgctxt "options_entries_caption"
msgid "User-Agent entries"
msgstr ""

#. Options page link for loading the entries defaults
#.
msgctxt "options_entries_reset"
msgid "Reset to default values"
msgstr ""

#. Text of popup that is shown after the reset link has been clicked
#.
msgctxt "options_entries_reset_undo_text"
msgid "Default values restored"
msgstr ""

#. Label of button for undoing a reset (displayed below the text that asserts that the options have just been reset)
#.
msgctxt "options_entries_reset_undo_button"
msgid "Undo reset"
msgstr ""

#. Options page link for switching from table to text mode
#.
msgctxt "options_entries_switch_text"
msgid "Switch to Text Mode"
msgstr ""

#. Options page link for switching from text to table mode
#.
msgctxt "options_entries_switch_table"
msgid "Switch to Table Mode"
msgstr ""

#. Options page table editor heading for the column allowing to enabled or disable items
#.
msgctxt "options_entries_heading_enabled"
msgid "Enabled"
msgstr ""

#. Options page table editor heading for the column with user-facing User-Agent-String labels
#.
msgctxt "options_entries_heading_label"
msgid "Label"
msgstr ""

#. Options page table editor heading for the column with general category of the given User-Agent-String
#.
msgctxt "options_entries_heading_category"
msgid "Category"
msgstr ""

#. Options page table editor heading for the column with the actual server-reported User-Agent-Strings
#.
msgctxt "options_entries_heading_string"
msgid "String"
msgstr ""

#. Default category label for browsers on desktop devices
#.
msgctxt "useragent_category_desktop"
msgid "Desktop"
msgstr ""

#. Default category label for browsers on mobile devices
#.
msgctxt "useragent_category_mobile"
msgid "Mobile"
msgstr ""

#. Default category label for search engine bots
#.
msgctxt "useragent_category_bot"
msgid "Bot"
msgstr ""

#. Default category label for unclassified client software
#.
msgctxt "useragent_category_other"
msgid "Other"
msgstr ""

#. Text displayed in the browser action icon tooltip when the User-Agent header is choosen at random
#.
msgctxt "icon_title_random"
msgid "Random Mode"
msgstr ""

#. Text displayed in the browser action icon tooltip when request processing is being done because of the global setting
#.
msgctxt "icon_title_enabled"
msgid "Enabled"
msgstr ""

#. Text displayed in the browser action icon tooltip when no request processing is being done because of the global setting
#.
msgctxt "icon_title_disabled"
msgid "Disabled"
msgstr ""

#. Text displayed in the browser action icon tooltip when request processing is being done because of an override being set
#.
msgctxt "icon_title_override_enabled"
msgid "Enabled by Override"
msgstr ""

#. Text displayed in the browser action icon tooltip when no request processing is being done because of an override being set
#.
msgctxt "icon_title_override_disabled"
msgid "Disabled by Override"
msgstr ""

#. The label of the menu item for opening the extension's preference page
#.
msgctxt "popup_item_options"
msgid "Preferences…"
msgstr ""

#. Label of the checkbox that can be used to switch from setting the global User-Agent string to a per-site value
#.
msgctxt "popup_item_override"
msgid "Override for Domain"
msgstr ""

#. Label of an individual selectable override domain
#.
#, case-insensitive, placeholders:$HOSTNAME$
msgctxt "popup_item_override_tpl"
msgid "“$HOSTNAME$”"
msgstr ""

#. The label of the menu item for enabling time-based User-Agent randomization
#.
msgctxt "popup_item_random"
msgid "Enable Random Mode"
msgstr ""

#. The section header of the categories selection of random selection mode
#.
msgctxt "popup_item_random_categories"
msgid "Categories:"
msgstr ""

#. The label of the menu item for enabling the time-based User-Agent randomization only on extension and browser startup
#.
msgctxt "popup_item_random_interval_mode_restart"
msgid "Only on startup"
msgstr ""

#. The label of the menu item for enabling the time-based User-Agent randomization at regular intervals
#.
msgctxt "popup_item_random_interval_mode_timed"
msgid "Every:"
msgstr ""

#. A User-Agent randomization time interval
#.
msgctxt "popup_item_random_interval_unit_minutes"
msgid "Minute(s)"
msgstr ""

#. A User-Agent randomization time interval
#.
msgctxt "popup_item_random_interval_unit_hours"
msgid "Hour(s)"
msgstr ""

#. A User-Agent randomization time interval
#.
msgctxt "popup_item_random_interval_unit_days"
msgid "Day(s)"
msgstr ""

