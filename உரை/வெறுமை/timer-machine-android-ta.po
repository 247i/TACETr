#, java-printf-format
msgctxt "billing_loading"
msgid "Loading"
msgstr "ஏற்றுகிறது"

#, java-printf-format
msgctxt "billing_purchase"
msgid "Purchase"
msgstr "வாங்க"

#, java-printf-format
msgctxt "billing_subscribe"
msgid "Subscribe"
msgstr "குழுசேர்"

