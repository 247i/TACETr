#, md-text, safe-html, strict-same
msgid "Difficulty"
msgstr ""

#: %2Blist-item.p
#, md-text, safe-html, strict-same
msgid "**Low Surdo:** easy"
msgstr ""

#: %2Blist-item.p
#, md-text, safe-html, strict-same
msgid "**Mid Surdo:** medium"
msgstr ""

#: %2Blist-item.p
#, md-text, safe-html, strict-same
msgid "**High Surdo:** easy"
msgstr ""

#: %2Blist-item.p
#, md-text, safe-html, strict-same
msgid "**Repi:** hard"
msgstr ""

#: %2Blist-item.p
#, md-text, safe-html, strict-same
