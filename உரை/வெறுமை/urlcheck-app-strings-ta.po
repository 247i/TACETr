msgctxt "menu_resetOrder"
msgid "Reset order"
msgstr "ஆர்டரை மீட்டமை"

#, java-printf-format
msgctxt "menu_toggleDecorations"
msgid "Toggle all titles"
msgstr "எல்லா தலைப்புகளையும் மாற்றவும்"

#, java-printf-format
msgctxt "desc_up"
msgid "Move up"
msgstr "மேலே செல்லுங்கள்"

#, java-printf-format
msgctxt "desc_down"
msgid "Move down"
msgstr "கீழே செல்லுங்கள்"

#, java-printf-format
