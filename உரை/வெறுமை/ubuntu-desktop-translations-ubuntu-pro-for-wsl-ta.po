#, icu-message-format
msgctxt "appTitle"
msgid "Ubuntu Pro for WSL"
msgstr "WSL க்கான உபுண்டு புரோ"

#, icu-message-format
msgctxt "tokenErrorEmpty"
msgid "Token cannot be empty"
msgstr "கிள்ளாக்கு காலியாக இருக்க முடியாது"

#, icu-message-format
msgctxt "tokenErrorInvalid"
msgid "Invalid token"
msgstr "தவறான கிள்ளாக்கு"

#, icu-message-format
msgctxt "tokenValid"
msgid "Valid token"
msgstr "செல்லுபடியாகும் கிள்ளாக்கு"

#, icu-message-format
msgctxt "tokenInputTitle"
msgid "Attach this machine"
msgstr "இந்த இயந்திரத்தை இணைக்கவும்"

#, case-insensitive, icu-message-format, placeholders:$PROLINK$
msgctxt "tokenInputDescription"
msgid "Enter a token from {proLink} or your system administrator"
msgstr ""
"{proLink} அல்லது உங்கள் கணினி நிர்வாகியிலிருந்து ஒரு கிள்ளாக்கை உள்ளிடவும்"

#, icu-message-format
msgctxt "tokenInputHint"
msgid "Token"
msgstr "கிள்ளாக்கு"

#, icu-message-format
msgctxt "attach"
msgid "Attach"
msgstr "இணைக்கவும்"

#, icu-message-format
msgctxt "applyProToken"
msgid "Apply Pro Token"
msgstr "புரோ கிள்ளாக்கைப் பயன்படுத்துங்கள்"

#, case-insensitive, icu-message-format, placeholders:$TOKEN$
msgctxt "applyingProToken"
msgid "Applying token {token}"
msgstr "கிள்ளாக்கைப் பயன்படுத்துதல் {token}"

#, icu-message-format
msgctxt "agent"
msgid "Ubuntu Pro background agent"
msgstr "உபுண்டு புரோ பின்னணி முகவர்"

#, icu-message-format
msgctxt "agentStateStarting"
msgid "Ubuntu Pro background agent is starting."
msgstr "உபுண்டு புரோ பின்னணி முகவர் தொடங்குகிறது."

#, icu-message-format
msgctxt "agentStatePingNonResponsive"
msgid "Ubuntu Pro background agent is not responding."
msgstr "உபுண்டு புரோ பின்னணி முகவர் பதிலளிக்கவில்லை."

#, icu-message-format
msgctxt "agentStateInvalid"
msgid "Waiting for Ubuntu Pro background agent initialization."
msgstr "உபுண்டு புரோ பின்னணி முகவர் துவக்கத்திற்காக காத்திருக்கிறது."

#, icu-message-format
msgctxt "agentStateCannotStart"
msgid "Ubuntu Pro background agent cannot be started."
msgstr "உபுண்டு புரோ பின்னணி முகவரைத் தொடங்க முடியாது."

#, icu-message-format
msgctxt "agentStateUnknownEnv"
msgid ""
"Ubuntu Pro background agent state cannot be verified. Check your environment"
" settings."
msgstr ""
"உபுண்டு புரோ பின்னணி முகவர் நிலையை சரிபார்க்க முடியாது. உங்கள் சுற்றுச்சூழல்"
" அமைப்புகளை சரிபார்க்கவும்."

#, icu-message-format
msgctxt "agentStateQuerying"
msgid "Checking Ubuntu Pro background agent's state."
msgstr "உபுண்டு புரோ பின்னணி முகவரின் நிலையை சரிபார்க்கிறது."

#, icu-message-format
msgctxt "agentStateUnreachable"
msgid "Attempting to recover Ubuntu Pro background agent."
msgstr "உபுண்டு புரோ பின்னணி முகவரை மீட்டெடுக்க முயற்சிக்கிறது."

#, case-insensitive, icu-message-format, placeholders:$ABOUTLINK$
msgctxt "proHeading"
msgid ""
"Open source software security by the publishers of Ubuntu, now available on "
"Windows. {aboutLink}"
msgstr ""
"உபுண்டுவின் வெளியீட்டாளர்களின் திறந்த மூல மென்பொருள் பாதுகாப்பு, இப்போது "
"சாளரங்களில் கிடைக்கிறது. {aboutLink}"

#, icu-message-format
msgctxt "getUbuntuPro"
msgid "Get Ubuntu Pro"
msgstr "உபுண்டு புரோவைப் பெறுங்கள்"

#, icu-message-format
msgctxt "learnMore"
msgid "Learn more"
msgstr "மேலும் அறிக"

#, icu-message-format
msgctxt "ubuntuProEnabled"
msgid "Ubuntu Pro is enabled on this machine"
msgstr "இந்த கணினியில் உபுண்டு புரோ இயக்கப்பட்டது"

#, icu-message-format
msgctxt "ubuntuProEnabledInfo"
msgid "All Ubuntu WSL instances have access to Ubuntu Pro security features."
msgstr ""
"அனைத்து உபுண்டு WSL நிகழ்வுகளும் உபுண்டு புரோ பாதுகாப்பு அம்சங்களுக்கான "
"அணுகலைக் கொண்டுள்ளன."

#, icu-message-format
msgctxt "manageUbuntuPro"
msgid "Manage Ubuntu Pro subscription"
msgstr "உபுண்டு புரோ சந்தாவை நிர்வகிக்கவும்"

#, icu-message-format
msgctxt "detachPro"
msgid "Detach Ubuntu Pro"
msgstr "உபுண்டு புரோவை பிரிக்கவும்"

#, icu-message-format
msgctxt "updatingSubscriptionInfo"
msgid "Updating the subscription information."
msgstr "சந்தா தகவலைப் புதுப்பித்தல்."

#, icu-message-format
msgctxt "purchaseStatusSuccess"
msgid "Thank you for subscribing to Ubuntu Pro."
msgstr "உபுண்டு புரோவுக்கு குழுசேர்ந்ததற்கு நன்றி."

#, icu-message-format
msgctxt "purchaseStatusAlreadyPurchased"
msgid "You are already subscribed to Pro."
msgstr "நீங்கள் ஏற்கனவே புரோவுக்கு குழுசேர்ந்தீர்கள்."

#, icu-message-format
msgctxt "purchaseStatusUserGaveUp"
msgid "Purchase attempt was canceled."
msgstr "கொள்முதல் முயற்சி ரத்து செய்யப்பட்டது."

#, icu-message-format
msgctxt "purchaseStatusNetwork"
msgid ""
"A network issue prevented the purchase to proceed. Please try again once "
"connected."
msgstr ""
"ஒரு பிணைய சிக்கல் வாங்குவதைத் தடுத்தது. இணைக்கப்பட்டவுடன் மீண்டும் "
"முயற்சிக்கவும்."

#, icu-message-format
msgctxt "purchaseStatusServer"
msgid "Something went wrong with Microsoft Store. Please try again later."
msgstr ""
"நுண்மென் ச்டோரில் ஏதோ தவறு ஏற்பட்டது. தயவுசெய்து பின்னர் மீண்டும் "
"முயற்சிக்கவும்."

#, icu-message-format
msgctxt "purchaseStatusUnknown"
msgid ""
"Unknown error when trying to purchase the subscription. Consider restarting "
"this app."
msgstr ""
"சந்தா வாங்க முயற்சிக்கும்போது தெரியாத பிழை. இந்த பயன்பாட்டை மறுதொடக்கம் "
"செய்வதைக் கவனியுங்கள்."

#, icu-message-format
msgctxt "landscapeSkip"
msgid "Skip for now"
msgstr "இப்போதைக்கு தவிர்க்கவும்"

#, icu-message-format
msgctxt "landscapeSkipDescription"
msgid "You can always configure Landscape later in this app"
msgstr "இந்த பயன்பாட்டில் நீங்கள் எப்போதும் நிலப்பரப்பை கட்டமைக்க முடியும்"

#, icu-message-format
msgctxt "landscapeSkipRegister"
msgid "Register with Landscape"
msgstr "நிலப்பரப்பில் பதிவு செய்யுங்கள்"

#, case-insensitive, icu-message-format, placeholders:$LANDSCAPELINK$
msgctxt "landscapeHeading"
msgid ""
"Configure the connection to Landscape to manage your Ubuntu WSL instances "
"remotely. {landscapeLink}"
msgstr ""
"உங்கள் உபுண்டு WSL நிகழ்வுகளை தொலைவிலிருந்து நிர்வகிக்க நிலப்பரப்புக்கான "
"இணைப்பை உள்ளமைக்கவும். {landscapeLink}"

#, icu-message-format
msgctxt "landscapeSetupManual"
msgid "Manual configuration"
msgstr "கையேடு உள்ளமைவு"

#, icu-message-format
msgctxt "landscapeSetupManualHint"
msgid "Register with your own Landscape server"
msgstr "உங்கள் சொந்த இயற்கை சேவையகத்துடன் பதிவு செய்யுங்கள்"

#, icu-message-format
msgctxt "landscapeSetupCustom"
msgid "Advanced configuration"
msgstr "மேம்பட்ட உள்ளமைவு"

#, icu-message-format
msgctxt "landscapeSetupCustomHint"
msgid "Load a custom Landscape client configuration file"
msgstr "தனிப்பயன் இயற்கை கிளையன்ட் உள்ளமைவு கோப்பை ஏற்றவும்"

#, icu-message-format
msgctxt "landscapeFQDNLabel"
msgid "Landscape FQDN"
msgstr "நிலப்பரப்பு fqdn"

#, icu-message-format
msgctxt "landscapeFQDNError"
msgid "Invalid FQDN. Format should be a hostname or IP address."
msgstr ""
"தவறான FQDN. வடிவம் ஒரு ஓச்ட்பெயர் அல்லது ஐபி முகவரியாக இருக்க வேண்டும்."

#, icu-message-format
msgctxt "landscapeFQDNSaaSError"
msgid "FQDN shouldn't be landscape.canonical.com."
msgstr "FQDN LandScape.canonical.com ஆக இருக்கக்கூடாது."

#, icu-message-format
msgctxt "landscapeKeyLabel"
msgid "Registration key (optional)"
msgstr "பதிவு விசை (விரும்பினால்)"

#, icu-message-format
msgctxt "landscapeSSLKeyLabel"
msgid "Server SSL public key (optional)"
msgstr "சேவையகம் SSL பொது விசை (விரும்பினால்)"

#, icu-message-format
msgctxt "landscapeFileLabel"
msgid "Config file path"
msgstr "கோப்பு பாதை"

#, icu-message-format
msgctxt "landscapeFileTooLarge"
msgid "Configuration file is too large"
msgstr "உள்ளமைவு கோப்பு மிகப் பெரியது"

#, icu-message-format
msgctxt "landscapeFileEmptyPath"
msgid "A path must be specified"
msgstr "ஒரு பாதை குறிப்பிடப்பட வேண்டும்"

#, icu-message-format
msgctxt "landscapeFileEmptyContents"
msgid "File must not be empty"
msgstr "கோப்பு காலியாக இருக்கக்கூடாது"

#, icu-message-format
msgctxt "landscapeFileNotFound"
msgid "File not found"
msgstr "கோப்பு கிடைக்கவில்லை"

#, icu-message-format
msgctxt "landscapeFileIsDir"
msgid "File cannot be a directory"
msgstr "கோப்பு ஒரு கோப்பகமாக இருக்க முடியாது"

#, icu-message-format
msgctxt "landscapeFileInvalidFormat"
msgid "File is in an incorrect format"
msgstr "கோப்பு தவறான வடிவத்தில் உள்ளது"

#, icu-message-format
msgctxt "landscapeFilePicker"
msgid "Select file..."
msgstr "கோப்பைத் தேர்ந்தெடுக்கவும் ..."

#, icu-message-format
msgctxt "landscapeConfigureButton"
msgid "Configure Landscape"
msgstr "நிலப்பரப்பை உள்ளமைக்கவும்"

#, case-insensitive, icu-message-format, placeholders:$MESSAGE$
msgctxt "landscapeApplyError"
msgid ""
"Error when attempting to apply the config:\n"
"\t{message}"
msgstr ""
"உள்ளமைவைப் பயன்படுத்த முயற்சிக்கும்போது பிழை:\n"
" {message}"

#, icu-message-format
msgctxt "landscapeRegister"
msgid "Register"
msgstr "பதிவு செய்யுங்கள்"

#, icu-message-format
msgctxt "buttonNext"
msgid "Next"
msgstr "அடுத்தது"

#, icu-message-format
msgctxt "buttonSkip"
msgid "Skip"
msgstr "தவிர்"

#, icu-message-format
msgctxt "buttonBack"
msgid "Back"
msgstr "பின்"

#, icu-message-format
msgctxt "statusBarReportBugTooltip"
msgid "Report a bug"
msgstr "ஒரு பிழையைப் புகாரளிக்கவும்"

#, icu-message-format
msgctxt "statusBarAgentRunningTooltip"
msgid "Ubuntu Pro agent is running"
msgstr "உபுண்டு புரோ முகவர் இயங்குகிறது"

#, icu-message-format
msgctxt "statusBarAgentConnectingTooltip"
msgid "Connecting to Ubuntu Pro agent..."
msgstr "உபுண்டு புரோ முகவருடன் இணைகிறது ..."

#, icu-message-format
msgctxt "statusBarAgentDownTooltip"
msgid "Ubuntu Pro agent is disconnected. Press to start the agent."
msgstr ""
"உபுண்டு புரோ முகவர் துண்டிக்கப்பட்டுள்ளது. முகவரைத் தொடங்க அழுத்தவும்."
