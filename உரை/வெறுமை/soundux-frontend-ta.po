msgctxt "windows.selectMic"
msgid "Please select your microphone and proceed"
msgstr "உங்கள் மைக்ரோஃபோனைத் தேர்ந்தெடுத்து தொடரவும்"

msgctxt "windows.automaticSetup"
msgid "Automatically set-up"
msgstr "தானாக அமைக்கவும்"

msgctxt "windows.administrativePrivilegesNeeded"
msgid "Administrative privileges needed!"
msgstr "நிர்வாக சலுகைகள் தேவை!"

msgctxt "windows.restartAsAdmin"
msgid "Restart as admin"
msgstr "நிர்வாகியாக மறுதொடக்கம் செய்யுங்கள்"

msgctxt "errors.0"
msgid "Failed to play"
msgstr "விளையாடத் தவறிவிட்டது"

msgctxt "errors.1"
msgid "Failed to seek"
msgstr "தேடத் தவறிவிட்டது"

msgctxt "errors.2"
msgid "Failed to pause"
msgstr "இடைநிறுத்துவதில் தோல்வி"

msgctxt "errors.3"
msgid "Failed to repeat"
msgstr "மீண்டும் செய்யத் தவறிவிட்டது"

msgctxt "errors.4"
msgid "Failed to resume"
msgstr "மீண்டும் தொடங்குவதில் தோல்வி"

msgctxt "errors.5"
msgid "Failed to move to sink"
msgstr "மூழ்குவதற்கு தவறிவிட்டது"

msgctxt "errors.6"
msgid "Sound not found"
msgstr "ஒலி கிடைக்கவில்லை"

msgctxt "errors.7"
msgid "Folder does not exist"
msgstr "கோப்புறை இல்லை"

msgctxt "errors.8"
msgid "Tab does not exist"
msgstr "தாவல் இல்லை"

msgctxt "errors.9"
msgid "Failed to set hotkey"
msgstr "ஆட்ச்கி அமைக்கத் தவறிவிட்டது"

msgctxt "errors.10"
msgid "Failed to start passthrough"
msgstr "பாச்ட்ரூவைத் தொடங்குவதில் தோல்வி"

msgctxt "errors.11"
msgid "Failed to move back"
msgstr "திரும்பிச் செல்லத் தவறிவிட்டது"

msgctxt "errors.12"
msgid "Failed to move back passthrough"
msgstr "பாச்ட்ரூவுக்கு திரும்பிச் செல்லத் தவறிவிட்டது"

msgctxt "errors.13"
msgid "Failed to revert default source"
msgstr "இயல்புநிலை மூலத்தை மாற்றுவதில் தோல்வி"

msgctxt "errors.14"
msgid "Failed to set default source"
msgstr "இயல்புநிலை மூலத்தை அமைப்பதில் தோல்வி"

msgctxt "errors.15"
msgid "youtube-dl not found"
msgstr "YouTube-dl கிடைக்கவில்லை"

msgctxt "errors.16"
msgid "Invalid URL"
msgstr "தவறான முகவரி"

msgctxt "errors.17"
msgid "Failed to parse youtube-dl JSON"
msgstr "YouTube-DL சாதொபொகு ஐ அலசத் தவறிவிட்டது"

msgctxt "errors.18"
msgid "Unknown youtube-dl failure"
msgstr "தெரியாத YouTube-DL தோல்வி"

msgctxt "errors.19"
msgid "Failed to delete the file"
msgstr "கோப்பை நீக்குவதில் தோல்வி"

msgctxt "errors.20"
msgid "Failed to mute"
msgstr "முடக்குவதில் தோல்வி"

msgctxt "errors.21"
msgid "Failed to set custom volume"
msgstr "தனிப்பயன் அளவை அமைக்கத் தவறிவிட்டது"

msgctxt "errors.error"
msgid "Error"
msgstr "பிழை"

msgctxt "errors.backendFunctionMissing"
msgid "Backend function missing: {functionName}"
msgstr "பின்தளத்தில் செயல்பாடு இல்லை: {functionName}"
