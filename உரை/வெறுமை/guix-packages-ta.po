msgstr ""

#: gnu/packages/javascript.scm:680
#, ignore-newline-count
msgid "Hybrid widget between a textbox and <select> box"
msgstr ""

#: gnu/packages/javascript.scm:681
#, ignore-newline-count
msgid ""
