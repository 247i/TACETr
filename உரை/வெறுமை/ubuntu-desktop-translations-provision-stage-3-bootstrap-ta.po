#, icu-message-format
msgctxt "appTitle"
msgid "Ubuntu Desktop Installer"
msgstr "உபுண்டு பணிமேடை நிறுவி"

#, case-insensitive, icu-message-format, placeholders:$RELEASE$
msgctxt "windowTitle"
msgid "Install {RELEASE}"
msgstr "நிறுவு{RELEASE}"

#, icu-message-format
msgctxt "autoinstallDirectTitle"
msgid "Automated installation"
msgstr "தானியங்கு நிறுவல்"

#, icu-message-format
msgctxt "autoinstallDirectHeader"
msgid "Import autoinstall file"
msgstr "ஆட்டோஇன்ச்டால் கோப்பை இறக்குமதி செய்க"

#, icu-message-format
msgctxt "autoinstallDirectUrlLabel"
msgid "You can enter the URL of an autoinstall file:"
msgstr "ஆட்டோஇன்ச்டால் கோப்பின் முகவரி ஐ உள்ளிடலாம்:"

#, icu-message-format
msgctxt "autoinstallDirectFileLabel"
msgid "Or select a local file:"
msgstr "அல்லது உள்ளக கோப்பைத் தேர்ந்தெடுக்கவும்:"

#, icu-message-format
msgctxt "autoinstallDirectFileButtonLabel"
msgid "Select file..."
msgstr "கோப்பைத் தேர்ந்தெடுக்கவும் ..."

#, icu-message-format
msgctxt "autoinstallDirectFileClearButtonLabel"
msgid "Clear file"
msgstr "கோப்பை அழிக்கவும்"

#, icu-message-format
msgctxt "autoinstallDirectFilePickerTitle"
msgid "Choose file"
msgstr "கோப்பைத் தேர்வுசெய்க"

#, icu-message-format
msgctxt "autoinstallDirectFilePickerFilterLabel"
msgid "YAML files"
msgstr "யாம்எல் கோப்புகள்"

#, icu-message-format
msgctxt "autoinstallDirectImportButtonLabel"
msgid "Import"
msgstr "இறக்குமதி"

#, icu-message-format
msgctxt "autoinstallDirectErrorNetworkTitle"
msgid "Autoinstall file is not reachable"
msgstr "ஆட்டோஇன்ச்டால் கோப்பு அடைய முடியாது"

#, icu-message-format
msgctxt "autoinstallDirectErrorNetworkBody"
msgid "Check the URL is correct, test your internet connection, or try later."
msgstr ""
"முகவரி சரியானது என்பதை சரிபார்க்கவும், உங்கள் இணைய இணைப்பை சோதிக்கவும் "
"அல்லது பின்னர் முயற்சிக்கவும்."

#, icu-message-format
msgctxt "autoinstallDirectErrorInvalidUrlTitle"
msgid "Invalid URL"
msgstr "தவறான முகவரி"

#, icu-message-format
msgctxt "autoinstallDirectErrorInvalidUrlBody"
msgid "Check the URL is correct or select a local file."
msgstr ""
"முகவரி சரியானது என்பதை சரிபார்க்கவும் அல்லது உள்ளக கோப்பைத் "
"தேர்ந்தெடுக்கவும்."

#, icu-message-format
msgctxt "autoinstallDirectErrorInvalidContentTitle"
msgid "Invalid autoinstall file"
msgstr "தவறான ஆட்டோஇன்ச்டால் கோப்பு"

#, icu-message-format
msgctxt "autoinstallDirectErrorInvalidContentBody"
msgid "Check the file or provide a different one."
msgstr "கோப்பைச் சரிபார்க்கவும் அல்லது வேறு ஒன்றை வழங்கவும்."

#, icu-message-format
msgctxt "autoinstallDirectErrorFileSystemTitle"
msgid "Autoinstall file is not readable"
msgstr "ஆட்டோஇன்ச்டால் கோப்பு படிக்க முடியாது"

#, icu-message-format
msgctxt "autoinstallDirectErrorFileSystemBody"
msgid "Check your permissions or provide a different file."
msgstr "உங்கள் அனுமதிகளை சரிபார்க்கவும் அல்லது வேறு கோப்பை வழங்கவும்."

#, icu-message-format
msgctxt "autoinstallDirectErrorUnkownTitle"
msgid "Unknown error"
msgstr "தெரியாத பிழை"

#, icu-message-format
msgctxt "autoinstallTitle"
msgid "Type of installation"
msgstr "நிறுவல் வகை"

#, case-insensitive, icu-message-format, placeholders:$DISTRO$
msgctxt "autoinstallHeader"
msgid "How would you like to install {DISTRO}?"
msgstr "{DISTRO} ஐ எவ்வாறு நிறுவ விரும்புகிறீர்கள்?"

#, icu-message-format
msgctxt "autoinstallInstructions"
msgid "Enter the autoinstall.yaml URL or local file path:"
msgstr "AutoInstall.yaml முகவரி அல்லது உள்ளக கோப்பு பாதையை உள்ளிடவும்:"

#, icu-message-format
msgctxt "autoinstallInteractiveOption"
msgid "Interactive installation"
msgstr "ஊடாடும் நிறுவல்"

#, icu-message-format
msgctxt "autoinstallInteractiveDescription"
msgid "For users who want to be guided step by step through the installation."
msgstr "நிறுவலின் மூலம் படிப்படியாக வழிநடத்த விரும்பும் பயனர்களுக்கு."

#, icu-message-format
msgctxt "autoinstallDirectOption"
msgid "Automated with autoinstall file"
msgstr "ஆட்டோஇன்ச்டால் கோப்புடன் தானியங்கி"

#, icu-message-format
msgctxt "autoinstallDirectDescription"
msgid ""
"For advanced users who have an autoinstall.yaml for consistent and "
"repeatable system setups."
msgstr ""
"நிலையான மற்றும் மீண்டும் நிகழகூடிய கணினி அமைப்புகளுக்கு "
"ஆட்டோஇன்ச்டால்.யாம்லைக் கொண்ட மேம்பட்ட பயனர்களுக்கு."

#, icu-message-format
msgctxt "autoinstallLandscapeOption"
msgid "Automated with Landscape"
msgstr "நிலப்பரப்புடன் தானியங்கி"

#, icu-message-format
msgctxt "autoinstallLandscapeDescription"
msgid ""
"For users in organizations that provide an autoinstall file via Landscape."
msgstr ""
"நிலப்பரப்பு வழியாக ஆட்டோஇன்ச்டால் கோப்பை வழங்கும் நிறுவனங்களில் உள்ள "
"பயனர்களுக்கு."

#, icu-message-format
msgctxt "changeButtonText"
msgid "Change"
msgstr "மாற்று"

#, icu-message-format
msgctxt "quitButtonText"
msgid "Quit installation"
msgstr "நிறுவலை விட்டு"

#, case-insensitive, icu-message-format, placeholders:$DISTRO$
msgctxt "loadingPageTitle"
msgid "Welcome to {DISTRO}"
msgstr "{DISTRO} வருக"

#, case-insensitive, icu-message-format, placeholders:$DISTRO$
msgctxt "loadingHeader"
msgid "Preparing {DISTRO}…"
msgstr "{DISTRO}…"

#, icu-message-format
msgctxt "warningLabel"
msgid "Warning:"
msgstr "எச்சரிக்கை:"

#, case-insensitive, icu-message-format, placeholders:$DISTRO$
msgctxt "tryOrInstallTitle"
msgid "Try or install {DISTRO}"
msgstr "{DISTRO} முயற்சி அல்லது நிறுவு"

#, case-insensitive, icu-message-format, placeholders:$DISTRO$
msgctxt "tryOrInstallHeader"
msgid "What do you want to do with {DISTRO}?"
msgstr "{DISTRO} உடன் நீங்கள் என்ன செய்ய விரும்புகிறீர்கள்?"

#, icu-message-format
msgctxt "tryOrInstallRepairOption"
msgid "Repair installation"
msgstr "நிறுவலை பழுதுபார்"

#, icu-message-format
msgctxt "tryOrInstallRepairDescription"
msgid ""
"Repairing will reinstall all installed software without touching documents "
"or settings."
msgstr ""
"பழுதுபார்ப்பு ஆவணங்கள் அல்லது அமைப்புகளைத் தொடாமல் நிறுவப்பட்ட அனைத்து "
"மென்பொருளையும் மீண்டும் நிறுவும்."

#, case-insensitive, icu-message-format, placeholders:$RELEASE$
msgctxt "tryOption"
msgid "Try {RELEASE}"
msgstr "சோதி {RELEASE}"

#, case-insensitive, icu-message-format, placeholders:$RELEASE$
msgctxt "tryDescription"
msgid "You can try {RELEASE} without making any changes to your computer."
msgstr "உங்கள் கணினியில் எந்த மாற்றமும் செய்யாமல் {RELEASE}ஐ சோதிக்கலாம்."

#, case-insensitive, icu-message-format, placeholders:$RELEASE$
msgctxt "installOption"
msgid "Install {RELEASE}"
msgstr "நிறுவு{RELEASE}"

#, case-insensitive, icu-message-format, placeholders:$RELEASE$
msgctxt "installDescription"
msgid ""
"Install {RELEASE} alongside (or instead of) your current operating system. "
"This shouldn't take too long."
msgstr ""
"உங்கள் தற்போதைய இயக்க முறைமையுடன் (அல்லது அதற்கு பதிலாக) {RELEASE} ஐ "
"நிறுவவும். இது அதிக நேரம் எடுக்கக்கூடாது."

#, case-insensitive, icu-message-format, placeholders:$URL$
msgctxt "tryOrInstallReleaseNotesLabel"
msgid "You may wish to read the <a href=\"{url}\">release notes</a>."
msgstr ""
"நீங்கள் <a href = \"{url}\"> வெளியீட்டு குறிப்புகள் </a> ஐப் படிக்க "
"விரும்பலாம்."

#, icu-message-format
msgctxt "rstTitle"
msgid "RST detected"
msgstr "RSTயை முடக்கு"

#, icu-message-format
msgctxt "rstHeader"
msgid "You must disable RST to continue installation"
msgstr "நிறுவலைத் தொடர நீங்கள் RST ஐ முடக்க வேண்டும்"

#, icu-message-format
msgctxt "rstDisable"
msgid ""
"Your computer uses Intel RST (Rapid Storage Technology). You can disable RST"
" either in:"
msgstr ""
"உங்கள் கணினி இன்டெல் ஆர்எச்டி (விரைவான சேமிப்பக தொழில்நுட்பம்) "
"பயன்படுத்துகிறது. நீங்கள் rst ஐ முடக்கலாம்:"

#, icu-message-format
msgctxt "rstDisableWindows"
msgid "Windows, if you are using a dual boot setup with Windows"
msgstr ""
"சாளரங்கள், நீங்கள் விண்டோசுடன் இரட்டை துவக்க அமைப்பைப் பயன்படுத்துகிறீர்கள் "
"என்றால்"

#, icu-message-format
msgctxt "rstDisableBios"
msgid "The BIOS settings"
msgstr "பயாச் அமைப்புகள்"

#, case-insensitive, icu-message-format, placeholders:$URL$
msgctxt "rstInstructions"
msgid ""
"For instructions, scan the QR code on another device or visit: <a "
"href=\"https://{url}\">{url}</a>"
msgstr ""
"வழிமுறைகளுக்கு, QR குறியீட்டை மற்றொரு சாதனத்தில் ச்கேன் செய்யுங்கள் அல்லது "
"பார்வையிடவும்: <a href = \"https: // {url}\"> {url} </a>"

#, icu-message-format
msgctxt "configureSecureBootTitle"
msgid "Configure Secure Boot"
msgstr "பாதுகாப்பான துவக்கத்தை உள்ளமைக்கவும்"

#, icu-message-format
msgctxt "configureSecureBootDescription"
msgid ""
"You've chosen to install third-party driver software. This requires turning off Secure Boot.\n"
"To do this, you need to choose a security key now, and enter it when the system restarts."
msgstr ""
"மூன்றாம் தரப்பு இயக்கி மென்பொருளை நிறுவ நீங்கள் தேர்ந்தெடுத்துள்ளீர்கள். இதற்கு பாதுகாப்பான துவக்கத்தை அணைக்க வேண்டும்.\n"
" இதைச் செய்ய, நீங்கள் இப்போது ஒரு பாதுகாப்பு விசையைத் தேர்வு செய்ய வேண்டும், மேலும் கணினி மறுதொடக்கம் செய்யும்போது அதை உள்ளிடவும்."

#, icu-message-format
msgctxt "configureSecureBootOption"
msgid "Configure Secure Boot"
msgstr "பாதுகாப்பான துவக்கத்தை உள்ளமைக்கவும்"

#, icu-message-format
msgctxt "chooseSecurityKey"
msgid "Choose a security key"
msgstr "பாதுகாப்பு விசையைத் தேர்வுசெய்க"

#, icu-message-format
msgctxt "confirmSecurityKey"
msgid "Confirm the security key"
msgstr "பாதுகாப்பு விசையை உறுதிப்படுத்தவும்"

#, icu-message-format
msgctxt "dontInstallDriverSoftwareNow"
msgid "Don't install the driver software for now"
msgstr "இப்போது இயக்கி மென்பொருளை நிறுவ வேண்டாம்"

#, icu-message-format
msgctxt "dontInstallDriverSoftwareNowDescription"
msgid "You can install it later from Software & Updates."
msgstr "மென்பொருள் மற்றும் புதுப்பிப்புகளிலிருந்து அதை பின்னர் நிறுவலாம்."

#, icu-message-format
msgctxt "configureSecureBootSecurityKeyRequired"
msgid "Security key is required"
msgstr "பாதுகாப்பு விசை தேவை"

#, icu-message-format
msgctxt "secureBootSecurityKeysDontMatch"
msgid "Security keys do not match"
msgstr "பாதுகாப்பு விசைகள் பொருந்தவில்லை"

#, icu-message-format
msgctxt "showSecurityKey"
msgid "Show"
msgstr "காட்டு"

#, icu-message-format
msgctxt "hideSecurityKey"
msgid "Hide"
msgstr "மறை"

#, icu-message-format
msgctxt "updatesOtherSoftwarePageTitle"
msgid "Applications"
msgstr "பயன்பாடுகள்"

#, icu-message-format
msgctxt "updatesOtherSoftwarePageDescription"
msgid "What apps would you like to install to start with?"
msgstr "தொடங்க என்ன பயன்பாடுகளை நிறுவ விரும்புகிறீர்கள்?"

#, icu-message-format
msgctxt "codecsAndDriversPageTitle"
msgid "Optimise your computer"
msgstr "உங்கள் கணினியை மேம்படுத்தவும்"

#, icu-message-format
msgctxt "codecsAndDriversPageDescription"
msgid "Install recommended proprietary software?"
msgstr "பரிந்துரைக்கப்பட்ட தனியுரிம மென்பொருளை நிறுவவா?"

#, case-insensitive, icu-message-format, placeholders:$DISTRO$
msgctxt "codecsAndDriversPageBody"
msgid ""
"{DISTRO} ships with no proprietary software by default. Installing "
"additional software may improve your computer's performance."
msgstr ""
"{DISTRO} முன்னிருப்பாக தனியுரிம மென்பொருள் இல்லாத கப்பல்கள். கூடுதல் "
"மென்பொருளை நிறுவுவது உங்கள் கணினியின் செயல்திறனை மேம்படுத்தலாம்."

#, icu-message-format
msgctxt "codecsAndDriversNvidiaNote"
msgid "NVIDIA graphics card detected"
msgstr "என்விடியா கிராபிக்ச் அட்டை கண்டறியப்பட்டது"

#, icu-message-format
msgctxt "codecsAndDriversNvidiaBody"
msgid ""
"For the best performance of NVIDIA graphic cards, installing additional "
"drivers is strongly recommended."
msgstr ""
"என்விடியா கிராஃபிக் கார்டுகளின் சிறந்த செயல்திறனுக்காக, கூடுதல் இயக்கிகளை "
"நிறுவுவது கடுமையாக பரிந்துரைக்கப்படுகிறது."

#, icu-message-format
msgctxt "fullInstallationTitle"
msgid "Extended selection"
msgstr "நீட்டிக்கப்பட்ட தேர்வு"

#, icu-message-format
msgctxt "fullInstallationSubtitle"
msgid ""
"An offline-friendly selection of office tools, utilities and web browser."
msgstr ""
"அலுவலக கருவிகள், பயன்பாடுகள் மற்றும் வலை உலாவியின் இணைப்பில்லாத நட்பு "
"தேர்வு."

#, icu-message-format
msgctxt "minimalInstallationTitle"
msgid "Default selection"
msgstr "இயல்புநிலை தேர்வு"

#, icu-message-format
msgctxt "minimalInstallationSubtitle"
msgid "Just the essentials, web browser and basic utilities."
msgstr "அத்தியாவசியங்கள், வலை உலாவி மற்றும் அடிப்படை பயன்பாடுகள்."

#, icu-message-format
msgctxt "otherOptions"
msgid "Other options"
msgstr "பிற விருப்பங்கள்"

#, icu-message-format
msgctxt "installThirdPartyTitle"
msgid ""
"Install third-party software for graphics and Wi-Fi hardware, as well as "
"additional media formats"
msgstr ""
"கிராபிக்ச் மற்றும் வைஃபை வன்பொருள் மற்றும் கூடுதல் ஊடக வடிவங்களுக்காக "
"மூன்றாம் தரப்பு மென்பொருளை நிறுவவும்"

#, icu-message-format
msgctxt "installThirdPartySubtitle"
msgid ""
"This software is subject to license terms included with its documentation. "
"Some are proprietary."
msgstr ""
"இந்த மென்பொருள் அதன் ஆவணங்களுடன் சேர்க்கப்பட்ட உரிம விதிமுறைகளுக்கு "
"உட்பட்டது. சில தனியுரிமமானவை."

#, icu-message-format
msgctxt "installDriversTitle"
msgid "Install third-party software for graphics and Wi-Fi hardware"
msgstr ""
