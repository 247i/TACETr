msgstr ""
"SPEED_FILE = ICE_G0240_0000_V_EPSG4326.nc\n"
"\n"
" gmt தொடங்கு ICELAND_GLACIER_SPEED PNG\n"
" GMT BASEMAP -BAF -BWNES -RIS+R0.2 -JM15C\n"
" gmt cost -wthinnest, அடர்சிவப்பு -gdarkseagreen1 -slightblue -a1\n"
" gmt makecpt -cbatlow -t0/400/10 -d -z -m - -color_nan = சாம்பல்\n"
" GMT grdimage $ speed_file\n"
