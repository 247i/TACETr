msgctxt "added"
msgid "Added"
msgstr "சேர்க்கப்பட்டது"

msgctxt "check_updates"
msgid "Check for updates"
msgstr "புதுப்பிப்புகளை சரிபார்க்கவும்"

msgctxt "no_updates"
msgid "No updates available"
msgstr "புதுப்பிப்புகள் எதுவும் கிடைக்கவில்லை"

msgctxt "updates_available"
msgid "Updates available {amount}"
msgstr "புதுப்பிப்புகள் கிடைக்கின்றன {amount}"

msgctxt "empty->title"
msgid "Empty"
msgstr "காலி"

msgctxt "empty->description"
msgid "No addons are installed"
msgstr "துணை நிரல்கள் எதுவும் நிறுவப்படவில்லை"

msgctxt "sideload->title"
msgid "Sideload"
msgstr "பக்கவாட்டு"

msgctxt "sideload->description"
msgid "Install a Mue addon not on the marketplace from your computer"
msgstr "உங்கள் கணினியிலிருந்து சந்தையில் இல்லாத ஒரு மியூ துணை நிரலை நிறுவவும்"

msgctxt "sideload->failed"
msgid "Failed to sideload addon"
msgstr "Addon ஐ ஓரங்கட்டத் தவறிவிட்டது"

msgctxt "sideload->errors->no_name"
msgid "No name provided"
msgstr "பெயர் எதுவும் வழங்கப்படவில்லை"

msgctxt "sideload->errors->no_author"
msgid "No author provided"
msgstr "எந்த எழுத்தாளரும் வழங்கப்படவில்லை"

msgctxt "sideload->errors->no_type"
msgid "No type provided"
msgstr "எந்த வகையும் வழங்கப்படவில்லை"

msgctxt "sideload->errors->invalid_photos"
msgid "Invalid photos object"
msgstr "தவறான புகைப்படங்கள் பொருள்"

msgctxt "sideload->errors->invalid_quotes"
msgid "Invalid quotes object"
msgstr "தவறான மேற்கோள்கள் பொருள்"

msgctxt "sort->title"
msgid "Sort"
msgstr "வரிசைப்படுத்து"

msgctxt "sort->newest"
msgid "Installed (Newest)"
msgstr "நிறுவப்பட்டது (புதியது)"

msgctxt "sort->oldest"
msgid "Installed (Oldest)"
msgstr "நிறுவப்பட்டது (பழமையானது)"

msgctxt "sort->a_z"
msgid "Alphabetical (A-Z)"
msgstr "அகரவரிசை (A-Z)"

msgctxt "sort->z_a"
msgid "Alphabetical (Z-A)"
msgstr "அகரவரிசை (z-a)"

msgctxt "create->title"
msgid "Create"
msgstr "உருவாக்கு"

msgctxt "create->moved_title"
msgid "Create add-on has moved!"
msgstr "கூடுதல் ஐ உருவாக்குங்கள்!"

msgctxt "create->moved_description"
msgid ""
"Following the 7.1 update, the creation of addons has now moved to the web UI"
msgstr ""
"7.1 புதுப்பிப்பைத் தொடர்ந்து, துணை நிரல்களின் உருவாக்கம் இப்போது வலை "
"இடைமுகம் க்கு நகர்ந்தது"

msgctxt "create->moved_button"
msgid "Coming Soon...."
msgstr "விரைவில் வருகிறது ...."
