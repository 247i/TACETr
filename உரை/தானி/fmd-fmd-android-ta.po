#, java-printf-format
msgctxt "Notification_Server"
msgid "FMD Server"
msgstr "எஃப்எம்டி சேவையகம்"

#, java-printf-format
msgctxt "Notification_Server_Description"
msgid "Notifications related to FMD Server"
msgstr "எஃப்எம்டி சேவையகம் தொடர்பான அறிவிப்புகள்"

#, java-printf-format
msgctxt "Notification_Security"
msgid "Security"
msgstr "பாதுகாப்பு"

#, java-printf-format
msgctxt "Notification_Security_Description"
msgid "Notifications about security updates"
msgstr "பாதுகாப்பு புதுப்பிப்புகள் பற்றிய அறிவிப்புகள்"

#, java-printf-format
msgctxt "Notification_FAIL"
msgid "Failures"
msgstr "தோல்விகள்"

#, java-printf-format
msgctxt "Notification_Fail_Description"
msgid "Notifications when something fails, like the connection to FMD Server"
msgstr ""
"அறிவிப்புகள் ஏதேனும் தோல்வியுற்றால், எஃப்எம்டி சேவையகத்திற்கான இணைப்பு "
"போன்றவை"

#, java-printf-format
msgctxt "Notification_InApp"
msgid "In-app transport channel"
msgstr "பயன்பாட்டு போக்குவரத்து சேனல்"

#, java-printf-format
msgctxt "Notification_InApp_Description"
msgid "Notifications from the in-app transport channel"
msgstr "பயன்பாட்டு போக்குவரத்து சேனலில் இருந்து அறிவிப்புகள்"

#.  Usage notifications
#, java-printf-format
msgctxt "usage_notification_title"
msgid "Command executed"
msgstr "செயல்படுத்தப்பட்டது"

#, java-printf-format
msgctxt "usage_notification_text"
msgid "The command \"%s\" was executed."
msgstr "\"%s\" கட்டளை செயல்படுத்தப்பட்டது."

#, java-printf-format
msgctxt "usage_notification_text_source"
msgid "The command \"%1$s\" was executed by \"%2$s\"."
msgstr "%1$s கட்டளை %2$s ஆல் செயல்படுத்தப்பட்டது."

#, java-printf-format
msgctxt "usage_notification_pin_title"
msgid "FMD PIN used"
msgstr "FMD முள் பயன்படுத்தப்பட்டது"

#, java-printf-format
msgctxt "usage_notification_pin_text"
msgid ""
"The PIN was used by this number: \"%s\". It is recommended to change the "
"PIN."
msgstr ""
"முள் இந்த எண்ணால் பயன்படுத்தப்பட்டது: \"%s\". முள் மாற்ற "
"பரிந்துரைக்கப்படுகிறது."

#.  Setup warnings
#, java-printf-format
msgctxt "setup_warnings_title"
msgid "Setup warnings"
msgstr "அமைவு எச்சரிக்கைகள்"

#, java-printf-format
msgctxt "missing_permissions_title"
msgid "Missing permissions"
msgstr "அனுமதிகள் இல்லை"

#, java-printf-format
msgctxt "missing_permissions_description"
msgid "The following permissions are needed for FMD to run."
msgstr "எஃப்எம்டி இயங்க பின்வரும் அனுமதிகள் தேவை."

#.  Lock screen message
#, java-printf-format
msgctxt "Locked_Lock_Screen"
msgid ""
"FMD\n"
"This device is locked!\n"
"Message from the owner:"
msgstr ""
