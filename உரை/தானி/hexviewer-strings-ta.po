msgid "Error"
msgstr "பிழை"

#, java-printf-format
msgctxt "error_no_line_selected"
msgid "No line was selected"
msgstr "எந்த வரியும் தேர்ந்தெடுக்கப்படவில்லை"

#, java-printf-format
msgctxt "error_edition_search_in_progress"
msgid "Line editing is not possible when a search is in progress."
msgstr "தேடல் நடந்து கொண்டிருக்கும்போது வரி திருத்துதல் சாத்தியமில்லை."

#, java-printf-format
msgctxt "error_edition_continuous_selection"
msgid "Line editing is only possible with a continuous selection."
msgstr "தொடர்ச்சியான தேர்வோடு மட்டுமே வரி திருத்துதல் சாத்தியமாகும்."

#, java-printf-format
msgctxt "error_no_file_access"
msgid "No access to the file. Open it from the menu."
msgstr "கோப்பிற்கான அணுகல் இல்லை. மெனுவிலிருந்து அதைத் திறக்கவும்."

#, java-printf-format
msgctxt "error_no_file"
msgid "File missing (deleted or moved)"
msgstr "கோப்பு காணவில்லை (நீக்கப்பட்டது அல்லது நகர்த்தப்பட்டது)"

#, java-printf-format
msgctxt "error_cant_exceed_xxx"
msgid "It is not possible to exceed %1$s"
msgstr "%1$s ஐ தாண்ட முடியாது"

#, java-printf-format
msgctxt "action_open_sequential_title"
msgid "Partial file opening…"
msgstr "பகுதி கோப்பு திறப்பு…"

#, java-printf-format
msgctxt "action_open_sequential_title_button"
msgid "Partial file opening"
msgstr "பகுதி கோப்பு திறப்பு"

#, java-printf-format
msgctxt "error_open_sequential_add_or_delete_data"
msgid "Change to a different mode if you want to add or delete data"
msgstr ""
"நீங்கள் தரவைச் சேர்க்க அல்லது நீக்க விரும்பினால் வேறு பயன்முறையில் மாற்றவும்"

#, java-printf-format
msgctxt "error_size_changed"
msgid ""
"The file size has changed. Click to specify the new size of the area to "
"open."
msgstr ""
"கோப்பு அளவு மாறிவிட்டது. திறக்க பகுதியின் புதிய அளவைக் குறிப்பிட சொடுக்கு "
"செய்க."

#, java-printf-format
msgctxt "start_offset"
msgid "Start"
msgstr "தொடங்கு"

#, java-printf-format
msgctxt "end_offset"
msgid "End"
msgstr "முடிவு"

#, java-printf-format
msgctxt "unit"
msgid "Unit"
msgstr "அலகு"

#, java-printf-format
msgctxt "input_type"
msgid "Input type"
msgstr "உள்ளீட்டு வகை"

#, java-printf-format
msgctxt "file_size"
msgid "File size"
msgstr "கோப்பு அளவு"

#, java-printf-format
msgctxt "size_opening_part"
msgid "Size of the part to open"
msgstr "திறக்க வேண்டிய பகுதியின் அளவு"

#, java-printf-format
msgctxt "partial_file_opening_description"
msgid "Enter the beginning and end of the area you want to open."
msgstr ""
"நீங்கள் திறக்க விரும்பும் பகுதியின் தொடக்கத்தையும் முடிவையும் உள்ளிடவும்."

#, java-printf-format
msgctxt "not_enough_memory"
msgid ""
"Unable to execute the command because there's not enough memory for this "
"app."
msgstr ""
"இந்த பயன்பாட்டிற்கு போதுமான நினைவகம் இல்லாததால் கட்டளையை இயக்க முடியவில்லை."

#, java-printf-format
msgctxt "memory_threshold_title"
msgid "Memory threshold"
msgstr "நினைவக வாசல்"

#, java-printf-format
msgctxt "memory_threshold_description"
msgid "Amount of memory allowed before opening a file is aborted."
msgstr ""
"ஒரு கோப்பைத் திறப்பதற்கு முன் அனுமதிக்கப்பட்ட நினைவகத்தின் அளவு "
"கைவிடப்படுகிறது."

#, java-printf-format
msgctxt "error_not_available"
msgid "Not available"
msgstr "கிடைக்கவில்லை"

#, java-printf-format
msgctxt "settings_pref_title_restore_default"
msgid "Restore default values"
msgstr "இயல்புநிலை மதிப்புகளை மீட்டெடுக்கவும்"

#, java-printf-format
msgctxt "dialog_restore_title"
msgid "Restore"
msgstr "மீட்டமை"

#, java-printf-format
msgctxt "dialog_restore_question"
msgid "Do you really want to restore the default values?"
msgstr "இயல்புநிலை மதிப்புகளை மீட்டெடுக்க விரும்புகிறீர்களா?"

#, java-printf-format
msgctxt "dialog_restore_recent"
msgid "Delete the list of recent files?"
msgstr "அண்மைக் கால கோப்புகளின் பட்டியலை நீக்கவா?"

#, java-printf-format
msgctxt "memory_thresholds_disable"
msgid "Disable"
msgstr "முடக்கு"

#, java-printf-format
msgctxt "text_copied"
msgid "%1$s copied."
msgstr "%1$s நகலெடுக்கப்பட்டன."

#, java-printf-format
msgctxt "error_too_many_text_copied"
msgid "It's not possible to copy so much text"
msgstr "இவ்வளவு உரையை நகலெடுக்க முடியாது"

#, java-printf-format
msgctxt "menu_select_all"
msgid "Select all"
msgstr "அனைத்தையும் தெரிவுசெய்"

#, java-printf-format
msgctxt "menu_delete"
msgid "Delete"
msgstr "நீக்கு"

#, java-printf-format
msgctxt "menu_edit"
msgid "Edit"
msgstr "தொகு"

#, java-printf-format
msgctxt "menu_copy"
msgid "Copy"
msgstr "நகலெடு"
