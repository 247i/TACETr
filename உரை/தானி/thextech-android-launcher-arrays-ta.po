#. Menu item for the "Touchscreen controller mode"
#, java-printf-format
msgctxt "touchscreenControllerType0"
msgid "Always enable"
msgstr "எப்போதும் இயக்கவும்"

#. Menu item for the "Touchscreen controller mode"
#, java-printf-format
msgctxt "touchscreenControllerType1"
msgid "Disable when hardware keyboard is plugged"
msgstr "வன்பொருள் விசைப்பலகை செருகப்படும்போது முடக்கு"

#. Menu item for the "Touchscreen controller mode"
#, java-printf-format
msgctxt "touchscreenControllerType2"
msgid "Always disable"
msgstr "எப்போதும் முடக்கு"

#. Menu item for the "Speedrun mode"
#, java-printf-format
msgctxt "speedRunMode0"
msgid "Off"
msgstr "அணை"

#. Menu item for the "Speedrun mode"
#, java-printf-format
msgctxt "speedRunMode1"
msgid "Mode 1 (TheXTech native)"
msgstr "பயன்முறை 1 (textech native)"

#. Menu item for the "Speedrun mode"
#, java-printf-format
msgctxt "speedRunMode2"
msgid "Mode 2 (Disable time-winning features)"
msgstr "பயன்முறை 2 (நேரத்தை வென்ற அம்சங்களை முடக்கு)"

#. Menu item for the "Speedrun mode"
#, java-printf-format
msgctxt "speedRunMode3"
msgid "Mode 3 (Strict SMBX 1.3, all bugs enabled)"
msgstr "பயன்முறை 3 (கடுமையான SMBX 1.3, அனைத்து பிழைகள் இயக்கப்பட்டன)"

#. Menu item for the "Touchpad style"
#, java-printf-format
msgctxt "touchPadStyle0"
msgid "In-Game Actions"
msgstr "விளையாட்டு செயல்கள்"

#. Menu item for the "Touchpad style"
#, java-printf-format
msgctxt "touchPadStyle1"
msgid "Nintendo/Xbox style (A/B/X/Y)"
msgstr "நிண்டெண்டோ/எக்ச்பாக்ச் பாணி (a/b/x/y)"

#. Menu item for the "Touchpad style"
#, java-printf-format
msgctxt "touchPadStyle2"
msgid "PlayStation style (X/O/D/A"
msgstr "பிளேச்டேசன் பாணி ​​(x/o/d/a"

#. Menu item for the "Battery status showing mode"
#, java-printf-format
msgctxt "batteryStatusShow0"
msgid "Never show"
msgstr "ஒருபோதும் காட்ட வேண்டாம்"

#. Menu item for the "Battery status showing mode"
#, java-printf-format
msgctxt "batteryStatusShow1"
msgid "Show when low battery"
msgstr "குறைந்த பேட்டரி போது காண்பி"

#. Menu item for the "Battery status showing mode"
#, java-printf-format
msgctxt "batteryStatusShow2"
msgid "Show always"
msgstr "எப்போதும் காட்டு"

#, java-printf-format
msgctxt "rendererDefault"
msgid "Default"
msgstr "இயல்புநிலை"

#, java-printf-format
msgctxt "rendererSdl"
msgid "SDL2"
msgstr "SDL2"

#, java-printf-format
msgctxt "rendererGles"
msgid "OpenGL ES 2.0+"
msgstr "OpenGL 2.0+ ஆகும்"

#, java-printf-format
msgctxt "rendererGles11"
msgid "OpenGL ES 1.1"
msgstr "ஓபன்சிஎல் 1.1"
