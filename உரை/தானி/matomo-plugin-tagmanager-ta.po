msgstr "ரேப்பர்களின் பேசனே பெயர்"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperAreaTitle"
msgid "etracker Wrapper area (et_area)"
msgstr "எட்ராக்கர் ரேப்பர் பகுதி (ET_AREA)"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperAreaDescription"
msgid "The area should be separated by Slash"
msgstr "பகுதியை ச்லாச் மூலம் பிரிக்க வேண்டும்"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperTargetTitle"
msgid "etracker Wrapper target (et_target)"
msgstr "எட்ராக்கர் ரேப்பர் இலக்கு (ET_TARGET)"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperTvalTitle"
msgid "etracker Wrapper total order value (et_tval)"
msgstr "எட்ராக்கர் ரேப்பர் மொத்த ஆர்டர் மதிப்பு (ET_TVAL)"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperTonrTitle"
msgid "etracker Wrapper order number (et_tonr)"
msgstr "எட்ராக்கர் ரேப்பர் ஆர்டர் எண் (ET_TONR)"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperTsaleTitle"
msgid "etracker Wrapper order status (et_tsale)"
msgstr "Etracker raper order நிலை (ET_TSALE)"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperTcustTitle"
msgid "etracker Wrapper customer status (et_cust)"
msgstr "எட்ராக்கர் ரேப்பர் வாடிக்கையாளர் நிலை (ET_CUST)"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagWrapperTBasketTitle"
msgid "etracker Wrapper basket (et_basket)"
msgstr "எட்ராக்கர் ரேப்பர் கூடை (ET_BASKET)"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventCategoryTitle"
msgid "etracker Category"
msgstr "எட்ராக்கர் வகை"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventCategoryDescription"
msgid ""
"The event's category, for example Navigation, Outbound Links, 404 Error…"
msgstr ""
"நிகழ்வின் வகை, எடுத்துக்காட்டாக வழிசெலுத்தல், வெளிச்செல்லும் இணைப்புகள், 404"
" பிழை…"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventObjectTitle"
msgid "etracker Object"
msgstr "எட்ராக்கர் பொருள்"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventObjectDescription"
msgid ""
"The event's object Name, for example a particular navigation element, a "
"clicked element, form name,…"
msgstr ""
"நிகழ்வின் பொருள் பெயர், எடுத்துக்காட்டாக ஒரு குறிப்பிட்ட வழிசெலுத்தல் "
"உறுப்பு, ஒரு சொடுக்கு செய்யப்பட்ட உறுப்பு, படிவ பெயர்,…"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventActionTitle"
msgid "etracker Action"
msgstr "எட்ராக்கர் நடவடிக்கை"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventActionDescription"
msgid "The event's action, for example, click, open, close, play, pause…"
msgstr ""
"நிகழ்வின் செயல், எடுத்துக்காட்டாக, சொடுக்கு செய்யவும், திறவும், மூடு, "
"விளையாடவும், இடைநிறுத்தவும்…"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventTypeTitle"
msgid "etracker Type"
msgstr "எட்ராக்கர் வகை"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagEventTypeDescription"
msgid "The event's type Name, for example an value of a send form…"
msgstr ""
"நிகழ்வின் வகை பெயர், எடுத்துக்காட்டாக ஒரு அனுப்பும் படிவத்தின் மதிப்பு…"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagTransactionTypeTitle"
msgid "etracker Order Status"
msgstr "எட்ராக்கர் ஆர்டர் நிலை"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagTransactionTypeDescription"
msgid "Sale / Lead / Partial Cancellation / Cancellation"
msgstr "விற்பனை / முன்னணி / பகுதி ரத்து / ரத்து செய்தல்"

#, ignore-optional-plural, php-format, safe-html
msgctxt "TagManager.EtrackerTagTransactionIDTitle"
msgid "etracker Order number"
msgstr "எட்ராக்கர் ஆர்டர் எண்"

#, ignore-optional-plural, php-format, safe-html
