msgctxt "pref_header_backup"
msgid "Cloud Backup"
msgstr "முகில் காப்புப்பிரதி"

#, java-printf-format
msgctxt "pref_title_backup_active"
msgid "Active"
msgstr "செயலில்"

#, java-printf-format
msgctxt "pref_backup_no_permission"
msgid "Please grant permission to you Google account for Google Drive API."
msgstr ""
"கூகிள் இயக்கி பநிஇ க்கான Google கணக்குக்கு உங்களுக்கு இசைவு வழங்கவும்."

#, java-printf-format
msgctxt "pref_title_backup_active_connected"
msgid "Connected to Google Drive"
msgstr "Google இயக்ககத்துடன் இணைக்கப்பட்டுள்ளது"

#, java-printf-format
msgctxt "pref_title_backup_active_not_connected"
msgid "Not connected to Google Drive"
msgstr "Google இயக்ககத்துடன் இணைக்கப்படவில்லை"

#, java-printf-format
msgctxt "pref_title_backup_active_failed"
msgid "Could not connect to Google Drive"
msgstr "Google இயக்ககத்துடன் இணைக்க முடியவில்லை"

#, java-printf-format
msgctxt "pref_title_backup_active_connecting"
msgid "Drive connecting…"
msgstr "இயக்கி இணைக்கிறது…"

#, java-printf-format
msgctxt "pref_title_backup_wifi_only"
msgid "Only transfer files on Wi-Fi"
msgstr "Wi-Fi இல் கோப்புகளை மட்டுமே மாற்றவும்"

#, java-printf-format
msgctxt "pref_title_backup_wifi_only_summary"
msgid "File transfers will pause when no Wi-Fi connection is available."
msgstr ""
"வைஃபை இணைப்பு எதுவும் கிடைக்காதபோது கோப்பு இடமாற்றங்கள் இடைநிறுத்தப்படும்."

#, java-printf-format
msgctxt "pref_title_backup_now"
msgid "Backup Now"
msgstr "இப்போது காப்புப்பிரதி"

#, java-printf-format
msgctxt "pref_title_backup_now_summary"
msgid "Last backup: %1$s"
msgstr "கடைசி காப்புப்பிரதி: %1$s"

#, java-printf-format
msgctxt "pref_title_backup_now_summary_never"
msgid "Never"
msgstr "ஒருபோதும்"

#, java-printf-format
msgctxt "pref_title_backup_frequency"
msgid "Frequency"
msgstr "மீடிறன், மீள்திறன், நிகழ்வெண், நிகழ்வு"

#, java-printf-format
msgctxt "pref_list_backup_frequency_none"
msgid "Only when I tap Backup Now"
msgstr "நான் இப்போது காப்புப்பிரதியைத் தட்டும்போது மட்டுமே"

#, java-printf-format
msgctxt "pref_list_backup_frequency_daily"
msgid "Daily"
msgstr "நாள்தோறும்"

#, java-printf-format
msgctxt "pref_list_backup_frequency_weekly"
msgid "Weekly"
msgstr "வாராந்திர"

#, java-printf-format
msgctxt "pref_list_backup_frequency_monthly"
msgid "Monthly"
msgstr "மாதாந்திர"

#, java-printf-format
msgctxt "pref_backup_unable_to_list_drive_backups"
msgid "Couldn't load Google Drive backups."
msgstr "கூகிள் இயக்கி காப்புப்பிரதிகளை ஏற்ற முடியவில்லை."

#, java-printf-format
msgctxt "pref_backup_restore_title"
msgid "Select a backup to restore:"
msgstr "மீட்டமைக்க காப்புப்பிரதியைத் தேர்ந்தெடுக்கவும்:"

#, java-printf-format
msgctxt "pref_backup_restore_not"
msgid "Do not restore"
msgstr "மீட்டெடுக்க வேண்டாம்"

#.  Strings for App Rate Dialog
#, java-printf-format
msgctxt "rate_dialog_title"
msgid "Rate this app"
msgstr "இந்த பயன்பாட்டை மதிப்பிடுங்கள்"

#, java-printf-format
msgctxt "rate_dialog_text"
msgid "If you enjoy using this app, rate it. Thank you for your support!"
msgstr ""
"இந்த பயன்பாட்டைப் பயன்படுத்துவதை நீங்கள் ரசிக்கிறீர்கள் என்றால், அதை "
"மதிப்பிடுங்கள். உங்கள் ஆதரவுக்கு நன்றி!"

#, java-printf-format
msgctxt "rate_dialog_yes"
msgid "Rate Now"
msgstr "இப்போது விகிதம்"

#, java-printf-format
msgctxt "rate_dialog_later"
msgid "Remind Me Later"
msgstr "பின்னர் எனக்கு நினைவூட்டுங்கள்"

#, java-printf-format
msgctxt "rate_dialog_never"
msgid "No, Thanks"
msgstr "இல்லை, நன்றி"

#.  Strings for Drive Dialog
#, java-printf-format
msgctxt "drive_dialog_title"
msgid "Connect Google Drive"
msgstr "Google இயக்ககத்தை இணைக்கவும்"

#, java-printf-format
msgctxt "drive_dialog_text"
msgid "Would you like to backup your data to your Google Drive account?"
msgstr ""
"உங்கள் Google இயக்கி கணக்கில் உங்கள் தரவை காப்புப் பிரதி எடுக்க "
"விரும்புகிறீர்களா?"

#, java-printf-format
msgctxt "drive_dialog_yes"
msgid "Yes, Do It Now"
msgstr "ஆம், இப்போது செய்யுங்கள்"

#, java-printf-format
msgctxt "drive_dialog_later"
msgid "Remind Me Later"
msgstr "பின்னர் எனக்கு நினைவூட்டுங்கள்"

#, java-printf-format
msgctxt "drive_dialog_never"
msgid "No, Thanks"
msgstr "இல்லை, நன்றி"

#.  Strings for Translate
#, java-printf-format
msgctxt "translate_dialog_title"
msgid "Translate Diet Diary"
msgstr "உணவு நாட்குறிப்பை மொழிபெயர்க்கவும்"

#, java-printf-format
msgctxt "translate_dialog_text"
msgid "Would you like to help us translate diet diary into your language?"
msgstr ""
"டயட் டைரியை உங்கள் மொழியில் மொழிபெயர்க்க எங்களுக்கு உதவ விரும்புகிறீர்களா?"

#, java-printf-format
msgctxt "translate_dialog_yes"
msgid "Yes, I Do"
msgstr "ஆம், நான் செய்கிறேன்"

#, java-printf-format
msgctxt "translate_dialog_later"
msgid "Remind Me Later"
msgstr "பின்னர் எனக்கு நினைவூட்டுங்கள்"

#, java-printf-format
msgctxt "translate_dialog_never"
msgid "No, Thanks"
msgstr "இல்லை, நன்றி"
