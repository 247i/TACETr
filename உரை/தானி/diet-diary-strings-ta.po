#. select a file format of exporting data.
#, java-printf-format
msgctxt "backup_type"
msgid "Type"
msgstr "வகை"

#. Will be shown under the progress bar while exporting data. Text indicates
#. to export operation is currently working.
#, java-printf-format
msgctxt "backup_progress_title"
msgid "Exporting…"
msgstr "ஏற்றுமதி செய்கிறது…"

#. Will be shown under the progress bar while exporting data. Text indicates
#. an exportable SD card is not plugged into user's device.
#, java-printf-format
msgctxt "backup_sd_card_unavailable"
msgid "SD Card unavailable."
msgstr "எச்டி அட்டை கிடைக்கவில்லை."

#. Will be shown under the progress bar while exporting data. Text indicates a
#. temporary file was not be able to created.
#, java-printf-format
msgctxt "backup_temp_file_creation_error"
msgid "Cannot create temporary file."
msgstr "தற்காலிக கோப்பை உருவாக்க முடியாது."

#. Will be shown under the progress bar while exporting data. Text indicates a
#. file was not be able to created.
#, java-printf-format
msgctxt "backup_file_creation_error"
msgid "Cannot create export file."
msgstr "ஏற்றுமதி கோப்பை உருவாக்க முடியாது."

#. Will be shown under the progress bar while exporting data. Text indicates
#. an unimplemented export destination has been selected.
#, java-printf-format
msgctxt "backup_unimplemented_destination"
msgid "Selected destination is not supported, yet."
msgstr "தேர்ந்தெடுக்கப்பட்ட இலக்கு இன்னும் ஆதரிக்கப்படவில்லை."

#. Will be shown under the progress bar while exporting data. Text shown when
#. an IO (Input/Output) error happens while exporting.
#, java-printf-format
msgctxt "backup_io_exception"
msgid "I/O error occurred during export."
msgstr "ஏற்றுமதியின் போது I/O பிழை ஏற்பட்டது."

#. Will be shown under the progress bar while exporting data. Text shown when
#. a DB (Database) error happens while exporting.
#, java-printf-format
msgctxt "backup_sql_exception"
msgid "DB error during export."
msgstr "ஏற்றுமதியின் போது DB பிழை."

#. Will be shown under the progress bar while exporting data. Text shown when
#. an unknown error happens while exporting.
#, java-printf-format
msgctxt "backup_exception"
msgid "Unidentified error during export."
msgstr "ஏற்றுமதியின் போது அடையாளம் தெரியாத பிழை."

#. Will be shown after export operation. Text indicates a file has been
#. created. %1$s will be replaced with the file name. %1$s should be written
#. where the file name should appear.
#, java-printf-format
msgctxt "backup_external_successful"
msgid "%1$s created"
msgstr "%1$s உருவாக்கப்பட்டது"

#. Will be shown after export operation. Text indicates a data has been
#. shared.
#, java-printf-format
msgctxt "backup_share_successful"
msgid "File shared"
msgstr "கோப்பு பகிரப்பட்டது"

#. Will be shown while exporting data. Text indicates a data cannot be written
#. because of required write permission has not been given. So, application
#. asks user to give required permission for the write operation.
#, java-printf-format
msgctxt "backup_no_permission"
msgid "Please give permission to write files."
msgstr "கோப்புகளை எழுத இசைவு கொடுங்கள்."

#. Will be shown under the progress bar while importing data.
#, java-printf-format
msgctxt "restore_progress_title"
msgid "Importing…"
msgstr "இறக்குமதி…"

#. Will be shown on the backup/restore screen's restore tab. Text indicates to
