#, icu-message-format
msgctxt "g_system"
msgid "System"
msgstr "மண்டலம்"

#. A tag can be attached to an article.
#, icu-message-format
msgctxt "g_tag"
msgid "Tag"
msgstr "குறிச்சொல்"

#. A version, as in the application version.
#, icu-message-format
msgctxt "g_version"
msgid "Version"
msgstr "பதிப்பு"

#, case-insensitive, icu-message-format, placeholders:$N$
msgctxt "humanize_days"
