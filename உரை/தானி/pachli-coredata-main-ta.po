msgctxt "server_repository_error_get_node_info"
msgid "fetching nodeinfo %1$s failed: %2$s"
msgstr "நோடின்ஃபோ %1$s தோல்வியுற்றன: %2$s"

#, java-printf-format
msgctxt "server_repository_error_validate_node_info"
msgid "validating nodeinfo %1$s failed: %2$s"
msgstr "Nodeinfo %1$s ஐ சரிபார்க்கவும் தோல்வியுற்றது: %2$s"

#, java-printf-format
msgctxt "server_repository_error_get_instance_info"
