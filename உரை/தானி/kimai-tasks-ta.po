#, placeholders:r"%[^% ]%"
msgctxt "report_tasks_assigned"
msgid "Tasks per user"
msgstr "ஒரு பயனருக்கு பணிகள்"

#, placeholders:r"%[^% ]%"
msgctxt "report_tasks_teams"
msgid "Team tasks"
msgstr "குழு பணிகள்"

#, placeholders:r"%[^% ]%"
msgctxt "task.start"
msgid "Start processing"
msgstr "செயலாக்கத்தைத் தொடங்குங்கள்"

#, placeholders:r"%[^% ]%"
msgctxt "task.stop"
msgid "Pause processing"
msgstr "இடைநிறுத்த செயலாக்கம்"

#, placeholders:r"%[^% ]%"
msgctxt "task.assign"
msgid "Assign to me"
msgstr "எனக்கு நியமிக்கவும்"

#, placeholders:r"%[^% ]%"
msgctxt "task.unassign"
msgid "Release task"
msgstr "வெளியீட்டு பணி"

#, placeholders:r"%[^% ]%"
msgctxt "task.reopen"
msgid "Re-open task"
msgstr "பணி மீண்டும் திறக்கவும்"

#, placeholders:r"%[^% ]%"
msgctxt "task.close"
msgid "Complete task"
msgstr "முழுமையான பணி"

#, placeholders:r"%[^% ]%"
msgctxt "task.create"
msgid "Create task"
msgstr "பணியை உருவாக்கவும்"

#, placeholders:r"%[^% ]%"
msgctxt "Tasks"
msgid "Tasks"
msgstr "பணிகள்"

#, placeholders:r"%[^% ]%"
msgctxt "tasks.pending"
msgid "Pending tasks"
msgstr "நிலுவையில் உள்ள பணிகள்"

#, placeholders:r"%[^% ]%"
msgctxt "tasks.my"
msgid "My tasks"
msgstr "என் பணிகள்"

#, placeholders:r"%[^% ]%"
msgctxt "task.help.title"
msgid "A short description of the task"
msgstr "பணியின் குறுகிய விளக்கம்"

#, placeholders:r"%[^% ]%"
msgctxt "task.help.end"
msgid "Task must be completed until this date"
msgstr "இந்த தேதி வரை பணி முடிக்கப்பட வேண்டும்"

#, placeholders:r"%[^% ]%"
msgctxt "task.help.user"
msgid "Assign this task to a user"
msgstr "இந்த பணியை ஒரு பயனருக்கு ஒதுக்குங்கள்"

#, placeholders:r"%[^% ]%"
msgctxt "task.help.team"
msgid "Assign this task to a team"
msgstr "இந்த பணியை ஒரு குழுவுக்கு ஒதுக்குங்கள்"

#, placeholders:r"%[^% ]%"
msgctxt "task.widget_rows"
msgid "Amount of tasks per page in dashboard widget"
msgstr "டாச்போர்டு விட்செட்டில் ஒரு பக்கத்திற்கு பணிகளின் அளவு"

#, placeholders:r"%[^% ]%"
msgctxt "estimation"
msgid "Time estimation"
msgstr "நேர மதிப்பீடு"

#, placeholders:r"%[^% ]%"
msgctxt "task.help.estimation"
msgid "Estimated time needed to complete the task in hh:mm"
msgstr "HH: MM இல் பணியை முடிக்க மதிப்பிடப்பட்ட நேரம் தேவை"

#, placeholders:r"%[^% ]%"
msgctxt "task.help.description"
msgid "This description will be copied into each timesheet record"
msgstr "இந்த விளக்கம் ஒவ்வொரு டைம்சீட் பதிவிலும் நகலெடுக்கப்படும்"

#, placeholders:r"%[^% ]%"
msgctxt "task.help.tags"
msgid "These tags will be copied into each timesheet record"
msgstr "இந்த குறிச்சொற்கள் ஒவ்வொரு டைம்சீட் பதிவிலும் நகலெடுக்கப்படும்"

#, placeholders:r"%[^% ]%"
msgctxt "task_no_estimation"
msgid "Without estimation"
msgstr "மதிப்பீடு இல்லாமல்"

#, placeholders:r"%[^% ]%"
msgctxt "task_with_estimation"
msgid "With estimation"
msgstr "மதிப்பீட்டோடு"

#, placeholders:r"%[^% ]%"
msgctxt "task_started"
msgid "Work started"
msgstr "வேலை தொடங்கியது"

#, placeholders:r"%[^% ]%"
msgctxt "task_not_started"
msgid "Not started"
msgstr "தொடங்கப்படவில்லை"

#, placeholders:r"%[^% ]%"
msgctxt "task_overdue"
msgid "Overdue"
msgstr "தாமதமானது"

#, placeholders:r"%[^% ]%"
msgctxt "task_todo"
msgid "Task"
msgstr "பணி"
