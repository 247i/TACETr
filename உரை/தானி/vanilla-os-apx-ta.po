msgctxt "apx->description"
msgid ""
"Apx is a package manager with support for multiple sources, allowing you to "
"install packages in subsystems."
msgstr ""
"Apx என்பது பல ஆதாரங்களுக்கான ஆதரவுடன் ஒரு தொகுப்பு மேலாளர் ஆகும், இது துணை "
"அமைப்புகளில் தொகுப்புகளை நிறுவ உங்களை அனுமதிக்கிறது."

msgctxt "apx->errors->unknownCommand"
msgid "Unknown command: %s"
msgstr "அறியப்படாத கட்டளை: %s"

msgctxt "apx->errors->invalidInput"
msgid "Invalid input."
msgstr "தவறான உள்ளீடு."

msgctxt "apx->errors->invalidChoice"
msgid "Invalid choice."
msgstr "தவறான தேர்வு."

msgctxt "apx->errors->noRoot"
msgid "Do not run Apx as root."
msgstr "Apx ஐ ரூட்டாக இயக்க வேண்டாம்."

msgctxt "apx->msg->help"
msgid "Show help for apx."
msgstr "apx க்கான உதவியைக் காட்டு."

msgctxt "apx->msg->version"
msgid "Show version for apx."
msgstr "apxக்கான பதிப்பைக் காட்டு."

msgctxt "apx->msg->usage"
msgid "Usage"
msgstr "பயன்பாடு"

msgctxt "apx->msg->aliases"
msgid "Aliases"
msgstr "மாற்றுப்பெயர்கள்"

msgctxt "apx->msg->examples"
msgid "Examples"
msgstr "எடுத்துக்காட்டுகள்"

msgctxt "apx->msg->availableCommands"
msgid "Available Commands"
msgstr "கிடைக்கும் கட்டளைகள்"

msgctxt "apx->msg->additionalCommands"
msgid "Additional Commands"
msgstr "கூடுதல் கட்டளைகள்"

msgctxt "apx->msg->flags"
msgid "Flags"
msgstr "கொடிகள்"

msgctxt "apx->msg->globalFlags"
msgid "Global Flags"
msgstr "உலகளாவிய கொடிகள்"

msgctxt "apx->msg->additionalHelpTopics"
msgid "Additional help topics"
msgstr "கூடுதல் உதவி தலைப்புகள்"

msgctxt "apx->msg->moreInfo"
msgid "Use %s for more information about a command"
msgstr "கட்டளையைப் பற்றிய கூடுதல் தகவலுக்கு %s ஐப் பயன்படுத்தவும்"

msgctxt "apx->info->aborting"
msgid "Aborting as requested."
msgstr "கோரிக்கையின்படி நிறுத்தப்படுகிறது."

msgctxt "apx->terminal->yes"
msgid "yes"
msgstr "ஆம்"

msgctxt "apx->terminal->no"
msgid "no"
msgstr "இல்லை"

msgctxt "runtimeCommand->description"
msgid ""
"Work with the specified subsystem, accessing the package manager and "
"environment."
msgstr ""
"தொகுப்பு மேலாளர் மற்றும் சூழலை அணுகுவதன் மூலம், குறிப்பிட்ட துணை அமைப்புடன் "
"வேலை செய்யுங்கள்."

msgctxt "runtimeCommand->error->enteringContainer"
msgid "An error occurred while entering the container: %s"
msgstr "கொள்கலனுக்குள் நுழையும் போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->error->noPackageSpecified"
msgid "No packages specified."
msgstr "தொகுப்புகள் எதுவும் குறிப்பிடப்படவில்லை."

msgctxt "runtimeCommand->error->cantAccessPkgManager"
msgid "An error occurred while accessing the package manager: %s"
msgstr "தொகுப்பு நிர்வாகியை அணுகும் போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->error->executingCommand"
msgid "An error occurred while executing the command: %s"
msgstr "கட்டளையை இயக்கும் போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->error->noAppNameOrBin"
msgid "--app-name or --bin must be specified."
msgstr "--app-name அல்லது --bin குறிப்பிடப்பட வேண்டும்."

msgctxt "runtimeCommand->error->sameAppOrBin"
msgid "--app-name and --bin cannot be both specified."
msgstr "--app-name மற்றும் --bin இரண்டையும் குறிப்பிட முடியாது."

msgctxt "runtimeCommand->error->exportingApp"
msgid "An error occurred while exporting the application: %s"
msgstr "பயன்பாட்டை ஏற்றுமதி செய்யும் போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->error->exportingBin"
msgid "An error occurred while exporting the binary: %s"
msgstr "பைனரியை ஏற்றுமதி செய்யும் போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->error->unexportingApp"
msgid "An error occurred while unexporting the application: %s"
msgstr "பயன்பாடுகளை ஏற்றுமதி செய்யும் போது பிழை ஏற்பட்டது"

msgctxt "runtimeCommand->error->unexportingBin"
msgid "An error occurred while unexporting the binary: %s"
msgstr "பைனரி ஏற்றுமதியை நீக்கும் போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->error->startingContainer"
msgid "An error occurred while starting the container: %s"
msgstr "கொள்கலனைத் தொடங்கும் போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->error->stoppingContainer"
msgid "An error occurred while stopping the container: %s"
msgstr "கொள்கலனை நிறுத்தும்போது பிழை ஏற்பட்டது: %s"

msgctxt "runtimeCommand->info->unexportedApps"
msgid "Unexported %d applications"
msgstr "ஏற்றுமதி செய்யப்படாத %d பயன்பாடுகள்"

msgctxt "runtimeCommand->info->exportedApps"
msgid "Exported %d applications"
msgstr "%d பயன்பாடுகள் ஏற்றுமதி செய்யப்பட்டன"

msgctxt "runtimeCommand->info->exportedApp"
msgid "Exported application %s"
msgstr "ஏற்றுமதி செய்யப்பட்ட பயன்பாடு %s"

msgctxt "runtimeCommand->info->exportedBin"
msgid "Exported binary %s"
msgstr "பைனரி %s ஏற்றுமதி செய்யப்பட்டது"

msgctxt "runtimeCommand->info->unexportedApp"
msgid "Unexported application %s"
msgstr "ஏற்றுமதி செய்யப்படாத பயன்பாடு %s"

msgctxt "runtimeCommand->info->unexportedBin"
msgid "Unexported binary %s"
msgstr "ஏற்றுமதி செய்யப்படாத பைனரி %s"

msgctxt "runtimeCommand->info->startingContainer"
msgid "Starting subsystem %s…"
msgstr "துணை அமைப்பு %sஐத் தொடங்குகிறது…"

msgctxt "runtimeCommand->info->startedContainer"
msgid "Started subsystem."
msgstr "துணை அமைப்பு தொடங்கப்பட்டது."

msgctxt "runtimeCommand->info->stoppingContainer"
msgid "Stopping subsystem %s…"
msgstr "%s துணை அமைப்பை நிறுத்துகிறது…"

msgctxt "runtimeCommand->info->stoppedContainer"
msgid "Stopped subsystem."
msgstr "துணை அமைப்பு நிறுத்தப்பட்டது."

msgctxt "runtimeCommand->autoremove->description"
msgid "Remove packages that are no longer required."
msgstr "இனி தேவைப்படாத தொகுப்புகளை அகற்றவும்."

msgctxt "runtimeCommand->clean->description"
msgid "Clean the package manager's cache."
msgstr "தொகுப்பு மேலாளரின் தற்காலிக சேமிப்பை தூய்மை செய்யவும்."

msgctxt "runtimeCommand->install->description"
msgid "Install the specified package."
msgstr "குறிப்பிட்ட தொகுப்பை நிறுவவும்."

msgctxt "runtimeCommand->install->options->noExport->description"
msgid "Do not export a desktop entry."
msgstr "டெச்க்டாப் உள்ளீட்டை ஏற்றுமதி செய்ய வேண்டாம்."

msgctxt "runtimeCommand->list->description"
msgid "List all installed packages."
msgstr "நிறுவப்பட்ட அனைத்து தொகுப்புகளையும் பட்டியலிடுங்கள்."

msgctxt "runtimeCommand->purge->description"
msgid "Purge the specified packages."
msgstr "குறிப்பிட்ட தொகுப்புகளை அகற்றவும்."

msgctxt "runtimeCommand->remove->description"
msgid "Remove the specified packages."
msgstr "குறிப்பிட்ட தொகுப்புகளை அகற்றவும்."

msgctxt "runtimeCommand->search->description"
msgid "Search for packages matching the specified query."
msgstr "குறிப்பிட்ட வினவலுடன் பொருந்தக்கூடிய தொகுப்புகளைத் தேடவும்."

msgctxt "runtimeCommand->show->description"
msgid "Show information about the specified package."
msgstr "குறிப்பிட்ட தொகுப்பு பற்றிய தகவலைக் காட்டு."

msgctxt "runtimeCommand->update->description"
msgid "Update the list of available packages."
msgstr "கிடைக்கக்கூடிய தொகுப்புகளின் பட்டியலைப் புதுப்பிக்கவும்."

msgctxt "runtimeCommand->upgrade->description"
msgid "Upgrade all installed packages."
msgstr "நிறுவப்பட்ட அனைத்து தொகுப்புகளையும் மேம்படுத்தவும்."

msgctxt "runtimeCommand->run->description"
msgid "Run command inside subsystem."
msgstr "துணை அமைப்புக்குள் கட்டளையை இயக்கவும்."

msgctxt "runtimeCommand->enter->description"
msgid "Enter the subsystem's environment."
msgstr "துணை அமைப்பின் சூழலை உள்ளிடவும்."

msgctxt "runtimeCommand->export->description"
msgid "Export an application or binary from the subsystem to the host."
msgstr ""
"துணை அமைப்பிலிருந்து ஓச்டுக்கு ஒரு பயன்பாடு அல்லது பைனரியை ஏற்றுமதி "
"செய்யவும்."

msgctxt "runtimeCommand->export->options->appName->description"
msgid "The name of the application to export."
msgstr "ஏற்றுமதி செய்வதற்கான பயன்பாட்டின் பெயர்."

msgctxt "runtimeCommand->export->options->bin->description"
msgid "The name of the binary to export."
msgstr "ஏற்றுமதி செய்ய பைனரியின் பெயர்."

msgctxt "runtimeCommand->export->options->binOutput->description"
msgid "Path of the binary output (default: ~/.local/bin/)."
msgstr "பைனரி வெளியீட்டின் பாதை (இயல்புநிலை: ~/.local/bin/)."

msgctxt "runtimeCommand->unexport->description"
msgid "Unexport an application or binary from the host."
msgstr "ஓச்டிலிருந்து ஒரு பயன்பாடு அல்லது பைனரி ஏற்றுமதியை நீக்கவும்."

msgctxt "runtimeCommand->unexport->options->appName->description"
msgid "The name of the application to unexport."
msgstr "ஏற்றுமதியை நீக்குவதற்கான பயன்பாட்டின் பெயர்."

msgctxt "runtimeCommand->unexport->options->bin->description"
msgid "The name of the binary to unexport."
msgstr "ஏற்றுமதி செய்யாத பைனரியின் பெயர்."

msgctxt "runtimeCommand->unexport->options->binOutput->description"
msgid "Path of the binary output (default: ~/.local/bin/)."
msgstr "பைனரி வெளியீட்டின் பாதை (இயல்புநிலை: ~/.local/bin/)."

msgctxt "runtimeCommand->start->description"
msgid "Start the subsystem."
msgstr "துணை அமைப்பைத் தொடங்கவும்."

msgctxt "runtimeCommand->stop->description"
msgid "Stop the subsystem."
msgstr "துணை அமைப்பை நிறுத்து."

msgctxt "pkgmanagers->description"
msgid "Work with the package managers that are available in apx."
msgstr "apx இல் கிடைக்கும் தொகுப்பு மேலாளர்களுடன் வேலை செய்யுங்கள்."

msgctxt "pkgmanagers->labels->name"
msgid "Name"
msgstr "பெயர்"

msgctxt "pkgmanagers->labels->builtIn"
msgid "Built-in"
msgstr "உள்ளமைக்கப்பட்ட"

msgctxt "pkgmanagers->list->description"
msgid "List all available package managers."
msgstr "கிடைக்கக்கூடிய அனைத்து தொகுப்பு மேலாளர்களையும் பட்டியலிடுங்கள்."

msgctxt "pkgmanagers->list->info->noPkgManagers"
msgid ""
"No package managers available. Create a new one with 'apx pkgmanagers new' "
"or contact the system administrator."
msgstr ""
"தொகுப்பு மேலாளர்கள் இல்லை. 'apx pkgmanagers new' உடன் புதிய ஒன்றை "
"உருவாக்கவும் அல்லது கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "pkgmanagers->list->info->foundPkgManagers"
msgid "Found %d package managers"
msgstr "%d தொகுப்பு நிர்வாகிகள் உள்ளனர்"

msgctxt "pkgmanagers->list->options->json->description"
msgid "Output in JSON format."
msgstr "சாதொபொகு வடிவத்தில் வெளியீடு."

msgctxt "pkgmanagers->show->description"
msgid "Show information about the specified package manager."
msgstr "குறிப்பிட்ட தொகுப்பு மேலாளர் பற்றிய தகவலைக் காட்டு."

msgctxt "pkgmanagers->new->description"
msgid "Create a new package manager."
msgstr "புதிய தொகுப்பு மேலாளரை உருவாக்கவும்."

msgctxt "pkgmanagers->new->success"
msgid "Package manager %s created successfully."
msgstr "தொகுப்பு மேலாளர் %s வெற்றிகரமாக உருவாக்கப்பட்டது."

msgctxt "pkgmanagers->new->error->emptyName"
msgid "The name cannot be empty."
msgstr "பெயர் காலியாக இருக்க முடியாது."

msgctxt "pkgmanagers->new->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "pkgmanagers->new->error->emptyCommand"
msgid "The command cannot be empty."
msgstr "கட்டளை காலியாக இருக்க முடியாது."

msgctxt "pkgmanagers->new->error->noCommand"
msgid "No command specified for '%s'."
msgstr "'%s'க்கு எந்த கட்டளையும் குறிப்பிடப்படவில்லை."

msgctxt "pkgmanagers->new->error->alreadyExists"
msgid "A package manager with the name '%s' already exists."
msgstr "'%s' என்ற பெயரில் ஒரு தொகுப்பு மேலாளர் ஏற்கனவே உள்ளது."

msgctxt "pkgmanagers->new->info->askName"
msgid "Choose a name:"
msgstr "ஒரு பெயரை தேர்வு செய்யவும்:"

msgctxt "pkgmanagers->new->info->askSudo"
msgid "Does the package manager need sudo to run?"
msgstr "தொகுப்பு மேலாளருக்கு இயக்க சூடோ தேவையா?"

msgctxt "pkgmanagers->new->info->askCommand"
msgid "Enter the command for '%s':"
msgstr "'%s' க்கான கட்டளையை உள்ளிடவும்:"

msgctxt "pkgmanagers->new->info->askCommandWithDefault"
msgid "Enter the command for '%s' (default: '%s'):"
msgstr "'%s' க்கான கட்டளையை உள்ளிடவும் (இயல்புநிலை: '%s'):"

msgctxt "pkgmanagers->new->info->askOverwrite"
msgid "A package manager with the name '%s' already exists. Overwrite it?"
msgstr "'%s' என்ற பெயரில் ஒரு தொகுப்பு மேலாளர் ஏற்கனவே உள்ளது. மேலெழுதவா?"

msgctxt "pkgmanagers->new->options->noPrompt->description"
msgid "Assume defaults to all prompts."
msgstr "அனைத்து அறிவுறுத்தல்களுக்கும் இயல்புநிலையை எடுத்துக் கொள்ளுங்கள்."

msgctxt "pkgmanagers->new->options->name->description"
msgid "The name of the package manager."
msgstr "தொகுப்பு மேலாளரின் பெயர்."

msgctxt "pkgmanagers->new->options->needSudo->description"
msgid "Whether the package manager needs sudo to run."
msgstr "பேக்கேச் மேனேசருக்கு இயக்க சூடோ தேவையா."

msgctxt "pkgmanagers->new->options->autoremove->description"
msgid "The command to run to autoremove packages."
msgstr "தொகுப்புகளை தானாக அகற்ற இயக்க கட்டளை."

msgctxt "pkgmanagers->new->options->clean->description"
msgid "The command to run to clean the package manager's cache."
msgstr ""
"தொகுப்பு மேலாளரின் தற்காலிக சேமிப்பை தூய்மை செய்ய இயக்குவதற்கான கட்டளை."

msgctxt "pkgmanagers->new->options->install->description"
msgid "The command to run to install packages."
msgstr "தொகுப்புகளை நிறுவ இயக்க கட்டளை."

msgctxt "pkgmanagers->new->options->list->description"
msgid "The command to run to list installed packages."
msgstr "நிறுவப்பட்ட தொகுப்புகளை பட்டியலிட இயக்க கட்டளை."

msgctxt "pkgmanagers->new->options->purge->description"
msgid "The command to run to purge packages."
msgstr "தொகுப்புகளை சுத்தப்படுத்த இயக்க கட்டளை."

msgctxt "pkgmanagers->new->options->remove->description"
msgid "The command to run to remove packages."
msgstr "தொகுப்புகளை அகற்ற இயக்க கட்டளை."

msgctxt "pkgmanagers->new->options->search->description"
msgid "The command to run to search for packages."
msgstr "தொகுப்புகளைத் தேட இயக்க கட்டளை."

msgctxt "pkgmanagers->new->options->show->description"
msgid "The command to run to show information about packages."
msgstr "தொகுப்புகளைப் பற்றிய தகவலைக் காட்ட இயக்க கட்டளை."

msgctxt "pkgmanagers->new->options->update->description"
msgid "The command to run to update the list of available packages."
msgstr ""
"கிடைக்கக்கூடிய தொகுப்புகளின் பட்டியலைப் புதுப்பிக்க இயக்க வேண்டிய கட்டளை."

msgctxt "pkgmanagers->new->options->upgrade->description"
msgid "The command to run to upgrade packages."
msgstr "தொகுப்புகளை மேம்படுத்த இயக்க கட்டளை."

msgctxt "pkgmanagers->update->description"
msgid "Update the specified package manager."
msgstr "குறிப்பிட்ட தொகுப்பு மேலாளரைப் புதுப்பிக்கவும்."

msgctxt "pkgmanagers->update->info->success"
msgid "Updated package manager '%s'."
msgstr "புதுப்பிக்கப்பட்ட தொகுப்பு மேலாளர் '%s'."

msgctxt "pkgmanagers->update->info->askNewCommand"
msgid "Enter new command for '%s' (leave empty to keep '%s'):"
msgstr ""
"'%s'க்கான புதிய கட்டளையை உள்ளிடவும் ('%s' ஐ வைத்திருக்க காலியாக விடவும்):"

msgctxt "pkgmanagers->update->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "pkgmanagers->update->error->missingCommand"
msgid "Missing command for %s."
msgstr "%sக்கான கட்டளை இல்லை."

msgctxt "pkgmanagers->update->error->builtIn"
msgid "Built-in package managers cannot be modified."
msgstr "உள்ளமைக்கப்பட்ட தொகுப்பு மேலாளர்களை மாற்ற முடியாது."

msgctxt "pkgmanagers->rm->description"
msgid "Remove the specified package manager."
msgstr "குறிப்பிட்ட தொகுப்பு மேலாளரை அகற்றவும்."

msgctxt "pkgmanagers->rm->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "pkgmanagers->rm->error->inUse"
msgid "The package manager is used in %d stacks:"
msgstr "தொகுப்பு மேலாளர் %d அடுக்குகளில் பயன்படுத்தப்படுகிறது:"

#. Translators: Don't translate the input prompt "[y/N]" as localized variants
#. aren't supported.
msgctxt "pkgmanagers->rm->info->askConfirmation"
msgid "Are you sure you want to remove '%s'?"
msgstr "'%s' ஐ நிச்சயமாக அகற்ற விரும்புகிறீர்களா?"

msgctxt "pkgmanagers->rm->info->success"
msgid "Removed package manager '%s'."
msgstr "தொகுப்பு மேலாளர் '%s' அகற்றப்பட்டது."

msgctxt "pkgmanagers->rm->info->aborting"
msgid "Aborting removal of package manager '%s'."
msgstr "தொகுப்பு மேலாளர் '%s' ஐ அகற்றுவதை நிறுத்துகிறது."

msgctxt "pkgmanagers->rm->options->force->description"
msgid "Force removal of the package manager."
msgstr "தொகுப்பு மேலாளரின் கட்டாய நீக்கம்."

msgctxt "pkgmanagers->rm->options->name->description"
msgid "The name of the package manager to remove."
msgstr "அகற்ற வேண்டிய தொகுப்பு மேலாளரின் பெயர்."

msgctxt "pkgmanagers->export->description"
msgid "Export the specified package manager."
msgstr "குறிப்பிட்ட தொகுப்பு மேலாளரை ஏற்றுமதி செய்யவும்."

msgctxt "pkgmanagers->export->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "pkgmanagers->export->error->noOutput"
msgid "No output specified."
msgstr "வெளியீடு குறிப்பிடப்படவில்லை."

msgctxt "pkgmanagers->export->info->success"
msgid "Exported package manager '%s' to '%s'."
msgstr "தொகுப்பு மேலாளர் '%s' க்கு '%s' க்கு ஏற்றுமதி செய்யப்பட்டது."

msgctxt "pkgmanagers->export->options->name->description"
msgid "The name of the package manager to export."
msgstr "ஏற்றுமதி செய்ய வேண்டிய தொகுப்பு மேலாளரின் பெயர்."

msgctxt "pkgmanagers->export->options->output->description"
msgid "The path to export the package manager to."
msgstr "தொகுப்பு மேலாளரை ஏற்றுமதி செய்வதற்கான பாதை."

msgctxt "pkgmanagers->import->description"
msgid "Import the specified package manager."
msgstr "குறிப்பிட்ட தொகுப்பு மேலாளரை இறக்குமதி செய்யவும்."

msgctxt "pkgmanagers->import->error->noInput"
msgid "No input specified."
msgstr "உள்ளீடு எதுவும் குறிப்பிடப்படவில்லை."

msgctxt "pkgmanagers->import->error->cannotLoad"
msgid "Cannot load package manager from '%s'."
msgstr "'%s' இலிருந்து தொகுப்பு நிர்வாகியை ஏற்ற முடியவில்லை."

msgctxt "pkgmanagers->import->info->success"
msgid "Imported package manager from '%s'."
msgstr "'%s' இலிருந்து இறக்குமதி செய்யப்பட்ட தொகுப்பு மேலாளர்."

msgctxt "pkgmanagers->import->options->input->description"
msgid "The path to import the package manager from."
msgstr "தொகுப்பு மேலாளரை இறக்குமதி செய்வதற்கான பாதை."

msgctxt "stacks->description"
msgid "Work with the stacks that are available in apx."
msgstr "apx இல் கிடைக்கும் அடுக்குகளுடன் வேலை செய்யுங்கள்."

msgctxt "stacks->labels->name"
msgid "Name"
msgstr "பெயர்"

msgctxt "stacks->labels->builtIn"
msgid "Built-in"
msgstr "உள்ளமைக்கப்பட்ட"

msgctxt "stacks->list->description"
msgid "List all available stacks."
msgstr "கிடைக்கக்கூடிய அனைத்து அடுக்குகளையும் பட்டியலிடுங்கள்."

msgctxt "stacks->list->info->noStacks"
msgid ""
"No stacks available. Create a new one with 'apx stacks new' or contact the "
"system administrator."
msgstr ""
"அடுக்குகள் இல்லை. 'apx stacks new' உடன் புதிய ஒன்றை உருவாக்கவும் அல்லது "
"கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "stacks->list->info->foundStacks"
msgid "Found %d stacks:"
msgstr "%d அடுக்குகள் உள்ளன:"

msgctxt "stacks->list->info->aborting"
msgid "Aborting removal of stack '%s'."
msgstr "'%s' அடுக்கை அகற்றுவதை நிறுத்துகிறது."

msgctxt "stacks->list->options->json->description"
msgid "Output in JSON format."
msgstr "சாதொபொகு வடிவத்தில் வெளியீடு."

msgctxt "stacks->show->description"
msgid "Show information about the specified stack."
msgstr "குறிப்பிட்ட அடுக்கைப் பற்றிய தகவலைக் காட்டு."

msgctxt "stacks->new->description"
msgid "Create a new stack."
msgstr "புதிய அடுக்கை உருவாக்கவும்."

msgctxt "stacks->new->error->emptyName"
msgid "The name cannot be empty."
msgstr "பெயர் காலியாக இருக்க முடியாது."

msgctxt "stacks->new->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "stacks->new->error->emptyBase"
msgid "The base cannot be empty."
msgstr "அடித்தளம் காலியாக இருக்க முடியாது."

msgctxt "stacks->new->error->noBase"
msgid "No base specified."
msgstr "எந்த அடிப்படையும் குறிப்பிடப்படவில்லை."

msgctxt "stacks->new->error->emptyPkgManager"
msgid "The package manager cannot be empty."
msgstr "தொகுப்பு மேலாளர் காலியாக இருக்க முடியாது."

msgctxt "stacks->new->error->noPkgManager"
msgid "No package manager specified."
msgstr "தொகுப்பு மேலாளர் குறிப்பிடப்படவில்லை."

msgctxt "stacks->new->error->noPkgManagers"
msgid ""
"Could not find any package managers. Create one with 'apx pkgmanagers new' "
"or contact the system administrator."
msgstr ""
"தொகுப்பு நிர்வாகிகள் எதையும் கண்டுபிடிக்க முடியவில்லை. 'apx pkgmanagers new'"
" உடன் ஒன்றை உருவாக்கவும் அல்லது கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "stacks->new->error->pkgManagerDoesNotExist"
msgid ""
"The specified package manager does not exist. Create it with 'apx "
"pkgmanagers new' or contact the system administrator."
msgstr ""
"குறிப்பிட்ட தொகுப்பு மேலாளர் இல்லை. அதை 'apx pkgmanagers new' மூலம் "
"உருவாக்கவும் அல்லது கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "stacks->new->error->emptyPackages"
msgid "The packages cannot be empty."
msgstr "தொகுப்புகள் காலியாக இருக்கக்கூடாது."

msgctxt "stacks->new->error->noPackages"
msgid "No packages specified."
msgstr "தொகுப்புகள் எதுவும் குறிப்பிடப்படவில்லை."

msgctxt "stacks->new->error->alreadyExists"
msgid "A stack with the name '%s' already exists."
msgstr "'%s' என்ற பெயரில் ஒரு அடுக்கு ஏற்கனவே உள்ளது."

msgctxt "stacks->new->info->askName"
msgid "Choose a name:"
msgstr "ஒரு பெயரை தேர்வு செய்யவும்:"

msgctxt "stacks->new->info->askBase"
msgid "Choose a base (e.g. 'vanillaos/pico'):"
msgstr "ஒரு தளத்தைத் தேர்ந்தெடுக்கவும் (எ.கா. 'வெண்ணிலாச்/பிகோ'):"

msgctxt "stacks->new->info->askPkgManager"
msgid "Choose a package manager:"
msgstr "தொகுப்பு மேலாளரைத் தேர்ந்தெடுக்கவும்:"

msgctxt "stacks->new->info->selectPkgManager"
msgid "Select a package manager [1-%d]:"
msgstr "தொகுப்பு மேலாளரைத் தேர்ந்தெடுக்கவும் [1-%d]:"

msgctxt "stacks->new->info->noPackages"
msgid ""
"You have not provided any packages to install in the stack. Do you want to "
"add some now?"
msgstr ""
"அடுக்கில் நிறுவுவதற்கு நீங்கள் எந்த தொகுப்புகளையும் வழங்கவில்லை. இப்போது "
"சிலவற்றைச் சேர்க்க விரும்புகிறீர்களா?"

msgctxt "stacks->new->info->confirmPackages"
msgid "Do you want to confirm the current packages list?"
msgstr "தற்போதைய தொகுப்புகளின் பட்டியலை உறுதிப்படுத்த விரும்புகிறீர்களா?"

msgctxt "stacks->new->info->askPackages"
msgid ""
"Please type the packages you want to install in the stack, separated by a "
"space:"
msgstr ""
"ச்பேசால் பிரிக்கப்பட்ட அடுக்கில் நீங்கள் நிறுவ விரும்பும் தொகுப்புகளை "
"தட்டச்சு செய்யவும்:"

msgctxt "stacks->new->info->success"
msgid "Created stack '%s'."
msgstr "'%s' அடுக்கு உருவாக்கப்பட்டது."

msgctxt "stacks->new->options->noPrompt->description"
msgid "Assume defaults to all prompts."
msgstr "அனைத்து அறிவுறுத்தல்களுக்கும் இயல்புநிலையை எடுத்துக் கொள்ளுங்கள்."

msgctxt "stacks->new->options->name->description"
msgid "The name of the stack."
msgstr "அடுக்கின் பெயர்."

msgctxt "stacks->new->options->base->description"
msgid ""
"The base distribution image to use. (For a list of compatible images view: "
"https://distrobox.it/compatibility/#containers-distros)"
msgstr ""
"பயன்படுத்த வேண்டிய அடிப்படை விநியோகப் படம். (இணக்கமான படங்களின் பட்டியலுக்கு"
" பார்க்க: https://distrobox.it/compatibility/#containers-distros)"

msgctxt "stacks->new->options->packages->description"
msgid "The packages to install."
msgstr "நிறுவ வேண்டிய தொகுப்புகள்."

msgctxt "stacks->new->options->pkgManager->description"
msgid "The package manager to use."
msgstr "பயன்படுத்த வேண்டிய தொகுப்பு மேலாளர்."

msgctxt "stacks->update->description"
msgid "Update the specified stack."
msgstr "குறிப்பிட்ட அடுக்கைப் புதுப்பிக்கவும்."

msgctxt "stacks->update->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "stacks->update->error->noBase"
msgid "No base specified."
msgstr "எந்த அடிப்படையும் குறிப்பிடப்படவில்லை."

msgctxt "stacks->update->error->noPkgManager"
msgid "No package manager specified."
msgstr "தொகுப்பு மேலாளர் குறிப்பிடப்படவில்லை."

msgctxt "stacks->update->error->pkgManagerDoesNotExist"
msgid ""
"The specified package manager does not exist. Create it with 'apx "
"pkgmanagers new' or contact the system administrator."
msgstr ""
"குறிப்பிட்ட தொகுப்பு மேலாளர் இல்லை. அதை 'apx pkgmanagers new' மூலம் "
"உருவாக்கவும் அல்லது கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "stacks->update->error->builtIn"
msgid "Built-in stacks cannot be modified."
msgstr "உள்ளமைக்கப்பட்ட அடுக்குகளை மாற்ற முடியாது."

msgctxt "stacks->update->info->askBase"
msgid "Type a new base or confirm the current one (%s):"
msgstr ""
"புதிய தளத்தைத் தட்டச்சு செய்யவும் அல்லது தற்போதைய ஒன்றை உறுதிப்படுத்தவும் "
"(%s):"

msgctxt "stacks->update->info->askPkgManager"
msgid "Choose a new package manager or confirm the current one (%s):"
msgstr ""
"புதிய தொகுப்பு மேலாளரைத் தேர்வு செய்யவும் அல்லது தற்போதைய ஒன்றை (%s) "
"உறுதிப்படுத்தவும்:"

msgctxt "stacks->update->info->confirmPackages"
msgid "Do you want to update the list of packages?"
msgstr "தொகுப்புகளின் பட்டியலைப் புதுப்பிக்க விரும்புகிறீர்களா?"

msgctxt "stacks->update->info->noPackages"
msgid ""
"You have not provided any packages to install in the stack. Do you want to "
"add some now?"
msgstr ""
"அடுக்கில் நிறுவுவதற்கு நீங்கள் எந்த தொகுப்புகளையும் வழங்கவில்லை. இப்போது "
"சிலவற்றைச் சேர்க்க விரும்புகிறீர்களா?"

msgctxt "stacks->update->info->askPackages"
msgid ""
"Type the packages you want to install in the stack, separated by a space:"
msgstr ""
"நீங்கள் நிறுவ விரும்பும் தொகுப்புகளை அடுக்கில் உள்ளிடவும், இடைவெளியால் "
"பிரிக்கவும்:"

msgctxt "stacks->update->info->success"
msgid "Updated stack '%s'."
msgstr "புதுப்பிக்கப்பட்ட அடுக்கு '%s'."

msgctxt "stacks->update->options->noPrompt->description"
msgid "Assume defaults to all prompts."
msgstr "அனைத்து அறிவுறுத்தல்களுக்கும் இயல்புநிலையை எடுத்துக் கொள்ளுங்கள்."

msgctxt "stacks->update->options->name->description"
msgid "The name of the stack."
msgstr "அடுக்கின் பெயர்."

msgctxt "stacks->update->options->base->description"
