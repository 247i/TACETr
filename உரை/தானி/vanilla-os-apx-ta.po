msgctxt "stacks->rm->info->success"
msgid "Removed stack '%s'."
msgstr "'%s' அடுக்கு அகற்றப்பட்டது."

msgctxt "stacks->rm->options->force->description"
msgid "Force removal of the stack."
msgstr "அடுக்கை வலுக்கட்டாயமாக அகற்றுதல்."

msgctxt "stacks->rm->options->name->description"
msgid "The name of the stack to remove."
msgstr "அகற்ற வேண்டிய அடுக்கின் பெயர்."

msgctxt "stacks->export->description"
msgid "Export the specified stack."
msgstr "குறிப்பிட்ட அடுக்கை ஏற்றுமதி செய்யவும்."

msgctxt "stacks->export->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "stacks->export->error->noOutput"
msgid "No output specified."
msgstr "வெளியீடு குறிப்பிடப்படவில்லை."

msgctxt "stacks->export->info->success"
msgid "Exported stack '%s' to '%s'."
msgstr "'%s' அடுக்கு '%s'க்கு ஏற்றுமதி செய்யப்பட்டது."

msgctxt "stacks->export->options->name->description"
msgid "The name of the stack to export."
msgstr "ஏற்றுமதி செய்ய வேண்டிய அடுக்கின் பெயர்."

msgctxt "stacks->export->options->output->description"
msgid "The path to export the stack to."
msgstr "ச்டாக்கை ஏற்றுமதி செய்வதற்கான பாதை."

msgctxt "stacks->import->description"
msgid "Import the specified stack."
msgstr "குறிப்பிட்ட அடுக்கை இறக்குமதி செய்யவும்."

msgctxt "stacks->import->error->noInput"
msgid "No input specified."
msgstr "உள்ளீடு எதுவும் குறிப்பிடப்படவில்லை."

msgctxt "stacks->import->error->cannotLoad"
msgid "Cannot load stack from '%s'."
msgstr "'%s' இலிருந்து அடுக்கை ஏற்ற முடியவில்லை."

msgctxt "stacks->import->info->success"
msgid "Imported stack from '%s'."
msgstr "'%s' இலிருந்து ச்டாக் இறக்குமதி செய்யப்பட்டது."

msgctxt "stacks->import->options->input->description"
msgid "The path to import the stack from."
msgstr "ச்டாக்கை இறக்குமதி செய்வதற்கான பாதை."

msgctxt "subsystems->description"
msgid "Work with the subsystems that are available in apx."
msgstr "apx இல் கிடைக்கும் துணை அமைப்புகளுடன் வேலை செய்யுங்கள்."

msgctxt "subsystems->labels->name"
msgid "Name"
msgstr "பெயர்"

msgctxt "subsystems->labels->status"
msgid "Status"
msgstr "நிலை"

msgctxt "subsystems->list->description"
msgid "List all available subsystems."
msgstr "கிடைக்கக்கூடிய அனைத்து துணை அமைப்புகளையும் பட்டியலிடுங்கள்."

msgctxt "subsystems->list->info->noSubsystems"
msgid ""
"No subsystems available. Create a new one with 'apx subsystems new' or "
"contact the system administrator."
msgstr ""
"துணை அமைப்புகள் எதுவும் இல்லை. 'apx subsystems new' உடன் புதிய ஒன்றை "
"உருவாக்கவும் அல்லது கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "subsystems->list->info->foundSubsystems"
msgid "Found %d subsystems:"
msgstr "%d துணை அமைப்புகள் உள்ளன:"

msgctxt "subsystems->list->options->json->description"
msgid "Output in JSON format."
msgstr "சாதொபொகு வடிவத்தில் வெளியீடு."

msgctxt "subsystems->new->description"
msgid "Create a new subsystem."
msgstr "புதிய துணை அமைப்பை உருவாக்கவும்."

msgctxt "subsystems->new->error->noStacks"
msgid ""
"A stack is needed to create a subsystem. Create a new one with 'apx stacks "
"new' or contact the system administrator."
msgstr ""
"ஒரு துணை அமைப்பை உருவாக்க ஒரு அடுக்கு தேவை. 'apx stacks new' உடன் புதிய "
"ஒன்றை உருவாக்கவும் அல்லது கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "subsystems->new->error->emptyName"
msgid "The name cannot be empty."
msgstr "பெயர் காலியாக இருக்க முடியாது."

msgctxt "subsystems->new->error->forbiddenName"
msgid "The name '%s' is forbidden. Please choose a different name"
msgstr ""
"'%s' என்ற பெயர் தடைசெய்யப்பட்டுள்ளது. தயவுசெய்து வேறு பெயரைத் "
"தேர்ந்தெடுக்கவும்"

msgctxt "subsystems->new->error->noName"
msgid "No name specified."
msgstr "பெயர் குறிப்பிடப்படவில்லை."

msgctxt "subsystems->new->error->emptyStack"
msgid "The stack cannot be empty."
msgstr "ச்டாக் காலியாக இருக்க முடியாது."

msgctxt "subsystems->new->error->noStack"
msgid "No stack specified."
msgstr "ச்டாக் எதுவும் குறிப்பிடப்படவில்லை."

msgctxt "subsystems->new->error->stackDoesNotExist"
msgid ""
"The specified stack does not exist. Create it with 'apx stacks new' or "
"contact the system administrator."
msgstr ""
"குறிப்பிடப்பட்ட அடுக்கு இல்லை. 'apx stacks new' உடன் உருவாக்கவும் அல்லது "
"கணினி நிர்வாகியைத் தொடர்பு கொள்ளவும்."

msgctxt "subsystems->new->error->alreadyExists"
msgid "A subsystem with the name '%s' already exists."
msgstr "'%s' என்ற பெயரில் ஒரு துணை அமைப்பு ஏற்கனவே உள்ளது."

msgctxt "subsystems->new->info->askName"
msgid "Choose a name:"
msgstr "ஒரு பெயரை தேர்வு செய்யவும்:"

msgctxt "subsystems->new->info->availableStacks"
msgid "Available stacks:"
msgstr "கிடைக்கும் அடுக்குகள்:"

msgctxt "subsystems->new->info->selectStack"
msgid "Select a stack [1-%d]:"
msgstr "ஒரு அடுக்கைத் தேர்ந்தெடு [1-%d]:"

