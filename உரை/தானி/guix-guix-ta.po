msgstr "தரவுத்தளத்தை அழிக்கிறது...~%"

#: guix/scripts/locate.scm:658
#, scheme-format
msgid "indexing files from ~a...~%"
msgstr "~a...~% இலிருந்து கோப்புகளை அட்டவணைப்படுத்துதல்"

#: guix/scripts/locate.scm:672
#, scheme-format
msgid "no files to search for~%"
msgstr "தேடுவதற்கு கோப்புகள் இல்லை ~%"

#: guix/scripts/locate.scm:673
#, scheme-format
msgid "file~{ '~a'~} not found in database '~a'~%"
msgid_plural "files~{ '~a'~} not found in database '~a'~%"
msgstr[0] "file~{ '~a'~} not found in database '~a'~%"
msgstr[1] "கோப்புகள்~{ '~a'~} தரவுத்தளத்தில் இல்லை '~a'~%"

#: guix/scripts/publish.scm:81
#, scheme-format
msgid ""
"Usage: guix publish [OPTION]...\n"
"Publish ~a over HTTP.\n"
msgstr ""
"பயன்பாடு: guix publish [<செயல்மாற்றி>]...\n"
"~a யை HTTP மூலம் வெளியிடு.\n"

#: guix/scripts/publish.scm:83
msgid ""
"\n"
"  -p, --port=PORT        listen on PORT"
msgstr ""
"\n"
"-p, --port=PORT இல் துறைமுகம் கேட்கவும்"

#: guix/scripts/publish.scm:85
msgid ""
"\n"
"      --listen=HOST      listen on the network interface for HOST"
msgstr ""
"\n"
"--listen=HOSTக்கான பிணைய இடைமுகத்தில் விருந்தோம்பி கேட்கிறது"

#: guix/scripts/publish.scm:87
msgid ""
"\n"
"  -u, --user=USER        change privileges to USER as soon as possible"
msgstr ""
"\n"
"-u, --user=USER உரிமைகளை USERக்கு விரைவில் மாற்றவும்"

#: guix/scripts/publish.scm:89
msgid ""
"\n"
"  -a, --advertise        advertise on the local network"
msgstr ""
"\n"
"-a, --உள்ளூர் நெட்வொர்க்கில் விளம்பரம் செய்யவும்"

#: guix/scripts/publish.scm:91
msgid ""
"\n"
"  -C, --compression[=METHOD:LEVEL]\n"
"                         compress archives with METHOD at LEVEL"
msgstr ""
"\n"
"-சி, --அமுக்கம்[=முறை:நிலை] \n"
"LEVEL இல் METHOD உடன் காப்பகங்களை சுருக்கவும்"

#: guix/scripts/publish.scm:94
msgid ""
"\n"
"  -c, --cache=DIRECTORY  cache published items to DIRECTORY"
msgstr ""
"\n"
"-c, --cache=DIRECTORY கேச் DIRECTORY க்கு உருப்படிகளை வெளியிட்டது"

#: guix/scripts/publish.scm:96
msgid ""
"\n"
"      --cache-bypass-threshold=SIZE\n"
"                         serve store items below SIZE even when not cached"
msgstr ""
"\n"
"--cache-bypass-threshold=SIZE \n"
"தற்காலிக சேமிப்பில் இல்லாவிட்டாலும், அளவு க்குக் கீழே உள்ள பொருட்களை வழங்கவும்"

#: guix/scripts/publish.scm:99
msgid ""
"\n"
"      --workers=N        use N workers to bake items"
msgstr ""
"\n"
"--workers=N பொருட்களை சுட N தொழிலாளர்களைப் பயன்படுத்துகிறது"

#: guix/scripts/publish.scm:101
msgid ""
"\n"
"      --ttl=TTL          announce narinfos can be cached for TTL seconds"
msgstr ""
"\n"
"--ttl=TTL அறிவிப்பு narinfos ஐ TTL வினாடிகளுக்கு தற்காலிகமாக சேமிக்க முடியும்"

#: guix/scripts/publish.scm:103
msgid ""
"\n"
"      --negative-ttl=TTL announce missing narinfos can be cached for TTL seconds"
msgstr ""
"\n"
"--negative-ttl=டிடிஎல் அறிவிக்கிறது விடுபட்ட நரின்ஃபோசை TTL வினாடிகளுக்கு தற்காலிகமாக சேமிக்க முடியும்"

#: guix/scripts/publish.scm:105
msgid ""
"\n"
"      --nar-path=PATH    use PATH as the prefix for nar URLs"
msgstr ""
"\n"
"--nar-path=NA URLகளுக்கான முன்னொட்டாக பாதை ஐப் பயன்படுத்துகிறது"

#: guix/scripts/publish.scm:107
msgid ""
"\n"
"      --public-key=FILE  use FILE as the public key for signatures"
msgstr ""
"\n"
"--public-key=FILE கையொப்பங்களுக்கான பொது விசையாக கோப்பு ஐப் பயன்படுத்துகிறது"

#: guix/scripts/publish.scm:109
msgid ""
"\n"
"      --private-key=FILE use FILE as the private key for signatures"
msgstr ""
"\n"
"--private-key=FILE கையொப்பங்களுக்கான தனிப்பட்ட விசையாக கோப்பு ஐப் பயன்படுத்துகிறது"

#: guix/scripts/publish.scm:111
msgid ""
"\n"
"  -r, --repl[=PORT]      spawn REPL server on PORT"
msgstr ""
"\n"
"-r, --repl[=PORT] துறைமுகம் இல் REPL சேவையகத்தை உருவாக்குகிறது"

