msgctxt "strings.FILENAME"
msgid "Name"
msgstr "பெயர்"

msgctxt "strings.ALGORITHM"
msgid "Algorithm"
msgstr "படிமுறை"

msgctxt "strings.COPIED"
msgid "✔ Copied"
msgstr ". நகலெடுக்கப்பட்டது"

msgctxt "strings.HASH"
msgid "Hash"
msgstr "ஆச்"

msgctxt "strings.CHECK_AGAINST"
msgid "Check against:"
msgstr "எதிராக சரிபார்க்கவும்:"

msgctxt "strings.EXPORT_TO"
msgid "Export to sumfile:"
msgstr "சம்ஃபைலுக்கு ஏற்றுமதி:"

msgctxt "strings.EXPORT_BTN"
msgid "Export…"
msgstr "ஏற்றுமதி…"

msgctxt "strings.SUMFILE"
msgid "Interpreted as sumfile."
msgstr "சம்ஃபைல் என விளக்கப்படுகிறது."

msgctxt "strings.DONE"
msgid "Done!"
msgstr "முடிந்தது!"

msgctxt "strings.SUMMARY"
msgid "Summary"
msgstr "சுருக்கம்"

msgctxt "strings.PROCESSING"
msgid "Processing…"
msgstr "செயலாக்கம்…"

#, ignore-same
msgctxt "strings.HASHES"
msgid "Hashes"
msgstr "ஆச்கள்"

msgctxt "strings.CLIPBOARD"
msgid "To clipboard"
msgstr "இடைநிலைப்பலகைக்கு"

msgctxt "strings.NOMATCH"
msgid "No match found."
msgstr "போட்டி எதுவும் கிடைக்கவில்லை."

msgctxt "strings.ERROR"
msgid "Error"
msgstr "பிழை"

msgctxt "strings.CANCEL"
msgid "Cancel"
msgstr "ரத்துசெய்"

msgctxt "strings.DISPLAY_UPPERCASE"
msgid "Display hashes in uppercase"
msgstr "பெரிய எழுத்தில் ஆச்களைக் காண்பி"

msgctxt "strings.DISPLAY_MONOSPACE"
msgid "Display hashes in monospace font"
msgstr "மோனோச்பேச் எழுத்துருவில் ஆச்களைக் காண்பி"

msgctxt "strings.LOOK_FOR_SUMFILES"
msgid "Look for sumfiles next to files being hashed"
msgstr "கோப்புகள் ஆசெட் செய்யப்படுவதற்கு அடுத்ததாக சம்ஃபைல்களைத் தேடுங்கள்"

msgctxt "strings.SUMFILE_UPPERCASE"
msgid "Export hashes in uppercase"
msgstr "பெரிய எழுத்தில் ஆச்களை ஏற்றுமதி செய்யுங்கள்"

msgctxt "strings.SUMFILE_UNIX_ENDINGS"
msgid "Export files with Unix line endings"
msgstr "யுனிக்ச் வரி முடிவுகளுடன் கோப்புகளை ஏற்றுமதி செய்யுங்கள்"

msgctxt "strings.SUMFILE_USE_DOUBLE_SPACE"
msgid "Export sumfiles with double space instead of space-asterisk"
msgstr ""
"விண்வெளி-அதிகப்படுவதற்கு பதிலாக இரட்டை இடத்துடன் சம்ஃபைல்களை ஏற்றுமதி "
"செய்யுங்கள்"

msgctxt "strings.SUMFILE_FORWARD_SLASHES"
msgid "Use forward slash paths in exported files"
msgstr ""
"ஏற்றுமதி செய்யப்பட்ட கோப்புகளில் முன்னோக்கி ச்லாச் பாதைகளைப் பயன்படுத்தவும்"

msgctxt "strings.SUMFILE_DOT_HASH_COMPATIBLE"
msgid "Export sumfiles in corz .hash compatible format"
msgstr "கோர்ச் .ஆச் இணக்கமான வடிவத்தில் சம்ஃபைல்களை ஏற்றுமதி செய்யுங்கள்"

msgctxt "strings.SUMFILE_BANNER"
msgid "Add banner to exported files"
msgstr "ஏற்றுமதி செய்யப்பட்ட கோப்புகளுக்கு பேனரைச் சேர்க்கவும்"

msgctxt "strings.SUMFILE_BANNER_DATE"
msgid "Add date to banner"
msgstr "பேனரில் தேதியைச் சேர்க்கவும்"

msgctxt "strings.CHECK_FOR_UPDATES"
msgid "Check for updates"
msgstr "புதுப்பிப்புகளை சரிபார்க்கவும்"

msgctxt "strings.COPY_HASH"
msgid "Copy hash"
msgstr "ஆச் நகலெடுக்கவும்"

msgctxt "strings.COPY_LINE"
msgid "Copy line"
msgstr "நகல் வரி"

msgctxt "strings.COPY_FILE"
msgid "Copy file name"
msgstr "கோப்பு பெயரை நகலெடுக்கவும்"

msgctxt "strings.COPY_EVERYTHING"
msgid "Copy everything"
msgstr "எல்லாவற்றையும் நகலெடுக்கவும்"

msgctxt "strings.VT_NO_COMPATIBLE"
msgid "No compatible algorithm enabled"
msgstr "இணக்கமான வழிமுறை எதுவும் இயக்கப்படவில்லை"

msgctxt "strings.VT_NOT_FOUND"
msgid "Not found in database"
msgstr "தரவுத்தளத்தில் காணப்படவில்லை"

msgctxt "strings.UPDATE_DISABLED_IN_DEV_BUILD"
msgid "Unavailable in development builds"
msgstr "வளர்ச்சி கட்டமைப்பில் கிடைக்கவில்லை"

msgctxt "strings.UPDATE_AVAILABLE_TITLE"
msgid "Update available"
msgstr "புதுப்பிப்பு கிடைக்கிறது"

#, c-format, ignore-unnamed-format
msgctxt "strings.UPDATE_AVAILABLE_TEXT"
msgid "New version %hu.%hu.%hu available"
msgstr "புதிய பதிப்பு%HU.%HU.%HU கிடைக்கிறது"

msgctxt "strings.UPDATE_NEWEST_TITLE"
msgid "Congratulations"
msgstr "வாழ்த்துக்கள்"

msgctxt "strings.UPDATE_NEWEST_TEXT"
msgid "You are already on the latest version"
msgstr "நீங்கள் ஏற்கனவே அண்மைக் கால பதிப்பில் இருக்கிறீர்கள்"

msgctxt "strings.CLIPBOARD_AUTOENABLE"
msgid ""
"If clipboard contains a hash, automatically enable algorithms of that length"
" for the run"
msgstr ""
"கிளிப்போர்டில் ஒரு ஆச் இருந்தால், தானாகவே அந்த நீளத்தின் வழிமுறைகளை "
"இயக்கவும்"

msgctxt "strings.CLIPBOARD_AUTOENABLE_IF_NONE"
msgid "Only if no algorithms of that length are enabled"
msgstr "அந்த நீளத்தின் வழிமுறைகள் எதுவும் இயக்கப்படவில்லை என்றால் மட்டுமே"

msgctxt "strings.CLIPBOARD_AUTOENABLE_EXCLUSIVE"
msgid "Disable all other algorithms"
msgstr "மற்ற எல்லா வழிமுறைகளையும் முடக்கவும்"

msgctxt "strings.SETTINGS_TITLE"
msgid "Settings"
msgstr "அமைப்புகள்"

msgctxt "strings.CHECKAGAINST_AUTOFORMAT"
msgid "Automatically reformat pasted hashes in checker field"
msgstr "செக்கர் துறையில் ஒட்டப்பட்ட ஆச்களை தானாக மறுவடிவமைப்பு செய்யுங்கள்"

msgctxt "strings.CHECKAGAINST_STRICT"
msgid "Strict mode for hash checker field"
msgstr "ஆச் செக்கர் புலத்திற்கான கடுமையான பயன்முறை"

msgctxt "strings.HASH_SUMFILE_TOO"
msgid "When processing a sumfile hash the sumfile too"
msgstr "ஒரு சம்ஃபைல் ஆசை செயலாக்கும்போது சம்ஃபைலும் கூட"

msgctxt "strings.ITEM_MATCHING"
msgid "1 matching file"
msgstr "பொருந்தக்கூடிய 1 கோப்பு"

msgctxt "strings.ITEMS_MATCHING"
msgid "%d matching files"
msgstr "பொருந்தக்கூடிய கோப்புகள் %d"

msgctxt "strings.SUMFILE_ALGORITHM_ONLY"
msgid "When processing a sumfile of known algorithm, disable other algorithms"
msgstr ""
"அறியப்பட்ட வழிமுறையின் ஒரு சம்ஃபைலை செயலாக்கும்போது, ​​பிற வழிமுறைகளை "
"முடக்கு"

msgctxt "strings.ERROR_GROUP"
msgid "Error"
msgstr "பிழை"

msgctxt "strings.MATCH_GROUP"
msgid "Match"
msgstr "போட்டி"

msgctxt "strings.MISMATCH_GROUP"
msgid "Mismatch"
msgstr "பொருந்தாத"

msgctxt "strings.INSECURE_GROUP"
msgid "Insecure"
msgstr "பாதுகாப்பற்றது"

msgctxt "strings.UNKNOWN_GROUP"
msgid "Unknown"
msgstr "தெரியவில்லை"

msgctxt "strings.FOREGROUND"
msgid "Foreground"
msgstr "முன்புறம்"

msgctxt "strings.BACKGROUND"
msgid "Background"
msgstr "பின்னணி"
