#: ../src/homeapplication.cpp:78
msgctxt "qtn_ap_lipstick"
msgid "System"
msgstr "மண்டலம்"

#: ../src/notifications/batterynotifier.cpp:212
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:142
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:166
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:181
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:258
msgctxt "qtn_ener_charging"
msgid "Charging"
msgstr "சார்சிங்"

#: ../src/notifications/batterynotifier.cpp:216
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:114
msgctxt "qtn_ener_charcomp"
msgid "Charging complete"
msgstr "சார்ச் முழுமையானது"

#: ../src/notifications/batterynotifier.cpp:220
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:172
msgctxt "qtn_ener_remcha"
msgid "Disconnect charger from power supply to save energy"
msgstr "ஆற்றலைச் சேமிக்க மின்சார விநியோகத்திலிருந்து சார்சரைத் துண்டிக்கவும்"

#: ../src/notifications/batterynotifier.cpp:224
msgctxt "qtn_ener_repcharger"
msgid "Not charging. Adjust or recycle charger."
msgstr ""
"கட்டணம் வசூலிக்கவில்லை. சார்சரை சரிசெய்யவும் அல்லது மறுசுழற்சி செய்யவும்."

#: ../src/notifications/batterynotifier.cpp:228
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:129
msgctxt "qtn_ener_rebatt"
msgid "Recharge battery"
msgstr "ரீசார்ச் பேட்டரி"

#: ../src/notifications/batterynotifier.cpp:232
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:205
msgctxt "qtn_ener_ent_psnote"
msgid "Entering power save mode"
msgstr "பவர் சேமி பயன்முறையில் நுழைகிறது"

#: ../src/notifications/batterynotifier.cpp:236
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:213
msgctxt "qtn_ener_exit_psnote"
msgid "Exiting power save mode"
msgstr "பவர் சேமி பயன்முறையிலிருந்து வெளியேறுதல்"

#: ../src/notifications/batterynotifier.cpp:240
#: ../tests/ut_batterynotifier/ut_batterynotifier.cpp:101
msgctxt "qtn_ener_lowbatt"
msgid "Low battery level"
msgstr "குறைந்த பேட்டரி நிலை"

#: ../src/notifications/batterynotifier.cpp:244
msgctxt "qtn_ener_nopowcharge"
msgid "Power input below charging threshold"
msgstr "சார்சிங் வாசலுக்கு கீழே உள்ள ஆற்றல் உள்ளீடு"

#: ../src/notifications/diskspacenotifier.cpp:56
msgctxt "qtn_memu_memlow_notification_src"
msgid "Storage space running out."
msgstr "சேமிப்பக இடம் முடிந்துவிட்டது."

#: ../src/notifications/thermalnotifier.cpp:34
#: ../tests/ut_thermalnotifier/ut_thermalnotifier.cpp:78
msgctxt "qtn_shut_high_temp_warning"
msgid "Device getting hot. Close all apps."
msgstr "சாதனம் சூடாகிறது. எல்லா பயன்பாடுகளையும் மூடு."

#: ../src/notifications/thermalnotifier.cpp:38
#: ../tests/ut_thermalnotifier/ut_thermalnotifier.cpp:84
msgctxt "qtn_shut_high_temp_alert"
msgid "Device overheating. Turn it off."
msgstr "சாதனம் அதிக வெப்பம். அதை அணைக்கவும்."

#: ../src/notifications/thermalnotifier.cpp:42
#: ../tests/ut_thermalnotifier/ut_thermalnotifier.cpp:90
msgctxt "qtn_shut_low_temp_warning"
msgid "The device is too cold"
msgstr "சாதனம் மிகவும் குளிராக இருக்கிறது"

#: ../src/shutdownscreen.cpp:79
#: ../tests/ut_shutdownscreen/ut_shutdownscreen.cpp:101
msgctxt "qtn_shut_high_temp"
msgid "Temperature too high. Device shutting down."
msgstr "வெப்பநிலை மிக அதிகம். சாதனம் மூடப்படுகிறது."

#: ../src/shutdownscreen.cpp:84
#: ../tests/ut_shutdownscreen/ut_shutdownscreen.cpp:108
msgctxt "qtn_shut_unplug_usb"
msgid "USB cable plugged in. Unplug it to shut down device."
msgstr ""
"யூ.எச்.பி கேபிள் செருகப்பட்டது. சாதனத்தை மூடுவதற்கு அதை அவிழ்த்து விடுங்கள்."

#: ../src/shutdownscreen.cpp:89
#: ../tests/ut_shutdownscreen/ut_shutdownscreen.cpp:115
msgctxt "qtn_shut_batt_empty"
msgid "Battery empty. Device shutting down."
msgstr "பேட்டரி காலியாக உள்ளது. சாதனம் மூடப்படுகிறது."

#: ../src/usbmodeselector.cpp:103
msgctxt "qtn_usb_device_locked"
msgid "Unlock device first"
msgstr "முதலில் சாதனத்தைத் திறக்கவும்"

#: ../src/usbmodeselector.cpp:127
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:180
msgctxt "qtn_usb_disconnected"
msgid "USB cable disconnected"
msgstr "யூ.எச்.பி கேபிள் துண்டிக்கப்பட்டது"

#: ../src/usbmodeselector.cpp:132
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:189
msgctxt "qtn_usb_connection_sharing_active"
msgid "USB tethering in use"
msgstr "பயன்பாட்டில் யூ.எச்.பி டெதரிங்"

#: ../src/usbmodeselector.cpp:135
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:185
msgctxt "qtn_usb_mtp_active"
msgid "MTP mode in use"
msgstr "பயன்பாட்டில் உள்ள எம்டிபி பயன்முறை"

#: ../src/usbmodeselector.cpp:138
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:183
msgctxt "qtn_usb_storage_active"
msgid "Mass storage in use"
msgstr "பயன்பாட்டில் வெகுசன சேமிப்பு"

#: ../src/usbmodeselector.cpp:141
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:184
msgctxt "qtn_usb_sdk_active"
msgid "SSH mode in use"
msgstr "பயன்பாட்டில் பாஓடு பயன்முறை"

#: ../src/usbmodeselector.cpp:144
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:181
msgctxt "qtn_usb_sync_active"
msgid "Sync-and-connect in use"
msgstr "பயன்பாட்டில் ஒத்திசைவு மற்றும் இணைக்கவும்"

#: ../src/usbmodeselector.cpp:147
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:186
msgctxt "qtn_usb_adb_active"
msgid "ADB mode in use"
msgstr "ADB பயன்முறை பயன்பாட்டில் உள்ளது"

#: ../src/usbmodeselector.cpp:150
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:187
msgctxt "qtn_usb_diag_active"
msgid "Diag mode in use"
msgstr "பயன்பாட்டில் உள்ள டயக் பயன்முறை"

#: ../src/usbmodeselector.cpp:153
#: ../tests/ut_usbmodeselector/ut_usbmodeselector.cpp:188
msgctxt "qtn_usb_host_mode_active"
msgid "USB switched to host mode (OTG)"
msgstr "யூ.எச்.பி புரவலன் பயன்முறைக்கு மாறியது (OTG)"
