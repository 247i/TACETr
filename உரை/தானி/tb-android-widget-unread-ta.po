#, java-printf-format
msgctxt "unread_widget_select_account"
msgid "Show unread count for…"
msgstr "படிக்காத எண்ணிக்கையைக் காட்டு…"

#, java-printf-format
msgctxt "unread_widget_account_title"
msgid "Account"
msgstr "கணக்கு"

#, java-printf-format
msgctxt "unread_widget_account_summary"
msgid "The account for which the unread count should be displayed"
msgstr "படிக்காத எண்ணிக்கை காண்பிக்கப்பட வேண்டிய கணக்கு"

#, java-printf-format
msgctxt "unread_widget_unified_inbox_account_summary"
msgid "Unified Inbox"
msgstr "ஒருங்கிணைந்த இன்பாக்ச்"

#, java-printf-format
msgctxt "unread_widget_folder_enabled_title"
msgid "Folder count"
msgstr "கோப்புறை எண்ணிக்கை"

#, java-printf-format
msgctxt "unread_widget_folder_enabled_summary"
msgid "Display the unread count of only a single folder"
msgstr "ஒரு கோப்புறையின் படிக்காத எண்ணிக்கையை மட்டுமே காண்பி"

#, java-printf-format
msgctxt "unread_widget_folder_title"
msgid "Folder"
msgstr "கோப்புறை"

#, java-printf-format
msgctxt "unread_widget_folder_summary"
msgid "The folder for which the unread count should be displayed"
msgstr "படிக்காத எண்ணிக்கை காட்டப்பட வேண்டிய கோப்புறை"

#, java-printf-format
msgctxt "unread_widget_action_done"
msgid "Done"
msgstr "முடிந்தது"

#, java-printf-format
msgctxt "unread_widget_title"
msgid ""
"<xliff:g id=\"account_name\">%1$s</xliff:g> - <xliff:g "
"id=\"folder_name\">%2$s</xliff:g>"
msgstr ""
"<xliff: g ஐடி = \"account_name\">%1$s </xliff: g> - <xliff: g ஐடி = "
"\"folder_name\">%2$s </xliff: g>"

#, java-printf-format
msgctxt "unread_widget_account_not_selected"
msgid "No account selected"
msgstr "கணக்கு எதுவும் தேர்ந்தெடுக்கப்படவில்லை"

#, java-printf-format
msgctxt "unread_widget_folder_not_selected"
msgid "No folder selected"
msgstr "கோப்புறை எதுவும் தேர்ந்தெடுக்கப்படவில்லை"

#, java-printf-format
msgctxt "unread_widget_choose_account_title"
msgid "Choose Account"
msgstr "கணக்கைத் தேர்வுசெய்க"

#, java-printf-format
msgctxt "unread_widget_label"
msgid "Unread count"
msgstr "படிக்காத எண்ணிக்கை"
