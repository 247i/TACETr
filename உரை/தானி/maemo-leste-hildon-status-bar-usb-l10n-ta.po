msgctxt "stab_me_usb_cannot_connect"
msgid ""
"Unable to connect.\n"
"Device not supported."
msgstr ""
"இணைக்க முடியவில்லை.\n"
" சாதனம் ஆதரிக்கப்படவில்லை."

msgctxt "stab_me_usb_cannot_eject"
msgid "%s in use. Unable to disconnect. Close files on device."
msgstr ""
"%s பயன்பாட்டில் உள்ளன. துண்டிக்க முடியவில்லை. சாதனத்தில் கோப்புகளை மூடு."

msgctxt "stab_me_usb_cannot_eject_ok"
msgid "OK"
msgstr "சரி"

msgctxt "stab_me_usb_connected"
msgid "PC connection active"
msgstr "பிசி இணைப்பு செயலில்"

msgctxt "stab_me_usb_connected_to"
msgid "Connected to %s"
msgstr "%s உடன் இணைக்கப்பட்டுள்ளது"

msgctxt "stab_me_usb_device_name"
msgid "USB device"
msgstr "யூ.எச்.பி சாதனம்"

msgctxt "stab_me_usb_eject"
msgid "Disconnect %s"
msgstr "%s துண்டிக்கவும்"

msgctxt "stab_me_usb_ejected"
msgid ""
"%s disconnected.\n"
"Remove cable."
msgstr ""
"%துண்டிக்கப்பட்டது.\n"
" கேபிளை அகற்று."

msgctxt "stab_me_usb_ejecting"
msgid "Disconnecting %s"
msgstr "%s துண்டிக்கப்படுகின்றன"

msgctxt "stab_me_usb_no_file_system_available"
msgid "Unable to connect, no file system available"
msgstr "இணைக்க முடியவில்லை, கோப்பு முறைமை எதுவும் கிடைக்கவில்லை"

msgctxt "stab_me_usb_otg_host_not_connected"
msgid "USB host not responding"
msgstr "யூ.எச்.பி புரவலன் பதிலளிக்கவில்லை"

msgctxt "stab_me_usb_otg_hub_not_supported"
msgid "Unsupported hub topology"
msgstr "ஆதரிக்கப்படாத மைய இடவியல்"

msgctxt "stab_me_usb_otg_hub_support"
msgid "USB hub not supported"
msgstr "யூ.எச்.பி அப் ஆதரிக்கப்படவில்லை"

msgctxt "stab_me_usb_otg_not_connected"
msgid "USB device not connected"
msgstr "யூ.எச்.பி சாதனம் இணைக்கப்படவில்லை"

msgctxt "stab_me_usb_otg_not_responding"
msgid "USB device not responding"
msgstr "யூ.எச்.பி சாதனம் பதிலளிக்கவில்லை"

msgctxt "stab_me_usb_otg_not_supported"
msgid "USB device not supported"
msgstr "யூ.எச்.பி சாதனம் ஆதரிக்கப்படவில்லை"

msgctxt "stab_me_usb_otg_vbus_shutdown"
