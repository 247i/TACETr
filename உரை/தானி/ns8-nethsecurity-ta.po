msgstr "யூனிட் குழுக்கள் எதுவும் கட்டமைக்கப்படவில்லை"

msgctxt "controller.unit_groups.filter_groups"
msgid "Filter groups"
msgstr "குழுக்களை வடிகட்டவும்"

msgctxt "controller.unit_groups.name"
msgid "Name"
msgstr "பெயர்"

msgctxt "controller.unit_groups.description"
msgid "Description"
msgstr "விவரம்"

msgctxt "controller.unit_groups.units"
msgid "Units"
msgstr "அலகுகள்"

msgctxt "controller.unit_groups.add_unit"
msgid "Add unit"
msgstr "அலகு சேர்க்கவும்"

msgctxt "controller.unit_groups.remove"
msgid "Remove"
msgstr "அகற்று"

msgctxt "controller.unit_groups.used_by"
msgid "Managed by"
msgstr "மூலம் நிர்வகிக்கப்படுகிறது"

msgctxt "controller.unit_groups.delete"
msgid "Delete unit group"
msgstr "யூனிட் குழுவை நீக்கு"

msgctxt "controller.unit_groups.delete_confirm"
msgid ""
"You are about to delete the unit group '{name}'. This action cannot be "
"undone."
msgstr ""
"'{name}' யூனிட் குழுவை நீக்க உள்ளீர்கள். இந்தச் செயலைச் செயல்தவிர்க்க "
"முடியாது."

msgctxt "controller.unit_groups.no_groups_found"
msgid "No unit groups found"
msgstr "யூனிட் குழுக்கள் எதுவும் இல்லை"

msgctxt "controller.unit_groups.group_edited"
msgid "Unit group updated"
msgstr "யூனிட் குழு புதுப்பிக்கப்பட்டது"

msgctxt "controller.unit_groups.group_added"
msgid "Unit group added"
msgstr "யூனிட் குழு சேர்க்கப்பட்டது"

msgctxt "controller.unit_groups.group_deleted"
msgid "Unit group deleted"
msgstr "யூனிட் குழு நீக்கப்பட்டது"

msgctxt "controller.unit_groups.units_number"
msgid "Number of units"
msgstr "அலகுகளின் எண்ணிக்கை"

msgctxt "controller.unit_groups.created"
msgid "Created"
msgstr "உருவாக்கப்பட்டது"

msgctxt "controller.unit_groups.updated"
