#, icu-message-format
msgctxt "appTitle"
msgid "Security Center"
msgstr "பாதுகாப்பு நடுவண்"

#, icu-message-format
msgctxt "snapdRuleCategorySessionAllowed"
msgid "Allow until logout"
msgstr "வெளியேறும் வரை அனுமதிக்கவும்"

#, icu-message-format
msgctxt "snapdRuleCategorySessionDenied"
msgid "Deny until logout"
msgstr "வெளியேறும் வரை மறுக்கவும்"

#, icu-message-format
msgctxt "snapdRuleCategoryForeverAllowed"
msgid "Allow always"
msgstr "எப்போதும் அனுமதிக்கவும்"

#, icu-message-format
msgctxt "snapdRuleCategoryForeverDenied"
msgid "Deny always"
msgstr "எப்போதும் மறுக்கவும்"

#, icu-message-format
msgctxt "snapPermissionReadLabel"
msgid "Read"
msgstr "படிக்க"

#, icu-message-format
msgctxt "snapPermissionWriteLabel"
msgid "Write"
msgstr "எழுதுங்கள்"

#, icu-message-format
msgctxt "snapPermissionExecuteLabel"
msgid "Execute"
msgstr "செயல்படுத்தவும்"

#, icu-message-format
msgctxt "snapPermissionsEnableTitle"
msgid "Require apps to ask for system permissions"
msgstr "கணினி அனுமதிகளைக் கேட்க பயன்பாடுகள் தேவை"

#, icu-message-format
msgctxt "snapPermissionsEnableWarning"
msgid ""
"This is an experimental feature for controlling access to your system’s "
"resources."
msgstr ""
"உங்கள் கணினியின் வளங்களுக்கான அணுகலைக் கட்டுப்படுத்த இது ஒரு சோதனை "
"அம்சமாகும்."

#, icu-message-format
msgctxt "snapPermissionsEnablingLabel"
msgid "Enabling, it might take a few seconds..."
msgstr "செயல்படுத்த, இது சில வினாடிகள் ஆகலாம் ..."

#, icu-message-format
msgctxt "snapPermissionsDisablingLabel"
msgid "Disabling, it might take a few seconds..."
msgstr "முடக்குதல், இதற்கு சில வினாடிகள் ஆகலாம் ..."

#, icu-message-format
msgctxt "snapPermissionsExperimentalLabel"
msgid "Experimental"
msgstr "சோதனை"

#, icu-message-format
msgctxt "snapPermissionsOtherDescription"
msgid "You can manage other permissions in Settings › Applications."
msgstr "அமைப்புகளில் பிற அனுமதிகளை நீங்கள் நிர்வகிக்கலாம் ›பயன்பாடுகள்."

#, icu-message-format
msgctxt "snapPermissionsPageTitle"
msgid "App Permissions"
msgstr "பயன்பாட்டு அனுமதிகள்"

#, case-insensitive, icu-message-format, placeholders:$N$
msgctxt "snapRulesCount"
msgid "{n, plural, =0{no rules} =1{1 rule} other{{n} rules}}"
msgstr "{n, பன்மை, = 0 {no rules} = 1 {1 rule} பிற {{n} விதிகள்}}"

#, case-insensitive, icu-message-format, placeholders:$INTERFACE$:$SNAP$
msgctxt "snapRulesPageDescription"
msgid "Manage {interface} permissions for {snap}."
msgstr "{ச்னாப் க்கு க்கான {interface} அனுமதிகளை நிர்வகிக்கவும்."

#, icu-message-format
msgctxt "snapRulesPageEmptyTileLabel"
msgid "No rules yet"
msgstr "இன்னும் விதிகள் இல்லை"

#, icu-message-format
msgctxt "snapRulesRemoveAll"
msgid "Remove all rules"
msgstr "எல்லா விதிகளையும் அகற்று"

#, icu-message-format
msgctxt "homeInterfacePageTitle"
msgid "Home folder"
msgstr "வீட்டு கோப்புறை"

#, icu-message-format
msgctxt "homeInterfacePageDescription"
msgid "Manage permissions to access files in your Home folder."
msgstr "உங்கள் வீட்டு கோப்புறையில் கோப்புகளை அணுக அனுமதிகளை நிர்வகிக்கவும்."

#, icu-message-format
msgctxt "interfacePageTitle"
msgid "Manage permissions"
msgstr "அனுமதிகளை நிர்வகிக்கவும்"

#, icu-message-format
msgctxt "interfacePageDescription"
msgid ""
"Strictly confined apps will ask you before accessing files and folders. This"
" feature is experimental and subject to changes."
msgstr ""
"கோப்புகள் மற்றும் கோப்புறைகளை அணுகுவதற்கு முன் கண்டிப்பாக வரையறுக்கப்பட்ட "
"பயன்பாடுகள் உங்களிடம் கேட்கும். இந்த நற்பொருத்தம் சோதனை மற்றும் "
"மாற்றங்களுக்கு உட்பட்டது."

#, icu-message-format
msgctxt "interfacePageLinkLearnMore"
msgid "Learn more"
msgstr "மேலும் அறிக"

#, icu-message-format
msgctxt "interfacePageLinkGiveFeedback"
msgid "Give feedback"
msgstr "கருத்து தெரிவிக்கவும்"

#, icu-message-format
msgctxt "interfacePageLinkReportIssues"
msgid "Report issues"
msgstr "சிக்கல்களைப் புகாரளிக்கவும்"

#, case-insensitive, icu-message-format, placeholders:$N$
msgctxt "interfaceSnapCount"
msgid "{n, plural, =0{no apps} =1{1 app} other {{n} apps}}"
msgstr "{n, பன்மை, = 0 {no apps} = 1 {1 app} பிற {{n} பயன்பாடுகள்}}"
