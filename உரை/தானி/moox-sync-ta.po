#. // Sync refers to the package name Moox Sync, and data transfer between
#. platforms
msgctxt "return[]->'sync'"
msgid "Sync"
msgstr "ஒத்திசைவு"

#. // Syncs refers to the package name Moox Sync, and data transfer between
#. platforms
msgctxt "return[]->'syncs'"
msgid "Syncs"
msgstr "ஒத்திசைவு"

#. // Platform refers to the platform that data is being transferred from or
#. to
msgctxt "return[]->'platform'"
msgid "Platform"
msgstr "இயங்குதளம்"

#. // Platforms refers to the platforms that data is being transferred from or
#. to
msgctxt "return[]->'platforms'"
msgid "Platforms"
msgstr "தளங்கள்"

#. // Source Platform refers to the platform that the data is being
#. transferred from
msgctxt "return[]->'source_platform'"
msgid "Source Platform"
msgstr "மூல தளம்"

#. // Target Platform refers to the platform that the data is being
#. transferred to
msgctxt "return[]->'target_platform'"
msgid "Target Platform"
msgstr "இலக்கு தளம்"

#. // Target Platforms refers to the platform that the data is being
#. transferred to
msgctxt "return[]->'target_platforms'"
msgid "Target Platforms"
msgstr "இலக்கு தளங்கள்"

#. // Source Platform refers to the platform that data is being transferred
#. from
msgctxt "return[]->'source_platform_id'"
msgid "Source Platform ID"
msgstr "மூல இயங்குதள ஐடி"

#. // Target Platform refers to the platform that data is being transferred to
msgctxt "return[]->'target_platform_id'"
msgid "Target Platform ID"
msgstr "இலக்கு இயங்குதள ஐடி"

#. // Sync error refers to any error that occurs in the package Moox Sync
msgctxt "return[]->'sync_error'"
msgid "Sync Error"
msgstr "பிழை"

#. // Sync error: if someone tries to make multiple platforms the master
#. platform
msgctxt "return[]->'sync_error_master'"
msgid "There can only be one master platform."
msgstr "ஒரு முதன்மை தளம் மட்டுமே இருக்க முடியும்."

#. // Sync error: if someone tries to make the same platform and model the
#. source and target
msgctxt "return[]->'sync_error_platforms'"
msgid "Source and Target Platform cannot be the same with the same model."
msgstr ""
"மூல மற்றும் இலக்கு தளம் ஒரே மாதிரியுடன் ஒரே மாதிரியாக இருக்க முடியாது."

#. // The source model refers to the model that data is being transferred from
msgctxt "return[]->'source_model'"
msgid "Source Model"
msgstr "மூல மாதிரி"

#. // The target model refers to the model that data is being transferred to
msgctxt "return[]->'target_model'"
msgid "Target Model"
msgstr "இலக்கு மாதிரி"

#. // Use relations from the model to platform
msgctxt "return[]->'use_platform_relations'"
msgid "Use Platform Relations"
msgstr "இயங்குதள உறவுகளைப் பயன்படுத்துங்கள்"

#. // If exists refers to the action to take if the record already exists
msgctxt "return[]->'if_exists'"
msgid "If exists"
msgstr "இருந்தால்"

#. // Filter ids refers to the ids to filter when syncing
msgctxt "return[]->'filter_ids'"
msgid "Filter IDs"
msgstr "ஐடிஎச் வடிகட்டி"

#. // Ignore ids refers to the ids to ignore when syncing
msgctxt "return[]->'ignore_ids'"
msgid "Ignore IDs"
msgstr "ஐடிகளை புறக்கணிக்கவும்"

#. // Sync all records refers to syncing all records
msgctxt "return[]->'sync_all_records'"
msgid "Sync all Records"
msgstr "எல்லா பதிவுகளையும் ஒத்திசைக்கவும்"

#. // Sync only ids refers to syncing only the ids
msgctxt "return[]->'sync_only_ids'"
msgid "Sync only IDs"
msgstr "ஐடிகளை மட்டும் ஒத்திசைக்கவும்"

#. // Sync all fields refers to syncing all fields
msgctxt "return[]->'sync_all_fields'"
msgid "Sync all Fields"
msgstr "அனைத்து புலங்களையும் ஒத்திசைக்கவும்"

#. // Sync only fields defined in the field mappings
msgctxt "return[]->'field_mappings'"
msgid "Field Mappings"
msgstr "புலம் மேப்பிங்ச்"

#. // Use a transformer class to transform the data when syncing
msgctxt "return[]->'use_transformer_class'"
msgid "Use Transformer Class"
msgstr "மின்மாற்றி வகுப்பைப் பயன்படுத்தவும்"

#. // When was the last time the data was synced
msgctxt "return[]->'last_sync'"
msgid "Last Sync"
msgstr "கடைசி ஒத்திசைவு"

#. // Source Platform and Model refers to the source of the data being
#. transferred
msgctxt "return[]->'source_platform_and_model'"
msgid "Source Platform and Model"
msgstr "மூல தளம் மற்றும் மாதிரி"

#. // Target Platform and Model refers to the target of the data being
#. transferred
msgctxt "return[]->'target_platform_and_model'"
msgid "Target Platform and Model"
msgstr "இலக்கு தளம் மற்றும் மாதிரி"

#. // Syncable ID refers to the ID of the record being synced
msgctxt "return[]->'syncable_id'"
msgid "Syncable ID"
msgstr "ஒத்திசைக்கக்கூடிய ஐடி"

#. // Syncable Type refers to the type of record being synced
msgctxt "return[]->'syncable_type'"
msgid "Syncable Type"
msgstr "ஒத்திசைக்கக்கூடிய வகை"

#. // API Token refers to the token used to authenticate the API
msgctxt "return[]->'api_token'"
msgid "Api Token"
msgstr "பநிஇ கிள்ளாக்கு"

#. // Generate Token refers to the action to generate a new token
msgctxt "return[]->'generate_token'"
msgid "Generate Token"
msgstr "கிள்ளாக்கை உருவாக்குங்கள்"

#. // Locked means the platform or model is locked and cannot be synced
msgctxt "return[]->'locked'"
msgid "Locked"
msgstr "பூட்டப்பட்டுள்ளது"

#. // Lock Reason refers to the reason the platform or model is locked
msgctxt "return[]->'lock_reason'"
msgid "Lock Reason"
msgstr "பூட்டு காரணம்"

#. // Model Compatibility Warning
msgctxt "return[]->'model_compatibility_warning'"
msgid "Model Compatibility Warning"
msgstr "மாதிரி பொருந்தக்கூடிய எச்சரிக்கை"

#. // The selected models are not fully compatible
msgctxt "return[]->'models_are_not_fully_compatible'"
msgid "The selected models are not fully compatible"
msgstr "தேர்ந்தெடுக்கப்பட்ட மாதிரிகள் முழுமையாக பொருந்தாது"

#. // Missing columns
msgctxt "return[]->'missing_columns'"
msgid "Missing Columns"
msgstr "நெடுவரிசைகளைக் காணவில்லை"

#. // Extra columns
msgctxt "return[]->'extra_columns'"
msgid "Extra Columns"
msgstr "கூடுதல் நெடுவரிசைகள்"

#. // Please map fields manually
msgctxt "return[]->'please_map_fields_manually'"
msgid "Please map fields manually"
msgstr "தயவுசெய்து புலங்களை கைமுறையாக வரைபடமாக்கவும்"
