#, java-printf-format
msgctxt "settings_import_title"
msgid "Import settings"
msgstr "அமைப்புகளை இறக்குமதி செய்யுங்கள்"

#, java-printf-format
msgctxt "settings_import_pick_document_button"
msgid "Select file"
msgstr "கோப்பைத் தேர்ந்தெடு"

#, java-printf-format
msgctxt "settings_import_scan_qr_code_button"
msgid "Scan QR code"
msgstr "QR குறியீட்டை ச்கேன் செய்யுங்கள்"

#, java-printf-format
msgctxt "settings_import_pick_app_button"
msgid "Import from app"
msgstr "பயன்பாட்டிலிருந்து இறக்குமதி"

#, java-printf-format
msgctxt "settings_import_pick_app_dialog_title"
msgid "Import from app"
msgstr "பயன்பாட்டிலிருந்து இறக்குமதி"

#, java-printf-format
msgctxt "settings_import_button"
msgid "Import"
msgstr "இறக்குமதி"

#, java-printf-format
msgctxt "settings_import_success_generic"
msgid "Settings successfully imported"
msgstr "அமைப்புகள் வெற்றிகரமாக இறக்குமதி செய்யப்படுகின்றன"

#.  Displayed after importing accounts when one or multiple accounts require
#. entering a password
#, java-printf-format
msgctxt "settings_import_password_required"
msgid "Please enter passwords"
msgstr "கடவுச்சொற்களை உள்ளிடவும்"

#.  Displayed after importing accounts when one or multiple accounts require
#. to use the OAuth authorization flow
#, java-printf-format
msgctxt "settings_import_authorization_required"
msgid "Please sign in"
msgstr "தயவுசெய்து உள்நுழைக"

#.  Displayed after importing accounts when some accounts require entering a
#. password and some to use the OAuth authorization flow
#, java-printf-format
msgctxt "settings_import_authorization_and_password_required"
msgid "Please sign in and enter passwords"
msgstr "தயவுசெய்து உள்நுழைந்து கடவுச்சொற்களை உள்ளிடவும்"

#, java-printf-format
msgctxt "settings_import_failure"
msgid "Failed to import settings"
msgstr "அமைப்புகளை இறக்குமதி செய்யத் தவறிவிட்டது"

#, java-printf-format
msgctxt "settings_import_read_failure"
msgid "Failed to read settings file"
msgstr "அமைப்புகள் கோப்பைப் படிக்கத் தவறிவிட்டது"

#, java-printf-format
msgctxt "settings_import_partial_failure"
msgid "Failed to import some settings"
msgstr "சில அமைப்புகளை இறக்குமதி செய்யத் தவறிவிட்டது"

#, java-printf-format
msgctxt "settings_import_status_success"
msgid "Imported successfully"
msgstr "வெற்றிகரமாக இறக்குமதி செய்யப்பட்டது"

#, java-printf-format
msgctxt "settings_import_status_password_required"
msgid "Password required"
msgstr "கடவுச்சொல் தேவை"

#.  Content description for the icon that is displayed next to an imported
#. account that requires sign-in
#, java-printf-format
msgctxt "settings_import_status_log_in_required"
msgid "Sign-in required"
msgstr "உள்நுழைவு தேவை"

#, java-printf-format
msgctxt "settings_import_status_not_imported"
msgid "Not imported"
msgstr "இறக்குமதி செய்யப்படவில்லை"

#, java-printf-format
msgctxt "settings_import_status_error"
msgid "Import failure"
msgstr "இறக்குமதி தோல்வி"

#, java-printf-format
msgctxt "settings_import_later_button"
msgid "Later"
msgstr "பின்னர்"

#, java-printf-format
msgctxt "settings_importing"
msgid "Importing settings…"
msgstr "அமைப்புகளை இறக்குமதி செய்தல்…"

#, java-printf-format
msgctxt "settings_import_password_prompt"
msgid ""
"To be able to use the account \"<xliff:g id=\"account\">%s</xliff:g>\" you "
"need to provide the server password."
msgid_plural ""
"To be able to use the account \"<xliff:g id=\"account\">%s</xliff:g>\" you "
"need to provide the server passwords."
msgstr[0] ""
"\"<Xliff: g ஐடி =\" கணக்கு \">%s </xliff: g>\" கணக்கைப் பயன்படுத்த நீங்கள் "
"சேவையக கடவுச்சொல்லை வழங்க வேண்டும்."
msgstr[1] ""
"\"<Xliff: g ஐடி =\" கணக்கு \">%s </xliff: g>\" கணக்கைப் பயன்படுத்த நீங்கள் "
"சேவையக கடவுச்சொற்களை வழங்க வேண்டும்."

#, java-printf-format
msgctxt "settings_import_incoming_server_password_hint"
msgid "Incoming server password"
msgstr "உள்வரும் சேவையக கடவுச்சொல்"

#, java-printf-format
msgctxt "settings_import_outgoing_server_password_hint"
msgid "Outgoing server password"
msgstr "வெளிச்செல்லும் சேவையக கடவுச்சொல்"

#, java-printf-format
msgctxt "settings_import_use_same_password_for_outgoing_server"
msgid "Use same password for outgoing server"
msgstr "வெளிச்செல்லும் சேவையகத்திற்கு அதே கடவுச்சொல்லைப் பயன்படுத்தவும்"

#, java-printf-format
msgctxt "settings_import_server_name_format"
msgid "Server name: <xliff:g id=\"hostname\">%s</xliff:g>"
msgstr "சேவையக பெயர்: <XLIFF: G ஐடி = \"HOSTNAME\">%s </xliff: g>"

#, java-printf-format
msgctxt "settings_import_oauth_description"
msgid ""
"To use this email account, you need to sign in and grant the app access to "
"your emails."
msgstr ""
"இந்த மின்னஞ்சல் கணக்கைப் பயன்படுத்த, நீங்கள் உள்நுழைந்து உங்கள் "
"மின்னஞ்சல்களுக்கான பயன்பாட்டு அணுகலை வழங்க வேண்டும்."

#, java-printf-format
msgctxt "settings_import_oauth_sign_in"
msgid "Sign in"
msgstr "விடுபதிகை"

#, java-printf-format
msgctxt "settings_import_oauth_sign_in_with_google"
msgid "Sign in with Google"
msgstr "Google உடன் உள்நுழைக"

#, java-printf-format
msgctxt "settings_import_oauth_error_oauth_flow_canceled"
msgid "Authorization canceled"
msgstr "ஏற்பு ரத்து செய்யப்பட்டது"

#, java-printf-format
msgctxt "settings_import_oauth_error_oauth_flow_failed"
msgid ""
"Authorization failed with the following error: <xliff:g "
"id=\"error\">%s</xliff:g>"
msgstr ""
"பின்வரும் பிழையுடன் ஏற்பு தோல்வியடைந்தது: <xliff: g ஐடி = \"பிழை\">%s "
"</xliff: g>"

#, java-printf-format
msgctxt "settings_import_oauth_error_oauth_not_supported"
msgid "OAuth 2.0 is currently not supported with this provider."
msgstr "OAUTH 2.0 தற்போது இந்த வழங்குநருடன் ஆதரிக்கப்படவில்லை."

#, java-printf-format
msgctxt "settings_import_oauth_error_browser_not_found"
msgid ""
"The app couldn't find a browser to use for granting access to your account."
msgstr ""
"உங்கள் கணக்கிற்கு அணுகலை வழங்க பயன்படுத்த உலாவியை பயன்பாட்டால் கண்டுபிடிக்க "
"முடியவில்லை."

#, java-printf-format
msgctxt "settings_import_general_settings_item"
msgid "General settings"
msgstr "பொது அமைப்புகள்"

#.  Displayed below the app name in the list of apps from which settings can
#. be imported when the app is too old and doesn't support exporting settings
#. to another app.
#, java-printf-format
msgctxt "settings_import_app_version_not_supported"
msgid "App version not supported."
msgstr "பயன்பாட்டு பதிப்பு ஆதரிக்கப்படவில்லை."
