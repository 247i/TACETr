#, java-printf-format
msgctxt "audio_max_bandwidth"
msgid ""
"Maximum server network bandwidth is only %1$d kbit/s. Adjusting audio "
"quality to %2$d kbit/s (%3$d ms)."
msgstr ""
"அதிகபட்ச சேவையக பிணையம் அலைவரிசை %1$d kbit/s மட்டுமே. ஆடியோ தரத்தை %2$d "
"kbit/s ( %3$d MS) க்கு சரிசெய்தல்."

#, java-printf-format
msgctxt "chat_notify_muted_deafened"
msgid "Muted and deafened."
msgstr "முடக்கிய மற்றும் காது கேளாதது."

#, java-printf-format
msgctxt "chat_notify_muted"
msgid "Muted."
msgstr "முடக்கிய."

#, java-printf-format
msgctxt "chat_notify_unmuted"
msgid "Unmuted."
msgstr "அன்ச்டட்."

#, java-printf-format
msgctxt "chat_notify_now_muted_deafened"
msgid "%s is now muted and deafened."
msgstr "%s இப்போது முடக்கப்பட்டு காது கேளாதவை."

#, java-printf-format
msgctxt "chat_notify_now_muted"
msgid "%s is now muted."
msgstr "%s இப்போது முடக்கப்பட்டுள்ளது."

#, java-printf-format
msgctxt "chat_notify_now_unmuted"
msgid "%s is now unmuted."
msgstr "%s இப்போது அசைக்காது."

#, java-printf-format
msgctxt "chat_notify_connected"
msgid "%s connected."
msgstr "%s இணைக்கப்பட்டுள்ளன."

#, java-printf-format
msgctxt "chat_notify_disconnected"
msgid "%s disconnected."
msgstr "%துண்டிக்கப்பட்டது."

#, java-printf-format
msgctxt "chat_notify_kick_ban_self"
msgid "You were kicked and banned from the server by %1$s: %2$s."
msgstr ""
"சேவையகத்திலிருந்து %1$s: %2$s ஆல் நீங்கள் உதைக்கப்பட்டு தடை "
"செய்யப்பட்டீர்கள்."

#, java-printf-format
msgctxt "chat_notify_kick_self"
msgid "You were kicked from the server by %1$s: %2$s."
msgstr "நீங்கள் சேவையகத்திலிருந்து %1$s: %2$s ஆல் உதைக்கப்பட்டீர்கள்."

#, java-printf-format
msgctxt "chat_notify_kick_ban"
msgid "%3$s was kicked and banned from the server by %1$s: %2$s."
msgstr ""
"%3$s உதைக்கப்பட்டு சேவையகத்திலிருந்து %1$s: %2$s ஆல் தடை செய்யப்பட்டது."

#, java-printf-format
msgctxt "chat_notify_kick"
msgid "%3$s was kicked from the server by %1$s: %2$s."
msgstr "%3$s சேவையகத்திலிருந்து %1$s: %2$s ஆல் உதைக்கப்பட்டது."

#, java-printf-format
msgctxt "chat_notify_self_recording_started"
msgid "Recording started"
msgstr "பதிவு தொடங்கியது"

#, java-printf-format
msgctxt "chat_notify_self_recording_stopped"
msgid "Recording stopped"
msgstr "பதிவு நிறுத்தப்பட்டது"

#, java-printf-format
msgctxt "chat_notify_user_recording_started"
msgid "%s started recording."
msgstr "%s பதிவு செய்யத் தொடங்கின."

#, java-printf-format
msgctxt "chat_notify_user_recording_stopped"
msgid "%s stopped recording."
msgstr "%s பதிவு செய்வதை நிறுத்தின."

#, java-printf-format
msgctxt "chat_notify_user_joined_channel"
msgid "%s entered channel."
msgstr "%s சேனலில் நுழைந்தன."

#, java-printf-format
msgctxt "chat_notify_user_left_channel"
msgid "%1$s moved to %2$s."
msgstr "%1$s %2$s க்கு நகர்ந்தன."

#, java-printf-format
msgctxt "chat_notify_user_joined_channel_by"
msgid "%1$s moved in from %2$s by %3$s."
msgstr "%1$s %2$s இலிருந்து %3$s ஆல் நகர்ந்தன."

#, java-printf-format
msgctxt "chat_notify_user_left_channel_by"
msgid "%1$s moved to %2$s by %3$s."
msgstr "%1$s %2$s க்கு %3$s க்கு நகர்ந்தன."

#, java-printf-format
msgctxt "deny_reason_channel_name"
msgid "Denied: Invalid channel name."
msgstr "மறுக்கப்பட்டது: தவறான சேனல் பெயர்."

#, java-printf-format
msgctxt "deny_reason_text_too_long"
msgid "Denied: Text message too long."
msgstr "மறுக்கப்பட்டது: உரை செய்தி மிக நீளமானது."

#, java-printf-format
msgctxt "deny_reason_no_operation_temp"
msgid "Denied: Operation not permitted in temporary channel."
msgstr "மறுக்கப்பட்டது: தற்காலிக சேனலில் செயல்பாடு அனுமதிக்கப்படவில்லை."

#, java-printf-format
msgctxt "deny_reason_no_certificate"
msgid "You need a certificate to perform this operation."
msgstr "இந்த செயல்பாட்டைச் செய்ய உங்களுக்கு சான்றிதழ் தேவை."

#, java-printf-format
msgctxt "deny_reason_invalid_username"
msgid "Invalid username."
msgstr "தவறான பயனர்பெயர்."

#, java-printf-format
msgctxt "deny_reason_channel_full"
msgid "Channel is full."
msgstr "சேனல் நிரம்பியுள்ளது."

#, java-printf-format
msgctxt "deny_reason_channel_nesting"
msgid "Channel nesting limit reached."
msgstr "சேனல் கூடு வரம்பை எட்டியது."

#, java-printf-format
msgctxt "deny_reason_other"
msgid "Reason: %s"
msgstr "காரணம்: %s"

#, java-printf-format
msgctxt "perm_denied"
msgid "Permission denied."
msgstr "இசைவு மறுக்கப்பட்டது."

#, java-printf-format
msgctxt "server"
msgid "Server"
msgstr "சேவையகம்"

#, java-printf-format
msgctxt "the_server"
msgid "the server"
msgstr "சேவையகம்"
