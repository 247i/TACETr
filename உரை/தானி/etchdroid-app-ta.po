#. App name, please do not translate
#, java-printf-format
msgctxt "app_name"
msgid "EtchDroid"
msgstr "எக்ச்ட்ராய்டு"

#. Cancel string, used all over the place
#, java-printf-format
msgctxt "cancel"
msgid "Cancel"
msgstr "ரத்துசெய்"

#. Notification title when the write operation is successful
#, java-printf-format
msgctxt "write_finished"
msgid "Write finished"
msgstr "எழுதப்பட்டது"

#. Notification title when the write operation is successful. Parameters:
#. filename, device name
#, java-printf-format
msgctxt "success_notif_content_text"
msgid "%1$s successfully written to %2$s"
msgstr "%1$s வெற்றிகரமாக %2$s க்கு எழுதப்பட்டது"

#. Notification title when a non-recoverable error occurs
#, java-printf-format
msgctxt "write_failed"
msgid "Write failed"
msgstr "எழுத்து தோல்வியுற்றது"

#. Notification title when a recoverable error occurs
#, java-printf-format
msgctxt "action_required"
msgid "Action required"
msgstr "நடவடிக்கை தேவை"

#. Notification channel description for long-running job progress, shown in
#. Android settings
#, java-printf-format
msgctxt "notif_channel_job_progress_desc"
msgid "Used to show the progress of the current job"
msgstr "தற்போதைய வேலையின் முன்னேற்றத்தைக் காட்டப் பயன்படுகிறது"

#. Notification channel name for long-running job progress, shown in Android
#. settings
#, java-printf-format
msgctxt "notif_channel_job_progress_name"
msgid "Job progress"
msgstr "வேலை முன்னேற்றம்"

#. Notification channel description for job outcome notifications, shown in
#. Android settings
#, java-printf-format
msgctxt "notif_channel_job_result_desc"
msgid "Used to show a completed job outcome"
msgstr "நிறைவு செய்யப்பட்ட வேலை முடிவைக் காட்டப் பயன்படுகிறது"

#. Notification channel name for job outcome notifications, shown in Android
#. settings
#, java-printf-format
msgctxt "notif_channel_job_result_name"
msgid "Job outcome"
msgstr "வேலை விளைவு"

#. Notification title used for long-running job status
#, java-printf-format
msgctxt "notification_write_progress_title"
msgid "Writing image…"
msgstr "படம் எழுதுதல்…"

#. Notification title used for long-running job status
#, java-printf-format
msgctxt "notification_verify_progress_title"
msgid "Verifying image…"
msgstr "படத்தை சரிபார்க்கிறது…"

#. Notification content for long-running job status, used when the user has
#. not granted the notification permission. The first parameter will be
#. replaced with the source file name, the second one with the USB stick name
#, java-printf-format
msgctxt "notification_write_no_notifications_text"
msgid "Enable notifications for progress • Writing %1$s to %2$s"
msgstr ""
"முன்னேற்றத்திற்கான அறிவிப்புகளை இயக்கவும் • %1$s முதல் %2$s வரை எழுதுதல்"

#. Notification content for long-running job status, used when the user has
#. granted the notification permission but no progress info is available. The
#. first parameter will be replaced with the source file name, the second one
#. with the USB stick name
#, java-printf-format
msgctxt "notification_write_tap_for_progress_text"
msgid "Tap for progress • Writing %1$s to %2$s"
msgstr "முன்னேற்றத்தைத் தட்டவும் • %1$s முதல் %2$s வரை எழுதுங்கள்"

#. Notification content: "{file name} to {device name}", used when the user
#. has granted the notification permission; The notification title will be:
#. "Writing image…" and this string will show up right below it.
#, java-printf-format
msgctxt "notification_write_progress_content"
msgid "%1$s to %2$s"
msgstr "%1$s முதல் %2$s"

#. Notification content: "{device name} against {file name}", used when the
#. user has granted the notification permission; The notification title will
#. be: "Verifying image…" and this string will show up right below it.
#, java-printf-format
msgctxt "notification_verify_progress_content"
msgid "%1$s against %2$s"
msgstr "%2$s க்கு எதிராக %1$s"

#. Placeholder file name
#, java-printf-format
msgctxt "unknown_filename"
msgid "Unknown file name"
msgstr "தெரியவில்லை கோப்பு பெயர்"

#. Displayed in a toast when the usb permission is denied
#, java-printf-format
msgctxt "permission_denied_for_usb_device"
msgid "Permission denied for USB device %1$s"
msgstr "Permission denied க்கு USB device %1$s"

#. Displayed in a toast when the USB device is unplugged
#, java-printf-format
msgctxt "usb_device_was_unplugged"
msgid "USB device was unplugged"
msgstr "யூ.எச்.பி சாதனம் அவிழ்க்கப்பட்டது"

#. Displayed in the home page main call-to-action button
#, java-printf-format
msgctxt "write_an_image"
msgid "Write an image"
msgstr "Write an படம்"

#. Displayed in the homepage; opens a page that lists all the supported
#. devices and formats
#, java-printf-format
msgctxt "whats_supported"
msgid "What's supported?"
msgstr "என்ன ஆதரவு?"

#. Title of the menu that allows selecting the app color scheme
#, java-printf-format
msgctxt "style"
msgid "Style"
msgstr "சூல் தண்டு"

#. Style menu option that selects the device default light/dark theme
#, java-printf-format
msgctxt "device_setting"
msgid "Device setting"
msgstr "சாதன அமைப்பு"

#. Style menu option that selects the light theme
#, java-printf-format
msgctxt "light"
msgid "Light"
msgstr "ஒளி"

#. Style menu option that selects the dark theme
#, java-printf-format
msgctxt "dark"
msgid "Dark"
msgstr "இருண்ட"

#. Style menu option that selects the Material You dynamic color scheme
#, java-printf-format
msgctxt "dynamic_colors"
msgid "Dynamic colors"
msgstr "மாறும் வண்ணங்கள்"

#. Menu option that opens the about page
#, java-printf-format
msgctxt "about"
msgid "About"
msgstr "பற்றி"

#. Dialog title; the dialog alerts the user that Windows images are not
#. supported
#, java-printf-format
msgctxt "is_this_a_windows_iso"
msgid "Is this a Windows ISO?"
msgstr "Is this a சாளரங்கள் ISO?"

#. Dialog message; the dialog alerts the user that Windows images are not
#. supported
#, java-printf-format
msgctxt "a_regular_windows_iso_won_t_work"
msgid ""
"A regular Windows ISO won't work with EtchDroid, unless it has specifically "
"been prepared for use with EtchDroid, Etcher or dd."
msgstr ""
"A regular சாளரங்கள் ISO won't வேலை with EtchDroid, unless it has "
"specifically been prepared க்கு use with EtchDroid, Etcher or dd."

#. Dialog button text: if the user clicks it, the app ignores the warning and
#. continues
#, java-printf-format
msgctxt "continue_anyway"
msgid "Continue anyway"
msgstr "எப்படியும் தொடரவும்"

#. Title of the dialog listing the available USB drives
#, java-printf-format
msgctxt "select_a_usb_drive"
msgid "Select a USB drive"
msgstr "யூ.எச்.பி டிரைவைத் தேர்ந்தெடுக்கவும்"

#. Title of the dialog listing the available USB drives, when no USB drive is
#. connected
#, java-printf-format
msgctxt "connect_a_usb_drive"
msgid "Connect a USB drive"
msgstr "யூ.எச்.பி டிரைவை இணைக்கவும்"

#, java-printf-format
msgctxt "looking_for_usb_drives"
msgid "Looking for USB drives"
msgstr "யூ.எச்.பி டிரைவ்களைத் தேடுகிறது"

#. Title of the page where the operation details are listed; the user can
#. confirm or cancel the operation from here
#, java-printf-format
msgctxt "confirm_operation"
msgid "Confirm operation"
msgstr "செயல்பாட்டை உறுதிப்படுத்தவும்"

#, java-printf-format
msgctxt "disk_image"
msgid "Disk image"
msgstr "வட்டு படம்"

#. Label for the card that displays the details about the image that is about
#. to be written
#, java-printf-format
msgctxt "image_to_write"
msgid "Image to write"
msgstr "எழுத படம்"

#, java-printf-format
msgctxt "usb_drive"
msgid "USB drive"
msgstr "யூ.எச்.பி இயக்கி"

#. Label for the card that displays the details about the USB drive that is
#. about to be used
#, java-printf-format
msgctxt "destination_usb_device"
msgid "Destination USB device"
msgstr "இலக்கு யூ.எச்.பி சாதனம்"

#. Device name placeholder
#, java-printf-format
msgctxt "unknown_device"
msgid "Unknown device"
msgstr "தெரியாத சாதனம்"

#. Displayed on the button to request the permission to write to the USB
#. drive. Keep it short.
#, java-printf-format
msgctxt "grant_access"
msgid "Grant access"
msgstr "அணுகல் வழங்கவும்"

#, java-printf-format
msgctxt "permission_granted"
msgid "Permission granted"
msgstr "இசைவு வழங்கப்பட்டது"

#. Title of the card that alerts the user that, if they continue, all data on
#. the USB drive will be lost
#, java-printf-format
msgctxt "be_careful"
msgid "Be careful!"
msgstr "கவனமாக இருங்கள்!"

#. Message displayed on the card that alerts the user that, if they continue,
#. all data on the USB drive will be lost
#, java-printf-format
msgctxt "writing_the_image_will_erase"
msgid "Writing the image will erase all data on the selected drive"
msgstr ""
"படத்தை எழுதுவது தேர்ந்தெடுக்கப்பட்ட இயக்ககத்தில் உள்ள எல்லா தரவையும் "
"அழிக்கும்"

#. Confirmation button; when pressed, the operation starts
#, java-printf-format
msgctxt "write_image"
msgid "Write image"
msgstr "படத்தை எழுதுங்கள்"

#. Title of the dialog that forces the user to lay the device flat on a table
#. before continuing, when the device has no gravity sensor
#, java-printf-format
msgctxt "lay_your_device_flat_no_accel"
msgid "Lay your device flat on a table before continuing."
msgstr "தொடர்வதற்கு முன் உங்கள் சாதனத்தை ஒரு அட்டவணையில் தட்டையாக வைக்கவும்."

#. Button displayed in the dialog that forces the user to lay the device flat
#. on a table before continuing, when the device has no gravity sensor; when
#. pressed the operation starts.
#, java-printf-format
msgctxt "continue_"
msgid "Continue"
msgstr "தொடரவும்"

#. Title of the dialog that forces the user to lay the device flat on a table
#. before continuing, when the device has a gravity sensor
#, java-printf-format
msgctxt "lay_your_device_flat"
msgid "Lay your device flat on a table and don't touch it"
msgstr "உங்கள் சாதனத்தை ஒரு அட்டவணையில் தட்டையாக வைத்து அதைத் தொட வேண்டாம்"

#. Button displayed in the dialog that forces the user to lay the device flat
#. on a table before continuing, when the device has a gravity sensor; when
#. pressed the flat check is skipped and the operation starts.
#, java-printf-format
msgctxt "skip"
msgid "Skip"
msgstr "தவிர்"

#. Description of a device or image type that is supported
#, java-printf-format
msgctxt "supported"
msgid "Supported"
msgstr "ஆதரிக்கப்பட்டது"

#. Description of a device or image type that might be supported
#, java-printf-format
msgctxt "maybe_supported"
msgid "Maybe supported"
msgstr "ஒருவேளை ஆதரிக்கலாம்"

#. Description of a device or image type that is not supported
#, java-printf-format
msgctxt "not_supported"
msgid "Not supported"
msgstr "ஆதரிக்கப்படவில்லை"

#. Title of the page that lists all the supported devices and images
#, java-printf-format
msgctxt "supported_devices_and_images"
msgid "Supported devices and images"
msgstr "உதவி சாதனங்கள் மற்றும் படங்கள்"

#. Title of the section that lists all the supported device types
#, java-printf-format
msgctxt "devices"
msgid "Devices"
msgstr "சாதனங்கள்"

#. Device type
#, java-printf-format
msgctxt "usb_flash_drives"
msgid "USB flash drives"
msgstr "யூ.எச்.பி ஃபிளாச் இயக்கிகள்"

#. Device type
#, java-printf-format
msgctxt "memory_cards_using_a_usb_adapter"
msgid "Memory cards, using a USB adapter"
msgstr "நினைவக கார்டுகள், யூ.எச்.பி அடாப்டரைப் பயன்படுத்தி"

#. Device type
#, java-printf-format
msgctxt "usb_hard_drives_ssds_some_might_work"
msgid "USB hard drives / SSDs (some might work)"
msgstr "யூ.எச்.பி ஆர்ட் டிரைவ்கள் / எச்.எச்.டி.எச் (சில வேலை செய்யலாம்)"

#. Device type
#, java-printf-format
msgctxt "usb_docks_and_hubs_might_have_power_issues"
msgid "USB docks and hubs (they might have power issues)"
msgstr ""
"யூ.எச்.பி கப்பல்துறைகள் மற்றும் மையங்கள் (அவர்களுக்கு ஆற்றல் சிக்கல்கள் "
"இருக்கலாம்)"

#. Device type
#, java-printf-format
msgctxt "memory_cards_using_the_internal_slot"
msgid "Memory cards, using the internal slot"
msgstr "நினைவக கார்டுகள், உள் ச்லாட்டைப் பயன்படுத்தி"

#. Device type
#, java-printf-format
msgctxt "optical_disk_drives"
msgid "Optical disk drives"
msgstr "ஆப்டிகல் வட்டு இயக்கிகள்"

#. Device type
#, java-printf-format
msgctxt "floppy_disk_drives"
msgid "Floppy disk drives"
msgstr "நெகிழ் வட்டு இயக்கிகள்"

#. Device type
#, java-printf-format
msgctxt "any_thunderbolt_only_device"
msgid "Any Thunderbolt-only device"
msgstr "எந்த தண்டர்போல்ட்-மட்டும் சாதனம்"

#. Title of the section that lists all the supported image types
#, java-printf-format
msgctxt "disk_images"
msgid "Disk images"
msgstr "வட்டு படங்கள்"

#. Image type
#, java-printf-format
msgctxt "microsoft_windows_isos_any_version"
msgid "Microsoft Windows ISOs (any version)"
msgstr "மைக்ரோசாப்ட் சாளரங்கள் ஐஎச்ஓஎச் (எந்த பதிப்பும்)"

#. Image type
#, java-printf-format
msgctxt "apple_dmg_disk_images"
msgid "Apple DMG disk images"
msgstr "ஆப்பிள் டி.எம்.சி வட்டு படங்கள்"

#. Image type
#, ignore-multiple-failures, java-printf-format
msgctxt "arch_linux_ubuntu_debian_fedora_pop_os_linux_mint_freebsd_etc"
msgid "Arch Linux, Ubuntu, Debian, Fedora, pop!_OS, Linux Mint, FreeBSD etc."
msgstr ""
"ஆர்ச் லினக்ச், உபுண்டு, டெபியன், ஃபெடோரா, பாப்! _ஓஎச், லினக்ச் புதினா, "
"ஃப்ரீ.பி.எச்.டி போன்றவை."

#. Image type
#, java-printf-format
msgctxt "other_modern_gnu_linux_and_bsd_distributions_live_isos"
msgid "Other modern GNU/Linux and BSD distributions' live ISOs"
msgstr ""
"பிற நவீன குனு/லினக்ச் மற்றும் பி.எச்.டி விநியோகங்களின் நேரடி ஐ.எச்.ஓ.எச்"

#. Image type
#, java-printf-format
msgctxt "raspberry_pi_sd_card_images_unzip_first"
msgid "Raspberry Pi SD card images (unzip first!)"
msgstr "ராச்பெர்ரி பை எச்டி அட்டை படங்கள் (முதலில் unzip!)"

#. Image type
#, java-printf-format
msgctxt "any_other_image_that_works_with_balena_etcher_or_dd"
msgid "Any other image that works with Balena Etcher or dd"
msgstr "பலேனா எட்சர் அல்லது டி.டி."

#. Image type
#, java-printf-format
msgctxt "older_gnu_linux_distributions_isos_2010"
msgid "Older GNU/Linux distributions' ISOs (< 2010)"
msgstr "பழைய குனு/லினக்ச் விநியோகங்கள் 'ஐசோச் (<2010)"

#. At the end of the supported devices and images page we state that DMGs are
#. no longer supported; the parameter is replaced with the word "GitHub"
#, java-printf-format
msgctxt "support_for_dmg_images_was_removed"
msgid ""
"Support for DMG images was removed in EtchDroid 2.0. Older versions can be "
"downloaded from %1$s."
msgstr ""
"டி.எம்.சி படங்களுக்கான உதவி எட்ச்ராய்டு 2.0 இல் அகற்றப்பட்டது. பழைய "
"பதிப்புகளை %1$s இலிருந்து பதிவிறக்கம் செய்யலாம்."

#. Displayed in the about page; parameters are: author name, the word
#. "contributors" (translated separately), "GitHub"
#, java-printf-format
msgctxt "developed_by"
msgid ""
"Developed by %1$s and other %2$s.\n"
"Source code available on %3$s.\n"
"Licensed under the GNU GPLv3 license."
msgstr ""
"%1$s மற்றும் பிற %2$s ஆல் உருவாக்கப்பட்டது.\n"
" மூலக் குறியீடு %3$s இல் கிடைக்கிறது.\n"
" குனு சி.பி.எல்.வி 3 உரிமத்தின் கீழ் உரிமம் பெற்றது."

#. Part of the following sentence: "Developed by Author Name and other
#. contributors"
#, java-printf-format
msgctxt "contributors"
msgid "contributors"
msgstr "பங்களிப்பாளர்கள்"

#. Displayed in the about page links; keep it very short!
#, java-printf-format
msgctxt "website"
msgid "Website"
msgstr "வலைத்தளம்"

#. Displayed in the about page links; keep it short!
#, java-printf-format
msgctxt "support_the_project"
msgid "Support financially"
msgstr "பொருள் ரீதியாக ஆதரிக்கவும்"

#. Title of the progress page while an image is being verified
#, java-printf-format
msgctxt "verifying_image"
msgid "Verifying image"
msgstr "படத்தை சரிபார்க்கிறது"

#. Title of the progress page while an image is being written
#, java-printf-format
msgctxt "writing_image"
msgid "Writing image"
msgstr "படம் எழுதுதல்"

#. Subtitle of the progress page while an image is being written
#, java-printf-format
msgctxt "please_avoid_using_your_device"
msgid "Please avoid using your device"
msgstr "உங்கள் சாதனத்தைப் பயன்படுத்துவதைத் தவிர்க்கவும்"

#. Displayed in a card in the progress page; keep it concise, there's not much
#. space
#, java-printf-format
msgctxt "would_you_like_to_be_notified"
msgid ""
"Would you like to be notified when done? Only progress updates, no spam or "
"ads."
msgstr ""
"முடிந்ததும் அறிவிக்க விரும்புகிறீர்களா? முன்னேற்ற புதுப்பிப்புகள் மட்டுமே, "
"ச்பேம் அல்லது விளம்பரங்கள் இல்லை."

#. Button to reject receiving notifications
#, java-printf-format
msgctxt "no_thanks"
msgid "No, thanks"
msgstr "இல்லை, நன்றி"

#. Button to accept receiving notifications
#, java-printf-format
msgctxt "sure"
msgid "Sure"
msgstr "நிச்சயமாக"

#. As part of the sentence: "Verifying DeviceName against FileName.iso"; keep
#. the same word order as English. Open an issue if it's not possible in your
#. language.
#, java-printf-format
msgctxt "verifying"
msgid "Verifying"
msgstr "சரிபார்க்கும்"

#. As part of the sentence: "Copying FileName.iso to DeviceName"; keep the
#. same word order as English. Open an issue if it's not possible in your
#. language.
#, java-printf-format
msgctxt "copying"
msgid "Copying"
msgstr "நகலெடுக்கும்"

#. As part of the sentence: "Verifying DeviceName against FileName.iso"; keep
#. the same word order as English. Open an issue if it's not possible in your
#. language.
#, java-printf-format
msgctxt "against"
msgid "against"
msgstr "எதிராக"

#. As part of the sentence: "Copying FileName.iso to DeviceName"; keep the
#. same word order as English. Open an issue if it's not possible in your
#. language.
#, java-printf-format
msgctxt "to"
msgid "to"
msgstr "பெறுநர்"

#. Displayed in the progress info indicator while no progress data is
#. available
#, java-printf-format
msgctxt "getting_ready"
msgid "Getting ready…"
msgstr "தயாராகி வருகிறது…"

#. Displayed on a button that, when pressed, skips the verification of the
#. written image
#, java-printf-format
msgctxt "skip_verification"
msgid "Skip verification"
msgstr "சரிபார்ப்பைத் தவிர்க்கவும்"

#. Title of the success page
#, java-printf-format
msgctxt "image_written_successfully"
msgid "Image written successfully!"
msgstr "படம் வெற்றிகரமாக எழுதப்பட்டது!"

#. Displayed in a button on the success page
#, java-printf-format
msgctxt "write_a_review"
msgid "Write a review"
msgstr "மதிப்பாய்வு எழுதுங்கள்"

#. Displayed in a button on the success page
#, java-printf-format
msgctxt "star_on_github"
msgid "Star on GitHub"
msgstr "கிட்அப்பில் விண்மீன்"

#. Title of the card that teaches the user about any "Issues with this drive"
#. notifications
#, java-printf-format
msgctxt "got_an_unsupported_drive_notification"
msgid "Got an ssues with this drive notification?"
msgstr "இந்த இயக்கி அறிவிப்பில் சிக்கல் உள்ளதா?"

#. Message of the card that teaches the user about any "Issues with this
#. drive" notifications; the parameter will be replaced with the translation
#. of "Learn what it means"
#, java-printf-format
msgctxt "it_s_safe_to_ignore"
msgid ""
"It's safe to ignore! Just unplug your drive and use it normally. %1$s and "
"see how to format your drive later."
msgstr ""
"புறக்கணிப்பது பாதுகாப்பானது! உங்கள் இயக்ககத்தை அவிழ்த்து சாதாரணமாகப் "
"பயன்படுத்துங்கள். %1$s மற்றும் பின்னர் உங்கள் இயக்ககத்தை எவ்வாறு வடிவமைப்பது"
" என்று பாருங்கள்."

#. Part of the message of the card that teaches the user about any "Issues
#. with this drive" notifications; the full sentence is "Learn what it means
#. and see how to format your drive later"
#, java-printf-format
msgctxt "learn_what_it_means"
msgid "Learn what it means"
msgstr "அதன் பொருள் என்ன என்பதை அறிக"

#. Title of the non-recoverable error page
#, java-printf-format
msgctxt "there_was_an_error"
msgid "There was an error"
msgstr "பிழை இருந்தது"

#, java-printf-format
msgctxt "error"
msgid "Error"
msgstr "பிழை"

#. Button in the error page that, when pressed, restarts the app
#, java-printf-format
msgctxt "start_over"
msgid "Start over"
msgstr "தொடக்க"

#. Button in the error page that, when pressed, retries the write from scratch
#, java-printf-format
msgctxt "try_again"
msgid "Try again"
msgstr "மீண்டும் முயற்சிக்கவும்"

#. Title of the card that, when a USB disconnection is detected, teaches the
#. user how it could have happened inadvertently
#, java-printf-format
msgctxt "i_did_not_unplug_it"
msgid "I did not unplug it!"
msgstr "நான் அதை அவிழ்க்கவில்லை!"

#. Title of the card that, when a recoverable error occurs, teaches the user
#. why it could have happened
#, java-printf-format
msgctxt "how_did_this_happen"
msgid "How did this happen?"
msgstr "இது எப்படி நடந்தது?"

#. Message of the card that, when a USB disconnection is detected, teaches the
#. user how it could have happened inadvertently
#, java-printf-format
msgctxt "perhaps_your_usb_port_is_dirty"
msgid ""
"Perhaps your USB port is dirty, your adapter is defective, or your device "
"can't provide enough electrical power. Consider trying a different adapter "
"or device."
msgstr ""
"உங்கள் யூ.எச்.பி துறைமுகம் அழுக்காக இருக்கலாம், உங்கள் அடாப்டர் "
"குறைபாடுடையது, அல்லது உங்கள் சாதனத்தால் போதுமான மின் சக்தியை வழங்க முடியாது."
" வேறு அடாப்டர் அல்லது சாதனத்தை முயற்சிப்பதைக் கவனியுங்கள்."

#. Message of the card that, when a recoverable error occurs, teaches the user
#. why it could have happened
#, java-printf-format
msgctxt "usb_drives_are_unreliable"
msgid ""
"USB drives are unreliable, and Android doesn't grant enough permissions to "
"recover automatically. We have to make do with what we have!"
msgstr ""
"யூ.எச்.பி டிரைவ்கள் நம்பமுடியாதவை, மேலும் ஆண்ட்ராய்டு தானாக மீட்க போதுமான "
"அனுமதிகளை வழங்காது. நம்மிடம் உள்ளதைச் செய்ய வேண்டும்!"

#. Message of the card about a recoverable unknown error
#, java-printf-format
msgctxt "this_is_unexpected_it_s_probably_a_bug_please_report_it"
msgid "This is unexpected, it's probably a bug. Please report it!"
msgstr "இது எதிர்பாராதது, இது ஒரு பிழை. தயவுசெய்து அதைப் புகாரளிக்கவும்!"

#. Text that explains what to do when a recoverable error has occurred
#, java-printf-format
msgctxt "to_recover_unplug"
msgid ""
"To recover, unplug your USB drive, plug it back in and grant the permission "
"to access it again."
msgstr ""
"மீட்டெடுக்க, உங்கள் யூ.எச்.பி டிரைவை அவிழ்த்து, அதை மீண்டும் செருகவும், அதை "
"மீண்டும் அணுக இசைவு வழங்கவும்."

#. Textual description (for accessibility purposes) of the graphic that shows
#. the required steps to recover from a recoverable error
#, java-printf-format
msgctxt "representation_of_the_required_steps"
msgid ""
"Representation of the required steps: unplug, reconnect, grant permission"
msgstr ""
"தேவையான படிகளின் பிரதிநிதித்துவம்: அவிழ்த்து, மீண்டும் இணைக்க, இசைவு "
"வழங்குதல்"

#. Message displayed when a user tries to recover from a recoverable error but
#. plugs in a different USB device
#, java-printf-format
msgctxt "plug_in_the_same_usb"
msgid "Plug in the same USB device you used before!"
msgstr "நீங்கள் முன்பு பயன்படுத்திய அதே யூ.எச்.பி சாதனத்தை செருகவும்!"

#. Message displayed in a toast when the write operation is successfully
#. resumed
#, java-printf-format
msgctxt "usb_device_reconnected_resuming"
msgid "USB device reconnected, resuming"
msgstr "யூ.எச்.பி சாதனம் மீண்டும் இணைக்கப்பட்டது, மீண்டும் தொடங்குகிறது"

#. Error message
#, java-printf-format
msgctxt "the_device_is_not_responding"
msgid "The device is not responding"
msgstr "சாதனம் பதிலளிக்கவில்லை"

#. Error message
#, java-printf-format
msgctxt "missing_permission_to_access_the_usb_device"
msgid "Missing permission to access the USB device"
msgstr "யூ.எச்.பி சாதனத்தை அணுக இசைவு இல்லை"

#. Error message; parameters are the image file size and the device size
#, java-printf-format
msgctxt "the_image_is_is_larger_than_the_device"
msgid "The image (%1$s) is larger than the device (%2$s)"
msgstr "படம் (%1$s) சாதனத்தை விட பெரியது (%2$s)"

#. Error message
#, java-printf-format
msgctxt "could_not_open_the_source_file"
msgid "Could not open the source file"
msgstr "மூல கோப்பைத் திறக்க முடியவில்லை"

#. Error message; parameter is the underlying error message
#, java-printf-format
msgctxt "an_unknown_error_occurred"
msgid "An unknown error occurred: %1$s"
msgstr "அறியப்படாத பிழை ஏற்பட்டது: %1$s"

#. Error message
#, java-printf-format
msgctxt "the_usb_drive_was_unplugged"
msgid "The USB drive was unplugged"
msgstr "யூ.எச்.பி இயக்கி அவிழ்க்கப்பட்டது"

#. Error message
#, java-printf-format
msgctxt "the_usb_drive_stopped_responding"
msgid "The USB drive stopped responding"
msgstr "யூ.எச்.பி இயக்கி பதிலளிப்பதை நிறுத்தியது"

#. Error message displayed when the verification failed
#, java-printf-format
msgctxt "the_usb_device_content_does_not_match_the_image"
msgid "The USB device content does not match the image"
msgstr "யூ.எச்.பி சாதன உள்ளடக்கம் படத்துடன் பொருந்தவில்லை"

#. Displayed in the successful write page, when clicked the app goes back to
#. the initial page
#, java-printf-format
msgctxt "write_another_image"
msgid "Write another image"
msgstr "மற்றொரு படத்தை எழுதுங்கள்"

#. Title of the screen that displays the summary of the operation about to be
#. performed
#, java-printf-format
msgctxt "title_activity_confirm_operation"
msgid "Confirm operation"
msgstr "செயல்பாட்டை உறுதிப்படுத்தவும்"

#. Error message (shown in a toast) when the operation confirmation screen is
#. opened without a source file; the app redirects back to the home page
#, java-printf-format
msgctxt "no_image_uri_provided"
msgid "No image URI provided"
msgstr "யூரி எந்த படமும் வழங்கப்படவில்லை"

#. Error message (shown in a toast) when the operation confirmation screen is
#. opened without a destination device; the app redirects back to the home
#. page
#, java-printf-format
msgctxt "no_destination_device_selected"
msgid "No destination device selected"
msgstr "இலக்கு சாதனம் தேர்ந்தெடுக்கப்படவில்லை"

#. Error message displayed when the background write service does not send any
#. progress updates for a suspiciously long time. In this case, the app
#. assumes the service got stuck and prompts the user to try again.
#, java-printf-format
msgctxt "service_timeout_message"
msgid "The writing process seems to have gotten stuck. Please try again."
msgstr "எழுதும் செயல்முறை சிக்கியதாக தெரிகிறது. மீண்டும் முயற்சிக்கவும்."

#. "You're on your own" can be translated with something similar to
#. "unsupported" or "be careful!" if it makes more sense in the target
#. language
#, java-printf-format
msgctxt "community_windows_images"
msgid ""
"Community-built Windows images, made for EtchDroid (you're on your own: they"
" might contain viruses!)"
msgstr ""
"சமூகத்தால் கட்டப்பட்ட சாளரங்கள் படங்கள், எட்ச்ராய்டுக்காக "
"தயாரிக்கப்படுகின்றன (நீங்கள் சொந்தமாக இருக்கிறீர்கள்: அவற்றில் வைரச்கள் "
"இருக்கலாம்!)"

#. Error message displayed when users get a mysterious NullPointerException
#. while starting a new write. The original translation tries to give the user
#. a general idea of the issue without being too technical.
#, java-printf-format
msgctxt "worker_null_device_error"
msgid ""
"A write attempt was started but unfortunately some info got lost along the "
"way. Please try again."
msgstr ""
"ஒரு எழுதும் முயற்சி தொடங்கப்பட்டது, ஆனால் துரதிர்ச்டவசமாக சில தகவல்கள் "
"வழியில் தொலைந்துவிட்டன. மீண்டும் முயற்சிக்கவும்."

#. Displayed in the fatal error screen, prompting the users to report the
#. issue on GitHub along with the steps to reproduce it
#, java-printf-format
msgctxt "please_report_fatal_issue"
msgid ""
"If you can, please report this issue on GitHub along with the steps that led"
" to this error."
msgstr ""
"உங்களால் முடிந்தால், இந்த பிழைக்கு வழிவகுத்த படிகளுடன் இந்த சிக்கலை கிதுபில்"
" புகாரளிக்கவும்."

#. Displayed in the fatal error screen, in a small button. When pressed, the
#. app opens the GitHub issues page. Since the button is small, please keep
#. this short. You can replace it with "GitHub issues" if "View GitHub issues"
#. is too long in your language.
#, java-printf-format
msgctxt "view_github_issues"
msgid "View GitHub issues"
msgstr "அறிவிலிமையம் சிக்கல்களைக் காண்க"

#. Displayed in the menu if the current app build may send telemetry (Google
#. Play builds)
#, java-printf-format
msgctxt "telemetry"
msgid "Telemetry"
msgstr "டெலிமெட்ரி"

#. Displayed in the app menu, as well as in other links to the privacy policy
#, java-printf-format
msgctxt "privacy_policy"
msgid "Privacy policy"
msgstr "தனியுரிமைக் கொள்கை"

#. Displayed in builds with telemetry enabled (Google Play) when the user
#. tries to opt-out of telemetry to try to convince them not to. '%1$s' is
#. replaced with a link to the privacy policy; the `privacy_policy` string is
#. used.
#, java-printf-format
msgctxt "telemetry_rationale"
msgid ""
"EtchDroid values your privacy and only uses minimal telemetry to investigate app issues.\n"
"\n"
"Be a part of our open-source community—keep telemetry enabled and help improve EtchDroid for everyone!\n"
"\n"
"While we don't collect any personally-identifying information, we believe in transparency and want you to know that opened image file names and USB device IDs are collected anonymously to help diagnose issues with specific hardware. Unless your image file names or device IDs are highly sensitive, it's safe to share this data. (%1$s)"
msgstr ""
"எட்ச்ராய்டு உங்கள் தனியுரிமையை மதிப்பிடுகிறது மற்றும் பயன்பாட்டு சிக்கல்களை விசாரிக்க குறைந்தபட்ச டெலிமெட்ரி மட்டுமே பயன்படுத்துகிறது.\n"
"\n"
" எங்கள் திறந்த மூல சமூகத்தின் ஒரு பகுதியாக இருங்கள்-கீப் டெலிமெட்ரி இயக்கப்பட்டது மற்றும் அனைவருக்கும் எட்ச்ராய்டை மேம்படுத்த உதவுகிறது!\n"
"\n"
" தனிப்பட்ட முறையில் அடையாளம் காணும் எந்த தகவலையும் நாங்கள் சேகரிக்கவில்லை என்றாலும், வெளிப்படைத்தன்மையை நாங்கள் நம்புகிறோம், குறிப்பிட்ட வன்பொருளுடன் சிக்கல்களைக் கண்டறிய உதவுவதற்காக திறந்த படக் கோப்பு பெயர்கள் மற்றும் யூ.எச்.பி சாதன ஐடிகள் அநாமதேயமாக சேகரிக்கப்படுகின்றன என்பதை நீங்கள் அறிந்து கொள்ள விரும்புகிறோம். உங்கள் பட கோப்பு பெயர்கள் அல்லது சாதன ஐடிகள் மிகவும் உணர்திறன் கொண்டவை எனில், இந்தத் தரவைப் பகிர்வது பாதுகாப்பானது. (%1$s)"

#. Title of the dialog that asks the user to enable telemetry
#, java-printf-format
msgctxt "we_need_your_help"
msgid "We need your help!"
msgstr "எங்களுக்கு உங்கள் உதவி தேவை!"

#. Description of the telemetry icon for accessibility purposes
#, java-printf-format
msgctxt "telemetry_icon"
msgid "Telemetry icon"
msgstr "டெலிமெட்ரி படவுரு"

#. Button to opt-out of telemetry; it can be translated as "Disable telemetry"
#. if it fits better in the target language
#, java-printf-format
msgctxt "telemetry_opt_out"
msgid "Opt-out"
msgstr "விலகல்"

#. Button to opt-in to telemetry; it can be translated as "Enable telemetry"
#. if it fits better in the target language
#, java-printf-format
msgctxt "telemetry_keep_enabled"
msgid "Keep enabled"
msgstr "இயக்கப்பட்டிருங்கள்"

#. Displayed in the main app screen on first launch in a toast/snackbar. It
#. should be short and concise.
#, java-printf-format
msgctxt "etchdroid_uses_telemetry"
msgid "EtchDroid sends telemetry info."
msgstr "எட்ச்ராய்டு டெலிமெட்ரி தகவலை அனுப்புகிறது."

#. Displayed in the main app screen on first launch as a snackbar action
#. button. It should be short and concise.
#, java-printf-format
msgctxt "learn_more"
msgid "Learn more"
msgstr "மேலும் அறிக"

#. Displayed in the main app menu; controls whether the app sends anonymous
#. telemetry info.
#, java-printf-format
msgctxt "send_anonymous_data"
msgid "Send anonymous data"
msgstr "அநாமதேய தரவை அனுப்பவும்"
