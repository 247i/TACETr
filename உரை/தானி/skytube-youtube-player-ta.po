#, java-printf-format
msgctxt "error_stream_err_unavailable"
msgid "Unavailable message."
msgstr "கிடைக்காத செய்தி."

#, java-printf-format
msgctxt "error_stream_decryption_fail"
msgid "Could not load decryption code for the YouTube service."
msgstr "YouTube சேவைக்கான மறைகுறியாக்கக் குறியீட்டை ஏற்ற முடியவில்லை."

#, java-printf-format
msgctxt "error_stream_parse_error"
msgid "Parsing error."
msgstr "பாகுபடுத்தும் பிழை."

#, java-printf-format
msgctxt "error_video_play"
msgid "Video Playback Error"
msgstr "வீடியோ பிளேபேக் பிழை"

#, java-printf-format
msgctxt "error_video_streams_empty"
msgid "The '%s' video has no streams."
msgstr "'%s' வீடியோவில் நீரோடைகள் இல்லை."

#, java-printf-format
msgctxt "error_video_streams"
msgid "Could not fetch the any of the video streams."
msgstr "எந்த வீடியோ ச்ட்ரீம்களையும் பெற முடியவில்லை."

#, java-printf-format
msgctxt "error_get_video_desc"
msgid "Could not fetch the video description."
msgstr "வீடியோ விளக்கத்தைப் பெற முடியவில்லை."

#, java-printf-format
msgctxt "error_invalid_url"
msgid "Invalid video URL: %s"
msgstr "தவறான வீடியோ URL: %s"

#, java-printf-format
msgctxt "error_downloaded_file_is_corrupted"
msgid ""
"The downloaded video looks <b>corrupted</b>: <br/> <br/> %s. <br/> <br/> "
"<b>Delete</b> the file?"
msgstr ""
"பதிவிறக்கம் செய்யப்பட்ட வீடியோ <b> சிதைந்த </b>: <br/> <br/> %s. <br/> <br/>"
" <b> நீக்கு </b> கோப்பு?"

#, java-printf-format
msgctxt "error_video_parse_error"
msgid "Could not start the video: <br/> <br/> %s."
msgstr "வீடியோவைத் தொடங்க முடியவில்லை: <br/> <br/> %s."

#, java-printf-format
msgctxt "metered_network"
msgid "Metered Network"
msgstr "மீட்டர் பிணையம்"

#, java-printf-format
msgctxt "warning_metered_network_play"
msgid ""
"Connect to an unmetered network to avoid data usage charges first. Play "
"video anyway?"
msgstr ""
"தரவு பயன்பாட்டு கட்டணங்களைத் தவிர்க்க முதலில் அளவிடப்படாத பிணையத்துடன் "
"இணைக்கவும். எப்படியும் வீடியோ விளையாடலாமா?"

#, java-printf-format
msgctxt "warning_metered_network_disable"
msgid "Never ask again."
msgstr "மீண்டும் ஒருபோதும் கேட்க வேண்டாம்."

#, java-printf-format
msgctxt "metered_network_blocked_by_policy"
msgid "Download blocked because your are connected through a metered network."
msgstr ""
"மீட்டமைக்கப்பட்ட பிணையம் மூலம் நீங்கள் இணைக்கப்பட்டுள்ளதால் தடுக்கப்பட்ட "
"பதிவிறக்க."

#, java-printf-format
msgctxt "play_video"
msgid "Play Video"
msgstr "வீடியோ விளையாடுங்கள்"

#, java-printf-format
msgctxt "warning_live_video"
msgid "Cannot play live streams for now. Open in another app?"
msgstr ""
"இப்போது நேரடி நீரோடைகளை விளையாட முடியாது. மற்றொரு பயன்பாட்டில் திறக்கவா?"

#, java-printf-format
msgctxt "ratings_disabled"
msgid "Ratings off"
msgstr "மதிப்பீடுகள் முடக்கப்படுகின்றன"

#, java-printf-format
msgctxt "loading_video"
msgid "Loading video…"
msgstr "வீடியோவை ஏற்றுகிறது…"

#, java-printf-format
msgctxt "reload_video"
msgid "Reload Video"
msgstr "வீடியோவை மீண்டும் ஏற்றவும்"

#, java-printf-format
msgctxt "open_in_browser"
msgid "Open in web browser"
msgstr "வலை உலாவியில் திறக்கவும்"

#, java-printf-format
msgctxt "open_with"
msgid "Open with…"
msgstr "உடன் திறந்திருக்கும்…"

#, java-printf-format
msgctxt "share"
msgid "Share"
msgstr "பங்கு"

#, java-printf-format
msgctxt "share_via"
msgid "Share via…"
msgstr "வழியாக பகிரவும்…"

#, java-printf-format
msgctxt "copy_url"
msgid "Copy URL"
msgstr "முகவரி ஐ நகலெடுக்கவும்"

#, java-printf-format
msgctxt "url_copied_to_clipboard"
msgid "URL copied to clipboard."
msgstr "முகவரி இடைநிலைப்பலகைக்கு நகலெடுக்கப்பட்டது."

#, java-printf-format
msgctxt "mark_watched"
msgid "Mark as Watched"
msgstr "பார்த்தபடி குறி"

#, java-printf-format
msgctxt "mark_unwatched"
msgid "Mark as Unwatched"
msgstr "குறிக்கப்படாத எனக் குறிக்கவும்"

#, java-printf-format
msgctxt "error_get_comments"
msgid "Could not fetch comments."
msgstr "கருத்துகளைப் பெற முடியவில்லை."

#, java-printf-format
msgctxt "error_get_comments_no_network"
msgid "Connect to the Internet to fetch comments first."
msgstr "முதலில் கருத்துகளைப் பெற இணையத்துடன் இணைக்கவும்."

#, java-printf-format
msgctxt "no_video_comments"
msgid "This video either has no comments, or they are turned off."
msgstr ""
"இந்த வீடியோவுக்கு எந்தக் கருத்தும் இல்லை, அல்லது அவை அணைக்கப்படுகின்றன."

#, java-printf-format
msgctxt "view_replies"
msgid "View replies"
msgstr "பதில்களைக் காண்க"

#, java-printf-format
msgctxt "hide_replies"
msgid "Hide replies"
msgstr "பதில்களை மறைக்கவும்"

#, java-printf-format
msgctxt "enter_video_url"
msgid "Enter Video URL"
msgstr "வீடியோ முகவரி ஐ உள்ளிடவும்"

#, java-printf-format
msgctxt "play"
msgid "Play"
msgstr "விளையாடுங்கள்"

#, java-printf-format
msgctxt "cancel"
msgid "Cancel"
msgstr "ரத்துசெய்"

#, java-printf-format
msgctxt "clear"
msgid "Clear"
msgstr "தெளிவான"

#, java-printf-format
msgctxt "should_resume"
msgid "Continue from where you stopped?"
msgstr "நீங்கள் நிறுத்திய இடத்திலிருந்து தொடரவா?"

#, java-printf-format
msgctxt "resume"
msgid "Resume"
msgstr "மீண்டும் தொடங்குங்கள்"

#, java-printf-format
msgctxt "sponsorblock_skipped"
msgid "Skipped segment: %s"
msgstr "தவிர்க்கப்பட்ட பிரிவு: %s"

#, java-printf-format
msgctxt "sponsorblock_category_sponsor"
msgid "Sponsor"
msgstr "ஒப்புரவாளர்"

#, java-printf-format
msgctxt "sponsorblock_category_selfpromo"
msgid "Self Promotion"
msgstr "தன்வய ஊக்குவிப்பு"

#, java-printf-format
msgctxt "sponsorblock_category_interaction"
msgid "Interaction Reminder"
msgstr "தொடர்பு நினைவூட்டல்"

#, java-printf-format
msgctxt "sponsorblock_category_music_offtopic"
msgid "Non-Music Section"
msgstr "இசை அல்லாத பிரிவு"

#, java-printf-format
msgctxt "sponsorblock_category_intro"
msgid "Intro/Intermission"
msgstr "அறிமுகம்/இடைவெளி"

#, java-printf-format
msgctxt "sponsorblock_category_outro"
msgid "Endcards/Credits"
msgstr "எண்ட்கார்டுகள்/வரவு"

#, java-printf-format
msgctxt "sponsorblock_category_preview"
msgid "Preview/Recap"
msgstr "முன்னோட்டம்/மறுபரிசீலனை"

#, java-printf-format
msgctxt "sponsorblock_category_filler"
msgid "Filler"
msgstr "தடி"
