#, java-printf-format
msgctxt "app_name_full"
msgid "F-Droid"
msgstr "எஃப்-ட்ராய்டு (F-Droid)"

#. This is the app name of the basic flavor of F-Droid client.
#, java-printf-format
msgctxt "app_name_basic"
msgid "F-Droid Basic"
msgstr "எஃப்-டிராய்டு அடிப்படை"

#, java-printf-format
msgctxt "SignatureMismatch"
msgid ""
"The new version is signed with a different key to the old one. To install "
"the new version, the old one must be uninstalled first. Please do this and "
"try again. (Note that uninstalling will erase any internal data stored by "
"the app)"
msgstr ""
"புதிய பதிப்பு பழைய ஒரு வேறு விசையுடன் ஒப்பமிடப்பட்டுள்ளது. புதிய பதிப்பை நிறுவ, "
"பழையது முதலில் நிறுவல் நீக்கப்பட வேண்டும். தயவுசெய்து இதைச் செய்து மீண்டும் முயலவும். "
"(நிறுவல் நீக்கம் செயலி சேமித்த உள் தரவை அழித்துவிடும் என்பதை குறித்துக்கொள்க)"

#, java-printf-format
msgctxt "installIncompatible"
msgid "App incompatible with your device, install anyway?"
msgstr "உஞ்சாதனத்துடன் ஒவ்வாத செயலி, எப்படியும் நிறுவவா?"

#, java-printf-format
msgctxt "version"
msgid "Version"
msgstr "பதிப்பு"

#, java-printf-format
msgctxt "by_author_format"
msgid "by %s"
msgstr "%s ஆல்"

#, java-printf-format
msgctxt "delete"
msgid "Delete"
msgstr "நீக்குக"

#, java-printf-format
msgctxt "prompt_to_send_crash_reports"
msgid "Prompt to send crash reports"
msgstr "சிதைவறிக்கைகளை அனுப்பத் தூண்டு"

#, java-printf-format
msgctxt "prompt_to_send_crash_reports_summary"
msgid "Gather data about crashes and ask to send them to the developer"
msgstr ""
"செயலிழப்புகளைப் பற்றிய தரவைச் சேகரித்து அவற்றை உருவாக்குபவருக்கு அனுப்பச் சொல்லுங்கள்"

#, java-printf-format
msgctxt "cache_downloaded"
msgid "Keep cached apps"
msgstr "பிடிதரவிலுள்ள செயலிகளை வைத்திரு"

#.  as in software updates 
#.
#, java-printf-format, priority:140
msgctxt "updates"
msgid "Updates"
msgstr "புதுப்பிப்புகள்"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "unstable_updates"
msgid "Unstable updates"
msgstr "நிலையற்ற புதுப்பிப்புகள்"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "unstable_updates_summary"
msgid "Suggest updates to unstable versions"
msgstr "நிலையற்ற பதிப்புகளுக்கு புதுப்பிப்புகளைப் பரிந்துரை"

#, java-printf-format
msgctxt "hide_all_notifications"
msgid "Hide all notifications"
msgstr "எல்லா அறிவிப்புகளையும் மறை"

#, java-printf-format
msgctxt "hide_all_notifications_summary"
msgid ""
"Prevent all actions from showing in the status bar and notification drawer."
msgstr ""
"அனைத்து செயல்களையும் நிலைப்பட்டி மற்றும் அறிவிப்பு இழுப்பறையில் காண்பிப்பதிலிருந்து தடு."

#, java-printf-format
msgctxt "send_install_history"
msgid "Send Install History"
msgstr "நிறுவல் வரலாற்றை அனுப்பு"

#, java-printf-format
msgctxt "send_fdroid_metrics_report"
msgid "Send %s Metrics Report"
msgstr "%s அளவீட்டு அறிக்கையை அனுப்பு"

#, java-printf-format
msgctxt "send_history_csv"
msgid "%s install history as CSV file"
msgstr "%s நிறுவல் வரலாறு CSV கோப்பாக"

#, java-printf-format
msgctxt "send_fdroid_metrics_json"
msgid "%s metrics report as JSON file"
msgstr "%s அளவீட்டு அறிக்கை JSON கோப்பாக"

#, java-printf-format
msgctxt "install_history"
msgid "Install history"
msgstr "நிறுவல் வரலாறு"

#, java-printf-format
msgctxt "install_history_and_metrics"
msgid "Install history and metrics"
msgstr "நிறுவல் வரலாறு மற்றும் அளவீடுகள்"

#, java-printf-format
msgctxt "fdroid_metrics_report"
msgid "%s Metrics Report"
msgstr "%s அளவீட்டு அறிக்கை"

#, java-printf-format
msgctxt "install_history_summary"
msgid "View the private log of all installs and uninstalls"
msgstr "அனைத்து நிறுவல்கள் மற்றும் நிறுவல்நீக்கங்களின் தனிப்பட்ட பதிவைப் பார்"

#, java-printf-format
msgctxt "keep_install_history"
msgid "Keep install history"
msgstr "நிறுவல் வரலாற்றை வைத்திருங்கள்"

#, java-printf-format
msgctxt "keep_install_history_summary"
msgid "Store a log of all installs and uninstalls in a private store"
msgstr ""
"அனைத்து நிறுவல்கள் மற்றும் நிறுவல் நீக்கங்களின் பதிவை ஒரு தனிப்பட்ட அங்காடியில் சேமி"

#, java-printf-format
msgctxt "send_to_fdroid_metrics"
msgid "Send usage data"
msgstr "பயன்பாட்டு தரவை அனுப்பு"

#, java-printf-format
msgctxt "send_to_fdroid_metrics_summary"
msgid ""
"Sends anonymous data weekly to F-Droid Metrics (requires Keep install "
"history)"
msgstr ""
"அநாமதேய தரவை வாரந்தோறும் எஃப்-ட்ராய்டு (F-Droid) அளவீடுகளுக்கு அனுப்புகிறது (நிறுவல் "
"வரலாற்றை வைத்திரு தேவை)"

#.  message shown as a "toast" when the user enables the Send to F-Droid Metrics preference 
#.
#, java-printf-format
msgctxt "toast_metrics_in_install_history"
msgid "The %s Metric report is viewable in the Install History viewer"
msgstr "%s மெட்ரிக் அறிக்கை நிறுவு வரலாற்று பார்வையாளரில் காணப்படுகிறது"

#, java-printf-format
msgctxt "send_version_and_uuid"
msgid "Send version and UUID to servers"
msgstr "பதிப்பு மற்றும் UUID ஐ சேவையகங்களுக்கு அனுப்பு"

#, java-printf-format
msgctxt "send_version_and_uuid_summary"
msgid ""
"Include this app's version and a random, unique ID when downloading, takes "
"effect next app restart."
msgstr ""
"இந்த பயன்பாட்டின் பதிப்பு மற்றும் பதிவிறக்கும்போது சீரற்ற, தனித்துவமான ஐடி ஆகியவற்றைச் "
"சேர்க்கவும், அடுத்த பயன்பாட்டு மறுதொடக்கம் நடைமுறைக்கு வருகிறது."

#, java-printf-format
msgctxt "force_old_index"
msgid "Force old index format"
msgstr "பழைய குறியீட்டு வடிவமைப்பை கட்டாயப்படுத்து"

#, java-printf-format
msgctxt "force_old_index_summary"
msgid "In case there are bugs or compatibility issues, use the XML app index"
msgstr ""
"பிழைகள் அல்லது ஒவ்வுமைச் சிக்கல்கள் இருந்தால், XML செயலி குறியீட்டை பயன்படுத்து"

#, java-printf-format
msgctxt "other"
msgid "Other"
msgstr "மற்றவை"

#, java-printf-format
msgctxt "update_interval"
msgid "Automatic update interval"
msgstr "தானியங்கி புதுப்பிப்பு இடைவெளி"

#, java-printf-format
msgctxt "over_wifi"
msgid "Over Wi-Fi"
msgstr "அருகலை (Wi-Fi) வழியாக"

#.  "data" as in cellular internet service 
#.
#, java-printf-format
msgctxt "over_data"
msgid "Over data"
msgstr "தரவுக்கு மேல்"

#, java-printf-format
msgctxt "over_network_always_summary"
msgid "Always use this connection when available"
msgstr "கிடைக்கும்போது எப்போதும் இவ்விணைப்பைப் பயன்படுத்து"

#, java-printf-format
msgctxt "over_network_on_demand_summary"
msgid "Only use this connection when I click to download"
msgstr "இவ்விணைப்பைப் பதிவிறக்க சொடுக்கினால் மட்டுமே பயன்படுத்து"

#, java-printf-format
msgctxt "over_network_never_summary"
msgid "Never download anything using this connection"
msgstr "இந்த இணைப்பைப் பயன்படுத்தி எதையும் பதிவிறக்க வேண்டாம்"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "update_auto_download"
msgid "Automatically fetch updates"
msgstr "புதுப்பிப்புகளைத் தானாக பெறு"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "update_auto_download_summary"
msgid ""
"Updates are downloaded automatically and you are notified to install them"
msgstr ""
"புதுப்பிப்புகள் தானாகவே பதிவிறக்கம் செய்யப்படுகின்றன, அவற்றை நிறுவுமாறு உங்களுக்கு "
"அறிவிக்கப்படும்"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "update_auto_install"
msgid "Automatically install updates"
msgstr "புதுப்பிப்புகளைத் தானாக நிறுவு"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "update_auto_install_summary"
msgid "Download and update apps in the background, showing a notification"
msgstr "அறிவிப்பைக் காட்டி, பின்னணியில் செயலிகளைப் பதிவிறக்கு மற்றும் புதுப்பி"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "notify"
msgid "Show available updates"
msgstr "கிடைக்கக்கூடிய புதுப்பிப்புகளைக் காட்டு"

#.  as in software updates 
#.
#, java-printf-format
msgctxt "notify_on"
msgid "Show a notification when updates are available"
msgstr "புதுப்பிப்புகள் கிடைக்கும்போது அறிவிப்பைக் காட்டு"

#, java-printf-format
msgctxt "force_old_installer_title"
msgid "Force old installer"
msgstr "பழைய நிறுவியை கட்டாயப்படுத்துங்கள்"

#, java-printf-format
msgctxt "force_old_installer_summary"
msgid ""
"Disable the Session Installer, including support for automatic background "
"updates."
msgstr ""
"தானியங்கி பின்னணி புதுப்பிப்புகளுக்கான உதவி உட்பட அமர்வு நிறுவியை முடக்கவும்."

#, java-printf-format
msgctxt "system_installer"
msgid "Privileged Extension"
msgstr "சிறப்புரிமை நீட்டிப்பு"

#, java-printf-format
msgctxt "system_installer_on"
msgid "Use Privileged Extension to install, update, and remove packages"
msgstr ""
"தொகுப்புகளை நிறுவ, புதுப்பிக்க மற்றும் நீக்க சிறப்புரிமை நீட்டிப்பைப் பயன்படுத்து"

#, java-printf-format
msgctxt "local_repo_name"
msgid "Name of your Local Repo"
msgstr "உங்கள் உள்ளக களஞ்சியத்தின் பெயர்"

#, java-printf-format
msgctxt "local_repo_name_summary"
msgid "The advertised title of your local repo: %s"
msgstr "உங்கள் உள்ளக களஞ்சியத்தின் விளம்பரப்படுத்தப்பட்ட தலைப்பு: %s"

#, java-printf-format
msgctxt "local_repo_https_on"
msgid "Use encrypted HTTPS:// connection for local repo"
msgstr "உள்ளக களஞ்சியத்திற்கு மறைகுறியாக்கப்பட்ட HTTPS:// இணைப்பைப் பயன்படுத்து"

#, java-printf-format
msgctxt "scan_removable_storage_title"
msgid "Scan removable storage"
msgstr "நீக்கக்கூடிய சேமிப்பிடத்தை வருடு"

#, java-printf-format
msgctxt "scan_removable_storage_toast"
msgid "Scanning %s…"
msgstr "வருடுகிறது %s…"

#, java-printf-format
msgctxt "scan_removable_storage_summary"
msgid ""
"Look for package repos on removable storage like SD Cards and USB thumb "
"drives"
msgstr ""
"எஸ்டி கார்டுகள் மற்றும் யூ.எஸ்.பி கட்டைவிரல் இயக்ககங்கள் போன்ற நீக்கக்கூடிய சேமிப்பகத்தில் "
"தொகுப்பு களஞ்சியங்களைத் தேடுங்கள்"

#, java-printf-format
msgctxt "login_title"
msgid "Authentication required"
msgstr "அங்கீகாரம் தேவைப்படுகிறது"

#, java-printf-format
msgctxt "login_name"
msgid "Username"
msgstr "பயனர் பெயர்"

#, java-printf-format
msgctxt "login_password"
msgid "Password"
msgstr "கடவுச்சொல்"

#, java-printf-format
msgctxt "repo_edit_credentials"
msgid "Change Password"
msgstr "கடவுச்சொல்லை மாற்று"

#, java-printf-format
msgctxt "repo_error_empty_username"
msgid "Empty username, credentials not changed"
msgstr "வெற்று பயனர் பெயர், நற்சான்றிதழ்கள் மாற்றப்படவில்லை"

#, java-printf-format, priority:120
msgctxt "app_details"
msgid "App Details"
msgstr "செயலி விவரங்கள்"

#, java-printf-format
msgctxt "no_such_app"
msgid "No such app found."
msgstr "அத்தகைய செயலி ஏதுமில்லை."

#.  Shown above the repository an app is in 
#.
#, java-printf-format
msgctxt "app_details_repository"
msgid "Repository"
msgstr "களஞ்சியம்"

#.  If an app is in more than one repository, this is shown above the selection dropdown 
#.
#, java-printf-format
msgctxt "app_details_repositories"
msgid "Repositories"
msgstr "களஞ்சியங்கள்"

#.  Shown next to the repository name of the repo that is currently preferred by the user.
#.          Information and updates for that app only come from the preferred repo.
#.          Other languages could use other words that work better in their language such as (chosen).
#.     
#.
#, java-printf-format
msgctxt "app_details_repository_preferred"
msgid "(preferred)"
msgstr "(விரும்பப்பட்டது)"

#.  A button label. When clicked, this changes the preferred repo to the currently shown one.
#.          Information and updates for that app only come from the preferred repo.
#.          Other languages could use other words that work better in their language such as (chosen).
#.     
#.
#, java-printf-format
msgctxt "app_details_repository_button_prefer"
msgid "Prefer Repository"
msgstr "களஞ்சியத்தை விரும்புங்கள்"

#, java-printf-format
msgctxt "app_details_repository_expand"
msgid "Expand repository list"
msgstr "களஞ்சிய பட்டியலை விரிவாக்கு"

#, java-printf-format
msgctxt "app_details_donate_prompt_unknown_author"
msgid "Buy the developers of %1$s a coffee!"
msgstr "%1$s இன் வளர்ச்சியாளர்களுக்கு ஒரு காபி வாங்க!"

#, java-printf-format
msgctxt "app_details_donate_prompt"
msgid "%1$s is created by %2$s. Buy them a coffee!"
msgstr "%1$s %2$s ஆல் உருவாக்கப்பட்டது. அவர்களுக்கு ஒரு காபி வாங்க!"

#, java-printf-format
msgctxt "app_details__incompatible_mismatched_signers"
msgid "Different signer to installed version"
msgstr "நிறுவப்பட்ட பதிப்பிற்கு வேறு கையொப்பமிடுபவர்"

#, java-printf-format
msgctxt "app_details__no_versions__show_incompat_versions"
msgid "To show incompatible versions here anyway, enable the \"%1$s\" setting."
msgstr "ஒவ்வா பதிப்புகளை இங்கே எப்படியும் காட்ட, \"%1$s\" அமைப்பை இயக்கு."

#, java-printf-format
msgctxt "app_details__no_versions__no_compatible_signers"
msgid "No versions with compatible signer"
msgstr "இணக்கமான கையொப்பத்துடன் பதிப்புகள் இல்லை"

#, java-printf-format
msgctxt "app_details__no_versions__none_compatible_with_device"
msgid "No versions compatible with device"
msgstr "சாதனத்துடன் ஒவ்வும் பதிப்புகளில்லை"

#, java-printf-format
msgctxt "app_details__no_versions__explain_incompatible_signers"
msgid ""
"The installed version is not compatible with any available versions. "
"Uninstalling the app will enable you to view and install compatible "
"versions. This often occurs with apps installed via Google Play or other "
"sources, if they are signed by a different certificate."
msgstr ""
"நிறுவப்பட்ட பதிப்பு கிடைக்கக்கூடிய பதிப்புகளுடன் இணக்கமாக இல்லை. பயன்பாட்டை நிறுவல் "
