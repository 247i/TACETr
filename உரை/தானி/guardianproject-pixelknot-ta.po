#, java-printf-format
msgctxt "title_new"
msgid "New"
msgstr "புதிய"

#, java-printf-format
msgctxt "title_albums"
msgid "Albums"
msgstr "ஆல்பம்"

#, java-printf-format
msgctxt "action_done"
msgid "Done"
msgstr "முடிந்தது"

#, java-printf-format
msgctxt "action_albums"
msgid "Show albums"
msgstr "ஆல்பங்களைக் காட்டு"

#, java-printf-format
msgctxt "action_pick_external"
msgid "Pick from external app"
msgstr "வெளிப்புற பயன்பாட்டிலிருந்து தேர்வு"

#, java-printf-format
msgctxt "action_pick_external_album_name"
msgid "Other"
msgstr "மற்றொன்று"

#, java-printf-format
msgctxt "gallery_info_message"
msgid "Tap a photo to start creating a hidden message"
msgstr "மறைக்கப்பட்ட செய்தியை உருவாக்கத் தொடங்க புகைப்படத்தைத் தட்டவும்"

#, java-printf-format
msgctxt "gallery_info_ok"
msgid "Got it"
msgstr "கிடைத்தது"

#, java-printf-format
msgctxt "gallery_need_permission"
msgid ""
"We need your permission to access your photos. Please tap anywhere to allow "
"it."
msgstr ""
"உங்கள் புகைப்படங்களை அணுக எங்களுக்கு உங்கள் இசைவு தேவை. அதை அனுமதிக்க "
"எங்கும் தட்டவும்."

#, java-printf-format
msgctxt "secret_message_hint"
msgid "Write your message…"
msgstr "உங்கள் செய்தியை எழுதுங்கள்…"

#, java-printf-format
msgctxt "secret_password_hint"
msgid "Set a message password"
msgstr "செய்தி கடவுச்சொல்லை அமைக்கவும்"

#, java-printf-format
msgctxt "secret_password_minlength_info"
msgid "Make it at least %d characters."
msgstr "குறைந்தது %d எழுத்துக்களை உருவாக்கவும்."

#, java-printf-format
msgctxt "secret_password_too_short"
msgid "Too short"
msgstr "மிகக் குறுகிய"

#, java-printf-format
msgctxt "password_skip"
msgid "Skip"
msgstr "தவிர்"

#, java-printf-format
msgctxt "password_set"
msgid "Set"
msgstr "கணம்"

#, java-printf-format
msgctxt "confirm_passphrase_override_title"
msgid "Skip password?"
msgstr "கடவுச்சொல்லைத் தவிர்க்கவா?"

#, java-printf-format
msgctxt "confirm_passphrase_override"
msgid ""
"Without protecting your image with a password, your secret message may be "
"discovered if intercepted."
msgstr ""
"கடவுச்சொல் மூலம் உங்கள் படத்தைப் பாதுகாக்காமல், இடைமறிக்கப்பட்டால் உங்கள் "
"ரகசிய செய்தி கண்டுபிடிக்கப்படலாம்."

#, java-printf-format
msgctxt "confirm_passphrase_override_back"
msgid "Go back"
msgstr "திரும்பிச் செல்லுங்கள்"

#, java-printf-format
msgctxt "confirm_passphrase_override_continue"
msgid "Continue"
msgstr "தொடரவும்"

#, java-printf-format
msgctxt "message_encrypting_title"
msgid "Almost done!"
msgstr "கிட்டத்தட்ட முடிந்தது!"

#, java-printf-format
msgctxt "message_encrypting"
msgid ""
"We're hiding your message. This will take us a few minutes. We'll notify you"
" when it's ready to send."
msgstr ""
"நாங்கள் உங்கள் செய்தியை மறைக்கிறோம். இது எங்களுக்கு சில நிமிடங்கள் ஆகும். "
"அனுப்பத் தயாராக இருக்கும்போது நாங்கள் உங்களுக்கு அறிவிப்போம்."

#, java-printf-format
msgctxt "image_sent_title"
msgid "Tell them it's coming!"
msgstr "அது வருகிறது என்று அவர்களிடம் சொல்லுங்கள்!"

#, java-printf-format
msgctxt "image_sent"
msgid ""
"Your recipient must open the image with Pixelknot to read your message."
msgstr ""
"உங்கள் செய்தியைப் படிக்க உங்கள் பெறுநர் பிக்சல்நாட் மூலம் படத்தைத் திறக்க "
"வேண்டும்."

#, java-printf-format
msgctxt "tap_to_send"
msgid "Tap to send"
msgstr "அனுப்ப தட்டவும்"

#, java-printf-format
msgctxt "tap_to_retry"
msgid "Error, tap to retry"
msgstr "பிழை, மீண்டும் முயற்சிக்கவும்"

#, java-printf-format
msgctxt "sending"
msgid "Sending…"
msgstr "அனுப்புகிறது…"

#, java-printf-format
msgctxt "dont_show_again"
msgid "Don't show again."
msgstr "மீண்டும் காட்ட வேண்டாம்."

#, java-printf-format
msgctxt "unsafe_share_title"
msgid "Unsafe share"
msgstr "பாதுகாப்பற்ற பங்கு"

#, java-printf-format
msgctxt "unsafe_share"
msgid "Not good for compressed images, etc etc."
msgstr "சுருக்கப்பட்ட படங்கள் போன்றவற்றுக்கு நல்லதல்ல."

#, java-printf-format
msgctxt "outbox_item_deleted"
msgid "Image deleted"
msgstr "படம் நீக்கப்பட்டது"

#, java-printf-format
msgctxt "outbox_item_deleted_undo"
msgid "Undo"
msgstr "செயல்தவிர்"

#, java-printf-format
msgctxt "outbox_item_created_recently"
msgid "Created less than a minute ago"
msgstr "ஒரு நிமிடத்திற்கு முன்பு உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "outbox_item_created_minutes"
msgid "Created %1$d minutes ago"
msgstr "%1$d நிமிடங்களுக்கு முன்பு உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "outbox_item_created_minute"
msgid "Created %1$d minute ago"
msgstr "%1$d நிமிடத்திற்கு முன்பு உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "outbox_item_created_hours"
msgid "Created %1$d hours ago"
msgstr "%1$d மணி நேரத்திற்கு முன்பு உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "outbox_item_created_hour"
msgid "Created %1$d hour ago"
msgstr "%1$d மணி நேரத்திற்கு முன்பு உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "outbox_item_created_days"
msgid "Created %1$d days ago"
msgstr "%1$d சில நாட்களுக்கு முன்பு உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "outbox_item_created_day"
msgid "Created %1$d day ago"
msgstr "%1$d நாள் முன்பு உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "notification_message_ready"
msgid "Message Ready"
msgstr "செய்தி தயாராக உள்ளது"

#, java-printf-format
msgctxt "notification_messages_ready"
msgid "%d Messages Ready"
msgstr "%d செய்திகள் தயாராக உள்ளன"

