#.  What is \u200C? English strings are used as keys, so they all
#.          need to be unique.  This glyph is non-printing and of
#.          zero-width, so it should do the trick:
#.          http://en.wikipedia.org/wiki/Zero-width_non-joiner 
#.       
#.
#.        ############################################################
#.        # :Screens:
#.        # Main screen (list of games)
#.        #
#.        # This is the first screen you see when you launch CrossWords
#.        ############################################################ 
#.       
#.  Used to format game name plus some other information as the
#.          one-line summary for each game in the main screen.  The name
#.          of the game is substituted for %1$s.  Something else
#.          (language, player names, etc), is substituted for %2$s. This
#.          does not require translation unless the parentheses or
#.          ordering is wrong for your language.
#.
#, java-printf-format
msgctxt "str_game_name_fmt"
msgid "%1$s (%2$s)"
msgstr "%1$s (%2$s)"

#.  Used to format robot player names in the lists of players
#.          found in each game listing 
#.
#, java-printf-format
msgctxt "robot_name_fmt"
msgid "%1$s (robot)"
msgstr "%1$s (பொறியன்)"

#.  When a "Known" player's been invited but hasn't yet
#.          connected, display it's name here so user knows who to
#.          expect. Displayed in the lists of players found in each game
#.          listing.
#.
#, java-printf-format
msgctxt "invitee_fmt"
msgid "(%1$s invited)"
msgstr "(%1$s அழைக்கப்பட்டுள்ளது)"

#.  Used as a substitute for the names of remote players when
#.          they aren't available yet because the connection is not
#.          complete (and when invitee_fmt above can't be used.)
#.          Displayed in the lists of players found in each game
#.          listing. 
#.
#, java-printf-format
msgctxt "missing_player"
msgid "(not here yet…)"
msgstr "(இன்னும் இங்கே இல்லை…)"

#.  The display of each networked game includes one of three
#.          states it can be in in the process of connecting to the
#.          relay.  These next three strings are used to create those
#.          three. 
#.
#, java-printf-format
msgctxt "summary_invites_out"
msgid "Players invited"
msgstr "வீரர்கள் அழைக்கப்பட்டனர்"

#, java-printf-format
msgctxt "summary_invites_out_fmt"
msgid "Players invited to room “%1$s”"
msgstr "அறைக்கு அழைக்கப்பட்ட வீரர்கள் “%1$s”"

#.  Games that have ended are listed with this string 
#.
#, java-printf-format
msgctxt "gameOver"
msgid "Game over"
msgstr "விளையாட்டு முடிந்து"

#.  Otherwise they're listed with this to give some indication of
#.          how far along they are.  I may list "tiles left" someday
#.          instead... 
#.
#, java-printf-format
msgctxt "moves_fmt"
msgid "One move played"
msgid_plural "%1$d moves played"
msgstr[0] "ஒரு நகர்வு விளையாடியது"
msgstr[1] "%1$d நகர்வுகள் விளையாடியது"

#.  When you select the gamel_menu_delete_all menuitem or
#.          list_item_delete contextual menu, you are asked to confirm.
#.          This is the text of the first button ("Cancel" is the
#.          second).  
#.
#, java-printf-format
msgctxt "button_delete"
msgid "Delete"
msgstr "நீக்கு"

#.  Text of button when a game is what's being deleted and it's
#.          not clear from context 
#.
#, java-printf-format
msgctxt "button_delete_game"
msgid "Delete Game"
msgstr "விளையாட்டை நீக்கு"

#.  When you select the list_item_reset contextual menu, you are
#.           asked to confirm.  This is the text of the first button
#.           ("Cancel" is the second).  
#.
#, java-printf-format
msgctxt "button_reset"
msgid "Reset"
msgstr "மீட்டமை"

#.     
#.        ############################################################
#.        # :Menus:
#.        # Main screen (Games list) menu
#.        #
#.        # One menu is not listed here because it appears elsewhere
#.        # and their text is reused: button_new_game.
#.        ############################################################ 
#.       
#.  Brings up the Wordlists (formerly Dictionaries) screen 
#.
#, java-printf-format
msgctxt "gamel_menu_dicts"
msgid "Wordlists"
msgstr "சொல்பட்டியல்கள்"

#.  text of menu that brings up the Settings (preferences) dialog 
#.
#, java-printf-format
msgctxt "menu_prefs"
msgid "App Settings"
msgstr "பயன்பாட்டு அமைப்புகள்"

#.  Brings up "About CrossWords" dialog 
#.
#, java-printf-format
msgctxt "board_menu_file_about"
msgid "About CrossWords"
msgstr "குறுக்குசொற்கள் பற்றி"

#.  
#.        ############################################################
#.        # :Menus:
#.        # Context menu for main games-list screen
#.        ############################################################ 
#.       
#.   ############## menu items ############## 
#.  pulls up dialog to configure the selected game 
#.
#, java-printf-format
msgctxt "list_item_config"
msgid "Game settings"
msgstr "விளையாட்டு அமைப்புகள்"

#.  pulls up dialog to rename (change name of) the selected game 
#.
#, java-printf-format
msgctxt "list_item_rename"
msgid "Rename"
msgstr "மறுபெயரிடு"

#.  pulls up dialog to change the group of the selected game 
#.
#, java-printf-format
msgctxt "list_item_move"
msgid "Move to group"
msgstr "குழுவிற்கு நகர்த்தவும்"

#.  pulls up dialog to delete the selected game 
#.
#, java-printf-format
msgctxt "list_item_delete"
msgid "Delete game"
msgstr "விளையாட்டை நீக்கு"

#.  pulls up dialog to reset the selected game, that is to remove
#.          all moves so that it's the same as a newly created game
#.          except for any configuration. 
#.
#, java-printf-format
msgctxt "list_item_reset"
msgid "Reset‌"
msgstr "மீட்டமை‌"

#.  makes a copy of the selected game.  This is currently
#.          disabled for networked games since there would be problems if
#.          two identically configured games started trying to talk to a
#.          remote game that expected there was only one of them. 
#.
#, java-printf-format
msgctxt "list_item_copy"
msgid "Copy"
msgstr "நகலெடு"

#.  Title of dialog for renaming game (triggered by selecting
#.          list_item_rename) 
#.  long-tap game list menuitem for showing info about game's connectivity 
#.
#, java-printf-format
msgctxt "list_item_netstats"
msgid "Connections"
msgstr "இணைப்புகள்"

#.  Debug-only menuitem that shows mqtt broker's awareness of
#.          devices in game 
#.
#, java-printf-format
msgctxt "list_item_relaypage"
msgid "Game traffic"
msgstr "விளையாட்டு போக்குவரத்து"

#.  Debug only: shown when game-view url copied to clipboard as
#.          part of "show traffic" action 
#.
#, java-printf-format
msgctxt "relaypage_url_copied"
msgid "Game URL copied to clipboard"
msgstr "விளையாட்டு முகவரி இடைநிலைப்பலகைக்கு நகலெடுக்கப்பட்டது"

#.  text within rename dialog (triggered by selecting
#.          list_item_rename) 
#.
#, java-printf-format
msgctxt "rename_label"
msgid "Change the name of this game to:"
msgstr "இந்த விளையாட்டின் பெயரை மாற்று:"

#, java-printf-format
msgctxt "rename_label_caveat"
msgid "Change the name of this game (on this device only) to:"
msgstr "இந்த விளையாட்டின் பெயரை (இந்த சாதனத்தில் மட்டும்) மாற்று:"

#, java-printf-format
msgctxt "confirm_seldeletes_fmt"
msgid ""
"Are you sure you want to delete the selected game? This action cannot be "
"undone."
msgid_plural ""
"Are you sure you want to delete the %1$d selected games? This action cannot "
"be undone."
msgstr[0] ""
"தேர்ந்தெடுக்கப்பட்ட விளையாட்டை நீக்க விரும்புகிறீர்களா? இந்த செயலை செயல்தவிர்க்க முடியாது."
msgstr[1] ""
"தேர்ந்தெடுக்கப்பட்ட %1$d விளையாட்டுகளை நீக்க விரும்புகிறீர்களா? இந்த செயலை செயல்தவிர்க்க "
"முடியாது."

#.  Same as above, but games are named rather than just called
#.          "selected." This may eventually replace the above (after some
#.          time for translation :-) 
#.
#, java-printf-format
msgctxt "confirm_nameddeletes_fmt"
msgid ""
"Are you sure you want to delete the game %1$s? This action cannot be undone."
msgid_plural ""
"Are you sure you want to delete the games %1$s? This action cannot be undone."
msgstr[0] ""
"%1$s விளையாட்டை நீக்க விரும்புகிறீர்களா? இந்த செயலை செயல்தவிர்க்க முடியாது."
msgstr[1] ""
"%1$s விளையாட்டுகளை நீக்க விரும்புகிறீர்களா? இந்த செயலை செயல்தவிர்க்க முடியாது."

#, java-printf-format
msgctxt "confirm_del_after_rematch_fmt"
msgid ""
"Rematch successful.\n"
"\n"
"Do you now want to delete the completed game %1$s? \n"
"\n"
"(This action cannot be undone.)"
msgstr ""
"மீண்டும்விளையாட வெற்றி.\n"
"\n"
" நிறைவு செய்யப்பட்ட விளையாட்டை %1$s ஐ நீக்க விரும்புகிறீர்களா?\n"
"\n"
" (இந்த செயலை செயல்தவிர்க்க முடியாது.)"

#.  Text of confirmation dialog posted when list_item_reset menu
#.          is selected 
#.
#, java-printf-format
msgctxt "confirm_reset_fmt"
msgid ""
"Are you sure you want to reset the selected game?\n"
"\n"
"(Resetting erases all moves and any connection information.)"
msgid_plural ""
"Are you sure you want to reset the %1$d selected games?\n"
"\n"
"(Resetting erases all moves and any connection information.)"
msgstr[0] ""
"தேர்ந்தெடுக்கப்பட்ட விளையாட்டை மீட்டமைக்க விரும்புகிறீர்களா?\n"
"\n"
" (மீட்டமைப்பது அனைத்து நகர்வுகளையும் எந்தவொரு இணைப்பு தகவல்களையும் அழிக்கிறது.)"
msgstr[1] ""
"தேர்ந்தெடுக்கப்பட்ட %1$d விளையாட்டுகளை மீட்டமைக்க விரும்புகிறீர்களா?\n"
"\n"
" (மீட்டமைப்பது அனைத்து நகர்வுகளையும் எந்தவொரு இணைப்பு தகவல்களையும் அழிக்கிறது.)"

#.
#.        ############################################################
#.        # :Screens:
#.        # Wordlists screen 
#.        #
#.        # list of installed wordlists (formerly called dictionaries)
#.        ############################################################ 
#.       
#.  text of item at bottom of dicts choice spinner.  It
#.          launches the browser pointed at the site where additional
#.          wordlists can be found. 
#.
#, java-printf-format
msgctxt "download_more"
msgid "Download more"
msgstr "மேலும் பதிவிறக்கு"

#.  One of the strings used in the right column of the list of
#.          installed wordlists to describe those that are part of
#.          CrossWords and that cannot be uninstalled or moved.
#.          loc_internal and loc_external are the other possible strings
#.          in this column. 
#.
#, java-printf-format
msgctxt "loc_builtin"
msgid "Built-in"
msgstr "உள்ளமைக்கப்பட்ட"

#.  Used to describe wordlists that are in the Downloads
#.          directory.  Currently I don't look there so this is unused,
#.          but I will eventually do so.  This should be the same name as
#.          the built-in Android web browser uses for the directory where
#.          it saves files it downloads. 
#.
#, java-printf-format
msgctxt "loc_downloads"
msgid "Downloads"
msgstr "பதிவிறக்கங்கள்"

#.
#.        ############################################################
#.        # :Menus:
#.        # Wordlists screen menus
#.        ############################################################ 
#.       
#.  title of contextual menu item.  If chosen it brings up a
#.          dialog that offers to move the selected wordlist from its
#.          current storage location to the other.  This menuitem is
#.          hidden if the selected wordlist is built in or if there is
#.          only one possible storage location, e.g. if the device has no
#.          SD card slot. 
#.
#, java-printf-format
msgctxt "dicts_item_move"
msgid "Change storage location"
msgstr "சேமிப்பக இருப்பிடத்தை மாற்று"

#.  Title of contextual menu item.  If chosen it brings up a
#.          dialog that sets the selected dictionary as a new-game
#.          default for robots or human players or both. (See
#.          key_default_dict and key_default_robodict.) 
#.
#, java-printf-format
msgctxt "dicts_item_select"
msgid "Make default"
msgstr "இயல்புநிலையாக செய்"

#.  text of confirmation dialog posted when the delete 'X' button
#.          beside the listing of a wordlist is tapped.  The name of the
#.          wordlist is substituted for %1$s.  Sometimes one of the two
#.          strings below is appended. 
#.
#, java-printf-format
msgctxt "confirm_delete_dict_fmt"
msgid "Are you sure you want to delete the wordlist %1$s?"
msgid_plural "Are you sure you want to delete the wordlists %1$s?"
msgstr[0] "சொல்பட்டியல் %1$s ஐ நீக்க விரும்புகிறீர்களா?"
msgstr[1] "சொல்பட்டியல்கள் %1$s ஐ நீக்க விரும்புகிறீர்களா?"

#.  Additional text appended to text confirm_delete_dictf in the
#.          wordlist delete confiration dialog in the case where the
#.          wordlist to be deleted is the last in its language.  The name
#.          of the language is substituted for %1$s. 
#.
#, java-printf-format
msgctxt "confirm_deleteonly_dicts_fmt"
msgid ""
"Deleting %1$s will leave you without any %2$s wordlists. One or more games "
"will be unopenable (until you download a replacement list.)"
msgstr ""
"%1$s ஐ நீக்குவது எந்த %2$s சொற்கள் இல்லாமல் உங்களை விட்டுச்செல்லும். ஒன்று அல்லது அதற்கு "
"மேற்பட்ட விளையாட்டுகள் திறக்க முடியாததாக இருக்கும் (மாற்று பட்டியலை நீங்கள் பதிவிறக்கும் "
"வரை.)"

#.
#.        ############################################################
#.        # :Dialogs:
#.        # Wordlists screen dialogs
#.        ############################################################ 
#.       
#.  Used as the text fo the confirming/querying dialog that goes
#.          up when the dicts_item_select menuitem is chosen.  The
#.          possible answers are the three button text strings below. 
#.
#, java-printf-format
msgctxt "set_default_message_fmt"
msgid ""
"For what players should the wordlist %1$s be the default for new games? (The "
"language %2$s will be the default for both.)"
msgstr ""
"சொல்பட்டியல் %1$s புதிய விளையாட்டுகளுக்கு இயல்புநிலையாக இருக்க வேண்டும்? (மொழி %2$s "
"இருவருக்கும் இயல்புநிலையாக இருக்கும்.)"

#.  These three strings are the text for three buttons giving
#.          choices in respose to the dialog launched in response to the
#.          dicts_item_select menu (see dicts_item_select above.) 
#.
#, java-printf-format
msgctxt "button_default_human"
msgid "Human"
msgstr "மனித"

#, java-printf-format
msgctxt "button_default_robot"
msgid "Robot"
msgstr "பொறியன்"

#, java-printf-format
msgctxt "button_default_both"
msgid "Both"
msgstr "இரண்டும்"

#.  When the dicts_item_move menu is chosen, this text is used in
#.          the confirmation dialog.  The name of the selected wordlist
#.          is substituted for %1$s.  The strings loc_internal and
#.          loc_internal are substitued for %2$s and %3$s (or vice-versa,
#.          depending on the current location of the wordlist.)
#.
#, java-printf-format
msgctxt "move_dict_fmt"
msgid "Location for wordlist %1$s"
msgstr "சொல் பட்டியல் %1$s க்கான இடம்"

#.  see move_dictf above 
#.
#, java-printf-format
msgctxt "loc_internal"
msgid "Internal"
msgstr "உள்"

#, java-printf-format
msgctxt "loc_external"
msgid "External"
msgstr "வெளிப்புறம்"

#.  
#.        ############################################################
#.        # :Screens:
#.        # Game configure screen 
#.        ############################################################ 
#.       
#.  window title (game name substituted for %1$s) 
#.
#, java-printf-format
msgctxt "title_game_config_fmt"
msgid "Settings for %1$s"
msgstr "%1$s க்கான அமைப்புகள்"

#.  alternate window title used when game is networked 
#.
#, java-printf-format
msgctxt "title_gamenet_config_fmt"
msgid "%1$s settings (networked)"
msgstr "%1$s அமைப்புகள் (பிணையம்)"

#.  title for popup list of langugages from which user picks 
#.
#, java-printf-format
msgctxt "title_langs_list"
msgid "Languages (based on installed wordlists)"
msgstr "மொழிகள் (நிறுவப்பட்ட சொல்பட்டியல்களின் அடிப்படையில்)"

#.  text of checkbox at top of dialog allowing to unlock in-play
#.     game to make changes 
#.
#, java-printf-format
msgctxt "game_locked"
msgid "Lock settings"
msgstr "பூட்டு அமைப்புகள்"

#.  text for separator above the list of players that's used for
#.          non-networked games 
#.
#, java-printf-format
msgctxt "players_label_standalone"
msgid "Players (tap to edit)"
msgstr "வீரர்கள் (திருத்த தட்டவும்)"

#.  text of button for adding new player to game 
#.
#, java-printf-format
msgctxt "button_add_player"
msgid "Add player"
msgstr "வீரரைச் சேர்"

#.  text of button to juggle (randomly rearrange order of) players 
#.
#, java-printf-format
msgctxt "button_juggle_players"
msgid "Shuffle players"
msgstr "கலக்கு வீரர்கள்"

#.  text of separator marking out the language area of the
#.          dialog.  First is used for single-device games, and second
#.          for networked games (where players aren't allowed to have
#.          different wordlists.) 
#.
#, java-printf-format
msgctxt "lang_label"
msgid "Game language"
msgstr "விளையாட்டு மொழி"

#, java-printf-format
msgctxt "langdict_label"
msgid "Game language/wordlist"
msgstr "விளையாட்டு மொழி/சொல்பட்டியல்"

#.  text of separator marking out the connection area of the dialog 
#.  Used as name of missing language, usually when a game's been
#.          created and the wordlist then deleted. 
#.
#, java-printf-format
msgctxt "langUnknownFmt"
msgid "Unknown (%1$s)"
msgstr "தெரியாத (%1$s)"

#, java-printf-format
msgctxt "connect_label_fmt"
msgid "Connection (via %1$s)"
msgstr "இணைப்பு ( %1$s வழியாக)"

#.  text of separator marking out other-setting area of the dialog 
#.
#, java-printf-format
msgctxt "settings_label"
msgid "Other settings"
msgstr "பிற அமைப்புகள்"

#.  text of checkbox.  If this checkbox is checked, buttons will
#.          appear by which the user can get high- and low-scoring moves
#.          generated based on his tray.  Can be used to cheat.  
#.
#, java-printf-format
msgctxt "hints_allowed"
msgid "Allow hints"
msgstr "குறிப்புகளை அனுமதி"

#.  text of checkbox.  If this checkbox is checked, games created
#.          for network play will by default have the hint feature
#.          enabled. 
#.
#, java-printf-format
msgctxt "nethints_allowed"
msgid "Allow hints (networked)"
