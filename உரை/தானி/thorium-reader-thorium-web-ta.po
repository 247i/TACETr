#. This is the text that is displayed when the publication and the reader are being loaded. Animated dots forming an ellipsis are added after the string.
#, i18next-interpolation
msgctxt "reader.app.loading"
msgid "Loading"
msgstr "ஏற்றுகிறது"

#. This is announced by screen reader when navigating into the wrapper component for the publication.
#, i18next-interpolation
msgctxt "reader.app.publicationWrapper"
msgid "You’re now in the publication."
msgstr "நீங்கள் இப்போது வெளியீட்டில் இருக்கிறீர்கள்."

#. Announced by screen readers when navigating into the left docking panel
#, i18next-interpolation
msgctxt "reader.app.docking.dockingLeft"
msgid "Docked panel left"
msgstr "நறுக்கப்பட்ட குழு இடது"

#. Announced by screen readers when navigating into the right docked panel
#, i18next-interpolation
msgctxt "reader.app.docking.dockingRight"
msgid "Docked panel right"
msgstr "நறுக்கப்பட்ட குழு சரி"

#. When the panel is empty and does not contain anything, screen readers will announce this string.
#, i18next-interpolation
msgctxt "reader.app.docking.dockingEmpty"
msgid "Currently empty"
msgstr "தற்போது காலியாக உள்ளது"

#. When the panel has been closed but contains the container of an action, the screen reader will announce this string.
#, i18next-interpolation
msgctxt "reader.app.docking.dockingClosed"
msgid "Is closed, contains {{ action }}"
msgstr "மூடப்பட்டுள்ளது, {{ action }} உள்ளது"

#. When the panel has been collapsed and contains the container for an action, screen readers will announce this string.
#, i18next-interpolation
msgctxt "reader.app.docking.dockingCollapsed"
msgid "Is Collapsed, contains {{ action }}"
msgstr "சரிந்தது, {{ action }} உள்ளது"

#. Screen readers will announce this when navigating into the back link in the top bar.
#, i18next-interpolation
msgctxt "reader.app.header.backLink.trigger"
msgid "Back to the library"
msgstr "மீண்டும் நூலகத்திற்குத் திரும்பவும்"

#. Text that will be displayed in the popup of the backlink when hovering the icon/logo
#, i18next-interpolation
msgctxt "reader.app.header.backLink.tooltip"
msgid "Library"
msgstr "நூலகம்"

#. Screen readers will announce this when navigating into the top bar, containing the running header, action icons, etc.
#, i18next-interpolation
msgctxt "reader.app.header.label"
msgid "Top bar"
msgstr "மேல் பட்டி"

#. Screen readers will announce this when navigating into the running head in the top bar.
#, i18next-interpolation
msgctxt "reader.app.header.runningHead"
msgid "Running head"
msgstr "தலை"

#. If there is no running head, this string will be displayed in the running head component.
#, i18next-interpolation
msgctxt "reader.app.header.runningHeadFallback"
msgid "..."
msgstr "..."

#. Screen readers will announce this when navigating into the actions’ toolbar, containing a button to open user settings, ToC, a toggle for fullscreen, etc.
#, i18next-interpolation
msgctxt "reader.app.header.actions"
msgid "Actions"
msgstr "செயல்கள்"

#. Screen readers will announce this when navigating into the bottom bar, which contains the progression, links to previous and next chapters, etc.
#, i18next-interpolation
msgctxt "reader.app.footer.label"
msgid "Bottom bar"
msgstr "அடிப்பகுதி"

#. Screen readers will announce this when navigating into the current progression component.
#, i18next-interpolation
msgctxt "reader.app.progression.wrapper"
msgid "Current progression"
msgstr "தற்போதைய முன்னேற்றம்"

#. This is a templated string used for the progression e.g. "24 of 121", "25% of current chapter", etc.
#, i18next-interpolation
msgctxt "reader.app.progression.of"
msgid "{{ current }} of {{ reference }}"
msgstr "{{ current }} இன் {{ reference }}"

#. If there is no chapter title available for the progression, this is the fallback that will be displayed.
#, i18next-interpolation
msgctxt "reader.app.progression.referenceFallback"
msgid "current chapter"
msgstr "தற்போதைய அத்தியாயம்"

#. If there is no publication title for the total progression, this string will be used as a fallback.
#, i18next-interpolation
msgctxt "reader.app.progression.pubFallback"
msgid "the publication"
msgstr "வெளியீடு"

#. Screen readers will announce this when navigating into the docking toolbar, which contains docking options for each specific action.
#, i18next-interpolation
msgctxt "reader.app.docker.wrapper"
msgid "Docking options"
msgstr "நறுக்குதல் விருப்பங்கள்"

#. Screen readers will announce this when selecting the button for Docking left option. Or it will displayed if it is a menu option.
#, i18next-interpolation
msgctxt "reader.app.docker.dockToLeft.trigger"
msgid "Dock to the left"
msgstr "இடதுபுறத்தில் கப்பல்துறை"

#. This is the string that is displayed in the tooltip when hovering over the docking left button.
#, i18next-interpolation
msgctxt "reader.app.docker.dockToLeft.tooltip"
msgid "Dock left"
msgstr "கப்பல்துறை இடது"

#. Screen readers will announce this when selecting the button for Docking right option. Or it will be displayed if it is a menu option.
#, i18next-interpolation
msgctxt "reader.app.docker.dockToRight.trigger"
msgid "Dock to the right"
msgstr "வலதுபுறம் கப்பல்துறை"

#. This is the text displayed in the tooltip when hovering hover the dock right button.
#, i18next-interpolation
msgctxt "reader.app.docker.dockToRight.tooltip"
msgid "Dock Right"
msgstr "கப்பல்துறை வலது"

#. Screen readers will announce this when selecting the button for the popover modal option. Or it will be displayed if it is a menu option
#, i18next-interpolation
msgctxt "reader.app.docker.popover.trigger"
msgid "Display in a window"
msgstr "ஒரு சாளரத்தில் காண்பி"

#. This is the text displayed in the tooltip when hovering over the popover Modal button
#, i18next-interpolation
msgctxt "reader.app.docker.popover.tooltip"
msgid "Pop over"
msgstr "பாப் ஓவர்"

#. Screen readers will announce this when selecting the button for fullscreen option. Or it will be displayed if set as a menu option
#, i18next-interpolation
msgctxt "reader.app.docker.fullscreen.trigger"
msgid "Display over contents"
msgstr "உள்ளடக்கங்களுக்கு மேல் காண்பி"

#. This is the text displayed in the tooltip when hovering over the fullscreen button.
#, i18next-interpolation
msgctxt "reader.app.docker.fullscreen.tooltip"
msgid "Overlay"
msgstr "மேலடுக்கு"

#. Screen readers will announce this when selecting the button for Closing the modal, fullscreen, or docked panel.
#, i18next-interpolation
msgctxt "reader.app.docker.close.trigger"
msgid "Close"
msgstr "மூடு"

#. This is the text displayed when hovering over the close button for modal, fullscreen, and docked panels.
#, i18next-interpolation
msgctxt "reader.app.docker.close.tooltip"
msgid "Close"
msgstr "மூடு"

#. This is the text that is displayed for the Back button (in user settings subpanels)
#, i18next-interpolation
msgctxt "reader.app.back.trigger"
msgid "Back"
msgstr "பின்"

#. This is the text that screen reader will announce when navigating into the left arrow in paginated mode. It will also be displayed in a tooltip when hovering over the button.
#, i18next-interpolation
msgctxt "reader.navigation.goForward"
msgid "Go forward"
msgstr "முன்னோக்கிச் செல்லுங்கள்"

#. This is the text that screen reader will announce when navigating into the right arrow in paginated mode. It will also be displayed in a tooltip when hovering over the button.
#, i18next-interpolation
msgctxt "reader.navigation.goBackward"
msgid "Go backward"
msgstr "பின்னோக்கிச் செல்லுங்கள்"

#. Screen readers will announce this when navigating into the scroll affordances in the bottom bar. It contains previous and next links, as well as the current progression.
#, i18next-interpolation
msgctxt "reader.navigation.scroll.wrapper"
msgid "Navigate through the publication"
msgstr "வெளியீடு மூலம் செல்லவும்"

#. This is the label that is displayed on smaller screens to go to the previous chapter/section in scroll mode.
#, i18next-interpolation
msgctxt "reader.navigation.scroll.prevLabel"
msgid "Previous"
msgstr "முந்தைய"

#. This is the label that is displayed on smaller screens to go to the next chapter/section in scroll mode.
#, i18next-interpolation
msgctxt "reader.navigation.scroll.nextLabel"
msgid "Next"
msgstr "அடுத்தது"

#. This is text contextualising the link to the previous chapter/section for screen readers user when the title of the chapter is displayed.
#, i18next-interpolation
msgctxt "reader.navigation.scroll.prevA11yLabel"
msgid "Go to the previous resource"
msgstr "முந்தைய வளத்திற்குச் செல்லுங்கள்"

#. This is text contextualising the link to the next chapter/section for screen readers user when the title of the chapter is displayed.
#, i18next-interpolation
msgctxt "reader.navigation.scroll.nextA11yLabel"
msgid "Go to the next resource"
msgstr "அடுத்த வளத்திற்குச் செல்லுங்கள்"

#. This is the text displayed in the tooltip when hovering over the fullscreen action icon button.
#, i18next-interpolation
msgctxt "reader.fullscreen.tooltip"
msgid "Fullscreen"
msgstr "முழு திரை"

#. Screen readers will announce this when navigating into the fullscreen button when you are not in fullscreen. This string is also used in the overflow menu options.
#, i18next-interpolation
msgctxt "reader.fullscreen.trigger"
msgid "Toggle fullscreen mode"
msgstr "முழுத்திரை பயன்முறையை மாற்றவும்"

#. Screen readers will announce this when navigating into the fullscreen button when in fullscreen. It is also used in the overflow menu options.
#, i18next-interpolation
msgctxt "reader.fullscreen.close"
msgid "Exit fullscreen mode"
msgstr "முழுத்திரை பயன்முறையிலிருந்து வெளியேறவும்"

#. This is the text displayed in the tooltip when hovering over the table of contents action button.
#, i18next-interpolation
msgctxt "reader.toc.tooltip"
msgid "Table of contents"
msgstr "உள்ளடக்க அட்டவணை"

#. Screen readers will announce this when navigating into the Table of Contents action button. It will also be displayed in the overflow menu options.
#, i18next-interpolation
msgctxt "reader.toc.trigger"
msgid "Table of contents"
msgstr "உள்ளடக்க அட்டவணை"

#. Screen readers will announce this when navigating into the button to close the table of contents.
#, i18next-interpolation
msgctxt "reader.toc.close"
msgid "Close table of contents"
msgstr "உள்ளடக்கங்களின் அட்டவணை"

#. This is the heading of the table of contents panel.
#, i18next-interpolation
msgctxt "reader.toc.heading"
msgid "Table of contents"
msgstr "உள்ளடக்க அட்டவணை"

#. Screen readers will announce this when navigating into the tree of the table of contents.
#, i18next-interpolation
msgctxt "reader.toc.entries"
msgid "Entries in the table"
msgstr "அட்டவணையில் உள்ளீடுகள்"

#. This string is displayed when there is no table of contents to display.
#, i18next-interpolation
msgctxt "reader.toc.empty"
msgid "The table of contents was not provided for this publication."
msgstr "இந்த வெளியீட்டிற்கு உள்ளடக்க அட்டவணை வழங்கப்படவில்லை."

#. This is the label for the search field of the Table of Contents. Screen readers will use it when navigating into this field.
#, i18next-interpolation
msgctxt "reader.toc.search.label"
msgid "Search"
msgstr "தேடல்"

#. This is the placeholder text displayed inside the search field of the  table of contents.
#, i18next-interpolation
msgctxt "reader.toc.search.placeholder"
msgid "Search"
msgstr "தேடல்"

#. Screen readers will announce this when navigating into the Clear button of the search field in the table of contents.
#, i18next-interpolation
msgctxt "reader.toc.search.clear"
msgid "Clear"
msgstr "தெளிவான"

#. This is the text displayed in the tooltip when hovering over the Jump To Position icon button.
#, i18next-interpolation
msgctxt "reader.jumpToPosition.tooltip"
msgid "Jump to…"
msgstr "குதிக்கவும்…"

#. Screen readers will announce this when navigating into the Jump to Position action button. It will also be displayed in the overflow menu options.
#, i18next-interpolation
msgctxt "reader.jumpToPosition.trigger"
msgid "Jump to position"
msgstr "நிலைக்குச் செல்"

#. Heading of the Jump to Position Panel
#, i18next-interpolation
msgctxt "reader.jumpToPosition.heading"
msgid "Jump to position"
msgstr "நிலைக்குச் செல்"

#. String for the button displayed next to the position number field. This button sends the user to the position they picked.
#, i18next-interpolation
msgctxt "reader.jumpToPosition.go"
msgid "Go"
msgstr "போ"

#. Screen readers will announce this when navigating into the close button of the jump to position panel.
#, i18next-interpolation
msgctxt "reader.jumpToPosition.close"
msgid "Close jump to position"
msgstr "நிலைக்குத் தாவ மூடு"

#. Template text which contextualises the number field for Jump to Position.
#, i18next-interpolation
msgctxt "reader.jumpToPosition.label"
msgid "Select a position from {{ positionStart }} to {{ positionEnd }}:"
msgstr ""
"{{ positionStart }}} முதல் {{ positionEnd }} வரை ஒரு நிலையைத் தேர்ந்தெடுக்கவும்:"

#. String displayed in the tooltip when hovering over the overflow menu icon, where options for different actions e.g. settings, table of contents, fullscreen, etc. can be displayed.
#, i18next-interpolation
msgctxt "reader.overflowMenu.active.tooltip"
msgid "More actions"
msgstr "மேலும் செயல்கள்"

#. Screen readers will announce this when navigating into the button to open the overflow menu.
#, i18next-interpolation
msgctxt "reader.overflowMenu.active.trigger"
msgid "Toggle menu"
msgstr "மெனுவை மாற்றவும்"

#. Text that is displayed in the tooltip when hovering over the overflow menu button.
#, i18next-interpolation
msgctxt "reader.overflowMenu.hint.tooltip"
msgid "Toggle actions"
msgstr "செயல்களை மாற்றவும்"

#. In immersive mode, when action icons are hidden, the overflow menu icon can be used to toggle immersive mode. Screen readers will use this when navigating into this button in immersive mode.
#, i18next-interpolation
msgctxt "reader.overflowMenu.hint.trigger"
msgid "Display actions"
msgstr "செயல்களைக் காண்பி"

#. Text displayed in the tooltip when hovering over the Settings action icon
#, i18next-interpolation
msgctxt "reader.settings.tooltip"
msgid "Settings"
msgstr "அமைப்புகள்"

#. Screen readers will announce this when navigating into the button for the settings menu.
#, i18next-interpolation
msgctxt "reader.settings.trigger"
msgid "Settings menu"
msgstr "அமைப்புகள் பட்டியல்"

#. Screen readers will announce this when navigating into the close button of the settings menu
#, i18next-interpolation
msgctxt "reader.settings.close"
msgid "Close settings menu"
msgstr "அமைப்புகள் மெனுவை மூடு"

#. Heading for the settings menu.
#, i18next-interpolation
msgctxt "reader.settings.heading"
msgid "Display settings"
msgstr "அமைப்புகளைக் காண்பி"

#. Title of the themes setting.
#, i18next-interpolation
msgctxt "reader.settings.themes.title"
msgid "Themes"
msgstr "கருப்பொருள்கள்"

#. Name of the "auto" value in themes, which maps the theme to the Operating System preferred color scheme e.g. dark/light.
#, i18next-interpolation
msgctxt "reader.settings.themes.auto"
msgid "Auto"
msgstr "தானி"

#. Name of light theme
#, i18next-interpolation
msgctxt "reader.settings.themes.light"
msgid "Light"
msgstr "ஒளி"

#. Name of sepia theme
#, i18next-interpolation
msgctxt "reader.settings.themes.sepia"
msgid "Sepia"
msgstr "செபியா"

#. Name of paper theme, which is a darker sepia theme
#, i18next-interpolation
msgctxt "reader.settings.themes.paper"
msgid "Paper"
msgstr "காகிதம்"

#. Name for dark theme
#, i18next-interpolation
msgctxt "reader.settings.themes.dark"
msgid "Dark"
msgstr "இருண்ட"

#. Name for contrast 1 theme, which is yellow text on black.
#, i18next-interpolation
msgctxt "reader.settings.themes.contrast1"
msgid "Contrast 1"
msgstr "மாறுபாடு 1"

#. Name for contrast 2 theme, which is white on dark blue.
#, i18next-interpolation
msgctxt "reader.settings.themes.contrast2"
msgid "Contrast 2"
msgstr "மாறுபாடு 2"

#. Name of contrast 3 theme, which is dark on light green (mint)
#, i18next-interpolation
msgctxt "reader.settings.themes.contrast3"
msgid "Contrast 3"
msgstr "மாறுபாடு 3"

#. Title of the zoom setting for fixed-layout publications e.g. comics, manga.
#, i18next-interpolation
msgctxt "reader.settings.zoom.title"
msgid "Zoom"
msgstr "பெரிதாக்கு"

#. Screen readers will announce this when navigating into the button to increase the zoom level in fixed-layout publications
#, i18next-interpolation
msgctxt "reader.settings.zoom.increase"
msgid "Increase zoom"
msgstr "சூம் அதிகரிக்கவும்"

#. Screen readers will announce this when navigating into the button to decrease the zoom level.
#, i18next-interpolation
msgctxt "reader.settings.zoom.decrease"
msgid "Decrease zoom"
msgstr "சூம் குறைக்கவும்"

#. Title of the font size setting.
#, i18next-interpolation
msgctxt "reader.settings.fontSize.title"
msgid "Font size"
msgstr "எழுத்துரு அளவு"

#. Screen readers will announce this when navigating into the button to increase the font-size.
#, i18next-interpolation
msgctxt "reader.settings.fontSize.increase"
msgid "Increase font size"
msgstr "எழுத்துரு அளவை அதிகரிக்கவும்"

#. Screen readers will announce this when navigating into the button to decrease font-size.
#, i18next-interpolation
msgctxt "reader.settings.fontSize.decrease"
msgid "Decrease font size"
msgstr "எழுத்துரு அளவைக் குறைக்கவும்"

#. Title for the font-family user setting
#, i18next-interpolation
msgctxt "reader.settings.fontFamily.title"
msgid "Font family"
msgstr "எழுத்துரு குடும்பம்"

#. Option for the font provided by the publisher
#, i18next-interpolation
msgctxt "reader.settings.fontFamily.labels.publisher"
msgid "Publisher’s font"
msgstr "வெளியீட்டாளரின் எழுத்துரு"

#. Option for old-style serif font
#, i18next-interpolation
msgctxt "reader.settings.fontFamily.labels.oldStyle"
msgid "Old style serif"
msgstr "பழைய பாணி செரிஃப்"

#. Option for modern serif font
#, i18next-interpolation
msgctxt "reader.settings.fontFamily.labels.modern"
msgid "Modern serif"
msgstr "நவீன செரிஃப்"

#. Option for neutral sans-serif font
#, i18next-interpolation
msgctxt "reader.settings.fontFamily.labels.sans"
msgid "Sans-serif"
msgstr "சான்ச்-சீரிஃப்"

#. Option for humanist sans-serif font
#, i18next-interpolation
msgctxt "reader.settings.fontFamily.labels.humanist"
msgid "Humanist sans-serif"
msgstr "மனிதநேய சான்ச்-செரிஃப்"

#. Option for monospace font
#, i18next-interpolation
msgctxt "reader.settings.fontFamily.labels.monospace"
msgid "Monospace"
msgstr "மினிவேன்"

#. Title of the font weight setting
#, i18next-interpolation
msgctxt "reader.settings.fontWeight.title"
msgid "Font weight"
msgstr "எழுத்துரு எடை"

#. Screen readers will announce this when navigating into the button to increase font weight
#, i18next-interpolation
msgctxt "reader.settings.fontWeight.increase"
msgid "Increase font weight"
msgstr "எழுத்துரு எடையை அதிகரிக்கவும்"

#. Screen readers will announce this when navigating into the button to decrease font weight
#, i18next-interpolation
msgctxt "reader.settings.fontWeight.decrease"
msgid "Decrease font weight"
msgstr "எழுத்துரு எடையைக் குறைக்கவும்"

#. Title for the text normalization setting. This setting remove styling such as bold, italics, small capitals, to make text easier to read to dyslexic users for instance.
#, i18next-interpolation
msgctxt "reader.settings.normalizeText.title"
msgid "Text normalization"
msgstr "உரை இயல்பாக்கம்"

#. Label that is used for the text normalization switch
#, i18next-interpolation
msgctxt "reader.settings.normalizeText.label"
msgid "Remove all text formatting"
msgstr "அனைத்து உரை வடிவமைப்பையும் அகற்று"

#. Title of the  text align setting
#, i18next-interpolation
msgctxt "reader.settings.align.title"
msgid "Text alignment"
msgstr "உரை சீரமைப்பு"

#. Option for the default a.k.a. the text alignment the publisher has set.
#, i18next-interpolation
msgctxt "reader.settings.align.publisher"
msgid "Default"
msgstr "இயல்புநிலை"

#. Option to align text on the left a.k.a. ragged right (only available in Left to Right languages)
#, i18next-interpolation
msgctxt "reader.settings.align.left"
msgid "Left"
msgstr "இடது"

#. Option to justify text
#, i18next-interpolation
msgctxt "reader.settings.align.justify"
msgid "Justify"
msgstr "நியாயப்படுத்துங்கள்"

#. Option to align right (only available in Right to Left Languages)
#, i18next-interpolation
msgctxt "reader.settings.align.right"
msgid "Right"
msgstr "வலது"

