#.  Name of Android spell checker service. The app name should not be
#. translated.
#, java-printf-format
msgctxt "spell_checker_service_name"
msgid "HeliBoard Spell Checker"
msgstr "எலிபோர்டு எழுத்துப்பிழை சரிபார்ப்பு"

#.  Title for IME settings screen. The app name should not be translated.
#, java-printf-format
msgctxt "ime_settings"
msgid "HeliBoard Settings"
msgstr "எலிபோர்டு அமைப்புகள்"

#.  Title for the spell checking service settings screen. The app name should
#. not be translated.
#, java-printf-format
msgctxt "android_spell_checker_settings"
msgid "HeliBoard Spell Checker Settings"
msgstr "எலிபோர்டு எழுத்துப்பிழை சரிபார்ப்பு அமைப்புகள்"

#.  Option to provide vibrate/haptic feedback on keypress
#, java-printf-format
msgctxt "vibrate_on_keypress"
msgid "Vibrate on keypress"
msgstr "விசையழுத்தின்போது அதிர்வுரு"

#.  Option to provide vibrate/haptic feedback on keypress even in do not
#. disturb mode
#, java-printf-format
msgctxt "vibrate_in_dnd_mode"
msgid "Vibrate in do not disturb mode"
msgstr "தொந்தரவு செய்யாதே பயன்முறையில் அதிர்வு"

#.  Option to play back sound on keypress in soft keyboard
#, java-printf-format
msgctxt "sound_on_keypress"
msgid "Sound on keypress"
msgstr "விசையழுத்தத்தின்போது ஒலியெழுப்பு"

#.  Option to suggest emojis
#, java-printf-format
msgctxt "suggest_emojis"
msgid "Suggest emojis"
msgstr "ஈமோசிகளைப் பரிந்துரைக்கவும்"

#.  Description for option to suggest emojis
#, java-printf-format
msgctxt "suggest_emojis_summary"
msgid "Use emoji dictionary in normal suggestions"
msgstr "சாதாரண பரிந்துரைகளில் ஈமோசி அகராதியைப் பயன்படுத்தவும்"

#.  Option to perform inline emoji search
#, java-printf-format
msgctxt "inline_emoji_search"
msgid "Inline emoji search"
msgstr "இன்லைன் ஈமோசி தேடல்"

#.  Description for option to perform inline emoji search
#, java-printf-format
msgctxt "inline_emoji_search_summary"
msgid "Suggest only emojis when a word is preceded by a colon"
msgstr ""
"ஒரு வார்த்தைக்கு முன் பெருங்குடல் இருக்கும் போது ஈமோசிகளை மட்டும் "
"பரிந்துரைக்கவும்"

#.  Warning text for when enabling inline emoji search with no emoji
#. dictionary. The %s will be replaced by dictionary_link_text.
#, java-printf-format
msgctxt "emoji_dictionary_required"
msgid ""
"An emoji dictionary is required for this feature to work. Dictionaries in "
".dict format can be downloaded %s."
msgstr ""
"இந்த நற்பொருத்தம் செயல்பட ஈமோசி அகராதி தேவை. .dict வடிவத்தில் உள்ள அகராதிகளை"
" %s பதிவிறக்கம் செய்யலாம்."

#.  Option to show emoji descriptions on long press
#, java-printf-format
msgctxt "show_emoji_descriptions"
msgid "Show emoji description on long press"
msgstr "நீண்ட நேரம் அழுத்தும்போது ஈமோசி விளக்கத்தைக் காட்டு"

#.  Option to control whether or not to show a popup with a larger font on
#. each key press.
#, java-printf-format
msgctxt "popup_on_keypress"
msgid "Popup on keypress"
msgstr "விழை அழுத்தத்தின்போது பாப்அப் செய்"

#.  Settings screen title for preferences
#, java-printf-format
msgctxt "settings_screen_preferences"
msgid "Preferences"
msgstr "விருப்பங்கள்"

#.  Settings screen title for gesture typing preferences
#, java-printf-format
msgctxt "settings_screen_gesture"
msgid "Gesture Typing"
msgstr "ஸ்வைப் தட்டச்சு"

#.  Settings screen title for text correction options
#, java-printf-format
msgctxt "settings_screen_correction"
msgid "Text correction"
msgstr "உரை திருத்தம்"

#.  Settings screen title for advanced settings
#, java-printf-format
msgctxt "settings_screen_advanced"
msgid "Advanced"
msgstr "மேம்பட்டவை"

#.  Settings screen title for keyboard theme settings
#, java-printf-format
msgctxt "settings_screen_theme"
msgid "Theme"
msgstr "தீம்"

#.  Settings category title for Preference/Input settings
#, java-printf-format
msgctxt "settings_category_input"
msgid "Input"
msgstr "உள்ளீடு"

#.  Settings category title for Preference/Additional keys settings
#, java-printf-format
msgctxt "settings_category_additional_keys"
msgid "Additional keys"
msgstr "கூடுதல் விசைகள்"

#.  Settings category title for Preference/Additional keys settings
#, java-printf-format
msgctxt "settings_category_clipboard_history"
msgid "Clipboard history"
msgstr "இடைநிலைப்பலகை வரலாறு"

#.  Settings category title for Text correction/Corrections
#, java-printf-format
msgctxt "settings_category_correction"
msgid "Corrections"
msgstr "திருத்தங்கள்"

#.  Settings category title for Text correction/Space
#, java-printf-format
msgctxt "settings_category_space"
msgid "Space"
msgstr "இடைவெளி"

#.  Settings category title for Text correction/Suggestions
#, java-printf-format
msgctxt "settings_category_suggestions"
msgid "Suggestions"
msgstr "பரிந்துரைகள்"

#.  Settings category title for Advanced/Experimental
#, java-printf-format
msgctxt "settings_category_experimental"
msgid "Experimental"
msgstr "ஆய்வு"

#.  Settings category title for Appearance & Layout/Misc
#, java-printf-format
msgctxt "settings_category_miscellaneous"
msgid "Miscellaneous"
msgstr "மற்றவை"

#.   Option for enabling or disabling the split keyboard layout in portrait
#. mode.
#, java-printf-format
msgctxt "enable_split_keyboard"
msgid "Enable split keyboard"
msgstr "பிரிக்கப்பட்ட விசைப்பலகையை இயக்கு"

#.   Option for enabling or disabling the split keyboard layout in landscape
#. mode.
#, java-printf-format
msgctxt "enable_split_keyboard_landscape"
msgid "Enable split keyboard (landscape)"
msgstr "பிளவு விசைப்பலகையை இயக்கு (இயற்கை)"

#.   Option for setting distance for split keyboard in portrait mode
#, java-printf-format
msgctxt "split_spacer_scale"
msgid "Split distance"
msgstr "பிளவு தூரம்"

#.  Option name for including other IMEs in the language key switch list
#, java-printf-format
msgctxt "language_switch_key_switch_input_method"
msgid "Switch to other input methods"
msgstr "பிற உள்ளீட்டு முறைகளுக்கு மாறு"

#.  Option name for switching language / subtype only
#, java-printf-format
msgctxt "switch_language"
msgid "Switch language"
msgstr "மொழியை மாற்றவும்"

#.  Option name for switching language / subtype and input method
#, java-printf-format
msgctxt "language_switch_key_switch_both"
msgid "Switch both"
msgstr "இரண்டையும் மாற்றவும்"

#.  Option to show language switch key
#, java-printf-format
msgctxt "show_language_switch_key"
msgid "Language switch key"
msgstr "மொழி மாற்ற விசை"

#.  Option for setting whether language switch key should switch internal
#. layouts only, or also to other keyboards
#, java-printf-format
msgctxt "language_switch_key_behavior"
msgid "Language switch key behavior"
msgstr "மொழி மாறுதல் முக்கிய நடத்தை"

#.  Option to show shortcut key to emoji palettes
#, java-printf-format
msgctxt "show_emoji_key"
msgid "Emoji key"
msgstr "ஈமோசி திறவுகோல்"

#.  Units abbreviation for the duration (milliseconds)
#, java-printf-format
msgctxt "abbreviation_unit_milliseconds"
msgid "%s ms"
msgstr "%sமி.வி."

#.  Units abbreviation for the duration (minutes)
#, java-printf-format
msgctxt "abbreviation_unit_minutes"
msgid "%s min"
msgstr "%s மணித்துளி"

#.  The text that represents the current settings value is the system default
#, java-printf-format
msgctxt "settings_system_default"
msgid "System default"
msgstr "அமைப்பின் இயல்புநிலை"

#.  The text that represents an unlimited delay
#, java-printf-format
msgctxt "settings_no_limit"
msgid "No limit"
msgstr "வரம்பு இல்லை"

#.  Option name for enabling or disabling the use of names of people in
#. Contacts for suggestion and correction
#, java-printf-format
msgctxt "use_contacts_dict"
msgid "Look up contact names"
msgstr "தொடர்பு பெயர்களை ஆய்வுசெய்"

#.  Description for option enabling or disabling the use of names of people in
#. Contacts for suggestion and correction
#, java-printf-format
msgctxt "use_contacts_dict_summary"
msgid "Use names from Contacts for suggestions and corrections"
msgstr ""
"பரிந்துரைகள், திருத்தங்கள் ஆகியவற்றிற்கு தொடர்புகளிலிருந்து பெயர்களை "
"பயன்படுத்து"

#.  Option name for enabling or disabling the use of installed app names for
#. suggestion and correction
#, java-printf-format
msgctxt "use_apps_dict"
msgid "Look up app names"
msgstr "பயன்பாட்டின் பெயர்களைப் பார்க்கவும்"

#.  Description for option enabling or disabling the use of installed app
#. names for suggestion and correction
#, java-printf-format
msgctxt "use_apps_dict_summary"
msgid "Use names of installed apps for suggestions and corrections"
msgstr ""
"பரிந்துரைகள் மற்றும் திருத்தங்களுக்கு நிறுவப்பட்ட பயன்பாடுகளின் பெயர்களைப் "
"பயன்படுத்தவும்"

#.  Option name for enabling the use by the keyboards of sent/received
#. messages, e-mail and typing history to improve suggestion accuracy
#, java-printf-format
msgctxt "use_personalized_dicts"
msgid "Personalized suggestions"
msgstr "தனிப்பட்ட பரிந்துரைகள்"

#.  Dialog message informing that dictionaries will be deleted on changing the
#. setting
#, java-printf-format
msgctxt "disable_personalized_dicts_message"
msgid "Warning: Disabling this setting will clear learned data"
msgstr "எச்சரிக்கை: இந்த அமைப்பை முடக்கினால், கற்ற தரவு அழிக்கப்படும்"

#.  Option name for adding learned words to personal dictionary
#, java-printf-format
msgctxt "add_to_personal_dictionary"
msgid "Add words to personal dictionary"
msgstr "தனிப்பட்ட அகராதியில் வார்த்தைகளைச் சேர்க்கவும்"

#.  Description for option to add learned words to system-wide user dictionary
#, java-printf-format
msgctxt "add_to_personal_dictionary_summary"
msgid "Use device personal dictionary to store learned words"
msgstr "கற்ற சொற்களைச் சேமிக்க சாதனத்தின் தனிப்பட்ட அகராதியைப் பயன்படுத்தவும்"

#.  Option name for enabling or disabling the double-space period feature that
#. lets double tap on spacebar insert a period followed by a space
#, java-printf-format
msgctxt "use_double_space_period"
msgid "Double-space period"
msgstr "புள்ளியைத் தொடர்ந்து இடைவெளியை உள்ளிடு"

#.  Description for option enabling or disabling the double-space period
#. feature that lets double tap on spacebar insert a period followed by a
#. space
#, java-printf-format
msgctxt "use_double_space_period_summary"
msgid "Double tap on spacebar inserts a period followed by a space"
msgstr ""
"ஸ்பேஸ் பட்டியை இருமுறை தட்டுவது, புள்ளியை தொடர்ந்து இடைவெளியை உள்ளிடும்"

#.  Option to enable auto capitalization of sentences
#, java-printf-format
msgctxt "auto_cap"
msgid "Auto-capitalization"
msgstr "தன்னியக்க பேரெழுத்தாக்கல்"

#.  Description for option to enable auto capitalization of sentences
#, java-printf-format
msgctxt "auto_cap_summary"
msgid "Capitalize the first word of each sentence"
msgstr "வரியின் முதல் எழுத்தை பேரெழுத்தாக்கும்"

#.  Option to edit personal dictionary.
#, java-printf-format
msgctxt "edit_personal_dictionary"
msgid "Personal dictionary"
msgstr "தனிப்பட்ட அகராதி"

#.  Name of the main dictionary, as opposed to auxiliary dictionaries
#. (medical/entertainment/sports...)
#, java-printf-format
msgctxt "main_dictionary"
msgid "Main dictionary"
msgstr "முதன்மை அகராதி"

#.  Option to enable showing suggestions
#, java-printf-format
msgctxt "prefs_show_suggestions"
msgid "Show correction suggestions"
msgstr "திருத்துதல் விருப்பங்களைக் காட்டு"

#.  Description for show suggestions
#, java-printf-format
msgctxt "prefs_show_suggestions_summary"
msgid "Display suggested words while typing"
msgstr "உள்ளிடும்போது பரிந்துரைக்கப்பட்ட வார்த்தைகளைக் காட்டும்"

#.  Option to override app flag to not show suggestions
#, java-printf-format
msgctxt "prefs_always_show_suggestions"
msgid "Always show suggestions"
msgstr "எப்போதும் பரிந்துரைகளைக் காட்டு"

#.  Description for override app flag to not show suggestions
#, java-printf-format
msgctxt "prefs_always_show_suggestions_summary"
msgid "Ignore other apps’ request to disable suggestions (may cause issues)"
msgstr ""
"பரிந்துரைகளை முடக்குவதற்கான பிற பயன்பாடுகளின் கோரிக்கையை புறக்கணிக்கவும் "
"(சிக்கல்களை ஏற்படுத்தலாம்)"

#.  Option to not override the above flags for InputType.WEB_EDIT_TEXT
#, java-printf-format
msgctxt "prefs_always_show_suggestions_except_web_text"
msgid "Don’t always show suggestions for web edit fields"
msgstr "இணையத் திருத்தப் புலங்களுக்கான பரிந்துரைகளை எப்போதும் காட்ட வேண்டாம்"

#.  Description for prefs_always_show_suggestions_except_web_text
#, java-printf-format
msgctxt "prefs_always_show_suggestions_except_web_text_summary"
msgid ""
"Web edit fields (mostly found in browsers) are a very common cause for "
"issues with the always show suggestions setting"
msgstr ""
"எப்போதும் காட்டும் பரிந்துரைகள் அமைப்பில் உள்ள சிக்கல்களுக்கு இணையத் "
"திருத்தப் புலங்கள் (பெரும்பாலும் உலாவிகளில் காணப்படுகின்றன) மிகவும் பொதுவான "
"காரணமாகும்"

#.  Option to block potentially offensive words to be shown
#, java-printf-format
msgctxt "prefs_block_potentially_offensive_title"
msgid "Block offensive words"
msgstr "புண்படுத்தும் சொற்களைத் தடு"

#.  Summary for option to block potentially offensive words to be shown
#, java-printf-format
msgctxt "prefs_block_potentially_offensive_summary"
msgid "Do not suggest potentially offensive words"
msgstr "மனதைப் புண்படுத்தக் கூடிய சொற்களைப் பரிந்துரைக்க வேண்டாம்"

#.  Option to enable auto correction
#, java-printf-format
msgctxt "autocorrect"
msgid "Auto-correction"
msgstr "தன்னியக்க திருத்தம்"

#.  Description for auto correction
#, java-printf-format
msgctxt "auto_correction_summary"
msgid "Spacebar and punctuation automatically correct mistyped words"
msgstr ""
"ஸ்பேஸ்பாரும், நிறுத்தற்குறிகளும் தவறாக உள்ளிடப்பட்ட வார்த்தைகளை தானாக "
"திருத்தும்"

#.  Option to enable auto correction for many fields that do not request auto
#. correction
#, java-printf-format
msgctxt "more_autocorrect"
msgid "More auto-correction"
msgstr "மேலும் தானாக திருத்தம்"

#.  Description for more_autocorrect
#, java-printf-format
msgctxt "more_autocorrect_summary"
msgid "Auto-correct even when not explicitly requested by the input field"
msgstr "உள்ளீட்டு புலத்தால் வெளிப்படையாகக் கோரப்படாதபோதும் தானாகச் சரிசெய்தல்"

#.  Option to change the confidence level of the auto correction
#, java-printf-format
msgctxt "auto_correction_confidence"
msgid "Auto-correction confidence"
msgstr "தானாக திருத்தம் செய்யும் நம்பிக்கை"

#.  Option to enable auto correction for shortcuts (will correct shortcut to
#. the actual word)
#, java-printf-format
msgctxt "auto_correct_shortcuts"
msgid "Auto-correct shortcuts"
msgstr "குறுக்குவழிகளைத் தானாகச் சரிசெய்யவும்"

#.  Description for auto_correct_shortcuts
#, java-printf-format
msgctxt "auto_correct_shortcuts_summary"
msgid "When enabled shortcuts might be expanded by autocorrect"
msgstr ""
"இயக்கப்படும் போது குறுக்குவழிகள் தானியங்கு திருத்தம் மூலம் விரிவாக்கப்படலாம்"

#.  Option to undo auto correction with backspace
#, java-printf-format
msgctxt "backspace_reverts_autocorrect"
msgid "Backspace reverts autocorrect"
msgstr "பேக்ச்பேச் தானாகத் திருத்தத்தை மாற்றுகிறது"

#.  Option to disable auto correction.
#, java-printf-format
msgctxt "auto_correction_threshold_mode_off"
msgid "Off"
msgstr "ஆஃப்"

#.  todo: use it or remove it
#.  Option to suggest auto correction suggestions modestly. Auto-corrects only
#. to a word which has small edit distance from typed word.
#, java-printf-format
msgctxt "auto_correction_threshold_mode_modest"
msgid "Modest"
msgstr "மிதமாக"

#.  Option to suggest auto correction suggestions aggressively. Auto-corrects
#. to a word which has even large edit distance from typed word.
#, java-printf-format
msgctxt "auto_correction_threshold_mode_aggressive"
msgid "Aggressive"
msgstr "தீவிரமாக"

#.  Option to suggest auto correction suggestions very aggressively. Auto-
#. corrects to a word which has even large edit distance from typed word.
#, java-printf-format
msgctxt "auto_correction_threshold_mode_very_aggressive"
msgid "Very aggressive"
msgstr "மிகத் தீவிரமாக"

#.  Option to show the text to be entered as the middle middle suggestion
#, java-printf-format
msgctxt "center_suggestion_text_to_enter"
msgid "Always use middle suggestion"
msgstr "எப்போதும் நடுத்தர பரிந்துரையைப் பயன்படுத்தவும்"

#.  Description for the center text to enter setting
#, java-printf-format
msgctxt "center_suggestion_text_to_enter_summary"
msgid ""
"On pressing space or punctuation, the middle suggestion will be entered"
msgstr ""
