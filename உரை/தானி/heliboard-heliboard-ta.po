msgctxt "key_code"
msgid "Key code"
msgstr "முக்கிய குறியீடு"

#.  Text for showing / setting long press code
#, java-printf-format
msgctxt "long_press_code"
msgid "Long press code"
msgstr "குறியீட்டை நீண்ட நேரம் அழுத்தவும்"

#.  Title of the setting for showing the toolbar automatically
#, java-printf-format
msgctxt "auto_show_toolbar"
msgid "Auto show toolbar"
msgstr "ஆட்டோ சோ கருவிப்பட்டி"

#.  Description of the setting for showing the toolbar automatically
#, java-printf-format
msgctxt "auto_show_toolbar_summary"
msgid "Show the toolbar if input starts or text is selected"
msgstr ""
"உள்ளீடு தொடங்கினால் அல்லது உரை தேர்ந்தெடுக்கப்பட்டால் கருவிப்பட்டியைக் "
"காட்டு"

#.  Title of the setting for hiding the toolbar automatically
#, java-printf-format
msgctxt "auto_hide_toolbar"
msgid "Auto hide toolbar"
msgstr "தானாக மறை கருவிப்பட்டி"

#.  Description of the setting for hiding the toolbar automatically
#, java-printf-format
msgctxt "auto_hide_toolbar_summary"
msgid "Hide the toolbar when suggestions become available"
msgstr "பரிந்துரைகள் கிடைக்கும்போது கருவிப்பட்டியை மறைக்கவும்"

#.  Toast message shown when content is copied to the clipboard
#, java-printf-format
msgctxt "toast_msg_clipboard_copy"
msgid "Content copied"
msgstr "உள்ளடக்கம் நகலெடுக்கப்பட்டது"

#.  Title of the setting to customize icons for keyboard and toolbar keys
#, java-printf-format
msgctxt "customize_icons"
msgid "Customize icons"
msgstr "ஐகான்களைத் தனிப்பயனாக்கு"

#.  Confirmation message when resetting all custom icons
#, java-printf-format
msgctxt "customize_icons_reset_message"
msgid "Really reset all customized icons?"
msgstr "அனைத்து தனிப்பயனாக்கப்பட்ட ஐகான்களையும் உண்மையில் மீட்டமைக்கவா?"

#.  Confirmation message when deleting a something (currently used for custom
#. colors)
#, java-printf-format
msgctxt "delete_confirmation"
msgid "Really delete %s?"
msgstr "உண்மையில் %s ஐ நீக்கவா?"

#.  Message when chosen name is invalid (empty or already taken)
#, java-printf-format
msgctxt "name_invalid"
msgid "Invalid name"
msgstr "தவறான பெயர்"

#.  Description indicating a subtype in Locales & Layouts has customizations
#, java-printf-format
msgctxt "custom_subtype"
msgid "Custom subtype"
msgstr "தனிப்பயன் துணை வகை"

#.  Name for landscape orientation, used in various settings
#, java-printf-format
msgctxt "landscape"
msgid "Landscape"
msgstr "நிலப்பரப்பு"
