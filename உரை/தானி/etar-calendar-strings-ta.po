#.  This is the app label used for alert notifications
#, java-printf-format
msgctxt "standalone_app_label"
msgid "Etar"
msgstr "போதுமானது"

#.  This is the label underneath the icon for Calendar
#, java-printf-format
msgctxt "app_label"
msgid "Calendar"
msgstr "நாட்காட்டி"

#.  This is the label for the time of an event [CHAR LIMIT=30]
#, java-printf-format
msgctxt "when_label"
msgid "When"
msgstr "எப்போது"

#.  Text to show when an event starts on the current day
#, java-printf-format
msgctxt "today"
msgid "Today"
msgstr "இன்று"

#.  Text to show when an event starts on the next day
#, java-printf-format
msgctxt "tomorrow"
msgid "Tomorrow"
msgstr "நாளை"

#.  Text for an event starting on the current day with a start and end time.
#.          For ex, "Today at 5:00pm-6:00pm" [CHAR LIMIT=NONE]
#, java-printf-format
msgctxt "today_at_time_fmt"
msgid "Today at<xliff:g id=\"time_interval\">%s</xliff:g>"
msgstr "இன்று <xliff இல்: g ஐடி = \"time_interval\">%s </xliff: g>"

#.  Text for an event starting on the next day with a start and end time.
#.          For ex, "Tomorrow at 5:00pm-6:00pm" [CHAR LIMIT=NONE]
#, java-printf-format
msgctxt "tomorrow_at_time_fmt"
msgid "Tomorrow at<xliff:g id=\"time_interval\">%s</xliff:g>"
msgstr "நாளை <xliff: g ஐடி = \"time_interval\">%s </xliff: g>"

#.  Format string for a date and time description.  For ex:
#.          "April 19, 2012, 3:00pm - 4:00pm" [CHAR LIMIT=NONE]
#, java-printf-format
msgctxt "date_time_fmt"
msgid ""
"<xliff:g id=\"date\">%1$s</xliff:g>, <xliff:g "
"id=\"time_interval\">%2$s</xliff:g>"
msgstr ""
"<xliff: g ஐடி = \"தேதி\">%1$s </xliff: g>, <xliff: g ஐடி = "
"\"time_interval\">%2$s </xliff: g>"

#.  Title of event when no explicit title is specified by the user
#, java-printf-format
msgctxt "no_title_label"
msgid "(No title)"
msgstr "(தலைப்பு இல்லை)"

#.  Reminder format strings
#, java-printf-format
msgctxt "no_reminder_label"
msgid "None"
msgstr "எதுவுமில்லை"

#, java-printf-format
msgctxt "no_snooze_label"
msgid "None"
msgstr "எதுவுமில்லை"

#, java-printf-format
msgctxt "Nminutes"
msgid "1 minute"
msgid_plural "<xliff:g id=\"count\">%d</xliff:g> minutes"
msgstr[0] "1 மணித்துளி"
msgstr[1] "<xliff: g ஐடி = \"count\">%d </xliff: g> நிமிடங்கள்"

#.  We use the abbreviation "mins" instead of "minutes" to keep the string
#. short.
#.          This is the number of minutes displayed for a calendar reminder.
#. For example,
#.          a reminder of 10 minutes would be displayed as '10 mins'.  The
#. translation
#.          should use the shortest acceptable abbreviation to save space.
#, java-printf-format
msgctxt "Nmins"
msgid "1 min"
msgid_plural "<xliff:g id=\"count\">%d</xliff:g> mins"
msgstr[0] "1 மணித்துளி"
msgstr[1] "<xliff: g ஐடி = \"count\">%d </xliff: g> நிமிடங்கள்"

#.  This is the number of hours displayed for a calendar reminder.  For
#. example,
#.          a reminder of 1 hour would be displayed as '1 hour'.  The
#. translation
#.          should use the shortest acceptable abbreviation of 'hour' to save
#. space.
#, java-printf-format
msgctxt "Nhours"
msgid "1 hour"
msgid_plural "<xliff:g id=\"count\">%d</xliff:g> hours"
msgstr[0] "1 மணி நேரம்"
msgstr[1] "<xliff: g ஐடி = \"count\">%d </xliff: g> மணிநேரம்"

#.  This is the number of days displayed for a calendar reminder.  For
#. example,
#.          a reminder of 2 days would be displayed as '2 days'.  The
#. translation
#.          should use the shortest acceptable abbreviation of 'day' to save
#. space.
#, java-printf-format
msgctxt "Ndays"
msgid "1 day"
msgid_plural "<xliff:g id=\"count\">%d</xliff:g> days"
msgstr[0] "1 நாள்"
msgstr[1] "<xliff: g ஐடி = \"count\">%d </xliff: g> நாட்கள்"

#.  This is the number of weeks displayed for a calendar reminder.  For
#. example,
#.          a reminder of 2 weeks would be displayed as '2 weeks'.  The
#. translation
#.          should use the shortest acceptable abbreviation of 'week' to save
#. space.
#, java-printf-format
msgctxt "Nweeks"
msgid "1 week"
msgid_plural "<xliff:g id=\"count\">%d</xliff:g> weeks"
msgstr[0] "1 வாரம்"
msgstr[1] "<xliff: g ஐடி = \"count\">%d </xliff: g> வாரங்கள்"

#.  This is for displaying the week of the year near the date. Eg. "January,
#. 2011 Week 1"
#. Used to display the calendar week.  Please only translate "week", the
#. syntax behind it must remain the same. The singular form must be the same
#. as the plural form!
#, java-printf-format
msgctxt "weekN"
msgid ""
msgid_plural "Week <xliff:g id=\"count\">%d</xliff:g>"
msgstr[0] ""
msgstr[1] "வாரம் <xliff: g ஐடி = \"count\">%d </xliff: g>"

