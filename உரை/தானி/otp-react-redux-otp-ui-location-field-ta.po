#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->beginTypingPrompt"
msgid "Begin typing to search for locations"
msgstr "இருப்பிடங்களைத் தேட தட்டச்சு செய்யத் தொடங்குங்கள்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->clearLocation"
msgid "Clear location"
msgstr "தெளிவான இடம்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->currentLocationUnavailable"
msgid "Current location not available"
msgstr "தற்போதைய இடம் கிடைக்கவில்லை"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->fetchingLocation"
msgid "Fetching location…"
msgstr "இருப்பிடத்தைப் பெறுதல்…"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->fetchingSuggestions"
msgid "Fetching suggestions…"
msgstr "பரிந்துரைகளைப் பெறுதல்…"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->geocoderUnreachable"
msgid "Unable to obtain suggestions"
msgstr "பரிந்துரைகளைப் பெற முடியவில்லை"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->homeLocation"
msgid "Home"
msgstr "வீடு"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->howToAccessResults"
msgid ""
"Use the down arrow key to browse the results list. To select a result, use "
"the Enter key."
msgstr ""
"முடிவுகள் பட்டியலை உலவ பேரூர் அம்பு விசையைப் பயன்படுத்தவும். ஒரு முடிவைத் "
"தேர்ந்தெடுக்க, ENTER விசையைப் பயன்படுத்தவும்."

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->myPlaces"
msgid "My Places"
msgstr "என் இடங்கள்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->nearby"
msgid "Nearby Stops"
msgstr "அருகிலுள்ள நிறுத்தங்கள்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->noResults"
msgid "No results"
msgstr "முடிவுகள் இல்லை"

#. This is an implicit plural (as in "Other results").
#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->other"
msgid "Other"
msgstr "மற்றொன்று"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->otherCount"
msgid "{count, plural, =1 {# other point} other {# other points}} of interest"
msgstr "{எண்ணிக்கை, பன்மை, = 1 {# பிற புள்ளி} பிற {# பிற புள்ளிகள்} the வட்டி"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->parenthesisFormat"
msgid "{main} ({detail})"
msgstr "{main} ({detail})"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->recentlySearched"
msgid "Recently Searched"
msgstr "அண்மைக் காலத்தில் தேடப்பட்டது"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->resultsFound"
msgid "{results} found for \"{input}\"."
msgstr "{results} \"{input}\" க்கு காணப்படுகிறது."

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->stationCount"
msgid "{count, plural, =1 {# station} other {# stations}}"
msgstr "{எண்ணிக்கை, பன்மை, = 1 {# நிலையம்} பிற {# நிலையங்கள்}}"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->stations"
msgid "Stations"
msgstr "நிலையங்கள்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->stopCount"
msgid "{count, plural, =1 {# stop} other {# stops}}"
msgstr "{எண்ணிக்கை, பன்மை, = 1 {# நிறுத்து} பிற {# நிறுத்தங்கள்}}"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->stops"
msgid "Stops"
msgstr "நிறுத்தங்கள்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->suggestedLocations"
msgid "Suggested locations"
msgstr "பரிந்துரைக்கப்பட்ட இடங்கள்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->suggestedLocationsLong"
msgid "Toggle displaying the list of suggested locations"
msgstr "பரிந்துரைக்கப்பட்ட இடங்களின் பட்டியலைக் காண்பிப்பதை மாற்றவும்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->useCurrentLocation"
msgid "Use Current Location"
msgstr "தற்போதைய இருப்பிடத்தைப் பயன்படுத்தவும்"

#, placeholders:r"\{\w+\}"
msgctxt "otpUi->LocationField->workLocation"
msgid "Work"
msgstr "வேலை"
