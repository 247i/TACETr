msgstr "/etc/default/slapd: N."

#. type: Content of:
#. <article><section><section><section><itemizedlist><listitem><itemizedlist><listitem><para>
#, xml-text
msgid "/etc/cups/cups-files.conf: Choose N."
msgstr "/etc/cups/cups-files.conf: N."

#. type: Content of:
#. <article><section><section><section><itemizedlist><listitem><itemizedlist><listitem><para>
#, xml-text
