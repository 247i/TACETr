#. Can be translated if there's a right word for "just" in the language.
#. "Just" should ideally cover the meaning of "simply", "only" and especially
#. "just" when used in "just do it".
#. It should also look "good" in launcher - not too long.
#, java-printf-format, read-only
msgctxt "app_name"
msgid "Just Player"
msgstr "வெறும் வீரர்"

#, java-printf-format
msgctxt "video_resize_fit"
msgid "Fit"
msgstr "பொருத்தம்"

#, java-printf-format
msgctxt "video_resize_crop"
msgid "Crop"
msgstr "பயிர்"

#, java-printf-format
msgctxt "video_orientation_video"
msgid "Video orientation"
msgstr "வீடியோ நோக்குநிலை"

#, java-printf-format
msgctxt "video_orientation_sensor"
msgid "Auto-rotate"
msgstr "தானாக சுழற்று"

#, java-printf-format
msgctxt "video_orientation_system"
msgid "Device orientation"
msgstr "சாதன நோக்குநிலை"

#, java-printf-format
msgctxt "onboarding_open_title"
msgid "Choose a video to play"
msgstr "விளையாட ஒரு வீடியோவைத் தேர்ந்தெடுக்கவும்"

#, java-printf-format
msgctxt "onboarding_open_description"
msgid "Tap to open a video file. Long tap to load external subtitles."
msgstr ""
"வீடியோ கோப்பைத் திறக்க தட்டவும். வெளிப்புற வசனங்களை ஏற்ற நீண்ட நேரம் "
"தட்டவும்."

#, java-printf-format
msgctxt "open_subtitles"
msgid "Load subtitles"
msgstr "வசன வரிகளை ஏற்றவும்"

#, java-printf-format
msgctxt "error_details"
msgid "Details"
msgstr "விவரங்கள்"

#. "Files" is a name of a Google app and should match "files_label" from
#. https://android.googlesource.com/platform/packages/apps/DocumentsUI/+/refs/heads/master/res/values/strings.xml
#, java-printf-format
msgctxt "error_files_missing"
msgid "Files app is disabled or missing"
msgstr "கோப்புகள் பயன்பாடு முடக்கப்பட்டுள்ளது அல்லது காணவில்லை"

#. "Movies" is a name of a default system folder for storing video files and
#. always available under this name on file system. Just Player uses it as the
#. default selection so it should not be translated.
#. https://android.googlesource.com/platform/frameworks/base/+/refs/tags/android-11.0.0_r45/core/java/android/os/Environment.java#767
#, java-printf-format
msgctxt "request_scope"
msgid ""
"To automatically load external subtitles or enable skipping to next file in "
"a directory, grant %s access to the topmost folder containing your videos "
"(e.g. Movies)."
msgstr ""
"வெளிப்புற வசனங்களைத் தானாக ஏற்ற அல்லது கோப்பகத்தில் அடுத்த கோப்பிற்குச் "
"செல்வதை இயக்க, உங்கள் வீடியோக்கள் (எ.கா. திரைப்படங்கள்) உள்ள மேல்மட்ட "
"கோப்புறைக்கு %s அணுகலை வழங்கவும்."

#, java-printf-format
msgctxt "delete_query"
msgid "Delete this file?"
msgstr "இந்தக் கோப்பை நீக்கவா?"

#, java-printf-format
msgctxt "delete_confirmation"
msgid "Delete"
msgstr "அழி"

#, java-printf-format
msgctxt "pref_title"
msgid "Settings"
msgstr "அமைப்புகள்"

#, java-printf-format
msgctxt "pref_general_header"
msgid "General"
msgstr "பொது"

#, java-printf-format
msgctxt "pref_framerate_matching"
msgid "Auto frame rate matching"
msgstr "ஆட்டோ பிரேம் வீத பொருத்தம்"

#, java-printf-format
msgctxt "pref_framerate_matching_on"
msgid "Switch display refresh rate to match video frame rate"
msgstr ""
"வீடியோ பிரேம் வீதத்துடன் பொருந்த காட்சி புதுப்பிப்பு வீதத்தை மாற்றவும்"

#, java-printf-format
msgctxt "pref_framerate_matching_off"
msgid "Do not change display refresh rate"
msgstr "காட்சி புதுப்பிப்பு வீதத்தை மாற்ற வேண்டாம்"

#, java-printf-format
msgctxt "pref_tunneling"
msgid "Tunneled playback"
msgstr "சுரங்கப்பாதை பின்னணி"

#, ignore-multiple-failures, java-printf-format
msgctxt "pref_tunneling_summary"
msgid ""
"Tunneling provides better support for 4K/HDR content, but may not work on "
"all devices"
msgstr ""
"சுரங்கப்பாதை 4K/HDR உள்ளடக்கத்திற்கு சிறந்த ஆதரவை வழங்குகிறது, ஆனால் எல்லா "
"சாதனங்களிலும் வேலை செய்யாது"

#, java-printf-format
msgctxt "pref_auto_pip"
msgid "Auto picture-in-picture"
msgstr "ஆட்டோ பிக்சர்-இன்-பிக்சர்"

#, java-printf-format
msgctxt "pref_auto_pip_on"
msgid "Switch to PiP when leaving the app"
msgstr "பயன்பாட்டை விட்டு வெளியேறும்போது PiPக்கு மாறவும்"

#, java-printf-format
msgctxt "pref_auto_pip_off"
msgid "Do not automatically switch to PiP"
msgstr "தானாகவே PiP க்கு மாற வேண்டாம்"

#, java-printf-format
msgctxt "pref_skip_silence"
msgid "Skip silence"
msgstr "மௌனத்தைத் தவிர்க்கவும்"

#, java-printf-format
msgctxt "pref_skip_silence_on"
msgid "Skip parts with silence on the audio stream"
msgstr "ஆடியோ ச்ட்ரீமில் ம silence னத்துடன் பகுதிகளைத் தவிர்க்கவும்"

#, java-printf-format
msgctxt "pref_skip_silence_off"
msgid "Play content as is"
msgstr "உள்ளடக்கத்தை அப்படியே இயக்கவும்"

#, java-printf-format
msgctxt "pref_repeat_toggle"
msgid "Repeat toggle"
msgstr "மாற்றவும்"

#, java-printf-format
msgctxt "pref_repeat_toggle_summary"
msgid "Extra control to allow indefinitely loop video (requires app restart)"
msgstr ""
"காலவரையின்றி லூப் வீடியோவை அனுமதிக்க கூடுதல் கட்டுப்பாடு (பயன்பாட்டை "
"மறுதொடக்கம் தேவை)"

#, java-printf-format
msgctxt "pref_map_dv7"
msgid "Dolby Vision profile 7 fallback"
msgstr "டால்பி பார்வை சுயவிவரம் 7 குறைவானது"

#, java-printf-format
msgctxt "pref_map_dv7_on"
msgid "Play Dolby Vision profile 7 content on HDR displays"
msgstr ""
"எச்.டி.ஆர் காட்சிகளில் டால்பி பார்வை சுயவிவரம் 7 உள்ளடக்கத்தை இயக்கவும்"

#, java-printf-format
msgctxt "pref_map_dv7_off"
msgid "Play UHD Blu-ray content containing Dolby Vision as is"
msgstr "டால்பி பார்வை கொண்ட UHD ப்ளூ-ரே உள்ளடக்கத்தை விளையாடுங்கள்"

#, java-printf-format
msgctxt "shortcut_videos"
msgid "Videos"
msgstr "வீடியோக்கள்"

#, java-printf-format
msgctxt "button_open"
msgid "Open"
msgstr "திற"

#, java-printf-format
msgctxt "button_crop"
msgid "Resize"
msgstr "அளவை மாற்றவும்"

#, java-printf-format
msgctxt "button_rotate"
msgid "Rotate"
msgstr "சுழற்று"

#, java-printf-format
msgctxt "button_pip"
msgid "Picture-in-picture"
msgstr "படத்தில்-படம்"

#, java-printf-format
msgctxt "pref_file_access"
msgid "File access"
msgstr "கோப்பு அணுகல்"

#, java-printf-format
msgctxt "pref_file_access_auto"
msgid "Auto"
msgstr "தானி"

#, java-printf-format
msgctxt "mediastore_empty"
msgid "No video files found"
msgstr "வீடியோ கோப்புகள் எதுவும் கிடைக்கவில்லை"

#, java-printf-format
msgctxt "pref_dangerous_header"
msgid "Dangerous ⚠️"
msgstr "ஆபத்தானது"

#, java-printf-format
msgctxt "pref_dangerous_warning"
msgid ""
"Switching options in this category may break playback of normally compatible"
" content"
msgstr ""
"இந்த பிரிவில் விருப்பங்களை மாற்றுவது பொதுவாக இணக்கமான உள்ளடக்கத்தின் "
"பின்னணியை உடைக்கக்கூடும்"

#, java-printf-format
msgctxt "pref_shortcuts_header"
msgid "Shortcuts"
msgstr "குறுக்குவழிகள்"

#, java-printf-format
msgctxt "pref_captioning_preferences"
msgid "Captioning preferences"
msgstr "தலைப்பு விருப்பத்தேர்வுகள்"

#, java-printf-format
msgctxt "pref_decoder_priority"
msgid "Decoder priority"
msgstr "டிகோடர் முன்னுரிமை"

#, java-printf-format
msgctxt "pref_decoder_priority_prefer_device"
msgid "Prefer device decoders (default)"
msgstr "சாதன டிகோடர்களை விரும்புங்கள் (இயல்புநிலை)"

#, java-printf-format
msgctxt "pref_decoder_priority_prefer_app"
msgid "Prefer app decoders"
msgstr "பயன்பாட்டு டிகோடர்களை விரும்புங்கள்"

#, java-printf-format
msgctxt "pref_decoder_priority_only_device"
msgid "Device decoders only"
msgstr "சாதன டிகோடர்கள் மட்டுமே"

#, java-printf-format
msgctxt "pref_language_audio"
msgid "Default audio track"
msgstr "இயல்புநிலை ஆடியோ டிராக்"

#, java-printf-format
msgctxt "pref_language_track_device"
msgid "Device languages"
msgstr "சாதன மொழிகள்"

#, java-printf-format
msgctxt "pref_language_track_default"
msgid "Default"
msgstr "இயல்புநிலை"

#, java-printf-format
msgctxt "pref_subtitle_header"
msgid "Subtitle"
msgstr "வசன வரிகள்"

#, java-printf-format
msgctxt "pref_subtitle_style_embedded"
msgid "Embedded styles"
msgstr "உட்பொதிக்கப்பட்ட பாணிகள்"

#, java-printf-format
msgctxt "pref_subtitle_style_embedded_on"
msgid "Apply embedded subtitle styles"
msgstr "உட்பொதிக்கப்பட்ட வசன பாணிகளைப் பயன்படுத்துங்கள்"

#, java-printf-format
msgctxt "pref_subtitle_style_embedded_off"
msgid "Do not apply embedded subtitle styles"
msgstr "உட்பொதிக்கப்பட்ட வசன பாணிகளைப் பயன்படுத்த வேண்டாம்"

#, java-printf-format
msgctxt "pref_subtitle_style_bold"
msgid "Bold style"
msgstr "தைரியமான நடை"

#, java-printf-format
msgctxt "pref_subtitle_style_bold_on"
msgid "Use bold typeface as regular"
msgstr "தைரியமான தட்டச்சுப்பொறியை வழக்கமானதாகப் பயன்படுத்தவும்"

#, java-printf-format
msgctxt "pref_subtitle_style_bold_off"
msgid "Use default regular typeface"
msgstr "இயல்புநிலை வழக்கமான தட்டச்சு பயன்படுத்தவும்"
