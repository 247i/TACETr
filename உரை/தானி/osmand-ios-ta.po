msgctxt "map_source"
msgid "Map source"
msgstr "வரைபட மூல"

msgctxt "download_srtm_maps"
msgid "Contour lines"
msgstr "சமநிலைக்கோடுகள்"

msgctxt "voices"
msgid "Voice prompts"
msgstr "குரல் தூண்டுகிறது"

msgctxt "temperature_layer"
msgid "Temperature layer"
msgstr "வெப்பநிலை அடுக்கு"

msgctxt "pressure_layer"
msgid "Pressure layer"
msgstr "அழுத்தம் அடுக்கு"

msgctxt "wind_layer"
msgid "Wind layer"
msgstr "காற்று அடுக்கு"

msgctxt "cloud_layer"
msgid "Cloud layer"
msgstr "மேகக்கணி அடுக்கு"

msgctxt "precipitation_layer"
msgid "Precipitation layer"
msgstr "மழைப்பொழிவு அடுக்கு"

msgctxt "app_profile"
msgid "App profile"
msgstr "பயன்பாட்டு சுயவிவரம்"

msgctxt "show_terrain"
msgid "Show Terrain"
msgstr "நிலப்பரப்பைக் காட்டு"

msgctxt "hide_terrain"
msgid "Hide Terrain"
msgstr "நிலப்பரப்பை மறைக்கவும்"

msgctxt "toggle_contour_lines"
msgid "Show/Hide contour lines"
msgstr "விளிம்பு கோடுகளைக் காட்டவும்/மறைக்கவும்"

msgctxt "toggle_hillshade"
msgid "Show/Hide terrain"
msgstr "நிலப்பரப்பைக் காட்டவும்/மறைக்கவும்"

msgctxt "show_from_zoom_level"
msgid "Displaying starts at zoom level"
msgstr "காண்பிக்கும் சூம் மட்டத்தில் தொடங்குகிறது"

msgctxt "quick_action_contour_lines_descr"
msgid "Button showing or hiding contour lines on the map."
msgstr ""
"வரைபடத்தில் விளிம்பு கோடுகளைக் காண்பிக்கும் அல்லது மறைக்கும் பொத்தான்."

msgctxt "hide_contour_lines"
msgid "Hide contour lines"
msgstr "விளிம்பு கோடுகளை மறைக்கவும்"

msgctxt "enable_contour_lines"
msgid "Enable to view contour lines on the map"
msgstr "வரைபடத்தில் விளிம்பு கோடுகளைக் காண இயக்கவும்"

msgctxt "enable_hillshade"
msgid "Enable to view terrain data from hillshade or slope map."
msgstr ""
"இல்சேட் மற்றும் சாய்வு வரைபடத்திலிருந்து நிலப்பரப்பு தரவைக் காண உதவுகிறது."

msgctxt "contour_lines_hillshades_slope"
msgid "Contour lines, hillshades, slope"
msgstr "விளிம்பு கோடுகள், இல்சேட்ச், சாய்வு"

msgctxt "quick_action_dir_from_descr"
msgid "Tap this action to navigate from the current screen center point."
msgstr "தற்போதைய திரை மைய புள்ளியிலிருந்து செல்ல இந்த செயலைத் தட்டவும்."

msgctxt "quick_action_transport_descr"
msgid "Button showing or hiding public transport on the map."
msgstr "வரைபடத்தில் பொது போக்குவரத்தை காண்பிக்கும் அல்லது மறைக்கும் பொத்தான்."

msgctxt "toggle_public_transport"
msgid "Show/hide public transport"
msgstr "பொது போக்குவரத்தைக் காட்டவும்/மறைக்கவும்"

msgctxt "public_transport_show"
msgid "Show public transport"
msgstr "பொது போக்குவரத்தைக் காட்டு"

msgctxt "public_transport_hide"
msgid "Hide public transport"
msgstr "பொது போக்குவரத்தை மறைக்கவும்"

msgctxt "quick_action_showhide_mapillary_title"
msgid "Show/hide Mapillary"
msgstr "மாபிலரி காட்டு/மறைக்க"

msgctxt "quick_action_showhide_mapillary_descr"
msgid "A toggle to show or hide the Mapillary layer on the map."
msgstr "வரைபடத்தில் உள்ள மேபிலரி லேயரைக் காட்ட அல்லது மறைக்க ஒரு மாற்று."

msgctxt "quick_action_mapillary_hide"
msgid "Hide Mapillary"
msgstr "மேபிலரி மறைக்கவும்"

msgctxt "quick_action_mapillary_show"
msgid "Show Mapillary"
msgstr "Mapilary காட்டு"

msgctxt "quick_action_remove_next_destination"
msgid "Next destination point"
msgstr "அடுத்த இலக்கு புள்ளி"

msgctxt "quick_action_remove_next_destination_descr"
msgid ""
"Deletes the next destination on your route. If that is the final "
"destination, the navigation will stop."
msgstr ""
"உங்கள் பாதையில் அடுத்த இலக்கை நீக்குகிறது. அது இறுதி இலக்கு என்றால், "
"வழிசெலுத்தல் நிறுத்தப்படும்."

