#, java-printf-format
msgctxt "service_is_not_available"
msgid "! Service is not available"
msgstr "! பணி கிடைக்கவில்லை"

#, java-printf-format
msgctxt "unable_to_gather_module_info_of_file"
msgid "! Unable to gather module info of file: %1$s"
msgstr "! கோப்பின் தொகுதி தகவலை சேகரிக்க முடியவில்லை: %1$s"

#, java-printf-format
msgctxt ""
"cannot_install_blacklisted_modules_settings_security_blacklist_alerts"
msgid ""
"! Cannot install blacklisted modules. Settings → Security → Blacklist Alerts"
msgstr ""
"! தடுப்புப்பட்டியல் தொகுதிகளை நிறுவ முடியாது. அமைப்புகள் → பாதுகாப்பு → "
"பிளாக்லிச்ட் விழிப்பூட்டல்கள்"

#, java-printf-format
msgctxt "installation_aborted_due_to_an_error"
msgid "! Installation aborted due to an error"
msgstr "! பிழை காரணமாக நிறுவல் கைவிடப்பட்டது"

#, java-printf-format
msgctxt "install_view_path"
msgid "- Path: %1$s"
msgstr "- பாதை: %1$s"

#, java-printf-format
msgctxt "copying_zip_to_temp_directory"
msgid "- Copying zip to temp directory"
msgstr "- தற்காலிக கோப்பகத்திற்கு சிப்பை நகலெடுக்கிறது"

#, java-printf-format
msgctxt "install_view_module_info"
msgid "- Module info: %1$s"
msgstr "- தொகுதி தகவல்: %1$s"

#, java-printf-format
msgctxt "copying_failed"
msgid "! Copying failed"
msgstr "! நகலெடுப்பது தோல்வியுற்றது"

#, java-printf-format
msgctxt "unable_to_gather_module_info"
msgid "! Unable to gather module info"
msgstr "! தொகுதி தகவலை சேகரிக்க முடியவில்லை"

#, java-printf-format
msgctxt "install_view_installing"
msgid "- Installing %1$s"
msgstr "- %1$s ஐ நிறுவுதல்"

#, java-printf-format
msgctxt "module_not_found"
msgid "! Module not found."
msgstr "! தொகுதி கிடைக்கவில்லை."

#, java-printf-format
msgctxt "this_module_don_t_have_an_action"
msgid "! This module don't have an action."
msgstr "! இந்த தொகுதிக்கு ஒரு செயல் இல்லை."

#, java-printf-format
msgctxt "module_is_disabled_or_removed_unable_to_execute_action"
msgid "! Module is disabled or removed. Unable to execute action."
msgstr ""
"! தொகுதி முடக்கப்பட்டுள்ளது அல்லது அகற்றப்பட்டது. செயலை செயல்படுத்த "
"முடியவில்லை."

#, java-printf-format
msgctxt ""
"execution_failed_try_to_use_shell_for_the_action_execution_settings_module_use_shell_for_module_action"
msgid ""
"- Execution failed. Try to use Shell for the Action execution, Settings → "
"Module → Use Shell for Module Action."
msgstr ""
"- சாவுஒறுப்பு தோல்வியடைந்தது. செயல் செயலாக்கத்திற்கு செல் பயன்படுத்த "
"முயற்சிக்கவும், அமைப்புகள் → தொகுதி மோட் தொகுதி செயலுக்கு செல்லைப் "
"பயன்படுத்தவும்."
