#, java-printf-format
msgctxt "app_name"
msgid "Tox Notify"
msgstr "நச்சு அறிவிக்கவும்"

#, java-printf-format
msgctxt "quickstart_message"
msgid ""
"This App's purpose is to receive Push Notifications and forward them to the "
"TRIfA App running on this device."
msgstr ""
"இந்த பயன்பாட்டின் நோக்கம் புச் அறிவிப்புகளைப் பெற்று அவற்றை இந்த சாதனத்தில் "
"இயங்கும் ட்ரிஃபா பயன்பாட்டிற்கு அனுப்புவதாகும்."

#, java-printf-format
msgctxt "log_token"
msgid "Send Token to TRIfA (for forwarding to ToxProxy)"
msgstr "ட்ரிஃபாவுக்கு கிள்ளாக்கை அனுப்பவும் (டோக்ச்பிராக்சிக்கு அனுப்ப)"

#, java-printf-format
msgctxt "msg_token_fmt"
msgid "FCM registration Token: %s"
msgstr "FCM பதிவு டோக்கன்: %s"

#, java-printf-format
msgctxt "default_notification_channel_id"
msgid "fcm_default_channel"
msgstr "fcm_default_channel"

#, java-printf-format
msgctxt "default_notification_channel_name"
msgid "Weather"
msgstr "வானிலை"

#, java-printf-format
msgctxt "msg_subscribe_failed"
msgid "Failed to subscribe to weather topic"
msgstr "வானிலை தலைப்புக்கு குழுசேரத் தவறிவிட்டது"

#, java-printf-format
msgctxt "fcm_message"
msgid "FCM Message"
msgstr "FCM செய்தி"

#, java-printf-format
msgctxt "title_activity_settings"
msgid "SettingsActivity"
msgstr "அமைப்புகள்"

#, java-printf-format
msgctxt "lookandfeel_header"
msgid "Look and Feel"
msgstr "பாருங்கள் மற்றும் உணருங்கள்"

#, java-printf-format
msgctxt "theme"
msgid "Choose theme"
msgstr "கருப்பொருள் தேர்வு செய்யவும்"

#, java-printf-format
msgctxt "show_notifications"
msgid "Show notifications"
msgstr "அறிவிப்புகளைக் காட்டு"

#, java-printf-format
msgctxt "show_notifications_summary"
msgid "for debugging, independent of TRIfA"
msgstr "பிழைத்திருத்தத்திற்காக, ட்ரிஃபாவிலிருந்து சுயாதீனமாக"

#, java-printf-format
msgctxt "prefer_fcm"
msgid "Prefer FCM"
msgstr "FCM ஐ விரும்புங்கள்"

#, java-printf-format
msgctxt "prefer_fcm_summary"
msgid ""
"use Google FCM for push messages if selected. use nfty.sh if not selected"
msgstr ""
"தேர்ந்தெடுக்கப்பட்டால் புச் செய்திகளுக்கு Google FCM ஐப் பயன்படுத்தவும். "
"தேர்ந்தெடுக்கப்படாவிட்டால் nfty.sh ஐப் பயன்படுத்தவும்"
