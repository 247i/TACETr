msgid "High contrast"
msgstr "உயர் வேறுபாடு"

#, icu-message-format
msgctxt "accessibilityLargeTextLabel"
msgid "Large text"
msgstr "பெரிய உரை"

#, icu-message-format
msgctxt "accessibilityReduceAnimationLabel"
msgid "Reduce animation"
msgstr "அனிமேசனைக் குறைக்கவும்"

#, icu-message-format
msgctxt "accessibilityScreenReaderLabel"
msgid "Screen reader"
msgstr "திரை ரீடர்"

#, icu-message-format
msgctxt "accessibilityVisualAlertsLabel"
msgid "Visual alerts"
msgstr "காட்சி எச்சரிக்கைகள்"

