#.  Option name for including other IMEs in the language switch list [CHAR
#. LIMIT=30]
#, java-printf-format
msgctxt "include_other_imes_in_language_switch_list"
msgid "Switch to other input methods"
msgstr "பிற உள்ளீட்டு முறைகளுக்கு மாறு"

#.  Option summary for including other IMEs in the language switch list [CHAR
#. LIMIT=65]
#, java-printf-format
msgctxt "include_other_imes_in_language_switch_list_summary"
msgid "Language switch key covers other input methods too"
msgstr "மொழி மாற்றல் விசை பிற உள்ளீட்டு முறைகளையும் கட்டுப்படுத்துகிறது"

#.  Option to show language switch key [CHAR LIMIT=30]
#, java-printf-format
msgctxt "show_language_switch_key"
msgid "Language switch key"
msgstr "மொழி மாற்ற விசை"

#.  Option summary for showing language switch key [CHAR LIMIT=65]
#, java-printf-format
msgctxt "show_language_switch_key_summary"
msgid "Show when multiple input languages are enabled"
msgstr "பல உள்ளீட்டு மொழிகள் இயக்கப்பட்டிருக்கும்போது காட்டு"

#.  Option to show shortcut key to emoji palettes
#, java-printf-format
msgctxt "show_emoji_key"
msgid "Emoji key"
msgstr "ஈமோசி விசை"

#.  Option for the dismiss delay of the key popup [CHAR LIMIT=25]
#, java-printf-format
msgctxt "key_preview_popup_dismiss_delay"
msgid "Key popup dismiss delay"
msgstr "விசை பாப்அப் விலக்கின் தாமதம்"

#.  Description for delay for dismissing a popup on keypress: no delay [CHAR
#. LIMIT=15]
#, java-printf-format
msgctxt "key_preview_popup_dismiss_no_delay"
msgid "No delay"
msgstr "தாமதம் வேண்டாம்"

#.  Description for delay for dismissing a popup on screen: default value of
#. the delay [CHAR LIMIT=15]
#, java-printf-format
msgctxt "key_preview_popup_dismiss_default_delay"
msgid "Default"
msgstr "இயல்புநிலை"

#.  Units abbreviation for the duration (milliseconds) [CHAR LIMIT=10]
#, java-printf-format
msgctxt "abbreviation_unit_milliseconds"
msgid ""
"<xliff:g xmlns:xliff=\"urn:oasis:names:tc:xliff:document:1.2\" "
"id=\"MILLISECONDS\">%s</xliff:g>ms"
msgstr ""
"<xliff:g xmlns:xliff=\"urn:oasis:names:tc:xliff:document:1.2\" "
"id=\"MILLISECONDS\">%s</xliff:g>மி.வி."

#.  Units abbreviation for the duration (minutes) [CHAR LIMIT=10]
#, java-printf-format
msgctxt "abbreviation_unit_minutes"
msgid ""
"<xliff:g xmlns:xliff=\"urn:oasis:names:tc:xliff:document:1.2\" "
"id=\"MINUTES\">%s</xliff:g>min."
msgstr ""
"<xliff: g xmlns: ஃலிப்ப் = \"urn: oasis: பெயர்கள்: tc: xliff: ஆவணம்: 1.2\" "
"ஐடி = \"நிமிடங்கள்\">%s </xliff: g> min."

#.  The text that represents the current settings value is the system default
#. [CHAR LIMIT=25]
#, java-printf-format
msgctxt "settings_system_default"
msgid "System default"
msgstr "அமைப்பின் இயல்புநிலை"

#.  The text that represents an unlimited delay [CHAR LIMIT=25]
#, java-printf-format
msgctxt "settings_no_limit"
msgid "No limit"
msgstr "வரம்பு இல்லை"

#.  Option name for enabling or disabling the use of names of people in
#. Contacts for suggestion and correction [CHAR LIMIT=25]
#, java-printf-format
msgctxt "use_contacts_dict"
msgid "Suggest Contact names"
msgstr "தொடர்பு பெயர்களைப் பரிந்துரை"

#.  Description for option enabling or disabling the use of names of people in
#. Contacts for suggestion and correction [CHAR LIMIT=65]
#, java-printf-format
msgctxt "use_contacts_dict_summary"
msgid "Use contact names for suggestions and corrections"
msgstr ""
"பரிந்துரைகள் மற்றும் திருத்தங்களுக்கு தொடர்பு பெயர்களைப் பயன்படுத்தவும்"

#.  Option name for enabling the use by the keyboards of sent/received
#. messages, e-mail and typing history to improve suggestion accuracy [CHAR
#. LIMIT=25]
#, java-printf-format
msgctxt "use_personalized_dicts"
msgid "Personalized suggestions"
msgstr "தனிப்பட்ட பரிந்துரைகள்"

#.  Option to enable sending usage statistics
#, java-printf-format
msgctxt "enable_metrics_logging"
msgid ""
"Improve <xliff:g xmlns:xliff=\"urn:oasis:names:tc:xliff:document:1.2\" "
"id=\"APPLICATION_NAME\" example=\"Android Keyboard\">%s</xliff:g>"
msgstr ""
"<xliff:g xmlns:xliff=\"urn:oasis:names:tc:xliff:document:1.2\" "
"id=\"APPLICATION_NAME\">%s</xliff:g> ஐ மேம்படுத்து"

#.  Option name for enabling or disabling the double-space period feature that
#. lets double tap on spacebar insert a period followed by a space [CHAR
#. LIMIT=30]
#, java-printf-format
msgctxt "use_double_space_period"
msgid "Double-space period"
msgstr "புள்ளியைத் தொடர்ந்து இடைவெளியை உள்ளிடு"

#.  Description for option enabling or disabling the double-space period
#. feature that lets double tap on spacebar insert a period followed by a
#. space [CHAR LIMIT=65]
#, java-printf-format
msgctxt "use_double_space_period_summary"
msgid "Double tap on spacebar inserts a period followed by a space"
msgstr ""
"ஸ்பேஸ் பட்டியை இருமுறை தட்டுவது, புள்ளியை தொடர்ந்து இடைவெளியை உள்ளிடும்"

#.  Option to enable auto capitalization of sentences
#, java-printf-format
msgctxt "auto_cap"
msgid "Auto-capitalization"
msgstr "தன்னியக்க பேரெழுத்தாக்கல்"

#.  Description for option to enable auto capitalization of sentences
#, java-printf-format
msgctxt "auto_cap_summary"
msgid "Capitalize the first word of each sentence"
msgstr "வரியின் முதல் எழுத்தை பேரெழுத்தாக்கும்"

#.  Option to edit personal dictionary. [CHAR_LIMIT=30]
#, java-printf-format
msgctxt "edit_personal_dictionary"
msgid "Personal dictionary"
msgstr "தனிப்பட்ட அகராதி"

#.  Option to configure dictionaries
#, java-printf-format
msgctxt "configure_dictionaries_title"
msgid "Add-on dictionaries"
msgstr "துணை அகராதிகள்"

#.  Name of the main dictionary, as opposed to auxiliary dictionaries
#. (medical/entertainment/sports...)
#, java-printf-format
msgctxt "main_dictionary"
msgid "Main dictionary"
msgstr "முதன்மை அகராதி"

#.  Option to enable showing suggestions
#, java-printf-format
msgctxt "prefs_show_suggestions"
msgid "Show correction suggestions"
msgstr "திருத்துதல் விருப்பங்களைக் காட்டு"

#.  Description for show suggestions
#, java-printf-format
msgctxt "prefs_show_suggestions_summary"
msgid "Display suggested words while typing"
msgstr "உள்ளிடும்போது பரிந்துரைக்கப்பட்ட வார்த்தைகளைக் காட்டும்"

#.  Option to block potentially offensive words to be shown [CHAR_LIMIT=30]
#, java-printf-format
msgctxt "prefs_block_potentially_offensive_title"
msgid "Block offensive words"
msgstr "புண்படுத்தும் சொற்களைத் தடு"

#.  Summary for option to block potentially offensive words to be shown
#. [CHAR_LIMIT=80 (two lines) or 40 (fits on one line, preferable)]
#, java-printf-format
msgctxt "prefs_block_potentially_offensive_summary"
msgid "Do not suggest potentially offensive words"
msgstr "மனதைப் புண்படுத்தக் கூடிய சொற்களைப் பரிந்துரைக்க வேண்டாம்"

#.  Option to decide the auto correction threshold score
#.  Option to enable auto correction [CHAR LIMIT=20]
#, java-printf-format
msgctxt "auto_correction"
msgid "Auto-correction"
msgstr "தன்னியக்க திருத்தம்"

#.  Description for auto correction [CHAR LIMIT=65 (two lines) or 30 (fits on
#. one line, preferable)]
#, java-printf-format
msgctxt "auto_correction_summary"
msgid "Spacebar and punctuation automatically correct mistyped words"
msgstr ""
"ஸ்பேஸ்பாரும், நிறுத்தற்குறிகளும் தவறாக உள்ளிடப்பட்ட வார்த்தைகளை தானாக "
"திருத்தும்"

#.  Option to change the confidence level of the auto correction [CHAR
#. LIMIT=20]
#, java-printf-format
msgctxt "auto_correction_confidence"
msgid "Auto-correction confidence"
msgstr "ஆட்டோ-திருத்தம் நம்பிக்கை"

#.  Option to disable auto correction. [CHAR LIMIT=20]
#, java-printf-format
msgctxt "auto_correction_threshold_mode_off"
msgid "Off"
msgstr "ஆஃப்"

#.  Option to suggest auto correction suggestions modestly. Auto-corrects only
#. to a word which has small edit distance from typed word. [CHAR LIMIT=20]
#, java-printf-format
msgctxt "auto_correction_threshold_mode_modest"
msgid "Modest"
msgstr "மிதமாக"

#.  Option to suggest auto correction suggestions aggressively. Auto-corrects
#. to a word which has even large edit distance from typed word. [CHAR
#. LIMIT=20]
#, java-printf-format
msgctxt "auto_correction_threshold_mode_aggressive"
msgid "Aggressive"
msgstr "தீவிரமாக"

#.  Option to suggest auto correction suggestions very aggressively. Auto-
#. corrects to a word which has even large edit distance from typed word.
#. [CHAR LIMIT=20]
#, java-printf-format
msgctxt "auto_correction_threshold_mode_very_aggressive"
msgid "Very aggressive"
msgstr "மிகத் தீவிரமாக"

#.  Option to enable using next word suggestions. After the user types a
#. space, with this option on, the keyboard will try to predict the next word.
#, java-printf-format
msgctxt "bigram_prediction"
msgid "Next-word suggestions"
msgstr "அடுத்த வார்த்தை பரிந்துரைகள்"

#.  Description for "next word suggestion" option. This displays suggestions
#. even when there is no input, based on the previous word.
#, java-printf-format
msgctxt "bigram_prediction_summary"
msgid "Use the previous word in making suggestions"
msgstr "பரிந்துரைகளை உருவாக்க முந்தைய வார்த்தைகளைப் பயன்படுத்தும்"

#.  Option to enable gesture input. The user can input a word by tracing the
#. letters of a word without releasing the finger from the screen. [CHAR
#. LIMIT=30]
#, java-printf-format
msgctxt "gesture_input"
msgid "Enable gesture typing"
msgstr "ஸ்வைப் உள்ளீட்டை இயக்கு"

#.  Description for "gesture_input" option. The user can input a word by
#. tracing the letters of a word without releasing the finger from the screen.
#. [CHAR LIMIT=65]
#, java-printf-format
msgctxt "gesture_input_summary"
msgid "Input a word by sliding through the letters"
msgstr "எழுத்துகள் வழியாக இழுப்பதன் மூலம் வார்த்தையை உள்ளிடலாம்"

#.  Option to enable gesture trail preview. The user can see a trail of the
#. gesture during gesture input. [CHAR LIMIT=30]
#, java-printf-format
msgctxt "gesture_preview_trail"
msgid "Show gesture trail"
msgstr "ஸ்வைப் தடத்தைக் காட்டு"

#.  Option to enable gesture floating text preview. The user can see a
#. suggested word floating under the moving finger during a gesture input.
#. [CHAR LIMIT=30]
#, java-printf-format
msgctxt "gesture_floating_preview_text"
msgid "Dynamic floating preview"
msgstr "நிகழ்நேர மிதக்கும் மாதிரிக்காட்சி"

#.  Description for "gesture_floating_preview_text" option. The user can see a
#. suggested word floating under the moving finger during a gesture input.
#. [CHAR LIMIT=65]
#, java-printf-format
msgctxt "gesture_floating_preview_text_summary"
msgid "See the suggested word while gesturing"
msgstr "சைகையிடும் போது பரிந்துரைக்கும் வார்த்தையைப் பார்க்கலாம்"

#.  Option to enable space aware gesture input. The user can input multiple
#. words by gliding through the space key during a gesture input. [CHAR
#. LIMIT=30]
#, java-printf-format
msgctxt "gesture_space_aware"
msgid "Phrase gesture"
msgstr "சொற்றொடர் ஸ்வைப்"

#.  Description for "gesture_space_aware" option. The user can input multiple
#. words by gliding through the space key during a gesture input.[CHAR
#. LIMIT=65]
#, java-printf-format
msgctxt "gesture_space_aware_summary"
msgid "Input spaces during gestures by gliding to the space key"
msgstr ""
"ஸ்பேஸ் விசைக்கு இழுப்பதன் மூலம் சைகைகளுக்கு இடையே இடைவெளிகளை உள்ளிடலாம்"

#.  Preferences item for enabling speech input
#, java-printf-format
msgctxt "voice_input"
msgid "Voice input key"
msgstr "குரல் உள்ளீட்டு விசை"

#.  The summary text to describe the reason why the "Voice input key" option
#. is disabled. [CHAR LIMIT=100]
#, java-printf-format
msgctxt "voice_input_disabled_summary"
