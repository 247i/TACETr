msgctxt "commons->or"
msgid "or"
msgstr "அல்லது"

msgctxt "commons->password"
msgid "Password"
msgstr "கடவுச்சொல்"

msgctxt "commons->register"
msgid "Create account"
msgstr "கணக்கை உருவாக்கவும்"

msgctxt "commons->username"
msgid "Username"
msgstr "பயனர்பெயர்"

msgctxt "commons->validation"
msgid "Validation"
