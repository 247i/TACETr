msgctxt "NotificationDeliveryMethod__unifiedpush"
msgid "UnifiedPush"
msgstr "UNIFIDEPUSH"

#, java-printf-format
msgctxt "NotificationDeliveryMethod__fcm"
msgid "FCM (Google Play Services)"
msgstr "எஃப்.சி.எம் (கூகிள் பிளே சர்வீசச்)"

#, java-printf-format
msgctxt "NotificationDeliveryMethod__websocket"
msgid "WebSocket"
msgstr "வெப்சாக்கெட்"

#, java-printf-format
msgctxt "IncomingMessageObserver_websocket_service"
msgid "WebSocket Service"
msgstr "வெப்சாக்கெட் பணி"

#, java-printf-format
msgctxt "MollySocketLink_scan_the_qr_code"
msgid ""
"Scan the QR code displayed on your MollySocket server's homepage or in the "
"console."
msgstr ""
"உங்கள் மோலி சாக்கெட் சேவையக முகப்புப்பக்கத்தில் அல்லது கன்சோலில் "
"காட்டப்படும் QR குறியீட்டை ச்கேன் செய்யுங்கள்."

#, java-printf-format
msgctxt ""
"MollySocketLink_try_scanning_another_image_containing_a_mollysocket_qr_code"
msgid "Try scanning another image containing a MollySocket QR code."
msgstr ""
"மோலிசாக்கெட் QR குறியீட்டைக் கொண்ட மற்றொரு படத்தை ச்கேன் செய்ய "
"முயற்சிக்கவும்."

#, java-printf-format
msgctxt "MollySocketLink_mollysocket_server_not_found_at_s"
msgid "MollySocket server not found at '%s'"
msgstr "மோலிசாக்கெட் சேவையகம் '%s' இல் காணப்படவில்லை"

#, java-printf-format
msgctxt "MollySocketLink_experienced_a_network_error_please_try_again"
msgid "Experienced a network error. Please try again."
msgstr "பிணைய பிழையை அனுபவித்தது. மீண்டும் முயற்சிக்கவும்."

#, java-printf-format
msgctxt "UnifiedPushSettingsFragment__mollysocket_server"
msgid "MollySocket server"
msgstr "மோலிச்கே சேவையகம்"

#, java-printf-format
msgctxt "UnifiedPushSettingsFragment__mollysocket_server_air_gapped"
msgid "Air gapped"
msgstr "காற்று நுழைந்தது"
