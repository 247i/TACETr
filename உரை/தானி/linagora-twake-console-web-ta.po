msgid "Custom date"
msgstr "தனிப்பயன் தேதி"

msgctxt "DATE_FILTER.LAST_15_DAYS"
msgid "Last 15 days"
msgstr "கடந்த 15 நாட்கள்"

msgctxt "DATE_FILTER.LAST_30_DAYS"
msgid "Last 30 days"
msgstr "கடந்த 30 நாட்கள்"

msgctxt "DATE_FILTER.LAST_QUARTER"
