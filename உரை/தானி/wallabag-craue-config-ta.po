msgctxt "settings_changed"
msgid "Configuration updated"
msgstr "உள்ளமைவு புதுப்பிக்கப்பட்டது"

#, ignore-long-untranslated, ignore-multiple-failures
msgctxt "download_pictures"
msgid "Download pictures onto your server"
msgstr "உங்கள் சேவையகத்தில் படங்களை பதிவிறக்கவும்"

msgctxt "diaspora_url"
msgid "diaspora* URL, if the service is enabled"
msgstr "டயச்போரா* முகவரி, பணி இயக்கப்பட்டிருந்தால்"

msgctxt "export_epub"
msgid "Enable ePub export"
msgstr "EPUB ஏற்றுமதியை இயக்கவும்"

msgctxt "export_pdf"
msgid "Enable PDF export"
msgstr "PDF ஏற்றுமதியை இயக்கவும்"

msgctxt "export_csv"
msgid "Enable CSV export"
msgstr "காபிம ஏற்றுமதியை இயக்கவும்"

msgctxt "export_json"
msgid "Enable JSON export"
msgstr "சாதொபொகு ஏற்றுமதியை இயக்கவும்"

msgctxt "export_txt"
msgid "Enable TXT export"
msgstr "TXT ஏற்றுமதியை இயக்கவும்"

msgctxt "export_xml"
msgid "Enable XML export"
msgstr "எக்ச்எம்எல் ஏற்றுமதியை இயக்கவும்"

msgctxt "import_with_rabbitmq"
msgid "Enable RabbitMQ to import data asynchronously"
msgstr "ஒத்திசைவற்ற முறையில் தரவை இறக்குமதி செய்ய ராபிட்எம்.கியூவை இயக்கவும்"

msgctxt "import_with_redis"
msgid "Enable Redis to import data asynchronously"
msgstr "ஒத்திசைவற்ற முறையில் தரவை இறக்குமதி செய்ய ரெடிசை இயக்கவும்"

msgctxt "shaarli_url"
msgid "Shaarli URL, if the service is enabled"
msgstr "சார்லி முகவரி, பணி இயக்கப்பட்டிருந்தால்"

msgctxt "unmark_url"
msgid "Unmark URL, if the service is enabled"
msgstr "பணி இயக்கப்பட்டிருந்தால், முகவரி ஐ குறைக்கிறது"

msgctxt "share_diaspora"
msgid "Enable share to diaspora*"
msgstr "புலம்பெயர்ந்தோருக்கு பங்கை இயக்கவும்*"

msgctxt "share_mail"
msgid "Enable share by e-mail"
msgstr "மின்னஞ்சல் மூலம் பங்கை இயக்கவும்"

msgctxt "share_shaarli"
msgid "Enable sharing to Shaarli"
msgstr "சார்லிக்கு பகிர்வை இயக்கவும்"

msgctxt "share_twitter"
msgid "Enable sharing to Twitter"
msgstr "ட்விட்டருக்கு பகிர்வதை இயக்கவும்"

msgctxt "share_unmark"
msgid "Enable sharing to Unmark.it"
msgstr "Unmark.it க்கு பகிர்வை இயக்கவும்"

msgctxt "share_linkding"
msgid "Enable sharing to Linkding"
msgstr "லிங்க்ட்இனுக்கு பகிர்வதை இயக்கவும்"

msgctxt "show_printlink"
msgid "Display a link to print content"
msgstr "உள்ளடக்கத்தை அச்சிட இணைப்பைக் காண்பி"

msgctxt "wallabag_support_url"
msgid "Support URL for wallabag"
msgstr "வாலபேக்கிற்கான முகவரி ஐ ஆதரிக்கவும்"

msgctxt "entry"
msgid "article"
msgstr "கட்டுரை"

msgctxt "export"
msgid "export"
msgstr "ஏற்றுமதி"

msgctxt "import"
msgid "import"
msgstr "இறக்குமதி"

#, ignore-same
msgctxt "misc"
msgid "misc"
msgstr "இதர"

msgctxt "modify_settings"
msgid "apply"
msgstr "இடு"

msgctxt "matomo_host"
msgid "Host of your website in Matomo (without http:// or https://)"
msgstr ""
"Matomo இல் உங்கள் வலைத்தளத்தின் புரவலன் (http: // அல்லது https: // இல்லாமல்)"

#, ignore-multiple-failures
msgctxt "matomo_site_id"
msgid "ID of your website in Matomo"
msgstr "மாடோமோவில் உங்கள் வலைத்தளத்தின் ஐடி"

#, ignore-multiple-failures
msgctxt "matomo_enabled"
msgid "Enable Matomo"
msgstr "மாடோமோவை இயக்கவும்"

msgctxt "share_public"
msgid "Allow public URLs for entries"
msgstr "உள்ளீடுகளுக்கு பொது முகவரி களை அனுமதிக்கவும்"

msgctxt "download_images_enabled"
msgid "Download images locally"
msgstr "படங்களை உள்ளூரில் பதிவிறக்கவும்"

msgctxt "restricted_access"
msgid "Enable authentication for paywalled websites"
msgstr "செலுத்தப்பட்ட வலைத்தளங்களுக்கான அங்கீகாரத்தை இயக்கவும்"

#, ignore-long-untranslated, ignore-multiple-failures
msgctxt "api_user_registration"
msgid "Turn on user registration via the API"
msgstr "பநிஇ வழியாக பயனர் பதிவை இயக்கவும்"

msgctxt "store_article_headers"
msgid "Enable if wallabag stores HTTP headers for each article"
msgstr ""
"ஒவ்வொரு கட்டுரைக்கும் வாலபாக் HTTP தலைப்புகளை சேமித்து வைத்தால் இயக்கவும்"

#, ignore-long-untranslated, ignore-multiple-failures
msgctxt "shaarli_share_origin_url"
msgid "Enable sharing origin URL to Shaarli, if the service is enabled"
msgstr ""
"பணி இயக்கப்பட்டிருந்தால், சார்லிக்கு பகிர்வு தோற்றம் முகவரி ஐ இயக்கவும்"
