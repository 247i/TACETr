msgstr "மேலும் செயல்பாடுகள்"

msgctxt "[repo]no_desc"
msgid "No description"
msgstr "விளக்கம் இல்லை"

msgctxt "[repo]quick_guide"
msgid "Quick guide"
msgstr "விரைவான வழிகாட்டி"

msgctxt "[repo]clone_this_repo"
msgid "Clone this repository"
msgstr "இந்த களஞ்சியத்தை நகலி செய்யவும்"

msgctxt "[repo]cite_this_repo"
