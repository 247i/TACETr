#. Generic prompt to reset something
#, icu-message-format
msgctxt "promptReset"
msgid "Reset"
msgstr "மீட்டமை"

#. Generic prompt to enable something
#, icu-message-format
msgctxt "promptEnable"
msgid "Enable"
msgstr "இயக்கு"

#. Generic prompt to restore something
#, icu-message-format
msgctxt "promptRestore"
msgid "Restore"
msgstr "மீட்டெடு"

#. Generic message to decline something, using nice manners :)
#, icu-message-format
msgctxt "promptPoliteNo"
msgid "No, thanks"
msgstr "இல்லை, நன்றி"

#. Prompt text to go backwards, probably for navigation
#, icu-message-format
msgctxt "promptBack"
msgid "Back"
msgstr "பின்"

#. Prompt to copy text
#, icu-message-format
msgctxt "promptCopy"
msgid "Copy"
msgstr "நகலெடு"

#. Prompt text for after a copy has been completed
#, icu-message-format
msgctxt "promptCopyComplete"
msgid "Copied!"
msgstr "நகலெடுக்கப்பட்டது!"

#. Text that is placed between two or more options: [button1] or [button2]
#, icu-message-format
msgctxt "labelOr"
msgid "Or"
msgstr "அல்லது"

#. Message to display when a user does a custom emote (/me)
#, case-insensitive, icu-message-format, placeholders:$USER$:$EMOTE$
msgctxt "messageUserEmote"
msgid "*{user} {emote}"
msgstr "*{user} {emote}"

#. Message body for when a user joins the room
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messagePlaceholderUserJoinedRoom"
msgid "{user} joined the room!"
msgstr "{user} அறையில் சேர்ந்தார்!"

#. Message body for when a user leaves the room
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messagePlaceholderUserLeftRoom"
msgid "{user} left the room"
msgstr "{user} அறையை விட்டு வெளியேறினார்"

#. Message body for when a user updates their avatar
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messagePlaceholderUserUpdatedAvatar"
msgid "{user} updated their avatar"
msgstr "{user} அவர்களின் அவதாரத்தை புதுப்பித்தார்"

#. Message body for when a user updates their display name
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messagePlaceholderUserUpdatedName"
msgid "{user} updated their display name"
msgstr "{user} அவர்களின் காட்சி பெயரை புதுப்பித்தார்"

#. Message body for when a user updates their display name
#, case-insensitive, icu-message-format, placeholders:$USER$:$NEWNAME$
msgctxt "messagePlaceholderUserUpdatedNameDetailed"
msgid "{user} changed their display name to \"{newName}\""
msgstr "{user} அவர்களின் காட்சி பெயரை \"{newName}\" என்று மாற்றினார்"

#. Message body for when a user invites another user to the room
#, case-insensitive, icu-message-format, placeholders:$SENDER$:$INVITEDUSER$
msgctxt "messagePlaceholderUserInvited"
msgid "{sender} invited {invitedUser}"
msgstr "{sender} அழைக்கப்பட்ட {invitedUser}"

#. Message body for when a user rejected an invitation to a room
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messagePlaceholderUserRejectedInvite"
msgid "{user} rejected the invitation"
msgstr "{user} அழைப்பை நிராகரித்தார்"

#. Message body for when a user's invitation to a room was withdrawn
#, case-insensitive, icu-message-format, placeholders:$SENDER$:$USER$
msgctxt "messageUserRetractedInvite"
msgid "{sender} uninvited {user}"
msgstr "{sender} அறிவிக்கப்படாத {user}"

#. Message body for when a user accepted an invitation to a room
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messageUserAcceptedInvite"
msgid "{user} accepted the invitiation"
msgstr "{user} அழைப்பை ஏற்றுக்கொண்டார்"

#. Message body for when a user bans another user from the room
#, case-insensitive, icu-message-format, placeholders:$SENDER$:$USER$
msgctxt "messageUserBanned"
msgid "{sender} banned {user}"
msgstr "{sender} தடைசெய்யப்பட்ட {user}"

#. Message body for when a user reverts a ban of another user
#, case-insensitive, icu-message-format, placeholders:$SENDER$:$USER$
msgctxt "messageUserUnbanned"
msgid "{sender} unbanned {user}"
msgstr "{sender} தடைசெய்யப்படாத {user}"

#. Message body for when a user adds an event to the room's pinned messages
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messageUserPinnedEvent"
msgid "{user} pinned a message!"
msgstr "{user} ஒரு செய்தியை பொருத்தினார்!"

#. Message body for when a user removes an event from the room's pinned
#. messages
#, case-insensitive, icu-message-format, placeholders:$USER$
msgctxt "messageUserUnpinnedEvent"
msgid "{user} unpinned a message"
msgstr "{user} ஒரு செய்தியை இணைக்கவில்லை"

#. Text that explains to the user that libolm dependency is not found
#, icu-message-format
msgctxt "matrixClientOlmMissingMessage"
msgid ""
"libolm is not installed or was not found. End to End Encryption will not be "
"available until this is resolved"
msgstr ""
"லிபோல்ம் நிறுவப்படவில்லை அல்லது கண்டுபிடிக்கப்படவில்லை. இது தீர்க்கப்படும் "
"வரை முடிவு குறியாக்கம் கிடைக்காது"

#. Title of a warning about encryption
#, icu-message-format
msgctxt "matrixClientEncryptionWarningTitle"
msgid "Encryption Warning"
msgstr "குறியாக்க எச்சரிக்கை"

#. Placeholder text to put in a notification when the user has privacy
#. enhanced notifications enabled.
#, icu-message-format
msgctxt "notificationModifiersPrivacyEnhanced"
msgid "Sent a message"
msgstr "ஒரு செய்தியை அனுப்பியது"
