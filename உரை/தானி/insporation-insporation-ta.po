msgid "You are not sharing with each other."
msgstr "நீங்கள் ஒருவருக்கொருவர் பகிரவில்லை."

#. Title for the contact aspects prompt dialog
#, case-insensitive, icu-message-format, placeholders:$NAME$
msgctxt "contactAspectsPrompt"
msgid "Select aspects for {name}"
msgstr "{பெயர் க்கு க்கான அம்சங்களைத் தேர்ந்தெடுக்கவும்"

#. Success message after the user started sharing with somebody
#, case-insensitive, icu-message-format, placeholders:$NAME$
msgctxt "startedSharing"
msgid "Started sharing with {name}."
msgstr "{பெயர் with உடன் பகிரத் தொடங்கியது."

#. Success message after the user stopped sharing with somebody
#, case-insensitive, icu-message-format, placeholders:$NAME$
msgctxt "stoppedSharing"
msgid "Stopped sharing with {name}."
msgstr "{பெயர் with உடன் பகிர்வதை நிறுத்தியது."

#. Success message after the user updated a contact's aspects
#, icu-message-format
msgctxt "contactAspectsUpdated"
msgid "Aspects updated."
msgstr "நற்பொருத்தங்கள் புதுப்பிக்கப்பட்டன."

#. Error message after updating a contact's aspects failed
#, icu-message-format
msgctxt "failedToUpdateContactAspects"
msgid "Failed to update aspects"
msgstr "அம்சங்களைப் புதுப்பிக்கத் தவறிவிட்டது"

#. Title for the publisher page
#, icu-message-format
msgctxt "publisherTitle"
msgid "Write a new post"
msgstr "புதிய இடுகையை எழுதுங்கள்"

#. Tooltip for the take a photo button
#, icu-message-format
msgctxt "takePhoto"
msgid "Take a photo"
msgstr "புகைப்படம் எடுக்கவும்"

#. Tooltip for the upload a photo button
#, icu-message-format
msgctxt "uploadPhoto"
msgid "Upload a photo"
msgstr "புகைப்படத்தைப் பதிவேற்றவும்"

#. Tooltip for the add poll button
#, icu-message-format
msgctxt "addPoll"
msgid "Add a poll"
msgstr "ஒரு வாக்கெடுப்பு சேர்க்கவும்"

#. Tooltip for the add location button
#, icu-message-format
msgctxt "addLocation"
msgid "Add your location"
msgstr "உங்கள் இருப்பிடத்தைச் சேர்க்கவும்"

#. Label for the publish post button
#, icu-message-format
msgctxt "publishPost"
msgid "Publish post"
msgstr "இடுகையை வெளியிடுங்கள்"

#. Publish target label for a public post
#, icu-message-format
msgctxt "publishTargetPublic"
msgid "Public"
msgstr "பொது"

#. Publish target label for a post to all aspects
#, icu-message-format
msgctxt "publishTargetAllAspects"
msgid "All aspects"
msgstr "அனைத்து அம்சங்களும்"

#. Publish target label for a post to a list of aspects. The zero and one
#. cases are never used
#, case-insensitive, icu-message-format, placeholders:$COUNT$
msgctxt "publishTargetAspects"
msgid "{count,plural, =0{No aspects}=1{One aspect}other{{count} aspects}}"
msgstr ""
"{எண்ணிக்கை, பன்மை, = 0 {No aspects} = 1 {One aspect} மற்ற {{count} "
"அம்சங்கள்}}"

#. Title for the publish target selection prompt dialog
#, icu-message-format
msgctxt "publishTargetPrompt"
msgid "Select post visibility"
msgstr "இடுகை தெரிவுநிலையைத் தேர்ந்தெடுக்கவும்"

#. Error message after uploading a photo failed
#, icu-message-format
msgctxt "failedToUploadPhoto"
msgid "Failed to upload photo"
msgstr "புகைப்படத்தைப் பதிவேற்றத் தவறிவிட்டது"

#. Hint text for the poll question field
#, icu-message-format
msgctxt "pollQuestionHint"
msgid "Enter a question"
msgstr "ஒரு கேள்வியை உள்ளிடவும்"

#. Hint text for a poll answer field
#, icu-message-format
msgctxt "pollAnswerHint"
msgid "Enter an answer"
msgstr "ஒரு பதிலை உள்ளிடவும்"

#. Label for the create poll button
#, icu-message-format
msgctxt "createPoll"
msgid "Create poll"
msgstr "வாக்கெடுப்பை உருவாக்கவும்"

#. Label for the edit poll button
#, icu-message-format
msgctxt "editPoll"
msgid "Edit poll"
msgstr "வாக்கெடுப்பைத் திருத்து"

#. Hint text for an address field
#, icu-message-format
msgctxt "enterAddressHint"
msgid "Enter an address"
msgstr "ஒரு முகவரியை உள்ளிடவும்"

#. Error message after searching for the entered address failed
#, icu-message-format
msgctxt "failedToSearchForAddresses"
msgid "Failed to search for addresses"
msgstr "முகவரிகளைத் தேடத் தவறிவிட்டது"

#. Label for search type selector for the people search
#, icu-message-format
msgctxt "searchTypePeople"
msgid "People"
msgstr "மக்கள்"

#. Label for search type selector for the people by tag search
#, icu-message-format
msgctxt "searchTypePeopleByTag"
msgid "People by tag"
msgstr "குறிச்சொல் மூலம் மக்கள்"

#. Label for search type selector for the tags search
#, icu-message-format
msgctxt "searchTypeTags"
msgid "Tags"
msgstr "குறிச்சொற்கள்"

#. Hint text for the people search field
#, icu-message-format
msgctxt "searchPeopleHint"
msgid "Start typing a name or diaspora* ID"
msgstr "ஒரு பெயர் அல்லது புலம்பெயர்* ஐடியைத் தட்டச்சு செய்யத் தொடங்குங்கள்"

#. Hint text for the the people by tag search field
#, icu-message-format
msgctxt "searchPeopleByTagHint"
msgid "Enter a tag"
msgstr "ஒரு குறிச்சொல்லை உள்ளிடவும்"

#. Hint text for the tags search field
#, icu-message-format
msgctxt "searchTagsHint"
msgid "Start typing tag"
msgstr "குறிச்சொல்லை தட்டச்சு செய்யத் தொடங்குங்கள்"

#. Aspect stream selector label for all aspects
#, icu-message-format
msgctxt "aspectStreamSelectorAllAspects"
msgid "All aspects"
msgstr "அனைத்து அம்சங்களும்"

#. Aspect stream selector label for a list of aspects. The zero and one cases
#. are never used
#, case-insensitive, icu-message-format, placeholders:$COUNT$
msgctxt "aspectStreamSelectorAspects"
msgid "{count,plural, =0{No aspects}=1{One aspect}other{{count} aspects}}"
msgstr ""
"{எண்ணிக்கை, பன்மை, = 0 {No aspects} = 1 {One aspect} மற்ற {{count} "
"அம்சங்கள்}}"

#. Label for the manage followed tags button
#, icu-message-format
msgctxt "manageFollowedTags"
msgid "Manage followed tags"
msgstr "பின்பற்றப்பட்ட குறிச்சொற்களை நிர்வகிக்கவும்"

#. Title for the followed tags page
#, icu-message-format
msgctxt "followedTagsPageTitle"
msgid "Followed tags"
msgstr "குறிச்சொற்கள் பின்பற்றப்பட்டன"

#. Error message after following a tag failed
#, case-insensitive, icu-message-format, placeholders:$TAG$
msgctxt "failedToFollowTag"
msgid "Failed to follow #{tag}"
msgstr "#{tag} ஐப் பின்பற்றுவதில் தோல்வி"

#. Error message after unfollowing a tag failed
#, case-insensitive, icu-message-format, placeholders:$TAG$
msgctxt "failedToUnfollowTag"
msgid "Failed to unfollow #{tag}"
msgstr "#{tag} ஐப் பின்தொடர்வதில் தோல்வி"

#. Error message after trying to open an invalid URL
#, icu-message-format
msgctxt "failedToOpenInvalidUrl"
msgid "URL looks invalid, cannot open"
msgstr "முகவரி தவறானது, திறக்க முடியாது"

#, icu-message-format
msgctxt "failedToOpenUrl"
msgid "Failed to open URL. Do you have a browser installed?"
msgstr "முகவரி ஐ திறக்கத் தவறிவிட்டது. உங்களிடம் உலாவி நிறுவப்பட்டுள்ளதா?"

#. Title for crash report dialog
#, icu-message-format
msgctxt "catcherLocalizationDialogReportModeTitle"
msgid "insporation* crashed :("
msgstr "உத்வேகம்* செயலிழந்தது :("

#. Description for crash report page
#, icu-message-format
msgctxt "catcherLocalizationDialogReportModeDescription"
msgid ""
"An unexpected error occurred, an error report is ready to be send to the "
"developers."
msgstr ""
"எதிர்பாராத பிழை ஏற்பட்டது, டெவலப்பர்களுக்கு அனுப்ப பிழை அறிக்கை தயாராக "
"உள்ளது."

#. Button label for accepting to send a crash report
#, icu-message-format
msgctxt "catcherLocalizationDialogReportModeAccept"
msgid "Send report"
msgstr "அறிக்கை அனுப்பவும்"

#. Button label for dismissing a crash report
#, icu-message-format
msgctxt "catcherLocalizationDialogReportModeCancel"
msgid "Dismiss"
msgstr "தள்ளுபடி"
