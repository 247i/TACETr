msgstr "ஒரு கோப்பைத் தேர்ந்தெடுக்கவும்"

msgctxt "SEND"
msgid "Send"
msgstr "அனுப்பு"

msgctxt "UP"
msgid "Uploading …"
msgstr "பதிவேற்றப்படுகிறது…"

msgctxt "ONE_TIME_DL"
msgid "One-time download"
msgstr "ஒரு முறை பதிவிறக்கம்"

msgctxt "PSW"
msgid "Password"
msgstr "கடவுச்சொல்"

msgctxt "TIME_LIM"
msgid "Time limit"
msgstr "நேர வரம்பு"

msgctxt "MAX_FILE_SIZE"
msgid "Maximum file size"
msgstr "அதிகபட்ச கோப்பு அளவு"

msgctxt "POWERED_BY"
msgid "powered by the copyleft, libre software project Jirafeau"
msgstr "மோதல், லிப்ரே மென்பொருள் திட்ட சிராஃபோ மூலம் இயக்கப்படுகிறது"

msgctxt "MADE_WITH"
msgid "Made with"
msgstr "உடன் தயாரிக்கப்பட்டது"

msgctxt "JI_PROJECT"
msgid "Jirafeau Project"
msgstr "ஜிராபியூ திட்டம்"

