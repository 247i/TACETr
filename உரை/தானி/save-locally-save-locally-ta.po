#, java-printf-format
msgctxt "app_name"
msgid "Save Locally"
msgstr "உள்ளூரில் சேமிக்கவும்"

#, java-printf-format
msgctxt "how_to_use"
msgid "How to Use"
msgstr "எவ்வாறு பயன்படுத்துவது"

#, java-printf-format
msgctxt "how_to_use_step_1"
msgid "Share the file via the Share icon"
msgstr "பகிர்வு படவுரு வழியாக கோப்பைப் பகிரவும்"

#, java-printf-format
msgctxt "how_to_use_step_2"
msgid "Select \"%s\" on the share options"
msgstr "பங்கு விருப்பங்களில் \"%s\" ஐத் தேர்ந்தெடுக்கவும்"

#, java-printf-format
msgctxt "how_to_use_step_3"
msgid "A screen with details about the file will open, press Save"
msgstr ""
"கோப்பைப் பற்றிய விவரங்களைக் கொண்ட ஒரு திரை திறக்கப்படும், சேமி அழுத்தவும்"

#, java-printf-format
msgctxt "how_to_use_step_4"
msgid "The file picker will open, select where to save and the filename"
msgstr ""
"கோப்பு எடுப்பவர் திறந்து, எங்கு சேமிக்க வேண்டும் மற்றும் கோப்பு பெயர் "
"என்பதைத் தேர்ந்தெடுக்கவும்"

#, java-printf-format
msgctxt "donation_title"
msgid "Donate"
msgstr "நன்கொடை"

#, java-printf-format
msgctxt "donation_content"
msgid "Like the app? Consider donating! For more information, visit %s"
msgstr ""
"பயன்பாடு போன்றதா? நன்கொடை அளிப்பதைக் கவனியுங்கள்! மேலும் தகவலுக்கு, %s ஐப் "
"பார்வையிடவும்"

#, java-printf-format
msgctxt "about_title"
msgid "About"
msgstr "பற்றி"

#, java-printf-format
msgctxt "about_developer"
msgid "Developed by Mateus Rodrigues Costa"
msgstr "மேடியச் ரோட்ரிக்ச் கோச்டாவால் உருவாக்கப்பட்டது"

#, java-printf-format
msgctxt "about_icon_credits"
msgid "App icon designed by @swyzera"
msgstr "@ச்விசெரா வடிவமைத்த பயன்பாட்டு படவுரு"

#, java-printf-format
msgctxt "about_github"
msgid "Source code available on Github at %s, licensed under AGPLv3+"
msgstr ""
"Github இல் %s இல் கிடைக்கும் மூலக் குறியீடு, AgPlv3+ இன் கீழ் உரிமம் பெற்றது"

#, java-printf-format
msgctxt "settings_title"
msgid "Settings"
msgstr "அமைப்புகள்"

#, java-printf-format
msgctxt "settings_category_file_picker"
msgid "File Picker"
msgstr "கோப்பு எடுப்பவர்"

#, java-printf-format
msgctxt "settings_default_save_location"
msgid "Default save location"
msgstr "இயல்புநிலை சேமிப்பு இருப்பிடம்"

#, java-printf-format
msgctxt "settings_default_save_location_last_used"
msgid "Last used folder"
msgstr "கடைசியாக பயன்படுத்தப்பட்ட கோப்புறை"

#, java-printf-format
msgctxt "settings_default_save_location_select_folder"
msgid "Select folder…"
msgstr "கோப்புறையைத் தேர்ந்தெடுக்கவும்…"

#, java-printf-format
msgctxt "settings_skip_file_picker"
msgid "Skip File Picker"
msgstr "கோப்பு தேர்வாளரைத் தவிர்க்கவும்"

#, java-printf-format
msgctxt "settings_skip_file_picker_info"
msgid ""
"Skip File Picker and immediately save files (only available when a default "
"folder is set)"
msgstr ""
"கோப்பு தேர்வாளரைத் தவிர்த்து உடனடியாக கோப்புகளைச் சேமிக்கவும் (இயல்புநிலை "
"கோப்புறை அமைக்கப்படும்போது மட்டுமே கிடைக்கும்)"

#, java-printf-format
msgctxt "settings_category_file_details"
msgid "File Details"
msgstr "கோப்பு விவரங்கள்"

#, java-printf-format
msgctxt "settings_skip_file_details_page"
msgid "Skip File Details page"
msgstr "கோப்பு விவரங்கள் பக்கத்தைத் தவிர்க்கவும்"

#, java-printf-format
msgctxt "settings_skip_file_details_page_info"
msgid "Open file picker as soon as file is received"
msgstr "கோப்பு கிடைத்தவுடன் கோப்பு பிக்கர் திறக்கவும்"

#, java-printf-format
msgctxt "settings_show_file_preview"
msgid "Show file preview"
msgstr "கோப்பு முன்னோட்டத்தைக் காட்டு"

#, java-printf-format
msgctxt "settings_show_file_preview_info"
msgid ""
"Show file previews for compatible file types. Currently only some image "
"files are supported."
msgstr ""
"இணக்கமான கோப்பு வகைகளுக்கான கோப்பு முன்னோட்டங்களைக் காட்டு. தற்போது சில படக்"
" கோப்புகள் மட்டுமே ஆதரிக்கப்படுகின்றன."

#, java-printf-format
msgctxt "settings_category_intents"
msgid "Intents"
msgstr "நோக்கங்கள்"

#, java-printf-format
msgctxt "settings_intercept_action_view_intents"
msgid "Intercept ACTION_VIEW intents"
msgstr "இடைமறிப்பு அதிரடி_வியூ நோக்கங்கள்"

#, java-printf-format
msgctxt "settings_intercept_action_view_intents_info"
msgid ""
"Allow registering as an option when apps try to automatically open a file "
"viewer"
msgstr ""
"பயன்பாடுகள் தானாகவே கோப்பு பார்வையாளரைத் திறக்க முயற்சிக்கும்போது பதிவு "
"செய்வதை ஒரு விருப்பமாக அனுமதிக்கவும்"

#, java-printf-format
msgctxt "no_file_found"
msgid "No file found"
msgstr "கோப்பு எதுவும் கிடைக்கவில்லை"

#, java-printf-format
msgctxt "saving_file"
msgid "Saving file…"
msgstr "சேமிப்பு கோப்பு…"

#, java-printf-format
msgctxt "file_details"
msgid "File Details"
msgstr "கோப்பு விவரங்கள்"

#, java-printf-format
msgctxt "file_name"
msgid "File name"
msgstr "கோப்பு பெயர்"

#, java-printf-format
msgctxt "file_type"
msgid "File type"
msgstr "கோப்பு வகை"

#, java-printf-format
msgctxt "file_size"
msgid "File size"
msgstr "கோப்பு அளவு"

#, java-printf-format
msgctxt "unknown"
msgid "Unknown"
msgstr "தெரியவில்லை"

#, java-printf-format
msgctxt "toast_saved_file_success"
msgid "File saved successfully!"
msgstr "கோப்பு வெற்றிகரமாக சேமிக்கப்பட்டது!"

#, java-printf-format
msgctxt "toast_saved_file_failure"
msgid "An error occurred while saving the file."
msgstr "கோப்பைச் சேமிக்கும்போது பிழை ஏற்பட்டது."

#, java-printf-format
msgctxt "clear_button"
msgid "Clear"
msgstr "தெளிவான"

#, java-printf-format
msgctxt "save_button"
msgid "Save"
msgstr "சேமி"

#, java-printf-format
msgctxt "back_arrow"
msgid "Back Arrow"
msgstr "பின் அம்பு"
