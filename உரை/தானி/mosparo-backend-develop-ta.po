msgid "Executor"
msgstr "நிர்வாகி"

msgctxt "administration->cleanupStatistic->list->submitTokens"
msgid "Submit tokens"
msgstr "டோக்கன்களை சமர்ப்பிக்கவும்"

msgctxt "administration->cleanupStatistic->list->submissions"
msgid "Submission"
msgstr "சமர்ப்பிப்பு"

msgctxt "administration->cleanupStatistic->list->executionTime"
msgid "Execution time"
msgstr "சாவுஒறுப்பு நேரம்"

msgctxt "administration->cleanupStatistic->list->cleanupStatus"
