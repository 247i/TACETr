#. The default name for a contact when the name is not yet known
#, icu-message-format
msgctxt "defaultContactName"
msgid "Unknown"
msgstr "தெரியவில்லை"

#. The title of the application
#, icu-message-format
msgctxt "title"
msgid "bTox (working title)"
msgstr "BTOX (வேலை தலைப்பு)"

#. The text for the button to add a contact
#, icu-message-format
msgctxt "addContact"
msgid "Add contact"
msgstr "தொடர்பைச் சேர்க்கவும்"

#. The text for the message field
#, icu-message-format
msgctxt "message"
msgid "Message"
msgstr "செய்தி"

#. The placeholder text for the message input field
#, icu-message-format
msgctxt "messageInput"
msgid "message"
msgstr "செய்தி"

#. The text for the Tox ID field
#, icu-message-format
msgctxt "toxId"
msgid "Tox ID"
msgstr "நச்சு ஐடி"

#. The default message to send when adding a contact
#, case-insensitive, icu-message-format, placeholders:$NICK$
msgctxt "defaultAddContactMessage"
msgid "Hey, I'm {nick}, come chat with me on bTox!"
msgstr "ஏய், நான் {nick}, BTOX இல் என்னுடன் அரட்டையடிக்க வாருங்கள்!"

#. The error message when the friend request message is too long
#, case-insensitive, icu-message-format, placeholders:$MAX$:$LENGTH$
msgctxt "addContactMessageLengthError"
msgid "Maximum {max} characters ({length} is too long)"
msgstr "அதிகபட்ச {max} எழுத்துக்கள் ({length} மிக நீளமானது)"

#. The text for the button to add a contact
#, icu-message-format
msgctxt "add"
msgid "Add"
msgstr "கூட்டு"

#. The error message when the Tox ID is not the right length or format
#, case-insensitive, icu-message-format, placeholders:$LENGTH$
msgctxt "toxIdLengthError"
msgid "Must be {length} hexadecimal characters"
msgstr "{length} எக்சாடெசிமல் எழுத்துக்கள் இருக்க வேண்டும்"

#. The error message when the message is empty
#, icu-message-format
msgctxt "messageEmptyError"
msgid "The message can't be empty"
msgstr "செய்தி காலியாக இருக்க முடியாது"

#. The text for the profile menu item
#, icu-message-format
msgctxt "menuProfile"
msgid "Profile"
msgstr "சுயவிவரம்"

#. The text for the quit menu item
#, icu-message-format
msgctxt "menuQuit"
msgid "Quit"
msgstr "வெளியேறு"

#. The text for the settings menu item
#, icu-message-format
msgctxt "menuSettings"
msgid "Settings"
msgstr "அமைப்புகள்"

#. The error message when the nickname is too short or too long
#, case-insensitive, icu-message-format, placeholders:$MAX$
msgctxt "nickLengthError"
msgid "Nickname must be between 1 and {max} characters"
msgstr "புனைப்பெயர் 1 முதல் {max} எழுத்துகளுக்கு இடையில் இருக்க வேண்டும்"

#. The error message when the status message is longer than the supported
#. maximum
#, case-insensitive, icu-message-format, placeholders:$MAX$
msgctxt "statusMessageLengthError"
msgid "Status message may not exceed {max} characters"
msgstr "நிலை செய்தி {max} எழுத்துக்களை விட அதிகமாக இருக்காது"

#. The text for the nickname field
#, icu-message-format
msgctxt "profileTextFieldNick"
msgid "Nickname"
msgstr "புனைப்பெயர்"

#. The text for the status message field
#, icu-message-format
msgctxt "profileTextFieldUserStatusMessage"
msgid "Status message"
msgstr "நிலை செய்தி"

#. The text for the button to apply changes
#, icu-message-format
msgctxt "applyChanges"
msgid "Apply changes"
msgstr "மாற்றங்களைப் பயன்படுத்துங்கள்"

#. The text when changes have been applied
#, icu-message-format
msgctxt "applied"
msgid "Applied"
msgstr "பயன்படுத்தப்பட்டது"

#. The title text for the create profile dialog
#, icu-message-format
msgctxt "newProfile"
msgid "New profile"
msgstr "புதிய சுயவிவரம்"

#. The text for the button create a new profile
#, icu-message-format
msgctxt "create"
msgid "Create"
msgstr "உருவாக்கு"

#. The text for the logout menu item
#, icu-message-format
msgctxt "logout"
msgid "Logout"
msgstr "வெளியேற்றம்"

#. The text for the delete profile menu item
#, icu-message-format
msgctxt "deleteProfile"
msgid "Delete profile"
msgstr "சுயவிவரத்தை நீக்கு"

#. The message to confirm the deletion of a profile
#, case-insensitive, icu-message-format, placeholders:$NAME$
msgctxt "deleteProfileMessage"
msgid ""
"Are you sure you want to delete your \"{name}\" profile? This action cannot "
"be undone. It will delete all your contacts and messages. Make sure you have"
" a backup."
msgstr ""
"உங்கள் \"{name}\" சுயவிவரத்தை நீக்க விரும்புகிறீர்களா? இந்த செயலை "
"செயல்தவிர்க்க முடியாது. இது உங்கள் எல்லா தொடர்புகளையும் செய்திகளையும் "
"நீக்கும். உங்களிடம் காப்புப்பிரதி இருப்பதை உறுதிப்படுத்திக் கொள்ளுங்கள்."

#. The text for the cancel button
#, icu-message-format
msgctxt "cancel"
msgid "Cancel"
msgstr "ரத்துசெய்"

#. The text for the delete button
#, icu-message-format
msgctxt "delete"
msgid "Delete"
msgstr "நீக்கு"

#. The text when a profile has been deleted
#, case-insensitive, icu-message-format, placeholders:$NAME$
msgctxt "profileDeleted"
msgid "Profile \"{name}\" deleted"
msgstr "சுயவிவரம் \"{name}\" நீக்கப்பட்டது"
