#. Initial message for the loading screen, virtually unseen.
#, icu-message-format
msgctxt "loadingStart"
msgid "Initializing…"
msgstr "தொடங்குதல்…"

#. Loading screen message for loading presets.
#, icu-message-format
msgctxt "loadingPresets"
msgid "Loading presets…"
msgstr "முன்னமைவுகளை ஏற்றுகிறது…"

#. Loading screen message for loading stored changes.
#, icu-message-format
msgctxt "loadingChanges"
msgid "Loading changes…"
msgstr "மாற்றங்களை ஏற்றுகிறது…"

#. Loading screen message for firing up GPS and waiting for a location.
#, icu-message-format
msgctxt "loadingLocation"
msgid "Acquiring location…"
msgstr "இருப்பிடத்தைப் பெறுதல்…"

#. Loading screen notification title for when loading changes has failed.
#, icu-message-format
msgctxt "loadingChangesFailed"
msgid "Failed to load changes"
msgstr "மாற்றங்களை ஏற்றுவதில் தோல்வி"

#. Title for a warning alert when there's too much data downloaded.
#, icu-message-format
msgctxt "loadingTooMuchDataTitle"
msgid "Too much data"
msgstr "அதிக தரவு"

#. Message of a warning alert when there's too much data downloaded.
#, icu-message-format, ignore-multiple-failures
msgctxt "loadingTooMuchData"
msgid "Purge downloaded data in Settings to make the editor faster."
msgstr ""
"எடிட்டரை விரைவாகச் செய்ய அமைப்புகளில் பதிவிறக்கம் செய்யப்பட்ட தரவை "
"தூய்மைப்படுத்துங்கள்."

#. Alert title for the error happened during a data download.
#, icu-message-format
msgctxt "dataDownloadFailed"
msgid "Download failed"
msgstr "பதிவிறக்கம் தோல்வியடைந்தது"

#. Alert title for when the data was successfully downloaded.
#, icu-message-format
msgctxt "dataDownloadSuccessful"
msgid "Download successful"
msgstr "வெற்றிகரமாக பதிவிறக்கவும்"

#. Alert contents when we've successfully downloaded some data.
#, case-insensitive, icu-message-format, ignore-icu-message-format, placeholders:$COUNT$
msgctxt "dataDownloadedCount"
msgid ""
"{count,plural, =1{Downloaded {count} element.} few{Downloaded {count} "
"elements.} other{Downloaded {count} elements.}}"
msgstr ""
"{count,plural,=1{பதிவிறக்கம் {count} உறுப்பு.} few{பதிவிறக்கம் {count} "
"கூறுகள்.} other{பதிவிறக்கம் {count} கூறுகள்.}}"

#. Prefix for a list of icons for attributes missing in amenity.
#, icu-message-format
msgctxt "tileNo"
msgid "no"
msgstr "இல்லை"

#. First part of the message displayed when there is nothing around.
#, case-insensitive, icu-message-format, placeholders:$DISTANCE$
msgctxt "tileNothingAround"
msgid "No amenities around you in {distance} meters."
msgstr "{distance} மீட்டரில் உங்களைச் சுற்றியுள்ள வசதிகள் இல்லை."

#. Message displayed when there is nothing around.
#, icu-message-format
msgctxt "tileDragTheMap"
msgid ""
"Please move closer to anything, drag the map, or add a missing amenity."
msgstr ""
"தயவுசெய்து எதற்கும் நெருக்கமாக நகர்த்தவும், வரைபடத்தை இழுக்கவும் அல்லது "
"காணாமல் போன வசதியைச் சேர்க்கவும்."

#. Message displayed when there's nothing around, and a filter is on.
#. Message displayer when there's nothing around, and a filter is on.
#, icu-message-format
msgctxt "tileDragOrUnfilter"
msgid "Please move around, add an amenity, or clear the filter."
msgstr ""
"தயவுசெய்து நகர்த்தவும், ஒரு வசதியைச் சேர்க்கவும் அல்லது வடிப்பானை "
"அழிக்கவும்."

#. Message displayed when there is nothing around, and a download button is
#. present.
#, icu-message-format
msgctxt "tileDragDownload"
msgid "Try tapping the download button."
msgstr "பதிவிறக்க பொத்தானைத் தட்ட முயற்சிக்கவும்."

#. Legend item for all other types.
#, icu-message-format
msgctxt "legendOther"
msgid "Other"
msgstr "மற்றொன்று"

#. Tooltip for the upload data button on the nav panel.
#, icu-message-format
msgctxt "navUpload"
msgid "Upload"
msgstr "பதிவேற்றும்"

#. Tooltip for the download data button on the nav panel.
#, icu-message-format
msgctxt "navDownload"
msgid "Download"
msgstr "பதிவிறக்கம்"

#. Tooltip for the map / imagery button on the nav panel.
#, icu-message-format
msgctxt "navImagery"
msgid "Switch imagery"
msgstr "படங்களை மாற்றவும்"

#. Tooltip for the POI / amenity mode button on the nav panel.
#, icu-message-format
msgctxt "navPoiMode"
msgid "Amenity mode"
msgstr "வசதி முறை"

#. Tooltip for the micromapping mode button on the nav panel.
#, icu-message-format
msgctxt "navMicromappingMode"
msgid "Micromapping mode"
msgstr "மைக்ரோமேப்பிங் பயன்முறை"

#. Tooltip for the entrances mode button on the nav panel.
#, icu-message-format
msgctxt "navEntrancesMode"
msgid "Entrances mode"
msgstr "நுழைவாயில்கள் பயன்முறை"

#. Tooltip for the notes mode button on the nav panel.
#, icu-message-format
msgctxt "navNotesMode"
msgid "Notes mode"
msgstr "குறிப்புகள் பயன்முறை"

#. An error message for no addresses nearby, used on filter pane and in
#. address editor.
#, icu-message-format
msgctxt "filterNoAddresses"
msgid "No addresses nearby"
msgstr "அருகிலுள்ள முகவரிகள் இல்லை"

#. Filter pane, label for choosing an address to filter by.
#, icu-message-format
msgctxt "filterByAddress"
msgid "By address"
msgstr "முகவரி மூலம்"

#. Filter pane, label for choosing a floor to filter by.
#, icu-message-format
msgctxt "filterByFloor"
msgid "By floor"
msgstr "தரையில்"

#. Filter pane, an option in the lists to show only amenities with empty floor
#. or address.
#, icu-message-format
msgctxt "filterEmpty"
msgid "empty"
msgstr "காலி"

#. Filter pane, a switch for showing only amenities that were not confirmed.
#, icu-message-format
msgctxt "filterNonConfirmed"
msgid "Only non-confirmed"
msgstr "உறுதிப்படுத்தப்படாதது மட்டுமே"

#. Tooltip for the "hamburger" button on a map to open app settings.
#, icu-message-format
msgctxt "mapSettings"
msgid "Settings"
msgstr "அமைப்புகள்"

#. Tooltip for the amenity filter button on a map.
#, icu-message-format
msgctxt "mapFilter"
msgid "Filter"
msgstr "வடிப்பி"

#. Tooltip for the crosshair button on a map to navigate to the user's
#. location.
#, icu-message-format
msgctxt "mapLocate"
msgid "Back to me"
msgstr "என்னிடம் திரும்பி"

#. Tooltip for the zoom in (+) button on a map.
#, icu-message-format
msgctxt "mapZoomIn"
msgid "Zoom in"
msgstr "பெரிதாக்கு"

#. Tooltip for the zoom out (-) button on a map.
#, icu-message-format
msgctxt "mapZoomOut"
msgid "Zoom out"
msgstr "சிறிதாக்கு"

#. Title for the screen for choosing a location on a map.
#, icu-message-format
msgctxt "chooseLocation"
msgid "Choose location"
msgstr "இருப்பிடத்தைத் தேர்வுசெய்க"

#. Search field hint on the type choosing screen.
#, icu-message-format
msgctxt "chooseType"
msgid "Choose type"
msgstr "வகையைத் தேர்வுசெய்க"

#. Tooltip for a button to clear search field contents.
#, icu-message-format
msgctxt "chooseTypeClear"
msgid "Clear"
msgstr "தெளிவான"

#. Warning message for an amenity=fixme object with just a name.
#, icu-message-format
msgctxt "warningFixmeNote"
msgid "Will be converted to an OSM note"
msgstr "OSM குறிப்பாக மாற்றப்படும்"

#. Warning message for when an object has a non-empty fixme value.
#, case-insensitive, icu-message-format, ignore-multiple-failures, placeholders:$VALUE$
msgctxt "warningFixme"
msgid "Fixme: {value}"
msgstr "Fixme: {value}"

#. Warning message for tags like shop=yes and amenity=no.
#, case-insensitive, icu-message-format, placeholders:$TAG$
msgctxt "warningWrongTag"
msgid "Wrong tag: {tag}"
msgstr "தவறான குறிச்சொல்: {tag}"

#. Warning message for when an amenity is many years old.
#, case-insensitive, icu-message-format, placeholders:$YEARS$
msgctxt "warningTooOld"
msgid "Amenity is {years} old"
msgstr "வசதி {years} பழையது"

#. Warning message after choosing a preset for an unsupported object type.
#. Meaning these kinds of objects are not visible on the map.
#, icu-message-format
msgctxt "warningUnsupported"
msgid "This object type is not supported"
msgstr "இந்த பொருள் வகை ஆதரிக்கப்படவில்லை"

#. Warning when adding an amenity from the micromapping mode and vice-versa.
#, icu-message-format
msgctxt "warningWrongMode"
msgid "Wrong mode, check for duplicates"
msgstr "தவறான பயன்முறை, நகல்களை சரிபார்க்கவும்"

#. Number of years. Used in the "amenity is N years old" message.
#, case-insensitive, icu-message-format, ignore-icu-message-format, placeholders:$COUNT$
msgctxt "years"
msgid ""
"{count,plural, =1{{count} year} few{{count} years} other{{count} years}}"
msgstr ""
"{count,plural,=1{{count} ஆண்டு} few{{count} ஆண்டுகள்} other{{count} "
"ஆண்டுகள்}}"

#. Save button title in the editor
#, icu-message-format
msgctxt "editorSave"
msgid "Save"
msgstr "சேமி"

#. Tooltip for a button to mark the amenity checked without changing
#. properties.
#, icu-message-format
msgctxt "editorMarkChecked"
msgid "Mark checked"
msgstr "மார்க் சரிபார்க்கப்பட்டது"

#. Tooltip for a button to open version history pane.
#, icu-message-format
msgctxt "editorHistory"
msgid "History"
msgstr "வரலாறு"

#. Tooltip for a button to open tags pane.
#, icu-message-format
msgctxt "editorTags"
msgid "Raw tags"
msgstr "மூல குறிச்சொற்கள்"

#. Title for the message on closing the editor without saving changes.
#, icu-message-format
msgctxt "editorCloseTitle"
msgid "Close the editor?"
msgstr "எடிட்டரை மூடு?"

#. Message on closing the editor without saving changes.
#, icu-message-format
msgctxt "editorCloseMessage"
msgid "Discard changes without saving?"
msgstr "சேமிக்காமல் மாற்றங்களை நிராகரிக்கவா?"

#. Title of a additional fields group in editor.
#, icu-message-format
msgctxt "editorMoreFields"
msgid "More Fields"
msgstr "மேலும் புலங்கள்"

#. Message shown when loading a preset in the editor.
#, icu-message-format
msgctxt "editorLoadingPreset"
msgid "Loading preset…"
msgstr "முன்னமைவை ஏற்றுகிறது…"

#. Label for the map, a hint that you can move the object.
#, icu-message-format
msgctxt "editorMove"
msgid "Move"
msgstr "நகர்த்தவும்"

#. Message about a possible duplicate
#, case-insensitive, icu-message-format, placeholders:$DISTANCE$
msgctxt "editorDuplicate"
msgid "Possible duplicate in {distance} m"
msgstr "{distance} சாத்தியமான நகல்"

#. Button title to mark an amenity active (non-defunct).
#, icu-message-format
msgctxt "editorMarkActive"
msgid "Mark Active"
msgstr "மார்க் செயலில்"

#. Button title to mark an amenity defunct (disused, closed, but still there).
#, icu-message-format
msgctxt "editorMarkDefunct"
msgid "Mark Defunct"
msgstr "மார்க் செயலிழந்தது"

#. Button title to restore a deleted amenity (before it's uploaded and final).
#, icu-message-format
msgctxt "editorRestore"
msgid "Restore"
msgstr "மீட்டமை"

#. Button title to delete an amenity (since it's missing).
#, icu-message-format
msgctxt "editorMissing"
msgid "Does Not Exist"
msgstr "இல்லை"

#. Button title to mark a building demolished.
#, icu-message-format
msgctxt "editorDeleteBuilding"
msgid "Demolished"
msgstr "இடிக்கப்பட்டது"

#. Title for question on restoring an edited defunct amenity.
#, icu-message-format
msgctxt "editorRestoreTitle"
msgid "Is it open?"
msgstr "இது திறந்திருக்கிறதா?"

#. Question on restoring an edited defunct amenity.
#, case-insensitive, icu-message-format, placeholders:$AMENITY$
msgctxt "editorRestoreMessage"
msgid "{amenity} is inactive. Do you want to mark it active?"
msgstr "{amenity} செயலற்றது. நீங்கள் அதை செயலில் குறிக்க விரும்புகிறீர்களா?"

#. Dialog title for deleting an amenity.
#, case-insensitive, icu-message-format, placeholders:$AMENITY$
msgctxt "editorDeleteTitle"
msgid "Delete {amenity}?"
msgstr "{amenity} நீக்கு?"

#. Delete
#, icu-message-format
msgctxt "editorDeleteButton"
msgid "Delete"
msgstr "நீக்கு"

#. Button for keeping the address
#, icu-message-format
msgctxt "editorDeleteKeepAddressButton"
msgid "Delete, but keep the address"
msgstr "நீக்கு, ஆனால் முகவரியை வைத்திருங்கள்"

#. Tooltip for the sharing button on the tags panel.
#, icu-message-format
msgctxt "tagsShare"
msgid "Share"
msgstr "பங்கு"

#. Popup message when the link to the object was copied to the clipboard.
#, icu-message-format
msgctxt "tagsUrlCopied"
msgid "URL copied to clipboard"
msgstr "முகவரி இடைநிலைப்பலகைக்கு நகலெடுக்கப்பட்டது"

#. Tag editor panel title for a new amenity.
#, icu-message-format
msgctxt "tagsNewPoint"
msgid "New Point"
msgstr "புதிய புள்ளி"

#. Tooltip for a button next to a tag value to restore the value to the
#. original.
#, icu-message-format
msgctxt "tagsRestore"
msgid "Restore"
msgstr "மீட்டமை"

#. Tooltip for a button next to a tag value to delete the tag.
#, icu-message-format
msgctxt "tagsDelete"
msgid "Clear"
msgstr "தெளிவான"

#. Tags panel button for adding a new tag.
#, icu-message-format
msgctxt "tagsAddTag"
msgid "Add a tag"
msgstr "ஒரு குறிச்சொல்லைச் சேர்க்கவும்"

#. Tags panel, add a new tag dialog: key label
#, icu-message-format
msgctxt "tagsKey"
msgid "Key"
msgstr "விசை"

#. Tags panel, add a new tag dialog: value label
#, icu-message-format
msgctxt "tagsValue"
msgid "Value"
msgstr "மதிப்பு"

#. Validation error for when an entered key is not in the top 5000 keys from
#. taginfo.
#, icu-message-format
msgctxt "tagsKeyError"
msgid "Key is too rare"
msgstr "விசை மிகவும் அரிதானது"

#. Validation error for when the value length exceeds 255 characters.
#, icu-message-format
msgctxt "tagsValueLong"
msgid "Value is too long"
msgstr "மதிப்பு மிக நீளமானது"

#. Title for the element versions page.
#, case-insensitive, icu-message-format, placeholders:$ELEMENTREF$
msgctxt "versionsTitle"
msgid "{elementRef} history"
msgstr "{elementRef} வரலாறு"

#. Message for when fetching element history fails due to lack of network
#. connection.
#, icu-message-format
msgctxt "versionsEnableInternet"
msgid "Connect to the internet to fetch version history"
msgstr "பதிப்பு வரலாற்றைப் பெற இணையத்துடன் இணைக்கவும்"

#. Message for when fetching element history fails due to unknown error.
#, icu-message-format
msgctxt "versionsFetchError"
msgid "Error fetching version history"
msgstr "பதிப்பு வரலாற்றைப் பெறுவதில் பிழை"

#. Button label to retry fetching history.
#, icu-message-format
msgctxt "versionsRetryFetch"
msgid "Retry"
msgstr "மீண்டும் முயற்சிக்கவும்"

#. Card header for the local changes diff.
#, icu-message-format
msgctxt "versionsLocalChanges"
msgid "Local changes"
msgstr "உள்ளக மாற்றங்கள்"

#. Card header for the diff for this element version.
#, case-insensitive, icu-message-format, placeholders:$NUMBER$
msgctxt "versionsVersionNumber"
msgid "Version #{number}"
msgstr "பதிப்பு #{number}"

#. Card body to show metadata about the element version.
#, case-insensitive, icu-message-format, placeholders:$USER$:$DATETIME$
msgctxt "versionsVersionMeta"
msgid "by {user} at {datetime}"
msgstr "{user} இல் {datetime}"

#. Card body for when no tag changes occurred in this version.
#, icu-message-format
msgctxt "versionsNoTagChanges"
msgid "No tag changes"
msgstr "குறிச்சொல் மாற்றங்கள் இல்லை"

#. App bar title for the settings panel.
#, icu-message-format
msgctxt "settingsTitle"
msgid "Configuration"
msgstr "உள்ளமைவு"

#. Section title for API server settings.
#, icu-message-format
msgctxt "settingsApiServer"
msgid "API Server"
msgstr "பநிஇ சேவையகம்"

#. Section title for data settings.
#, icu-message-format
msgctxt "settingsDataManagement"
msgid "Data Management"
msgstr "தரவு மேலாண்மை"

#. Section title for presentation options.
#, icu-message-format
msgctxt "settingsPresentation"
msgid "Presentation"
msgstr "விளக்கக்காட்சி"

#. Settings entry for logging in to OSM.
#, icu-message-format
msgctxt "settingsLoginDetails"
msgid "OSM Account"
msgstr "OSM கணக்கு"

#. Settings entry for editing changeset hashtags.
#, icu-message-format
msgctxt "settingsHashtags"
msgid "Changeset Hashtags"
msgstr "மாற்றுதல் ஏச்டேக்குகள்"

#. Settings entry for purging obsolete OSM data.
#, icu-message-format
msgctxt "settingsPurgeData"
msgid "Purge obsolete cached data"
msgstr "வழக்கற்றுப் போன தற்காலிக சேமிப்பு தரவை தூய்மைப்படுத்துங்கள்"

#. Settings entry for purging all cached OSM data.
#, icu-message-format
msgctxt "settingsPurgeAllData"
msgid "Purge all downloaded data"
msgstr "பதிவிறக்கம் செய்யப்பட்ட அனைத்து தரவையும் தூய்மைப்படுத்துங்கள்"

#. Confirmation dialog title for when purging all downloaded data.
#, icu-message-format
msgctxt "settingsPurgeDataTitle"
msgid "Delete Data?"
msgstr "தரவை நீக்கவா?"

#. Confirmation dialog message for when purging all downloaded data.
#, icu-message-format
msgctxt "settingsPurgeDataMessage"
msgid "You will have to re-download everything. Proceed?"
msgstr "நீங்கள் எல்லாவற்றையும் மீண்டும் ஏற்ற வேண்டும். தொடரவா?"

#. Notification title for cleaning obsolete data from the cache.
#, icu-message-format
msgctxt "settingsPurgedObsoleteTitle"
msgid "Obsolete Data"
msgstr "வழக்கற்றுப் போன தரவு"

#. Notification title for cleaning all downloaded data from the cache.
#, icu-message-format
msgctxt "settingsPurgedAllTitle"
msgid "Deleted Data"
msgstr "தரவு நீக்கப்பட்டது"

#. Notification message after some cached elements were deleted.
#, case-insensitive, icu-message-format, ignore-icu-message-format, placeholders:$COUNT$
msgctxt "settingsPurgedMessage"
msgid ""
"{count,plural, =1{Purged {count} element} few{Purged {count} elements} "
"other{Purged {count} elements}}"
