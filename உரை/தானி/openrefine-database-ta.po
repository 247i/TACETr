msgctxt "database-import/importer-name"
msgid "Database"
msgstr "தரவுத்தளம்"

msgctxt "database-import/title"
msgid "Database Servers"
msgstr "தரவுத்தள சேவையகங்கள்"

msgctxt "database-import/preparing"
msgid "Preparing result …"
msgstr "முடிவைத் தயாரித்தல்…"

msgctxt "database-import/checking"
msgid "Validating query …"
msgstr "வினவலை சரிபார்க்கிறது…"

msgctxt "database-import/creating"
msgid "Creating project …"
msgstr "திட்டத்தை உருவாக்குதல்…"

msgctxt "database-source/alert-host"
msgid "You must specify a database host"
msgstr "நீங்கள் ஒரு தரவுத்தள ஓச்டைக் குறிப்பிட வேண்டும்"

msgctxt "database-source/alert-port"
msgid "You must specify a database port"
msgstr "நீங்கள் ஒரு தரவுத்தள போர்ட்டைக் குறிப்பிட வேண்டும்"

msgctxt "database-source/alert-user"
msgid "You must specify a database user"
msgstr "தரவுத்தள பயனரை நீங்கள் குறிப்பிட வேண்டும்"

msgctxt "database-source/alert-password"
msgid "You must specify a database password"
msgstr "தரவுத்தள கடவுச்சொல்லை நீங்கள் குறிப்பிட வேண்டும்"

msgctxt "database-source/alert-connection-name"
msgid "You must specify a connection name"
msgstr "இணைப்பு பெயரை நீங்கள் குறிப்பிட வேண்டும்"

msgctxt "database-source/alert-initial-database"
msgid "You must specify an initial database"
msgstr "ஆரம்ப தரவுத்தளத்தை நீங்கள் குறிப்பிட வேண்டும்"

msgctxt "database-source/alert-query"
msgid "You must specify a valid query"
msgstr "செல்லுபடியாகும் வினவலைக் குறிப்பிட வேண்டும்"

msgctxt "database-source/alert-invalid-query-select"
msgid "Query must start with SELECT Keyword"
msgstr "வினவல் தேர்ந்தெடுக்கப்பட்ட முக்கிய சொல்லுடன் தொடங்க வேண்டும்"

msgctxt "database-source/form-validation-failure"
msgid "New connection form invalid!"
msgstr "புதிய இணைப்பு படிவம் தவறானது!"

msgctxt "database-source/alert-connection-edit"
msgid "Connection edited"
msgstr "இணைப்பு திருத்தப்பட்டது"

msgctxt "database-source/connectionNameLabel"
msgid "Name"
msgstr "பெயர்"

msgctxt "database-source/databaseTypeLabel"
msgid "Type"
msgstr "வகை"

msgctxt "database-source/databaseHostLabel"
msgid "Host"
msgstr "விருந்தோம்பி"

msgctxt "database-source/databasePortLabel"
msgid "Port"
msgstr "துறைமுகம்"

msgctxt "database-source/databaseUserLabel"
msgid "User"
msgstr "பயனர்"

msgctxt "database-source/databasePasswordLabel"
msgid "Password"
msgstr "கடவுச்சொல்"

msgctxt "database-source/databaseNameLabel"
msgid "Database name"
msgstr "தரவுத்தள பெயர்"

msgctxt "database-source/databaseFileNameLabel"
msgid "Database file"
msgstr "தரவுத்தள கோப்பு"

msgctxt "database-source/databaseSchemaLabel"
msgid "Schema"
msgstr "திட்டம்"

msgctxt "database-source/connectionNamePlaceholder"
msgid "Enter a new Connection Name"
msgstr "புதிய இணைப்பு பெயரை உள்ளிடவும்"

msgctxt "database-source/databaseHostPlaceholder"
msgid "Enter the database server domain or IP"
msgstr "தரவுத்தள சேவையக டொமைன் அல்லது ஐபி உள்ளிடவும்"

msgctxt "database-source/databasePortPlaceholder"
msgid "Enter the database port e.g. 3306"
msgstr "தரவுத்தள துறைமுகம் எ.கா. 3306"

msgctxt "database-source/databaseUserPlaceholder"
msgid "Enter the database user"
msgstr "தரவுத்தள பயனரை உள்ளிடவும்"

msgctxt "database-source/databasePasswordPlaceholder"
msgid "Enter the database password"
msgstr "தரவுத்தள கடவுச்சொல்லை உள்ளிடவும்"

msgctxt "database-source/databaseNamePlaceholder"
msgid "Enter the name of the database"
msgstr "தரவுத்தளத்தின் பெயரை உள்ளிடவும்"

msgctxt "database-source/databaseFileNamePlaceholder"
msgid "Enter the full path to the database file"
msgstr "தரவுத்தள கோப்பிற்கு முழு பாதையை உள்ளிடவும்"

msgctxt "database-source/databaseSchemaPlaceholder"
msgid "Enter the initial schema of the database"
msgstr "தரவுத்தளத்தின் ஆரம்ப திட்டத்தை உள்ளிடவும்"

msgctxt "database-source/connectionNameDefaultValue"
msgid "New_Connection_Name"
msgstr "New_connection_name"

msgctxt "database-source/databaseTestButton"
msgid "Test"
msgstr "தேர்வு"

msgctxt "database-source/databaseSaveButton"
msgid "Save"
msgstr "சேமி"

msgctxt "database-source/databaseConnectButton"
msgid "Connect"
msgstr "இணை"

msgctxt "database-source/newConnectionButtonDiv"
msgid "New connection"
msgstr "புதிய இணைப்பு"

msgctxt "database-source/new-connection-legend"
msgid "New Connection Editor"
msgstr "புதிய இணைப்பு ஆசிரியர்"

msgctxt "database-source/savedConnectionSpan"
msgid "Saved connections"
msgstr "சேமித்த இணைப்புகள்"

msgctxt "database-parsing/start-over"
msgid "&laquo; Start over"
msgstr "& லாகோ; தொடக்க"

msgctxt "database-parsing/conf-pars"
msgid "Configure parsing options"
msgstr "பாகுபடுத்தும் விருப்பங்களை உள்ளமைக்கவும்"

msgctxt "database-parsing/proj-name"
msgid "Project&nbsp;name"
msgstr "திட்டம் & nbsp; பெயர்"

msgctxt "database-parsing/create-proj"
msgid "Create Project &raquo;"
msgstr "திட்டத்தை உருவாக்கு &raquo;"

msgctxt "database-parsing/updating-preview"
msgid "Updating preview …"
msgstr "முன்னோட்டத்தைப் புதுப்பித்தல்…"

msgctxt "database-parsing/worksheet"
msgid "Worksheets"
msgstr "பணித்தாள்"

msgctxt "database-parsing/option"
msgid "Options"
msgstr "விருப்பங்கள்"

msgctxt "database-parsing/preview-button"
msgid "Update&nbsp;preview"
msgstr "புதுப்பிப்பு & nbsp; முன்னோட்டம்"

msgctxt "database-parsing/disable-auto-preview"
msgid "Disable auto preview"
msgstr "ஆட்டோ முன்னோட்டத்தை முடக்கு"

msgctxt "database-parsing/ignore-first"
msgid "Ignore first"
msgstr "முதலில் புறக்கணிக்கவும்"

msgctxt "database-parsing/ignore"
msgid "line(s) at beginning of file"
msgstr "கோப்பின் தொடக்கத்தில் வரி (கள்)"

msgctxt "database-parsing/parse-next"
msgid "Parse next"
msgstr "அடுத்து பாகுபடுத்துங்கள்"

msgctxt "database-parsing/parse"
msgid "line(s) as column headers"
msgstr "நெடுவரிசை தலைப்புகளாக வரி (கள்)"

msgctxt "database-parsing/discard-next"
msgid "Discard initial"
msgstr "ஆரம்ப நிராகரிக்கவும்"

msgctxt "database-parsing/discard"
msgid "row(s) of data"
msgstr "தரவின் வரிசை (கள்)"

msgctxt "database-parsing/limit-next"
msgid "Load at most"
msgstr "அதிகபட்சம் ஏற்றவும்"

msgctxt "database-parsing/limit"
msgid "row(s) of data"
msgstr "தரவின் வரிசை (கள்)"

msgctxt "database-parsing/store-row"
msgid "Store blank rows"
msgstr "வெற்று வரிசைகளை சேமிக்கவும்"

msgctxt "database-parsing/store-cell"
msgid "Store blank cells as nulls"
msgstr "வெற்று செல்களை பூச்யமாக சேமிக்கவும்"

msgctxt "database-source/alert-conn-name-invalid-character"
msgid ""
"Connection Name Input Error: Illegal Character in Input. Only [a-zA-Z0-9._-] "
"Allowed"
msgstr ""
"இணைப்பு பெயர் உள்ளீட்டு பிழை: உள்ளீட்டில் சட்டவிரோத எழுத்து. [A-ZA-Z0-9 ._-] "
"அனுமதிக்கப்படுகிறது"

msgctxt "database-source/alert-db-host-invalid-character"
msgid ""
"Database Host Error: Illegal Character in Input. Only Alphanumeric "
"characters allowed"
msgstr ""
"தரவுத்தள புரவலன் பிழை: உள்ளீட்டில் சட்டவிரோத எழுத்து. எண்ணெழுத்து எழுத்துக்கள் மட்டுமே "
"அனுமதிக்கப்படுகின்றன"

msgctxt "database-source/alert-db-user-invalid-character"
msgid ""
"Database User Error: Illegal Character in Input. Only Alphanumeric "
"characters allowed"
msgstr ""
"தரவுத்தள பயனர் பிழை: உள்ளீட்டில் சட்டவிரோத எழுத்து. எண்ணெழுத்து எழுத்துக்கள் மட்டுமே "
"அனுமதிக்கப்படுகின்றன"

msgctxt "database-source/alert-db-port-invalid-character"
msgid ""
"Database Port Error: Illegal Character in Input. Only Numeric values allowed."
msgstr ""
"தரவுத்தள துறைமுகம் பிழை: உள்ளீட்டில் சட்டவிரோத எழுத்து. எண் மதிப்புகள் மட்டுமே "
"அனுமதிக்கப்படுகின்றன."
