#, icu-message-format
msgctxt "flags"
msgid "Flags"
msgstr "கொடிகள்"

#, icu-message-format
msgctxt "fwupdDeviceFlagInternal"
msgid "Internal device"
msgstr "உள் சாதனம்"

#, icu-message-format
msgctxt "fwupdDeviceFlagUpdatable"
msgid "Updatable"
msgstr "புதுப்பிக்கத்தக்கது"

#, icu-message-format
msgctxt "fwupdDeviceFlagOnlyOffline"
msgid "Update requires a reboot"
msgstr "புதுப்பிப்புக்கு மறுதொடக்கம் தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagRequireAc"
msgid "System requires external power source"
msgstr "கணினிக்கு வெளிப்புற ஆற்றல் மூல தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagLocked"
msgid "Device is locked"
msgstr "சாதனம் பூட்டப்பட்டுள்ளது"

#, icu-message-format
msgctxt "fwupdDeviceFlagSupported"
msgid "Supported on remote server"
msgstr "தொலை சேவையகத்தில் ஆதரிக்கப்படுகிறது"

#, icu-message-format
msgctxt "fwupdDeviceFlagNeedsBootloader"
msgid "Requires a bootloader"
msgstr "துவக்க ஏற்றி தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagRegistered"
msgid "Registered"
msgstr "பதிவுசெய்யப்பட்டது"

#, icu-message-format
msgctxt "fwupdDeviceFlagNeedsReboot"
msgid "Needs a reboot after installation"
msgstr "நிறுவலுக்குப் பிறகு மறுதொடக்கம் தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagNeedsShutdown"
msgid "Needs shutdown after installation"
msgstr "நிறுவலுக்குப் பிறகு பணிநிறுத்தம் தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagReported"
msgid "Reported to remote server"
msgstr "தொலை சேவையகத்திற்கு அறிக்கை"

#, icu-message-format
msgctxt "fwupdDeviceFlagNotified"
msgid "User has been notified"
msgstr "பயனருக்கு அறிவிக்கப்பட்டுள்ளது"

#, icu-message-format
msgctxt "fwupdDeviceFlagInstallParentFirst"
msgid "Install to parent device first"
msgstr "முதலில் பெற்றோர் சாதனத்தில் நிறுவவும்"

#, icu-message-format
msgctxt "fwupdDeviceFlagIsBootloader"
msgid "Is in bootloader mode"
msgstr "துவக்க ஏற்றி பயன்முறையில் உள்ளது"

#, icu-message-format
msgctxt "fwupdDeviceFlagWaitForReplug"
msgid "Hardware is waiting to be replugged"
msgstr "வன்பொருள் மாற்றப்படுவதற்கு காத்திருக்கிறது"

#, icu-message-format
msgctxt "fwupdDeviceFlagIgnoreValidation"
msgid "Ignore validation safety checks"
msgstr "சரிபார்ப்பு பாதுகாப்பு சோதனைகளை புறக்கணிக்கவும்"

#, icu-message-format
msgctxt "fwupdDeviceFlagTrusted"
msgid "Trusted"
msgstr "நம்பகமான"

#, icu-message-format
msgctxt "fwupdDeviceFlagNeedsActivation"
msgid "Device update needs activation"
msgstr "சாதன புதுப்பிப்புக்கு செயல்படுத்தல் தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagWillDisappear"
msgid "Device will not re-appear after update completes"
msgstr "புதுப்பிப்பு முடிந்ததும் சாதனம் மீண்டும் தோன்றாது"

#, icu-message-format
msgctxt "fwupdDeviceFlagCanVerify"
msgid "Cryptographic hash verification is available"
msgstr "கிரிப்டோகிராஃபிக் ஆச் சரிபார்ப்பு கிடைக்கிறது"

#, icu-message-format
msgctxt "fwupdDeviceFlagDualImage"
msgid "Device stages updates"
msgstr "சாதன நிலைகள் புதுப்பிப்புகள்"

#, icu-message-format
msgctxt "fwupdDeviceFlagSelfRecovery"
msgid "Device can recover flash failures"
msgstr "சாதனம் ஃபிளாச் தோல்விகளை மீட்டெடுக்க முடியும்"

#, icu-message-format
msgctxt "fwupdDeviceFlagUsableDuringUpdate"
msgid "Device is usable for the duration of the update"
msgstr "புதுப்பிப்பின் காலத்திற்கு சாதனம் பயன்படுத்தக்கூடியது"

#, icu-message-format
msgctxt "fwupdDeviceFlagVersionCheckRequired"
msgid "Device firmware is required to have a version check"
msgstr "பதிப்பு காசோலை வைத்திருக்க சாதன நிலைபொருள் தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagInstallAllReleases"
msgid "Device is required to install all provided releases"
msgstr "வழங்கப்பட்ட அனைத்து வெளியீடுகளையும் நிறுவ சாதனம் தேவை"

#, icu-message-format
msgctxt "fwupdDeviceFlagHasMultipleBranches"
msgid "Device supports switching to a different branch of firmware"
msgstr "சாதனம் ஃபார்ம்வேரின் வேறு கிளைக்கு மாறுவதை ஆதரிக்கிறது"

#, icu-message-format
msgctxt "fwupdDeviceFlagBackupBeforeInstall"
msgid "Device will backup firmware before installing"
msgstr "நிறுவும் முன் சாதனம் ஃபார்ம்வேரை காப்புப் பிரதி எடுக்கும்"

#, icu-message-format
msgctxt "fwupdDeviceFlagWildcardInstall"
msgid "All devices of the same type will be updated at the same time"
msgstr "ஒரே வகையின் அனைத்து சாதனங்களும் ஒரே நேரத்தில் புதுப்பிக்கப்படும்"

#, icu-message-format
msgctxt "fwupdDeviceFlagOnlyVersionUpgrade"
msgid "Only version upgrades are allowed"
msgstr "பதிப்பு மேம்படுத்தல்கள் மட்டுமே அனுமதிக்கப்படுகின்றன"

#, icu-message-format
msgctxt "fwupdDeviceFlagUnreachable"
msgid "Device is unreachable"
msgstr "சாதனம் அணுக முடியாதது"

#, icu-message-format
msgctxt "fwupdDeviceFlagAffectsFde"
msgid "Full disk encryption secrets may be invalidated when updating"
msgstr "புதுப்பிக்கும்போது முழு வட்டு குறியாக்க ரகசியங்கள் செல்லுபடியாகாது"

#, icu-message-format
msgctxt "fwupdDeviceFlagEndOfLife"
msgid "End of life"
msgstr "வாழ்க்கையின் முடிவு"

#, icu-message-format
msgctxt "fwupdDeviceFlagSignedPayload"
msgid "Signed Payload"
msgstr "கையொப்பமிடப்பட்ட பேலோட்"

#, icu-message-format
msgctxt "fwupdDeviceFlagUnsignedPayload"
msgid "Unsigned Payload"
msgstr "கையொப்பமிடாத பேலோட்"

#, icu-message-format
msgctxt "fwupdErrorInternal"
msgid "Internal error"
msgstr "உள் பிழை"

#, icu-message-format
msgctxt "fwupdErrorVersionNewer"
msgid "Installed newer firmware version"
msgstr "புதிய ஃபார்ம்வேர் பதிப்பை நிறுவியது"

#, icu-message-format
msgctxt "fwupdErrorVersionSame"
msgid "Installed same firmware version"
msgstr "அதே ஃபார்ம்வேர் பதிப்பை நிறுவியது"

#, icu-message-format
msgctxt "fwupdErrorAlreadyPending"
msgid "Already set to be installed offline"
msgstr "ஏற்கனவே ஆஃப்லைனில் நிறுவப்பட உள்ளது"

#, icu-message-format
msgctxt "fwupdErrorAuthFailed"
msgid "Failed to get authentication"
msgstr "அங்கீகாரத்தைப் பெறுவதில் தோல்வி"

#, icu-message-format
msgctxt "fwupdErrorRead"
msgid "Failed to read from device"
msgstr "சாதனத்திலிருந்து படிக்கத் தவறிவிட்டது"

#, icu-message-format
msgctxt "fwupdErrorWrite"
