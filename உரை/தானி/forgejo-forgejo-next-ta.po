msgctxt "relativetime.1week"
msgid "last week"
msgstr "கடந்த வாரம்"

msgctxt "relativetime.2weeks"
msgid "two weeks ago"
msgstr "இரண்டு வாரங்களுக்கு முன்பு"

msgctxt "relativetime.1month"
msgid "last month"
msgstr "கடந்த மாதம்"

msgctxt "relativetime.2months"
msgid "two months ago"
msgstr "இரண்டு மாதங்களுக்கு முன்பு"

msgctxt "relativetime.1year"
msgid "last year"
