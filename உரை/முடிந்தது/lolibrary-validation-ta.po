#. /*
#.     |--------------------------------------------------------------------------
#.     | Validation Language Lines
#.     |--------------------------------------------------------------------------
#.     |
#.     | The following language lines contain the default error messages used by
#.     | the validator class. Some of these rules have multiple versions such
#.     | as the size rules. Feel free to tweak each of these messages here.
#.     |
#.     */
#, placeholders:r":[\w\d]+"
msgctxt "return[]->'array'"
msgid "The :attribute must be an array."
msgstr "அந்த :attribute ஒரு வரிசையாக இருக்க வேண்டும்."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'confirmed'"
msgid "The :attribute confirmation does not match."
msgstr "அந்த :attribute உறுதிப்படுத்தல் பொருந்தவில்லை."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'email'"
msgid "The :attribute must be a valid email address."
msgstr "அந்த :attribute சரியான மின்னஞ்சல் முகவரியாக இருக்க வேண்டும்."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'integer'"
msgid "The :attribute must be an integer."
msgstr "அந்த :attribute ஒரு முழு எண்ணாக இருக்க வேண்டும்."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'max'[]->'string'"
msgid "The :attribute may not be greater than :max characters."
msgstr "அந்த :attribute :max எழுத்துக்கள் விட அதிகமாக இருக்காது."

#. // 'numeric' => 'The :attribute may not be greater than :max.',
#. // 'file' => 'The :attribute may not be greater than :max kilobytes.',
#. // 'array' => 'The :attribute may not have more than :max items.',
#, placeholders:r":[\w\d]+"
msgctxt "return[]->'min'[]->'string'"
msgid "The :attribute must be at least :min characters."
msgstr "அந்த :attribute குறைந்தபட்சம் :min எழுத்துக்கள் இருக்க வேண்டும்."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'min'[]->'numeric'"
msgid "The :attribute must be at least :min."
msgstr "அந்த :attribute குறைந்தபட்சம் :min இருக்க வேண்டும்."

#. // 'file' => 'The :attribute must be at least :min kilobytes.',
#. // 'array' => 'The :attribute must have at least :min items.',
#, placeholders:r":[\w\d]+"
msgctxt "return[]->'regex'"
msgid "The :attribute format is invalid."
msgstr "அந்த :attribute வடிவம் தவறானது."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'required'"
msgid "The :attribute field is required."
msgstr "அந்த :attribute புலம் தேவை."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'string'"
msgid "The :attribute must be a string."
msgstr "அந்த :attribute ஒரு சரமாக இருக்க வேண்டும்."

#, placeholders:r":[\w\d]+"
msgctxt "return[]->'unique'"
msgid "The :attribute has already been taken."
msgstr "அந்த :attribute ஏற்கனவே எடுக்கப்பட்டுள்ளது."
