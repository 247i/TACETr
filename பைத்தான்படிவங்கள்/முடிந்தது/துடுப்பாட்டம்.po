msgid "air-borne"
msgstr "அந்தரத்தில்"

msgid "all-rounder"
msgstr "சகலத்துறையர், முழுவல்லார்"

msgid "appeal"
msgstr "முறையீடு"

msgid "asking rate"
msgstr "வேட்பீட்டுகை"

msgid "attacking field"
msgstr "இறுக்கக் களம்"

msgid "attacking shot"
msgstr "தாக்கடி"

msgid "back-foot"
msgstr "பின்கால்"

msgid "bail(s)"
msgstr "தண்டு(கள்), குருத்து(க்கள்)"

msgid "batsman"
msgstr "மட்டையாளர், மட்டையர், மட்டையாள்"

msgid "batting average"
msgstr "மட்டையாட்டச் சராசரி"

msgid "beamer"
msgstr "தலைவீச்சு"

msgid "bouncer"
msgstr "குத்துவீச்சு"

msgid "boundary (rope)"
msgstr "எல்லை(க்கயிறு)"

msgid "boundary (run)"
msgstr "எல்லையடி, கயிறடி"

msgid "bowler"
msgstr "பந்துவீச்சாளர், பந்தாள்"

msgid "catch (n., v)"
msgstr "பிடிப்பு, பிடி"

msgid "cover-drive"
msgstr "மறையவடி"

msgid "covers"
msgstr "மறையம்"

msgid "doosra"
msgstr "பிறழ்சுழல்வீச்சு"

msgid "drive (v., n.)"
msgstr "செலுத்தியடி (வினை வேற்சொல்), செலுத்தடி"

msgid "fine leg"
msgstr "சரிவியம்"

msgid "four"
msgstr "நான்கு, நாலடி, நான்கடி"

msgid "gully"
msgstr "கொல்லையம்"

msgid "hook (v., n.)"
msgstr "கொக்கியடி, சுழற்றியடி (வினை வேற்சொல்), கொக்கியடி, சுழலடி"

msgid "inswinger"
msgstr "உள்நாட்ட வீச்சு"

msgid "leg before wicket"
msgstr "குச்சம் முன் கால்"

msgid "leg-spin"
msgstr "காற்சுழல்(வீச்சு), வெளிச்சுழல்(வீச்சு)"

msgid "leg-spinner"
msgstr "காற்சுழல்வீச்சாளர், வெளிச் சுழல்வீச்சாளர்"

msgid "long-off"
msgstr "தொலை விலகம்"

msgid "long-on"
msgstr "தொலை நெருங்கம்"

msgid "maiden over"
msgstr "வெற்றலகு"

msgid "maiden wicket"
msgstr "வீழ்வெற்றலகு"

msgid "mid-off"
msgstr "நடு விலகம்"

msgid "mid-on"
msgstr "நடு நெருங்கம்"

msgid "off-side"
msgstr "விலகுபுறம்"

msgid "off-spin"
msgstr "விலகுசுழல்(வீச்சு)"

msgid "off-spinner"
msgstr "விலகுசுழல்வீச்சாளர்"

msgid "on-side"
msgstr "நெருங்குகுபுறம்"

msgid "outswinger"
msgstr "வெளிநாட்ட வீச்சு"

msgid "over"
msgstr "வீச்சலகு"

msgid "pace bowling"
msgstr "வேக வீச்சு"

msgid "point"
msgstr "புள்ளியம்"

msgid "pull (v., n.)"
msgstr "இழுத்தடி (வினை வேற்சொல்), இழுப்படி"

msgid "run-rate"
msgstr "ஓட்ட ஈட்டுகை"

msgid "runout"
msgstr "ஓட்டமிழப்பு, ஓடுபலி"

msgid "score"
msgstr "கெலிப்பெண்"

msgid "score-board"
msgstr "கெலிப்புப்பலகை"

msgid "score-card"
msgstr "கெலிப்பட்டை"

msgid "silly mid-off"
msgstr "அருகு நடு விலகம்"

msgid "silly point"
msgstr "அருகுப் புள்ளியம்"

msgid "six(er)"
msgstr "ஆறு, ஆறடி"

msgid "slip(s)"
msgstr "நழுவியம்(ங்கள்)"

msgid "square cut"
msgstr "சதுரவடி"

msgid "square leg"
msgstr "சதுரியம்"

msgid "sweep shot"
msgstr "பெருக்கடி, பெருக்கலடி, துடுப்பு வலிப்படி"

msgid "target (score)"
msgstr "இலக்கு"

msgid "teesra"
msgstr "மாற்றான்சுழல் வீச்சு"

msgid "third man"
msgstr "மூன்றம்"

msgid "wicket (loss of wicket, second wicket)"
msgstr "மட்டையிலக்கு"

msgid "wicketkeeper"
msgstr "குச்சக்காப்பாளர், முக்குச்சியாளர், முக்குச்சிக்காரர்"

msgid "york (v.)"
msgstr "பள்ளத்தாக்கு, நேர்க்கூர் வீசு"

msgid "yorked"
msgstr "பள்ளத்தாகப்படு"

msgid "yorker"
msgstr "பள்ளப்பந்து, நேர்க்கூர் வீச்சு"
